{
    "Txt_Name": "Link_107",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "はぁ……。どこかへ行きたい……。",
            "voice": "link/Link_107/link_107_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "わかるー。体を、思いっっきり動かしたい。",
            "voice": "link/Link_107/link_107_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "最近さ、全然走りに行けてないんだよね。　",
            "voice": "link/Link_107/link_107_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "雨続きだからねー。あたしもどっかでスッキリしたいなー。\nパーっと汗かいてさ！",
            "voice": "link/Link_107/link_107_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "行こうよ！\n今日は、午後から晴れだったはず。　",
            "voice": "link/Link_107/link_107_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "本当？　どこ行く？　やっぱ海！？　",
            "voice": "link/Link_107/link_107_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そこは山でしょ！　ハイキング！\n大森林の空気を浴びて、絶景を見るの！",
            "voice": "link/Link_107/link_107_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "海の方が絶対良いって！\n潮風を感じながら、波の上を走ろうよ。サーフィン！",
            "voice": "link/Link_107/link_107_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "山！",
            "voice": "link/Link_107/link_107_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "海！",
            "voice": "link/Link_107/link_107_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "……わかった。\nじゃあ、間を取ろう。　",
            "voice": "link/Link_107/link_107_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "間、って、陸？　",
            "voice": "link/Link_107/link_107_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "いいや、空だよ！",
            "voice": "link/Link_107/link_107_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "空って……、何があったっけ？　",
            "voice": "link/Link_107/link_107_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "それはね、ズバリ。\nスカイダイビング！　",
            "voice": "link/Link_107/link_107_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "スカイダイビング……。　",
            "voice": "link/Link_107/link_107_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "いいね。すごくいいね、それ！　",
            "voice": "link/Link_107/link_107_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたし、やってみたいことがあってさ。　",
            "voice": "link/Link_107/link_107_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "やってみたいこと？　",
            "voice": "link/Link_107/link_107_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "スカイダイビングって、痩せるんだって。\n前にテレビで見た。",
            "voice": "link/Link_107/link_107_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "なんだって。……本当なの？　",
            "voice": "link/Link_107/link_107_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "さあ？　検証してみたかったんだよねー。\nこれで痩せたら、サイコーじゃん？",
            "voice": "link/Link_107/link_107_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "行こう。今すぐ行こう！\n出発できるね？　",
            "voice": "link/Link_107/link_107_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "えっ？\nう、うん、いいけど……。どうしたの急に？　",
            "voice": "link/Link_107/link_107_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "準備はいいね？　さっさと行こう！　",
            "voice": "link/Link_107/link_107_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "えっ！　準備って、まだ何も……。\nええっ！？　ちょっと、ほのかー！？",
            "voice": "link/Link_107/link_107_026.mp3"
        }
    ]
}