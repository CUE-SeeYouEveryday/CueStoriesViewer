{
    "Txt_Name": "Event_012_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ほら、やっぱり、海の家！　結構混んでるなー。",
            "voice": "event/event_012/Event_012_03/event_012_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "今日は暑いもんね。\nみんな考えることは同じだよ。",
            "voice": "event/event_012/Event_012_03/event_012_03_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "くんくん。\nソースの焼ける匂い……、これは焼きそばだな。",
            "voice": "event/event_012/Event_012_03/event_012_03_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "焼きそば……。　",
            "voice": "event/event_012/Event_012_03/event_012_03_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ラーメンにカレー、ホットドッグ……。　",
            "voice": "event/event_012/Event_012_03/event_012_03_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "でも、まだお昼にはちょっと早いよね……。　",
            "voice": "event/event_012/Event_012_03/event_012_03_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……。　",
            "voice": "event/event_012/Event_012_03/event_012_03_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "舞花？　",
            "voice": "event/event_012/Event_012_03/event_012_03_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "これは、その……。　",
            "voice": "event/event_012/Event_012_03/event_012_03_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ちゃんと朝ご飯を食べないからだぞ、舞花。\n私はしっかり食べてきた。",
            "voice": "event/event_012/Event_012_03/event_012_03_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "いや、そういうのじゃなくて！　",
            "voice": "event/event_012/Event_012_03/event_012_03_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ふふ、すごくいい匂いだもんね。　",
            "voice": "event/event_012/Event_012_03/event_012_03_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "わたしも……。\nここにずっといたら、食べたくなってきちゃったなぁって……。",
            "voice": "event/event_012/Event_012_03/event_012_03_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "え？　陽菜も？　",
            "voice": "event/event_012/Event_012_03/event_012_03_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "えへへ……、ダメかな？",
            "voice": "event/event_012/Event_012_03/event_012_03_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "うーん……。\nじゃあ、ちょっと早めのお昼にしようか？",
            "voice": "event/event_012/Event_012_03/event_012_03_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "さんせーい！　",
            "voice": "event/event_012/Event_012_03/event_012_03_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ま、それでもいい。　",
            "voice": "event/event_012/Event_012_03/event_012_03_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ありがとう、ほのかちゃん。　",
            "voice": "event/event_012/Event_012_03/event_012_03_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "いいって、いいって。\nほら、行こう？",
            "voice": "event/event_012/Event_012_03/event_012_03_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "うわあ、いただきまーす！　",
            "voice": "event/event_012/Event_012_03/event_012_03_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "うん！　フルーツたっぷりで、美容にも良さそう！",
            "voice": "event/event_012/Event_012_03/event_012_03_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ふふっ、夏と言えばコレだね！",
            "voice": "event/event_012/Event_012_03/event_012_03_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……ひとついいか？　",
            "voice": "event/event_012/Event_012_03/event_012_03_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ん？　どうしたの？　志穂。　",
            "voice": "event/event_012/Event_012_03/event_012_03_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "みんな、どうしてかき氷を頼んでるんだ？\nお腹減ってたんじゃないのか。",
            "voice": "event/event_012/Event_012_03/event_012_03_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "えへへ。なんだか前の人が頼んでるのを見てたら、つい……。　",
            "voice": "event/event_012/Event_012_03/event_012_03_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ほら、こんなにかわいくフルーツが盛れてるかき氷って、\nなかなかないでしょ？",
            "voice": "event/event_012/Event_012_03/event_012_03_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "まぁ、確かに、映えそうなかき氷なのはわかる。　",
            "voice": "event/event_012/Event_012_03/event_012_03_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "志穂、朝ご飯、しっかり食べたって言ってたじゃん。\nなんで焼きそば？",
            "voice": "event/event_012/Event_012_03/event_012_03_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "海の家といったら焼きそばだろ。\nこの立ち上る香りの誘惑に、人は抗えない。",
            "voice": "event/event_012/Event_012_03/event_012_03_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ほれ。この香ばしいソースの匂いはどうだ？　",
            "voice": "event/event_012/Event_012_03/event_012_03_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ごくり……、確かにすごくいい匂いだけど……。",
            "voice": "event/event_012/Event_012_03/event_012_03_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "うん……、すごくおいしそう……。",
            "voice": "event/event_012/Event_012_03/event_012_03_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "他の人が頼んだものって、何故かおいしそうに見えるよね。",
            "voice": "event/event_012/Event_012_03/event_012_03_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "なんだ？　少し食べるか？　",
            "voice": "event/event_012/Event_012_03/event_012_03_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "食べたい！　",
            "voice": "event/event_012/Event_012_03/event_012_03_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "わたしも！　",
            "voice": "event/event_012/Event_012_03/event_012_03_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "全部食べるなよ？　",
            "voice": "event/event_012/Event_012_03/event_012_03_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "じゃあ、みんなで一口ずつ分けようよ。\nわたしのはイチゴ味。",
            "voice": "event/event_012/Event_012_03/event_012_03_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "いいね！　わたしは青リンゴ味。\nちょっとめずらしいよね。",
            "voice": "event/event_012/Event_012_03/event_012_03_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "自分のはブルーハワイ！　かき氷で一番好きな味なんだよねー。",
            "voice": "event/event_012/Event_012_03/event_012_03_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "うむ。全部いただこう。　",
            "voice": "event/event_012/Event_012_03/event_012_03_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "え！　志穂ちゃん、一気に食べちゃ……。　",
            "voice": "event/event_012/Event_012_03/event_012_03_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "モグモグ……、うっ！",
            "voice": "event/event_012/Event_012_03/event_012_03_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "志穂ちゃん？　",
            "voice": "event/event_012/Event_012_03/event_012_03_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "今は話しかけないでくれ……。　",
            "voice": "event/event_012/Event_012_03/event_012_03_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "……？　",
            "voice": "event/event_012/Event_012_03/event_012_03_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "あーっ！　頭がキーンってしてきた！\nでも、これがかき氷って感じがする！",
            "voice": "event/event_012/Event_012_03/event_012_03_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あはは。やっぱり砂浜で食べるっていいね！　",
            "voice": "event/event_012/Event_012_03/event_012_03_050.mp3"
        }
    ]
}