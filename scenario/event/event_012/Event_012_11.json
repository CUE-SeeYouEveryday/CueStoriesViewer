{
    "Txt_Name": "Event_012_11",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "わあ……。",
            "voice": "event/Event_012/Event_012_11/event_012_11_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "キレイ……。",
            "voice": "event/Event_012/Event_012_11/event_012_11_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "陽菜ー、舞花！\nやっと追いついた！",
            "voice": "event/Event_012/Event_012_11/event_012_11_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ほのかちゃん、見て見て。",
            "voice": "event/Event_012/Event_012_11/event_012_11_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "えっ……？",
            "voice": "event/Event_012/Event_012_11/event_012_11_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "うわあっ、すごい夕焼け！",
            "voice": "event/Event_012/Event_012_11/event_012_11_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "志穂も見て！",
            "voice": "event/Event_012/Event_012_11/event_012_11_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "うん。言われなくても見ているぞ。",
            "voice": "event/Event_012/Event_012_11/event_012_11_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……。",
            "voice": "event/Event_012/Event_012_11/event_012_11_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "……見とれちゃうね。",
            "voice": "event/Event_012/Event_012_11/event_012_11_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "だな。",
            "voice": "event/Event_012/Event_012_11/event_012_11_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……ねえ。",
            "voice": "event/Event_012/Event_012_11/event_012_11_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "今日……、楽しかったね。",
            "voice": "event/Event_012/Event_012_11/event_012_11_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "うん、とっても楽しかった。",
            "voice": "event/Event_012/Event_012_11/event_012_11_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "かき氷もおいしかったし。",
            "voice": "event/Event_012/Event_012_11/event_012_11_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "焼きそばも団子もおいしかったぞ。",
            "voice": "event/Event_012/Event_012_11/event_012_11_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "うん、なんでもおいしかったね。\n分けっこしたのも楽しかった。",
            "voice": "event/Event_012/Event_012_11/event_012_11_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "舞花ちゃん、ほのかちゃん……。ありがとう。",
            "voice": "event/Event_012/Event_012_11/event_012_11_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "え、どうしたの？　急に。",
            "voice": "event/Event_012/Event_012_11/event_012_11_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "今日こうやって楽しい思い出が出来たのは、\n行こうって誘ってくれた2人のおかげだから。",
            "voice": "event/Event_012/Event_012_11/event_012_11_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ふふ。やっぱり、夏は海だもん。\n来て良かったでしょ？",
            "voice": "event/Event_012/Event_012_11/event_012_11_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "うん、とっても楽しかった。",
            "voice": "event/Event_012/Event_012_11/event_012_11_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "レッスンとかオーディションとか、毎日大変だけど、\nたまにはこうやって息抜きしないとね。",
            "voice": "event/Event_012/Event_012_11/event_012_11_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "そうそう、息抜きは大事だぞ。",
            "voice": "event/Event_012/Event_012_11/event_012_11_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "志穂は、ちょっと息抜き多めじゃない？",
            "voice": "event/Event_012/Event_012_11/event_012_11_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "わたし、こうやってみんなで一緒にお出かけできて、\n本当に楽しかった。",
            "voice": "event/Event_012/Event_012_11/event_012_11_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "また、一緒にどこかに行きたいね。",
            "voice": "event/Event_012/Event_012_11/event_012_11_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "うん。\nでも……、次はいつこうやって過ごせるかな……。",
            "voice": "event/Event_012/Event_012_11/event_012_11_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そうだね……。\nみんな、しばらく忙しいみたいだし。",
            "voice": "event/Event_012/Event_012_11/event_012_11_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "……。",
            "voice": "event/Event_012/Event_012_11/event_012_11_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……。",
            "voice": "event/Event_012/Event_012_11/event_012_11_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "……よーっし！",
            "voice": "event/Event_012/Event_012_11/event_012_11_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "なんだ急に。",
            "voice": "event/Event_012/Event_012_11/event_012_11_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "どうしたの？　靴、脱いだりして……？",
            "voice": "event/Event_012/Event_012_11/event_012_11_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "えへへっ……！",
            "voice": "event/Event_012/Event_012_11/event_012_11_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あっ、ほのかちゃん！？",
            "voice": "event/Event_012/Event_012_11/event_012_11_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……よし、自分も！\n……！",
            "voice": "event/Event_012/Event_012_11/event_012_11_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ええっ、舞花ちゃん！？",
            "voice": "event/Event_012/Event_012_11/event_012_11_039.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "最後の最後まで元気なヤツらだ。",
            "voice": "event/Event_012/Event_012_11/event_012_11_040.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ふふっ、そうだね。",
            "voice": "event/Event_012/Event_012_11/event_012_11_042.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "陽菜もおいでよー！\n波、気持ちいいよー！",
            "voice": "event/Event_012/Event_012_11/event_012_11_043.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "写真もこっちで撮ろうよ！",
            "voice": "event/Event_012/Event_012_11/event_012_11_044.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あっ……！",
            "voice": "event/Event_012/Event_012_11/event_012_11_045.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "仕方ない。行くぞ、陽菜。",
            "voice": "event/Event_012/Event_012_11/event_012_11_046.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "うん。今、行くよーっ！",
            "voice": "event/Event_012/Event_012_11/event_012_11_047.mp3"
        }
    ]
}