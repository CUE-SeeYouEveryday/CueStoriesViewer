{
    "Txt_Name": "Event_004_06",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『さあ、みなのもの、踊るのじゃ！　\n宴じゃー！』",
            "voice": "event/event_004/Event_004_06/event_004_06_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "Whoooo！ ",
            "voice": "event/event_004/Event_004_06/event_004_06_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "さあみんな、立ち上がって踊ろう！",
            "voice": "event/event_004/Event_004_06/event_004_06_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ちょっと、悠希ちゃん、何が始まったの？",
            "voice": "event/event_004/Event_004_06/event_004_06_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "宴。みんな退屈そうだから。",
            "voice": "event/event_004/Event_004_06/event_004_06_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "そんなことないよ、集中して聞いてくれてるよ。",
            "voice": "event/event_004/Event_004_06/event_004_06_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "さっきクスって笑ったじゃん！　\n今こそ漫才の借りを返す時だよ。　",
            "voice": "event/event_004/Event_004_06/event_004_06_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "さあみんな、手をたたいてぇ。",
            "voice": "event/event_004/Event_004_06/event_004_06_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "千紗ちゃんまで！？",
            "voice": "event/event_004/Event_004_06/event_004_06_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ちょっとだけ。だって子どもたち見てよ。\nみんな笑い始めたよ。",
            "voice": "event/event_004/Event_004_06/event_004_06_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（やっぱり、漫才でスベったこと気にしてたんだぁ。）",
            "voice": "event/event_004/Event_004_06/event_004_06_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（これじゃ物語が台無しになっちゃう……。\nなんとかしなきゃ！）",
            "voice": "event/event_004/Event_004_06/event_004_06_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『アントニオは、賑やかなクリスマスパーティーにあこがれていました』",
            "voice": "event/event_004/Event_004_06/event_004_06_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "メリークリスマース！",
            "voice": "event/event_004/Event_004_06/event_004_06_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                0
            ],
            "name": "子どもたち",
            "message": "メリークリスマース！",
            "voice": ""
        },
        {
            "index": 16,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『そうです、こんな楽しいパーティーを、\nいつか経験してみたいと思っているのです！』",
            "voice": "event/event_004/Event_004_06/event_004_06_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "『こうしてアントニオは、楽しいクリスマスを過ごしたのでしたぁ』",
            "voice": "event/event_004/Event_004_06/event_004_06_019.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "柚葉ちゃん、お話終わっちゃう！？",
            "voice": "event/event_004/Event_004_06/event_004_06_020.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "いいじゃん、だってみんなノリノリだよ？\nイェーイ！",
            "voice": "event/event_004/Event_004_06/event_004_06_021.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                0
            ],
            "name": "子どもたち",
            "message": "イェーイ！",
            "voice": ""
        },
        {
            "index": 21,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（どうしよう、なんとか主人公を教会に向かわせなきゃ……。）",
            "voice": "event/event_004/Event_004_06/event_004_06_023.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『……と！　アントニオはベットから飛び起きました。\n今のは夢だったのです』",
            "voice": "event/event_004/Event_004_06/event_004_06_024.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『こんなクリスマスパーティーに一度でいいから出てみたいなぁ、\nとアントニオは思いました』",
            "voice": "event/event_004/Event_004_06/event_004_06_025.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『兄弟の帰る場所。そう、それは町の教会。\n2人は教会に向かうことにしました！』",
            "voice": "event/event_004/Event_004_06/event_004_06_026.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                0
            ],
            "name": "子どもたち",
            "message": "えー。教会に帰るなんておかしいよぉ。",
            "voice": ""
        },
        {
            "index": 26,
            "heroineId": [
                0
            ],
            "name": "子どもたち",
            "message": "帰るのは家でしょ？　っていうか今家じゃん。",
            "voice": ""
        },
        {
            "index": 27,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（どうしよう、客席が荒れてきた……。）",
            "voice": "event/event_004/Event_004_06/event_004_06_029.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『……と、その時です。激しい吹雪がやってきました』",
            "voice": "event/event_004/Event_004_06/event_004_06_030.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『ビュービュー、ビューン！』",
            "voice": "event/event_004/Event_004_06/event_004_06_031.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（千紗ちゃんも戻ってきてくれた！）",
            "voice": "event/event_004/Event_004_06/event_004_06_032.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『吹雪はさらに激しさを増し、\n兄弟の家は吹き飛ばされてしまいました』",
            "voice": "event/event_004/Event_004_06/event_004_06_033.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "『あぁーれー！』",
            "voice": "event/event_004/Event_004_06/event_004_06_034.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『僕たちの家が、家がぁーっ！』",
            "voice": "event/event_004/Event_004_06/event_004_06_035.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "（お？　ウケけたぞ。）",
            "voice": "event/event_004/Event_004_06/event_004_06_036.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "『家がなくなっては仕方がない。\n教会に行くしかない』",
            "voice": "event/event_004/Event_004_06/event_004_06_037.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『よしお兄ちゃん、なけなしの銀貨1枚キリだけど、\nこれを握りしめて教会へ向かおう』",
            "voice": "event/event_004/Event_004_06/event_004_06_038.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "『そうしようアントニオ。さあ、出発だ！』",
            "voice": "event/event_004/Event_004_06/event_004_06_039.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『こうして、貧しくも仲の良い、2人の幼い兄弟は、\n教会へ向かうことにしました』",
            "voice": "event/event_004/Event_004_06/event_004_06_040.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（ふぅ……、やっと本線に戻ったぁ。）",
            "voice": "event/event_004/Event_004_06/event_004_06_041.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（次は、兄弟と、行き倒れた女性が、会うシーン。\nナレーションの技術を思い出して、仕切りなおさないと……。）",
            "voice": "event/event_004/Event_004_06/event_004_06_042.mp3"
        }
    ]
}