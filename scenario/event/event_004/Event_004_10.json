{
    "Txt_Name": "Event_004_10",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『どうした？　アントニオ！　\nもう攻撃は終わりか！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『くっ……。会場のみんな、僕を応援してくれ！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "子どもたち",
            "message": "アントニオ！　アントニオ！",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『よおし！　勇気が湧いてきたぞ！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『さあ、どうする！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『勇気が湧いただけだ！　何もしない！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "いばって言うことじゃないでしょっ。",
            "voice": "event/Event_004/Event_004_10/event_004_10_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "（おぉー入れ食いだなぁ。今、何やってもウケるぞ。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『そうだ！　こういう時はドラゴンを呼ぶんだ！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "『わお！　呼ぼう呼ぼう！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "何言ってるのよ。そんなの無理に決まってるでしょ？",
            "voice": "event/Event_004/Event_004_10/event_004_10_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ドラゴン……。\nそうよ、飛べればいいのよね！",
            "voice": "event/Event_004/Event_004_10/event_004_10_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『そうよ、銀貨よ。\nアントニオが持ってる、この、なけなしの銀貨！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『これを……、どうするの？』",
            "voice": "event/Event_004/Event_004_10/event_004_10_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『おばちゃんがこの銀貨に魔法をかけて、\n天高く飛ばせるようにしてあげるから』",
            "voice": "event/Event_004/Event_004_10/event_004_10_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『アントニオはこの銀貨を、鐘に向かって思いっきり投げるの』",
            "voice": "event/Event_004/Event_004_10/event_004_10_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『なるほど、銀貨で鐘を鳴らすってことか！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『鐘が鳴りさえすれば、きっとみんなの思いが叶うはず……！\nそうすれば、大魔王だってきっと救われる』",
            "voice": "event/Event_004/Event_004_10/event_004_10_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "『なるほど、グッドアイデアだね』",
            "voice": "event/Event_004/Event_004_10/event_004_10_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『どう？　これでハッピーエンドになれるんじゃない？』",
            "voice": "event/Event_004/Event_004_10/event_004_10_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（……会場が、水を打ったように静かになっている。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（ずっとヤジをとばしていたあの子の目……。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（次の瞬間、あいりが何を言うかに釘付けになってる……。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（あの子だけじゃない。この会場全体が、物語の結末を待ってる。\nそんな重要な問題？　こんなハチャメチャな物語……。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（……でも、世界中の関心が今、あいりに集まってるみたい。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（あれ？　あいり今、客席が見えてる……。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（このライブが始まった時は、\n緊張で、台本から目を離すことなんか出来なかったのに……。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（あいりたちが、本当に悩み、本当に葛藤し、\n本当に純粋に語るときに、お客さんは……。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（今はわかるよ……。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（今、この会場にいるすべての人が望んでること……。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（そう……、鐘を鳴らすんだ。クリスマスの鐘を。）",
            "voice": "event/Event_004/Event_004_10/event_004_10_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "（あいり……、どうする？）",
            "voice": "event/Event_004/Event_004_10/event_004_10_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『……魔法使いは、銀貨に魔法をかけました』",
            "voice": "event/Event_004/Event_004_10/event_004_10_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『……あ。ピピティパピテぷぅ～！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『そしてアントニオは、その銀貨を、\n力いっぱい、天にそびえる鐘に向かって投げました』",
            "voice": "event/Event_004/Event_004_10/event_004_10_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『とどけー！！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『すると……』",
            "voice": "event/Event_004/Event_004_10/event_004_10_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "……。",
            "voice": "event/Event_004/Event_004_10/event_004_10_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……。",
            "voice": "event/Event_004/Event_004_10/event_004_10_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "……。",
            "voice": "event/Event_004/Event_004_10/event_004_10_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『すると鐘は……』",
            "voice": "event/Event_004/Event_004_10/event_004_10_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "鐘は？",
            "voice": "event/Event_004/Event_004_10/event_004_10_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "鳴ったの？　鳴らなかったの？",
            "voice": "event/Event_004/Event_004_10/event_004_10_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "どっち？？",
            "voice": "event/Event_004/Event_004_10/event_004_10_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『それでも鐘は鳴りませんでした』",
            "voice": "event/Event_004/Event_004_10/event_004_10_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "どういうこと……。",
            "voice": "event/Event_004/Event_004_10/event_004_10_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "どうして？　",
            "voice": "event/Event_004/Event_004_10/event_004_10_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『おい大魔王！　俺たちの鐘を返せ！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                0
            ],
            "name": "子どもたち",
            "message": "かえせ！　かえせ！　かえせ！",
            "voice": ""
        },
        {
            "index": 50,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『みんな……、気づいて！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "えっ……。",
            "voice": "event/Event_004/Event_004_10/event_004_10_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『クリスマスの鐘は、力づくで鳴らせるものなの？』",
            "voice": "event/Event_004/Event_004_10/event_004_10_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "それは……。",
            "voice": "event/Event_004/Event_004_10/event_004_10_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『魔法を使って銀貨を投げて、鐘が鳴りさえすれば、\nそれでいいの？』",
            "voice": "event/Event_004/Event_004_10/event_004_10_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "……。",
            "voice": "event/Event_004/Event_004_10/event_004_10_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『クリスマスの鐘を鳴らせるのは、純真な心だけ』",
            "voice": "event/Event_004/Event_004_10/event_004_10_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『私たちが本当に、世界の幸せを願った時、\nその時初めて、鐘は鳴るの』",
            "voice": "event/Event_004/Event_004_10/event_004_10_057.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『みんな！　人々の幸せを、本気で願ってみて！』",
            "voice": "event/Event_004/Event_004_10/event_004_10_058.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "（お願い、鳴って……！）",
            "voice": "event/Event_004/Event_004_10/event_004_10_059.mp3"
        },
        {
            "index": 60,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "（頼む……！）",
            "voice": "event/Event_004/Event_004_10/event_004_10_060.mp3"
        },
        {
            "index": 61,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "(My god……。)",
            "voice": "event/Event_004/Event_004_10/event_004_10_061.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "すごーい！　本当に教会の鐘が鳴ってる！",
            "voice": "event/Event_004/Event_004_10/event_004_10_062.mp3"
        },
        {
            "index": 63,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "すごい、きれいな音……。",
            "voice": "event/Event_004/Event_004_10/event_004_10_063.mp3"
        },
        {
            "index": 64,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "やったー、やったぞー！",
            "voice": "event/Event_004/Event_004_10/event_004_10_064.mp3"
        },
        {
            "index": 65,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『……こうして、自らの祈りによって、幸せを掴んだ人々は、\nいつまでも幸せに暮らしましたとさ』",
            "voice": "event/Event_004/Event_004_10/event_004_10_065.mp3"
        },
        {
            "index": 66,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "以上！　エールブルーによります。",
            "voice": "event/Event_004/Event_004_10/event_004_10_066.mp3"
        },
        {
            "index": 67,
            "heroineId": [
                5,
                6,
                7,
                8
            ],
            "name": "4人",
            "message": "「クリスマスの鐘」。",
            "voice": "event/Event_004/Event_004_10/event_004_10_067.mp3"
        },
        {
            "index": 68,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "でした！　みんな、楽しんでもらえたかなー？？",
            "voice": "event/Event_004/Event_004_10/event_004_10_068.mp3"
        },
        {
            "index": 69,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ありがとう！！",
            "voice": "event/Event_004/Event_004_10/event_004_10_069.mp3"
        },
        {
            "index": 70,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "メルシー！",
            "voice": "event/Event_004/Event_004_10/event_004_10_070.mp3"
        },
        {
            "index": 71,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "また会おうねー！！",
            "voice": "event/Event_004/Event_004_10/event_004_10_071.mp3"
        }
    ]
}