{
    "Txt_Name": "Event_004_05",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（あぁぁ……、会場がザワザワしてきた……。）",
            "voice": "event/Event_004/Event_004_05/event_004_05_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『わしが子どもの頃は、毎年鐘が鳴っていたのだがのぅ……。\nあぁ、またあの美しい鐘の音を聞いてみたいものじゃのぅ……』",
            "voice": "event/Event_004/Event_004_05/event_004_05_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "千紗ちゃん！　やっと気づいてくれてありがとう。",
            "voice": "event/Event_004/Event_004_05/event_004_05_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "何がよ。あいり、1行飛ばしてない？\n進まないから読んじゃった。",
            "voice": "event/Event_004/Event_004_05/event_004_05_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "え？　\n……ああ！",
            "voice": "event/Event_004/Event_004_05/event_004_05_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あいりのナレーション、待ってたんだよ！",
            "voice": "event/Event_004/Event_004_05/event_004_05_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ご、ごめんなさい！",
            "voice": "event/Event_004/Event_004_05/event_004_05_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "コッホン……。\n『町一番の長老ジョセフは、こう言いました』",
            "voice": "event/Event_004/Event_004_05/event_004_05_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『……ジョセフ、……ジョセフ！』",
            "voice": "event/Event_004/Event_004_05/event_004_05_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『よ、呼んだかの？』",
            "voice": "event/Event_004/Event_004_05/event_004_05_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『ジョセフ。おおジョセフ。おかえりジョセフ！\n待っていたよ……、ところで話ってなんだい？』",
            "voice": "event/Event_004/Event_004_05/event_004_05_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『お前さんに、話なんてないがのう……』",
            "voice": "event/Event_004/Event_004_05/event_004_05_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『はははっ！　ジョセフ。\nジョセフが子どもの頃は、鐘が鳴っていたんだよね？』",
            "voice": "event/Event_004/Event_004_05/event_004_05_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『……鳴ってたのぉ』",
            "voice": "event/Event_004/Event_004_05/event_004_05_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『それはそれは美しい鐘の音だったんだろうなぁ』",
            "voice": "event/Event_004/Event_004_05/event_004_05_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『そうじゃのぉ』",
            "voice": "event/Event_004/Event_004_05/event_004_05_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『僕も聞いてみたいなぁ』",
            "voice": "event/Event_004/Event_004_05/event_004_05_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『そうじゃが……、あんた誰じゃ？』",
            "voice": "event/Event_004/Event_004_05/event_004_05_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『長老ジョセフの記憶はあいまいになっていました。\nそれほど長い期間、鐘は鳴っていなかったのです』",
            "voice": "event/Event_004/Event_004_05/event_004_05_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（なんとか、元に戻ったかな……。）",
            "voice": "event/Event_004/Event_004_05/event_004_05_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（あれ？　笑われてる？　\n呼吸だ、呼吸をするんだ。口角を上げて、滑舌良く……！）",
            "voice": "event/Event_004/Event_004_05/event_004_05_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『さて、ペドロ兄弟の住む家は、その教会のある町からは、\nだいぶ離れた山の中にありました』",
            "voice": "event/Event_004/Event_004_05/event_004_05_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（さぁ、悠希ちゃん。弟のセリフだよ。\nもう漫才のダメージは消えたよね？）",
            "voice": "event/Event_004/Event_004_05/event_004_05_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ニヤリ。",
            "voice": "event/Event_004/Event_004_05/event_004_05_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『鳴らぬなら、鳴るまで待とう、鐘の音』",
            "voice": "event/Event_004/Event_004_05/event_004_05_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "『拙者、クリスマスの妖精でござる！』",
            "voice": "event/Event_004/Event_004_05/event_004_05_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "『クリスマスの妖精だ！　\n妖精がこの町にやってきたよぉ！』",
            "voice": "event/Event_004/Event_004_05/event_004_05_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ちょっと！　\n何が始まったの！？",
            "voice": "event/Event_004/Event_004_05/event_004_05_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "わかんないけど、面白そう！　\nイッツ、ショータイム！",
            "voice": "event/Event_004/Event_004_05/event_004_05_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ええ……？　\nもしかしてこれが、悠希ちゃんの言ってた、アドリブ？",
            "voice": "event/Event_004/Event_004_05/event_004_05_030.mp3"
        }
    ]
}