{
    "Txt_Name": "Event_013_11",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "やっぱり夏は何が起こるかわからない。",
            "voice": "event/Event_013/Event_013_11/event_013_11_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "トラブル続きの時間の先に残ったのは、\nぬるくなったジンジャーエールと、溶けちゃったアイス。",
            "voice": "event/Event_013/Event_013_11/event_013_11_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "でもやっぱり、辛いことの後には嬉しいことがやってくる。",
            "voice": "event/Event_013/Event_013_11/event_013_11_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "例えばほら、ラジオのマイクの前に座っている今みたいに。",
            "voice": "event/Event_013/Event_013_11/event_013_11_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そんなふうに考えていけたらきっと、\nこれから、先も……。",
            "voice": "event/Event_013/Event_013_11/event_013_11_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "ディレクター",
            "message": "はい。BGM明けまで30秒。\nBGM明けゲストの挨拶からです。よろしくお願いしますねー。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "始まるよ莉子。",
            "voice": "event/Event_013/Event_013_11/event_013_11_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。最後まで楽しみましょうね。",
            "voice": "event/Event_013/Event_013_11/event_013_11_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あ、ブースの外、みんなこっち見てる……。",
            "voice": "event/Event_013/Event_013_11/event_013_11_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "よーし。みんな楽しんじゃおう！",
            "voice": "event/Event_013/Event_013_11/event_013_11_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。",
            "voice": "event/Event_013/Event_013_11/event_013_11_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。そうね。",
            "voice": "event/Event_013/Event_013_11/event_013_11_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はあ、収録中にまで何か起きなきゃいいけど。",
            "voice": "event/Event_013/Event_013_11/event_013_11_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                0
            ],
            "name": "ディレクター",
            "message": "10秒前……、８、７、６、５、４、３……。",
            "voice": ""
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ラジオを聞いてくれてるみなさん、こんにちはー。",
            "voice": "event/Event_013/Event_013_11/event_013_11_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "本日、ゲストに呼んでいただきました。\nアニメ「あしバシッ！」、主人公せつは役の日名倉莉子です。",
            "voice": "event/Event_013/Event_013_11/event_013_11_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "けい役の……、神室絢です。",
            "voice": "event/Event_013/Event_013_11/event_013_11_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "くるみ役の、夜峰美晴です。",
            "voice": "event/Event_013/Event_013_11/event_013_11_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あおい役宮路まほろです。",
            "voice": "event/Event_013/Event_013_11/event_013_11_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "それではおハガキ読ませてもらいますね。\nまず最初は、ラジオネーム「夢見るウサギちゃん」さんから……。",
            "voice": "event/Event_013/Event_013_11/event_013_11_020.mp3"
        }
    ]
}