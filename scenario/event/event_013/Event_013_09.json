{
    "Txt_Name": "Event_013_09",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ああ、止まっちゃった……。",
            "voice": "event/event_013/Event_013_09/event_013_09_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちょっと、莉子、どうにもなんないの？",
            "voice": "event/event_013/Event_013_09/event_013_09_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "んー……。",
            "voice": "event/event_013/Event_013_09/event_013_09_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは、やっぱもうエンジンかかんないね。",
            "voice": "event/event_013/Event_013_09/event_013_09_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "最悪。",
            "voice": "event/event_013/Event_013_09/event_013_09_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ガス欠は、業者に電話すればいいとして……。",
            "voice": "event/event_013/Event_013_09/event_013_09_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ただ……、ずっとここで待ってはいられないよね。\n13時までに会場入りしなきゃいけないし。",
            "voice": "event/event_013/Event_013_09/event_013_09_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ねえ、まほろ。\nひとまずマネージャーさんに連絡してくれる？",
            "voice": "event/event_013/Event_013_09/event_013_09_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "わかった。\n莉子がガス欠で動かないって言えばいい？",
            "voice": "event/event_013/Event_013_09/event_013_09_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ちょっとちょっと？\nなんかそれ、おかしな伝わり方しない？",
            "voice": "event/event_013/Event_013_09/event_013_09_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あ、もしもし、マネージャー？",
            "voice": "event/event_013/Event_013_09/event_013_09_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ねえ、莉子、会場まであとどれくらいなの？",
            "voice": "event/event_013/Event_013_09/event_013_09_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "もうちょっとだったんだよ？\nこの道まっすぐ、あと500mくらいじゃない？",
            "voice": "event/event_013/Event_013_09/event_013_09_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "500m？\nあとちょっとだったのに……。",
            "voice": "event/event_013/Event_013_09/event_013_09_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "じゃあ……、みんなで押しましょ？",
            "voice": "event/event_013/Event_013_09/event_013_09_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "押す？",
            "voice": "event/event_013/Event_013_09/event_013_09_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "え？　嘘でしょ？",
            "voice": "event/event_013/Event_013_09/event_013_09_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ほんと。\nだって、あと500mなんでしょ？",
            "voice": "event/event_013/Event_013_09/event_013_09_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "車を置きっぱなしにして会場に行くわけにはいかないし……。",
            "voice": "event/event_013/Event_013_09/event_013_09_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "業者さんが来てくれるまで、\n誰かがここにずっといるわけにもいかないもの。",
            "voice": "event/event_013/Event_013_09/event_013_09_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いやいやいや。マネージャー。とにかく急いで車まで来て！\n今、現在地の場所スクショして送るから。",
            "voice": "event/event_013/Event_013_09/event_013_09_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "お。さすがまほろ、スマホを使いこなすね。",
            "voice": "event/event_013/Event_013_09/event_013_09_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そんなこと言ってる場合じゃないでしょ。",
            "voice": "event/event_013/Event_013_09/event_013_09_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ねえ美晴。\nマネージャーが来てくれるまで待ってちゃダメなの？",
            "voice": "event/event_013/Event_013_09/event_013_09_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "マネージャーさんが来るのをただ待っているより、\n少しでも先に進んだほうがいいでしょ？",
            "voice": "event/event_013/Event_013_09/event_013_09_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "きっとすぐマネージャーさん来てくれるから。\nだから今は少しだけでも前へ。ね？",
            "voice": "event/event_013/Event_013_09/event_013_09_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……確かに美晴の言う通り。\nそれが1番早いかもね。",
            "voice": "event/event_013/Event_013_09/event_013_09_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "でも、押すって言っても……。",
            "voice": "event/event_013/Event_013_09/event_013_09_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "たった500mだもの、すぐ着くんじゃない？　きっと。",
            "voice": "event/event_013/Event_013_09/event_013_09_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "すごいこと言うね……。",
            "voice": "event/event_013/Event_013_09/event_013_09_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "わかった。私も押す。",
            "voice": "event/event_013/Event_013_09/event_013_09_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。ありがと、絢。\nほら、まほろも一緒に。莉子はハンドル持ってて。",
            "voice": "event/event_013/Event_013_09/event_013_09_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "わかった。\nじゃあ、ギアをニュートラルに入れるからちょっと待って。",
            "voice": "event/event_013/Event_013_09/event_013_09_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……はあ、なんて日なの。まったく……。",
            "voice": "event/event_013/Event_013_09/event_013_09_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい、みんな、せーの。",
            "voice": "event/event_013/Event_013_09/event_013_09_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "よいっ……、しょ。",
            "voice": "event/event_013/Event_013_09/event_013_09_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "せーの。",
            "voice": "event/event_013/Event_013_09/event_013_09_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "よいっしょっ！",
            "voice": "event/event_013/Event_013_09/event_013_09_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "意外と車って押せるのね。",
            "voice": "event/event_013/Event_013_09/event_013_09_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まあ、ギアがニュートラルなら箱に車輪が付いてるだけだしね……。\n地面が平らなら、そんなに力はいらないよね。",
            "voice": "event/event_013/Event_013_09/event_013_09_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ、力持ちになったみたいで楽しいわね。",
            "voice": "event/event_013/Event_013_09/event_013_09_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いや、ごめん、そんなに楽しくない。",
            "voice": "event/event_013/Event_013_09/event_013_09_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私、美晴の気持ちわかるかも。",
            "voice": "event/event_013/Event_013_09/event_013_09_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ほら、やっぱり絢は美晴を甘やかしてる。",
            "voice": "event/event_013/Event_013_09/event_013_09_045.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　そんなことないよ。",
            "voice": "event/event_013/Event_013_09/event_013_09_046.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ああ、もう1個のアイス、食べるの忘れてた。\nもう溶けてるだろうな。",
            "voice": "event/event_013/Event_013_09/event_013_09_047.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "急いで会場に着けば大丈夫じゃない？",
            "voice": "event/event_013/Event_013_09/event_013_09_048.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ほらほら、2人とも、話してないで。\nタイミング合わせなきゃダーメ。",
            "voice": "event/event_013/Event_013_09/event_013_09_050.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "せーの。",
            "voice": "event/event_013/Event_013_09/event_013_09_051.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "よいっしょ！",
            "voice": "event/event_013/Event_013_09/event_013_09_052.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "せーの！",
            "voice": "event/event_013/Event_013_09/event_013_09_053.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "よいっしょ。",
            "voice": "event/event_013/Event_013_09/event_013_09_054.mp3"
        }
    ]
}