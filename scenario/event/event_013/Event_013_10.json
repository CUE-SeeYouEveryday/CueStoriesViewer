{
    "Txt_Name": "Event_013_10",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "せーの。",
            "voice": "event/Event_013/Event_013_10/event_013_10_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "よいっしょ！",
            "voice": "event/Event_013/Event_013_10/event_013_10_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "せーの！",
            "voice": "event/Event_013/Event_013_10/event_013_10_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "よいっしょ。",
            "voice": "event/Event_013/Event_013_10/event_013_10_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ、みんな！　マネージャーだよ！\nこっちに走ってくる。",
            "voice": "event/Event_013/Event_013_10/event_013_10_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ちょっとちょっと、皆何があったの？",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。ひとことで言うと色々あったってやつで……。",
            "voice": "event/Event_013/Event_013_10/event_013_10_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "もう、誰のせいなのかよくわかんないね。\nこの状況。",
            "voice": "event/Event_013/Event_013_10/event_013_10_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "とりあえず、皆は会場行って。\n車は自分が見とくから。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ガス欠ってことだけど、業者さんは呼んだの？",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "はい。\nあと30分くらいで来てくれるみたいです。",
            "voice": "event/Event_013/Event_013_10/event_013_10_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そう。それなら、自分も本番には間に合いそうだね。\nじゃあ皆、あとは任せて先に行って。",
            "voice": ""
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ディレクターさんには状況を話しておいたから。",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい。ありがとうございます。",
            "voice": "event/Event_013/Event_013_10/event_013_10_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "それじゃあ、あとはよろしくね！\nマネージャー！",
            "voice": "event/Event_013/Event_013_10/event_013_10_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "いやー。\n間に合って良かったね。",
            "voice": "event/Event_013/Event_013_10/event_013_10_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "美晴のおかげじゃない？",
            "voice": "event/Event_013/Event_013_10/event_013_10_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そう？",
            "voice": "event/Event_013/Event_013_10/event_013_10_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。",
            "voice": "event/Event_013/Event_013_10/event_013_10_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。でも、せっかくメイクしてもらったのに、\nみんな汗かいちゃったね。",
            "voice": "event/Event_013/Event_013_10/event_013_10_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そりゃ、この炎天下に車押してたらそうなるよ。",
            "voice": "event/Event_013/Event_013_10/event_013_10_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "よーし、あたしが直してあげる。",
            "voice": "event/Event_013/Event_013_10/event_013_10_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "メイク道具は持ってきてるし。\n新しいテクも覚えたし。",
            "voice": "event/Event_013/Event_013_10/event_013_10_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "それに……、\nメイクさん愛用のアイライナー、貰ったしね。",
            "voice": "event/Event_013/Event_013_10/event_013_10_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "怪我の功名ね。",
            "voice": "event/Event_013/Event_013_10/event_013_10_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ガソリン入ってないことに気付いてれば、\n何も問題なかったわけだけどね。",
            "voice": "event/Event_013/Event_013_10/event_013_10_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まあまあ、細かいことはいいじゃん。",
            "voice": "event/Event_013/Event_013_10/event_013_10_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふふ。",
            "voice": "event/Event_013/Event_013_10/event_013_10_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あ、しまった……。\n車の中にアイス置きっぱなしにしてきちゃった。",
            "voice": "event/Event_013/Event_013_10/event_013_10_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ああ、それはご愁傷様……。",
            "voice": "event/Event_013/Event_013_10/event_013_10_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "袋開けてないから、持って帰って寮でもう1回凍らせて食べる。",
            "voice": "event/Event_013/Event_013_10/event_013_10_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "まほろ、そういうとこあるよね。\n意外と物を粗末にしないっていうか。",
            "voice": "event/Event_013/Event_013_10/event_013_10_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。あたしも飲んでたジンジャーエール、\n完全にぬるくなっちゃった。",
            "voice": "event/Event_013/Event_013_10/event_013_10_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふっ。夏だからね。",
            "voice": "event/Event_013/Event_013_10/event_013_10_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうだね。うん。確かに。\n今は夏だからね。",
            "voice": "event/Event_013/Event_013_10/event_013_10_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\n今日のこと、今年の夏の1番の思い出になりそう。",
            "voice": "event/Event_013/Event_013_10/event_013_10_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうかも。",
            "voice": "event/Event_013/Event_013_10/event_013_10_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "思い出か、確かにね。",
            "voice": "event/Event_013/Event_013_10/event_013_10_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "良く言えばね。",
            "voice": "event/Event_013/Event_013_10/event_013_10_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふっ。",
            "voice": "event/Event_013/Event_013_10/event_013_10_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ほら、謎の感動に浸ってる場合じゃないよ。\nさっさともう1回メイク済ませないと。",
            "voice": "event/Event_013/Event_013_10/event_013_10_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "おっけー！\nみんな、任せて！",
            "voice": "event/Event_013/Event_013_10/event_013_10_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……あれ？　ちょっと待って。もしかして結局……、\nまた手のひらの上だった……、とか？　いやいや……。",
            "voice": "event/Event_013/Event_013_10/event_013_10_043.mp3"
        }
    ]
}