{
    "Txt_Name": "Event_015_08",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "何も染まらず。",
            "voice": "event/Event_015/Event_015_08/event_015_08_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "何も持たず。",
            "voice": "event/Event_015/Event_015_08/event_015_08_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "君がいるだけで。",
            "voice": "event/Event_015/Event_015_08/event_015_08_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ここは美しい。",
            "voice": "event/Event_015/Event_015_08/event_015_08_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ここは？",
            "voice": "event/Event_015/Event_015_08/event_015_08_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "どこ、ここ？",
            "voice": "event/Event_015/Event_015_08/event_015_08_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "さあ。",
            "voice": "event/Event_015/Event_015_08/event_015_08_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。懐かしい景色……。",
            "voice": "event/Event_015/Event_015_08/event_015_08_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "懐かしいって？",
            "voice": "event/Event_015/Event_015_08/event_015_08_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "美晴が昔住んでた街とか？",
            "voice": "event/Event_015/Event_015_08/event_015_08_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうみたい。",
            "voice": "event/Event_015/Event_015_08/event_015_08_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そうみたい……、って……。",
            "voice": "event/Event_015/Event_015_08/event_015_08_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ここが美晴の地元……。",
            "voice": "event/Event_015/Event_015_08/event_015_08_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あれ？　あそこに誰かいる……。",
            "voice": "event/Event_015/Event_015_08/event_015_08_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                0
            ],
            "name": "母親らしき女性",
            "message": "ほら、美晴。もう泣かないの。\n飛んでった風船はもう二度と帰ってこないんだから。",
            "voice": ""
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　今、あの人、美晴って……。",
            "voice": "event/Event_015/Event_015_08/event_015_08_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "……うん。",
            "voice": "event/Event_015/Event_015_08/event_015_08_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "今、何を見てるの？\nあたしたち。",
            "voice": "event/Event_015/Event_015_08/event_015_08_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まほろに聞かないでくれる？",
            "voice": "event/Event_015/Event_015_08/event_015_08_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                0
            ],
            "name": "幼い少女",
            "message": "うえっうえっ、ヒック。\nだって、せっかくお兄ちゃんが、買ってくれ、たのに、ヒック。",
            "voice": ""
        },
        {
            "index": 21,
            "heroineId": [
                0
            ],
            "name": "母親らしき女性",
            "message": "お母さんがまた同じの買ってきてあげるから。",
            "voice": ""
        },
        {
            "index": 22,
            "heroineId": [
                0
            ],
            "name": "幼い少女",
            "message": "ダメ！　あの風船じゃなきゃ、ダメなの。",
            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                0
            ],
            "name": "母親らしき女性",
            "message": "もう……、困ったわね。",
            "voice": ""
        },
        {
            "index": 24,
            "heroineId": [
                0
            ],
            "name": "少年",
            "message": "……取ってきてやるよ。あれがいいんだろ？",
            "voice": ""
        },
        {
            "index": 25,
            "heroineId": [
                0
            ],
            "name": "幼い少女",
            "message": "お、お兄ちゃん。",
            "voice": ""
        },
        {
            "index": 26,
            "heroineId": [
                0
            ],
            "name": "母親らしき女性",
            "message": "もう、姿も見えないのよ。探しようがないじゃない。",
            "voice": ""
        },
        {
            "index": 27,
            "heroineId": [
                0
            ],
            "name": "少年",
            "message": "それでも、取ってきてやるから。美晴は待ってろ。",
            "voice": ""
        },
        {
            "index": 28,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "……。",
            "voice": "event/Event_015/Event_015_08/event_015_08_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あれは……、子どもの頃の美晴？",
            "voice": "event/Event_015/Event_015_08/event_015_08_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\nわたしあんなに小さかったのね。",
            "voice": "event/Event_015/Event_015_08/event_015_08_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "で、この光景の意味するところは何？",
            "voice": "event/Event_015/Event_015_08/event_015_08_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まさか、子どもの頃の思い出をもう一度なんて、\n思ってたわけじゃないんでしょ？",
            "voice": "event/Event_015/Event_015_08/event_015_08_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あの日……。ううん、今見てるこの日、\nわたしは、お兄ちゃんに買ってもらった風船を手放したの。",
            "voice": "event/Event_015/Event_015_08/event_015_08_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ああ、それ……。",
            "voice": "event/Event_015/Event_015_08/event_015_08_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\n覚えてる？",
            "voice": "event/Event_015/Event_015_08/event_015_08_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。私たちがチームになった日に聞いた話……。",
            "voice": "event/Event_015/Event_015_08/event_015_08_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そう言えば聞いたね。\nずいぶん昔のことみたいな気分だけど。",
            "voice": "event/Event_015/Event_015_08/event_015_08_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そう、あの時話した、わたしの記憶。",
            "voice": "event/Event_015/Event_015_08/event_015_08_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ねえ、美晴。\n……美晴はなんで手放したの？",
            "voice": "event/Event_015/Event_015_08/event_015_08_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "その風船。",
            "voice": "event/Event_015/Event_015_08/event_015_08_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なんで……、ってそうね……。\nわたしはあの時……。",
            "voice": "event/Event_015/Event_015_08/event_015_08_041.mp3"
        }
    ]
}