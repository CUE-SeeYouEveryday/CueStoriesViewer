{
    "Txt_Name": "Event_015_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まほろー、トリックオアトリート！",
            "voice": "event/Event_015/Event_015_02/event_015_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はいはい。\nあとで飴あげるから。",
            "voice": "event/Event_015/Event_015_02/event_015_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら、莉子たちの声……。",
            "voice": "event/Event_015/Event_015_02/event_015_02_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ふふ。みんな来たみたい。",
            "voice": "event/Event_015/Event_015_02/event_015_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なんだか今日の風は、いつもより透き通っているような。",
            "voice": "event/Event_015/Event_015_02/event_015_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ピアノの音が、外の声が、\n窓から吹き込んでくる風に運ばれて……。",
            "voice": "event/Event_015/Event_015_02/event_015_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "おはようございまーす！",
            "voice": "event/Event_015/Event_015_02/event_015_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "おはよう。",
            "voice": "event/Event_015/Event_015_02/event_015_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "おはようございます。",
            "voice": "event/Event_015/Event_015_02/event_015_02_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい。みんな、おはようございます。",
            "voice": "event/Event_015/Event_015_02/event_015_02_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "4人でレッスンするの、少し久しぶりだね。",
            "voice": "event/Event_015/Event_015_02/event_015_02_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうだねー。",
            "voice": "event/Event_015/Event_015_02/event_015_02_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まほろは1人でもレッスン怠ってないけど。",
            "voice": "event/Event_015/Event_015_02/event_015_02_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたしも怠ってないよ！",
            "voice": "event/Event_015/Event_015_02/event_015_02_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\nみんな自分で出来ることは自分でやってるもの。",
            "voice": "event/Event_015/Event_015_02/event_015_02_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "でも、4人集まった時には、\nまた違う時間に出来るんじゃない？",
            "voice": "event/Event_015/Event_015_02/event_015_02_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうそう。\nやっぱ美晴はいいこと言うね。",
            "voice": "event/Event_015/Event_015_02/event_015_02_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。\n1人では出来ないこともあるから。",
            "voice": "event/Event_015/Event_015_02/event_015_02_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "で？　\n今日は何から始める？",
            "voice": "event/Event_015/Event_015_02/event_015_02_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "美晴の言うように、\n4人いるから出来ること、したほうがいいんじゃない？",
            "voice": "event/Event_015/Event_015_02/event_015_02_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まあまあ。今来たばっかなんだし、\nそんな焦んなくてもいいじゃん。",
            "voice": "event/Event_015/Event_015_02/event_015_02_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ねえ美晴……、前から思ってたんだけど、\nさっき弾いてた曲って、すごくキレイな曲だよね。",
            "voice": "event/Event_015/Event_015_02/event_015_02_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "曲の名前、なんて言うの？",
            "voice": "event/Event_015/Event_015_02/event_015_02_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うんうん。\n美晴、よく弾いてるよね。",
            "voice": "event/Event_015/Event_015_02/event_015_02_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。あの曲はね。\n昔、教えてもらった曲なの。",
            "voice": "event/Event_015/Event_015_02/event_015_02_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "昔？",
            "voice": "event/Event_015/Event_015_02/event_015_02_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そう。ずーっと昔。",
            "voice": "event/Event_015/Event_015_02/event_015_02_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ずっと昔から弾いてるから、\nどの音ももうすっかり身体の一部みたいに感じられるの。",
            "voice": "event/Event_015/Event_015_02/event_015_02_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ずっと昔、って。また大袈裟な言い方をするね。\n子どもの頃、でいいんじゃないの？",
            "voice": "event/Event_015/Event_015_02/event_015_02_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\n子どもの頃って、ずっとずっと昔じゃない？",
            "voice": "event/Event_015/Event_015_02/event_015_02_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そう？",
            "voice": "event/Event_015/Event_015_02/event_015_02_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。\nまあ、人それぞれの感じ方があるよね。",
            "voice": "event/Event_015/Event_015_02/event_015_02_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "この曲、歌もあるのよ？\n歌詞は英語だけど。",
            "voice": "event/Event_015/Event_015_02/event_015_02_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "へえ。どんな歌？",
            "voice": "event/Event_015/Event_015_02/event_015_02_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "とても優しい歌。\nでも……。",
            "voice": "event/Event_015/Event_015_02/event_015_02_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "でも？",
            "voice": "event/Event_015/Event_015_02/event_015_02_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "歌詞の意味を、ずっと考えてるの。",
            "voice": "event/Event_015/Event_015_02/event_015_02_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "なになに？　そんなに難しい歌詞なの？",
            "voice": "event/Event_015/Event_015_02/event_015_02_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "英語も日本語も、\n言葉はいつだって難しいじゃない。",
            "voice": "event/Event_015/Event_015_02/event_015_02_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そう？　英語が難しいのは同意だけどね。",
            "voice": "event/Event_015/Event_015_02/event_015_02_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "莉子……。\n美晴が言ってるのはそういう意味じゃないと思うんだけど。",
            "voice": "event/Event_015/Event_015_02/event_015_02_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ねえ、美晴。\n歌ってみてよ。",
            "voice": "event/Event_015/Event_015_02/event_015_02_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なあに？　絢、聞きたいの？",
            "voice": "event/Event_015/Event_015_02/event_015_02_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。聞いてみたいな。\nもしかしたら、私、歌詞の意味わかるかも……、なんて。",
            "voice": "event/Event_015/Event_015_02/event_015_02_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ふふ。そうね。\nそれなら、みんなに教えてもらおうかしら。",
            "voice": "event/Event_015/Event_015_02/event_015_02_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "わっ！　強い風……。",
            "voice": "event/Event_015/Event_015_02/event_015_02_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "何？　急に。",
            "voice": "event/Event_015/Event_015_02/event_015_02_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "窓、閉めよっか。",
            "voice": "event/Event_015/Event_015_02/event_015_02_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あれ……？",
            "voice": "event/Event_015/Event_015_02/event_015_02_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "赤い……、風船……。",
            "voice": "event/Event_015/Event_015_02/event_015_02_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "えっ？",
            "voice": "event/Event_015/Event_015_02/event_015_02_051.mp3"
        }
    ]
}