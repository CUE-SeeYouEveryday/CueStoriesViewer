{
    "Txt_Name": "Event_015_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ここは……？",
            "voice": "event/Event_015/Event_015_04/event_015_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "サービスエリアだね……。",
            "voice": "event/Event_015/Event_015_04/event_015_04_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "なんで急に場所が変わったの？",
            "voice": "event/Event_015/Event_015_04/event_015_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。夢に理屈求めてもしょうがないじゃん。",
            "voice": "event/Event_015/Event_015_04/event_015_04_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そっか……。",
            "voice": "event/Event_015/Event_015_04/event_015_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "でも、ここ……。\nまた、見覚えのある場所だね。",
            "voice": "event/Event_015/Event_015_04/event_015_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ほんとね。",
            "voice": "event/Event_015/Event_015_04/event_015_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、富士山の近くのアウトレット行った日に寄ったとこだ。",
            "voice": "event/Event_015/Event_015_04/event_015_04_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうそう。",
            "voice": "event/Event_015/Event_015_04/event_015_04_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。あの日は楽しかったねえ。\n美晴がビール飲んでてさ。",
            "voice": "event/Event_015/Event_015_04/event_015_04_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。莉子、いつも運転ありがとう。",
            "voice": "event/Event_015/Event_015_04/event_015_04_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "いいっていいって。\nそれよりさ、せっかくだからちょっとフードコート行ってみない？",
            "voice": "event/Event_015/Event_015_04/event_015_04_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふっ。\nなんだか、夢なのに夢じゃないみたい。",
            "voice": "event/Event_015/Event_015_04/event_015_04_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ああ、もう深く考えるのやめた。\nマジメに考えるだけバカみたい。",
            "voice": "event/Event_015/Event_015_04/event_015_04_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……って。あ、そうか。",
            "voice": "event/Event_015/Event_015_04/event_015_04_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ん？　どうかした？",
            "voice": "event/Event_015/Event_015_04/event_015_04_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ねえ、みんな。\nあたし、ちょっと思ったんだけどさ。",
            "voice": "event/Event_015/Event_015_04/event_015_04_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なあに？",
            "voice": "event/Event_015/Event_015_04/event_015_04_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ここ、せっかく夢なんだし、好きなことしてもいいかな？",
            "voice": "event/Event_015/Event_015_04/event_015_04_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "どういうこと？",
            "voice": "event/Event_015/Event_015_04/event_015_04_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "例えば……。\nえーっと……、あたしは今、ラジオの生放送中。",
            "voice": "event/Event_015/Event_015_04/event_015_04_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "日名倉莉子のオールナイトジャパーン！",
            "voice": "event/Event_015/Event_015_04/event_015_04_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "なんて言ってみたり……。",
            "voice": "event/Event_015/Event_015_04/event_015_04_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？",
            "voice": "event/Event_015/Event_015_04/event_015_04_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。やっぱり。思った通り。",
            "voice": "event/Event_015/Event_015_04/event_015_04_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "夢を操ったってこと？",
            "voice": "event/Event_015/Event_015_04/event_015_04_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そう。普段は夢を見てても、\nこれは夢だーなんて、気付けることなんかないじゃん？",
            "voice": "event/Event_015/Event_015_04/event_015_04_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "でもほら！　今は、これが夢だって気付いてるわけでしょ？\nだったら、好きな夢見てみようかなって思って。",
            "voice": "event/Event_015/Event_015_04/event_015_04_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ほら、莉子、始まるみたいよ。",
            "voice": "event/Event_015/Event_015_04/event_015_04_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "今週も始まりました。毎週金曜日は私日名倉莉子がお届けする、\n日名倉莉子のオールナイトジャパン！",
            "voice": "event/Event_015/Event_015_04/event_015_04_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "今週も張り切って週末の夜を過ごしましょう！",
            "voice": "event/Event_015/Event_015_04/event_015_04_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふっ。莉子、本物みたい。",
            "voice": "event/Event_015/Event_015_04/event_015_04_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。\nこれ、最高の夢だね。",
            "voice": "event/Event_015/Event_015_04/event_015_04_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いやいやいやいやいや。",
            "voice": "event/Event_015/Event_015_04/event_015_04_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "じゃあ次。あたしは今、サーフィンにやってきたの。\nウエットスーツ着て、板を持って。",
            "voice": "event/Event_015/Event_015_04/event_015_04_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "どう？",
            "voice": "event/Event_015/Event_015_04/event_015_04_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うわー！　海だよ海！\nこんな便利な夢、見たことないかも。",
            "voice": "event/Event_015/Event_015_04/event_015_04_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "んー、でもあたしの想像してた場所とは違ったな。\nまほろと行った海岸思い浮かべてたんだけど。",
            "voice": "event/Event_015/Event_015_04/event_015_04_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ま、いっか。海には変わりないし。\nじゃあ、あたしちょっと波に乗ってくるから。",
            "voice": "event/Event_015/Event_015_04/event_015_04_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちょっとちょっと。待ちなって。",
            "voice": "event/Event_015/Event_015_04/event_015_04_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "え？　なんで？",
            "voice": "event/Event_015/Event_015_04/event_015_04_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "なんでじゃないでしょ。\n1人だけ楽しんでんじゃないっての。",
            "voice": "event/Event_015/Event_015_04/event_015_04_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "えー、まだ楽しんでないよ。",
            "voice": "event/Event_015/Event_015_04/event_015_04_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ふふふ。",
            "voice": "event/Event_015/Event_015_04/event_015_04_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……ん？　あれ？\nってことは……。",
            "voice": "event/Event_015/Event_015_04/event_015_04_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "さっきの新幹線もサービスエリアも、\n誰かが見たいと望んでた夢の景色ってこと？",
            "voice": "event/Event_015/Event_015_04/event_015_04_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "誰かって誰？",
            "voice": "event/Event_015/Event_015_04/event_015_04_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まほろに聞かないでよ。そんなの。",
            "voice": "event/Event_015/Event_015_04/event_015_04_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "絢はどう？　何か願ってみる？",
            "voice": "event/Event_015/Event_015_04/event_015_04_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私？\n私はえーっと……。",
            "voice": "event/Event_015/Event_015_04/event_015_04_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ！　\nじゃあ、みいちゃんに会いたい。",
            "voice": "event/Event_015/Event_015_04/event_015_04_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。絢らしいわね。",
            "voice": "event/Event_015/Event_015_04/event_015_04_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                0
            ],
            "name": "みいちゃん",
            "message": "ニャア",
            "voice": ""
        },
        {
            "index": 54,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "わ！　みいちゃん……。\n会いたかった……！",
            "voice": "event/Event_015/Event_015_04/event_015_04_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                0
            ],
            "name": "みいちゃん",
            "message": "ニャオ",
            "voice": ""
        },
        {
            "index": 56,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ねえ、みいちゃん。元気だった？",
            "voice": "event/Event_015/Event_015_04/event_015_04_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。絢、嬉しそうだねー。",
            "voice": "event/Event_015/Event_015_04/event_015_04_057.mp3"
        }
    ]
}