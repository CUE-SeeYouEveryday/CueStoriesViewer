{
    "Txt_Name": "Event_015_10",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ねえ、聞こえますか。",
            "voice": "event/event_015/Event_015_10/event_015_10_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "笑っていますか。",
            "voice": "event/event_015/Event_015_10/event_015_10_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "色とりどり響く。",
            "voice": "event/event_015/Event_015_10/event_015_10_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "その声を。",
            "voice": "event/event_015/Event_015_10/event_015_10_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あれ……、わたし……。",
            "voice": "event/event_015/Event_015_10/event_015_10_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　",
            "voice": "event/event_015/Event_015_10/event_015_10_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "赤い風船が……。\nあの日の赤い風船が見えた気がして……。",
            "voice": "event/event_015/Event_015_10/event_015_10_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。\nさっき強い風が吹いたもんね。",
            "voice": "event/event_015/Event_015_10/event_015_10_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。ごめんなさい。\nちょっと夢を見てた気分。",
            "voice": "event/event_015/Event_015_10/event_015_10_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ううん。夢と言うよりも、もっと長い幻想……。\n白昼夢を。",
            "voice": "event/event_015/Event_015_10/event_015_10_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……。",
            "voice": "event/event_015/Event_015_10/event_015_10_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ん？　まほろ、どうかした？",
            "voice": "event/event_015/Event_015_10/event_015_10_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いや、この感覚……、デジャヴっていうの？\nなんだか覚えがある気がして。",
            "voice": "event/event_015/Event_015_10/event_015_10_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いや、あれはもっと暗くて、孤独で……、\n焦り続けた先に光を見たような……。",
            "voice": "event/event_015/Event_015_10/event_015_10_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まほろまでおかしなこと言ってるね。\n大丈夫？",
            "voice": "event/event_015/Event_015_10/event_015_10_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ああ、ごめん。\nどっちにしても記憶にないし、気のせいだね。",
            "voice": "event/event_015/Event_015_10/event_015_10_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ねえ、美晴。",
            "voice": "event/event_015/Event_015_10/event_015_10_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なあに？",
            "voice": "event/event_015/Event_015_10/event_015_10_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "歌ってよ。聞きたいな、美晴の歌。",
            "voice": "event/event_015/Event_015_10/event_015_10_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。あのね、絢。\nわたし、わかったの。",
            "voice": "event/event_015/Event_015_10/event_015_10_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "わかったって何が？",
            "voice": "event/event_015/Event_015_10/event_015_10_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そんなの決まってるじゃない。\n歌詞の意味……、よ。",
            "voice": "event/event_015/Event_015_10/event_015_10_022.mp3"
        }
    ]
}