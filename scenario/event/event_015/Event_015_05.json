{
    "Txt_Name": "Event_015_05",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "みいちゃん、最近はどう？\n楽しい？",
            "voice": "event/event_015/Event_015_05/event_015_05_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "みいちゃん",
            "message": "ニャア",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふ。",
            "voice": "event/event_015/Event_015_05/event_015_05_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "みんなの喜ぶ顔が見られて、わたし、嬉しい。\nうふふ。莉子の言う通り、いい夢ね。",
            "voice": "event/event_015/Event_015_05/event_015_05_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "だよねー。",
            "voice": "event/event_015/Event_015_05/event_015_05_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "まほろは？\nなんでも言ってみたら？",
            "voice": "event/event_015/Event_015_05/event_015_05_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まほろはいいよ。\n見たい夢なんてないし。",
            "voice": "event/event_015/Event_015_05/event_015_05_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "えー？　どうして？\nせっかくなんだし、夢ならではの望みとか叶えちゃえば？",
            "voice": "event/event_015/Event_015_05/event_015_05_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "夢……、ねえ。\n誰にとっての夢なんだか。",
            "voice": "event/event_015/Event_015_05/event_015_05_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。まほろ、まだそんなこと言ってる。",
            "voice": "event/event_015/Event_015_05/event_015_05_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あんたは夢の中でもお気楽だね。",
            "voice": "event/event_015/Event_015_05/event_015_05_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "みんな、夢の中でもいつも通りね。",
            "voice": "event/event_015/Event_015_05/event_015_05_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふ。美晴の言う通りかも。",
            "voice": "event/event_015/Event_015_05/event_015_05_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "じゃなくて。\nおかしいと思わないの？",
            "voice": "event/event_015/Event_015_05/event_015_05_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "莉子、あんたも言ってたでしょ？　\nさっきまでレッスン室にいたはずって。",
            "voice": "event/event_015/Event_015_05/event_015_05_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうだけどさ、覚えてないだけで寮に帰ってて、\nで、実際は今、ベッドの中なのかもしれないじゃん？",
            "voice": "event/event_015/Event_015_05/event_015_05_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まほろは起きる。起きるよ。\nこんな夢、今すぐ覚めたいし。",
            "voice": "event/event_015/Event_015_05/event_015_05_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "起きるってどうするの？\nほっぺたでもつまむの？　",
            "voice": "event/event_015/Event_015_05/event_015_05_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。漫画じゃないんだから。",
            "voice": "event/event_015/Event_015_05/event_015_05_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あんたのほっぺたつねってあげようか？",
            "voice": "event/event_015/Event_015_05/event_015_05_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まあまあ、そう言わないでさ。\n試しに何か言ってみたら？",
            "voice": "event/event_015/Event_015_05/event_015_05_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "だから……、まほろは別に、\n見たい夢なんかないっての。",
            "voice": "event/event_015/Event_015_05/event_015_05_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あ、わたしわかっちゃった。\nまほろが言い出しにくい理由も。",
            "voice": "event/event_015/Event_015_05/event_015_05_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はあ？　何よ？　",
            "voice": "event/event_015/Event_015_05/event_015_05_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。いつも並んでるラーメン屋さんに、行きたいんでしょ？\nもう、わたしたちにまで隠さなくていいのにー。",
            "voice": "event/event_015/Event_015_05/event_015_05_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ああ、なるほど。\nまほろも、ちょっと恥ずかしかったりするんだね。",
            "voice": "event/event_015/Event_015_05/event_015_05_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はあ……。\nああもう、わかったって。じゃあ、名古屋。",
            "voice": "event/event_015/Event_015_05/event_015_05_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "最近、実家帰れてないし……、\n久しぶりに名古屋の景色を見たいかな。",
            "voice": "event/event_015/Event_015_05/event_015_05_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ここ、名古屋なの？",
            "voice": "event/event_015/Event_015_05/event_015_05_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "え？　嘘でしょ？",
            "voice": "event/event_015/Event_015_05/event_015_05_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "っていうか、なんでバスの中？",
            "voice": "event/event_015/Event_015_05/event_015_05_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら、この景色懐かしい……。",
            "voice": "event/event_015/Event_015_05/event_015_05_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ん？　美晴、このバス乗ったことあるの？",
            "voice": "event/event_015/Event_015_05/event_015_05_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "以前まほろと一緒に。ね？　まほろ。\nあれもいい思い出ね。",
            "voice": "event/event_015/Event_015_05/event_015_05_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちょっと待って。名古屋の景色が見たいとは言ったけど……。\nこれ、本当にまほろの夢？",
            "voice": "event/event_015/Event_015_05/event_015_05_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あれ？　なんか学校みたいなの見えてきたよ？",
            "voice": "event/event_015/Event_015_05/event_015_05_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "小学校……、って書いてあるね。",
            "voice": "event/event_015/Event_015_05/event_015_05_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いやいやいやいやいや。ストップストップ。\nこんなの、まほろが見たかった名古屋じゃないから。",
            "voice": "event/event_015/Event_015_05/event_015_05_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "どういうこと？　\nあ、あの小学校、もしかしてまほろの母校だとか？",
            "voice": "event/event_015/Event_015_05/event_015_05_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "だったら何よ？",
            "voice": "event/event_015/Event_015_05/event_015_05_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ううん。別になんでもないけど。",
            "voice": "event/event_015/Event_015_05/event_015_05_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ほらほら、夢の中までケンカしないの。",
            "voice": "event/event_015/Event_015_05/event_015_05_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふっ。",
            "voice": "event/event_015/Event_015_05/event_015_05_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "レッスン室！　まほろ、レッスン室の夢が見たい。",
            "voice": "event/event_015/Event_015_05/event_015_05_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はあはあ……。",
            "voice": "event/event_015/Event_015_05/event_015_05_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。なんでそんなに焦ってんの？",
            "voice": "event/event_015/Event_015_05/event_015_05_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "うるさいな。",
            "voice": "event/event_015/Event_015_05/event_015_05_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら、まほろが見たかった夢じゃなかったの？",
            "voice": "event/event_015/Event_015_05/event_015_05_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "全然違うし……。",
            "voice": "event/event_015/Event_015_05/event_015_05_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いや、もしかして……。\n思ったんだけど、美晴。",
            "voice": "event/event_015/Event_015_05/event_015_05_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "これ……、あんたの夢でしょ？",
            "voice": "event/event_015/Event_015_05/event_015_05_051.mp3"
        }
    ]
}