{
    "Txt_Name": "Event_015_07",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ねえ、美晴。\nさっき言ってたじゃない。",
            "voice": "event/Event_015/Event_015_07/event_015_07_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なあに？　言ってたって。",
            "voice": "event/Event_015/Event_015_07/event_015_07_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "さっき……。なのかな？\nえっと、さっきって言葉が正しいのかわからないんだけど。",
            "voice": "event/Event_015/Event_015_07/event_015_07_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ほら、美晴がピアノで弾いてた曲の歌詞。\n歌詞の意味をずっと考えてるって。",
            "voice": "event/Event_015/Event_015_07/event_015_07_006.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。そうね。",
            "voice": "event/Event_015/Event_015_07/event_015_07_007.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "美晴、知りたいんじゃない？\nもしかしてそれを知るために来たんじゃない？　私たち。",
            "voice": "event/Event_015/Event_015_07/event_015_07_008.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わかるのかしら？",
            "voice": "event/Event_015/Event_015_07/event_015_07_009.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "さあ？",
            "voice": "event/Event_015/Event_015_07/event_015_07_010.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "どちらにしろ、1回聞いてみないとね。その歌。",
            "voice": "event/Event_015/Event_015_07/event_015_07_011.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ねえ、美晴。\n聞かせてみてよ。",
            "voice": "event/Event_015/Event_015_07/event_015_07_012.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "もしかしたら、私、歌詞の意味わかるかも……。\nって、ふふっ。さっきも言ったね。これ。",
            "voice": "event/Event_015/Event_015_07/event_015_07_013.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あっ……。",
            "voice": "event/Event_015/Event_015_07/event_015_07_014.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "どうかした？",
            "voice": "event/Event_015/Event_015_07/event_015_07_015.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ううん。なんでもない。うふふ。\nじゃあ、いくわね。",
            "voice": "event/Event_015/Event_015_07/event_015_07_016.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そう、あの時……、わたしは……。\nみんなに教えてほしいと願って……。",
            "voice": "event/Event_015/Event_015_07/event_015_07_017.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "それで……、赤い風船を見て……。",
            "voice": "event/Event_015/Event_015_07/event_015_07_018.mp3"
        }
    ]
}