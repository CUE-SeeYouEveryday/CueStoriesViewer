{
    "Txt_Name": "Event_002_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "おっはよー！\nみんな！",
            "voice": "event/Event_002/Event_002_01/event_002_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "おはよう。\nなによ、朝からテンション高いじゃない。",
            "voice": "event/Event_002/Event_002_01/event_002_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "いつも通りじゃない。",
            "voice": "event/Event_002/Event_002_01/event_002_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "実はわたしね～。\n昨日、すごくラッキーなことがあったんだ。",
            "voice": "event/Event_002/Event_002_01/event_002_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "宝くじでも当たったの？",
            "voice": "event/Event_002/Event_002_01/event_002_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うーん、おしい！",
            "voice": "event/Event_002/Event_002_01/event_002_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "100円拾ったとか？",
            "voice": "event/Event_002/Event_002_01/event_002_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "拾った？\n「拾った」……、が近いかな。",
            "voice": "event/Event_002/Event_002_01/event_002_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "千円？",
            "voice": "event/Event_002/Event_002_01/event_002_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ブブー。",
            "voice": "event/Event_002/Event_002_01/event_002_01_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "1万円？",
            "voice": "event/Event_002/Event_002_01/event_002_01_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "お金じゃありませ～ん。",
            "voice": "event/Event_002/Event_002_01/event_002_01_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "おみくじ大吉とか？",
            "voice": "event/Event_002/Event_002_01/event_002_01_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "大吉？　お～近づいた！\n今、さとちんが断トツトップ！",
            "voice": "event/Event_002/Event_002_01/event_002_01_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "わかった！\n「待ち人」来た……！",
            "voice": "event/Event_002/Event_002_01/event_002_01_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ブブー。",
            "voice": "event/Event_002/Event_002_01/event_002_01_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "「待ち人」来ない……？",
            "voice": "event/Event_002/Event_002_01/event_002_01_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ブブー。\n……ごめん、待ち人って何？　だれのこと？",
            "voice": "event/Event_002/Event_002_01/event_002_01_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "よくおみくじに書いてあるから……。",
            "voice": "event/Event_002/Event_002_01/event_002_01_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "「待ち人」っていうのは、\n本人が望んでいる何かってこと。",
            "voice": "event/Event_002/Event_002_01/event_002_01_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "へえ～、なるほど。\n「人」じゃなくてもいいんだ。",
            "voice": "event/Event_002/Event_002_01/event_002_01_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "早く言いなさいよ。",
            "voice": "event/Event_002/Event_002_01/event_002_01_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ヒント！\n非常にラッキーなものです。",
            "voice": "event/Event_002/Event_002_01/event_002_01_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ヒントはいいから、何があったの？",
            "voice": "event/Event_002/Event_002_01/event_002_01_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "正解はー……、ジャジャ～ン！\n四つ葉のクローバーでしたぁ！",
            "voice": "event/Event_002/Event_002_01/event_002_01_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "……なんだ。",
            "voice": "event/Event_002/Event_002_01/event_002_01_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あれ、その程度……？",
            "voice": "event/Event_002/Event_002_01/event_002_01_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "良かったじゃない、おめでとう。",
            "voice": "event/Event_002/Event_002_01/event_002_01_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "どこで見つけたの？",
            "voice": "event/Event_002/Event_002_01/event_002_01_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "偶然、道端で～。わたし、北海道に居た頃は、\nしょっちゅう見つけてたからさぁ～。嬉しくなっちゃって……。",
            "voice": "event/Event_002/Event_002_01/event_002_01_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "なんで、ラッキーなの？\n四つ葉のクローバーって。",
            "voice": "event/Event_002/Event_002_01/event_002_01_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "珍しいからでしょ？",
            "voice": "event/Event_002/Event_002_01/event_002_01_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "遺伝的にできた四葉ならね。",
            "voice": "event/Event_002/Event_002_01/event_002_01_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "遺伝？\nどういうこと？",
            "voice": "event/Event_002/Event_002_01/event_002_01_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "自然発生的にできた四つ葉のクローバーなら\n確かに珍しいけど。",
            "voice": "event/Event_002/Event_002_01/event_002_01_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ほとんどの場合、成長過程で踏みつけられたり、\n傷ついたりしたから、葉っぱが4つになっただけよ。",
            "voice": "event/Event_002/Event_002_01/event_002_01_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そうなんだぁ。\nお前、苦労してたんだなぁ。",
            "voice": "event/Event_002/Event_002_01/event_002_01_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "葉っぱに話しかけてどうするの……。",
            "voice": "event/Event_002/Event_002_01/event_002_01_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "それでも、珍しいことには変わりないんでしょ？",
            "voice": "event/Event_002/Event_002_01/event_002_01_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "まあ、そうね……。",
            "voice": "event/Event_002/Event_002_01/event_002_01_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "なにか良いことが起こるんじゃない？",
            "voice": "event/Event_002/Event_002_01/event_002_01_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そうだよ！　よーし、願っちゃおう！\n待ち人よ！　来い！",
            "voice": "event/Event_002/Event_002_01/event_002_01_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "おはよう！\nちょうど皆揃ってた！",
            "voice": ""
        },
        {
            "index": 44,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "わっ！　来た！",
            "voice": "event/Event_002/Event_002_01/event_002_01_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ただのマネージャーさんだから。",
            "voice": "event/Event_002/Event_002_01/event_002_01_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ただの？",
            "voice": ""
        },
        {
            "index": 47,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "おはよう。\n我が盟友よ……。",
            "voice": "event/Event_002/Event_002_01/event_002_01_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ちょっと、皆に話があるんだけど、今いいかな？",
            "voice": ""
        },
        {
            "index": 49,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "話？　なんですか？",
            "voice": "event/Event_002/Event_002_01/event_002_01_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "朗報だよ！\nアニメの仕事が決まったよ！",
            "voice": ""
        },
        {
            "index": 51,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えっ！　ほんとに待ち人来た！",
            "voice": "event/Event_002/Event_002_01/event_002_01_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "待ち人？\n誰のこと？",
            "voice": ""
        },
        {
            "index": 53,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "いえ、なんでも。\n続きをどうぞ。",
            "voice": "event/Event_002/Event_002_01/event_002_01_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "先日オーディションを受けてもらった、\n「C.Q.」というタイトルのアニメ、覚えてる？",
            "voice": ""
        },
        {
            "index": 55,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "えっ？\nあれ、決まったの！？",
            "voice": "event/Event_002/Event_002_01/event_002_01_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "あー、現役のアイドルとか、アクション女優とか、\nオーディションにたくさん来てたやつですね。",
            "voice": "event/Event_002/Event_002_01/event_002_01_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そうそう！\nあの、異種格闘技戦のようなオーディション！",
            "voice": ""
        },
        {
            "index": 58,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "我が悪魔軍団が、勝利したということだな……。",
            "voice": "event/Event_002/Event_002_01/event_002_01_058.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "違うでしょ。\n我々声優が、順当に仕事を得たってことでしょ。",
            "voice": "event/Event_002/Event_002_01/event_002_01_059.mp3"
        },
        {
            "index": 60,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "すっごーい！",
            "voice": "event/Event_002/Event_002_01/event_002_01_060.mp3"
        },
        {
            "index": 61,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "しかしよく受かりましたね……。\n私てっきり……。",
            "voice": "event/Event_002/Event_002_01/event_002_01_061.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "落ちると思ってた？",
            "voice": ""
        },
        {
            "index": 63,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "はい……。\nだって鳴が……。",
            "voice": "event/Event_002/Event_002_01/event_002_01_063.mp3"
        },
        {
            "index": 64,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "私？",
            "voice": "event/Event_002/Event_002_01/event_002_01_064.mp3"
        },
        {
            "index": 65,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "最後の質疑応答の時、\nとんでもないこと言ったじゃない？",
            "voice": "event/Event_002/Event_002_01/event_002_01_065.mp3"
        },
        {
            "index": 66,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "そうだっけ？",
            "voice": "event/Event_002/Event_002_01/event_002_01_066.mp3"
        },
        {
            "index": 67,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "えっ？\n覚えてないの？",
            "voice": "event/Event_002/Event_002_01/event_002_01_067.mp3"
        },
        {
            "index": 68,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "言った、言った！\nもう、冷や冷やしたよ。",
            "voice": ""
        },
        {
            "index": 69,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "覚えてない……。",
            "voice": "event/Event_002/Event_002_01/event_002_01_069.mp3"
        },
        {
            "index": 70,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "しかし、鳴のコメントが逆に功を奏したとも言える……。",
            "voice": "event/Event_002/Event_002_01/event_002_01_070.mp3"
        },
        {
            "index": 71,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "すごいよね！\nなるちゃん！　",
            "voice": "event/Event_002/Event_002_01/event_002_01_071.mp3"
        },
        {
            "index": 72,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "まあ……、聡里も相当なものだったけどね。",
            "voice": "event/Event_002/Event_002_01/event_002_01_072.mp3"
        },
        {
            "index": 73,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "私？\nそうだった？",
            "voice": "event/Event_002/Event_002_01/event_002_01_073.mp3"
        },
        {
            "index": 74,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "すごいよ、さとちん！",
            "voice": "event/Event_002/Event_002_01/event_002_01_074.mp3"
        },
        {
            "index": 75,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "凛音も覚えてないでしょ？",
            "voice": ""
        },
        {
            "index": 76,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！\nてへへー。",
            "voice": "event/Event_002/Event_002_01/event_002_01_076.mp3"
        },
        {
            "index": 77,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "凛音のコメントだって冷や冷やしたよ。",
            "voice": ""
        },
        {
            "index": 78,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "へぇ？\nわたしも？",
            "voice": "event/Event_002/Event_002_01/event_002_01_078.mp3"
        },
        {
            "index": 79,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "まず利恵が、相変わらずそつのないコメントをして……。",
            "voice": ""
        },
        {
            "index": 80,
            "heroineId": [
                0
            ],
            "name": "審査員",
            "message": "では、この作品に対する意気込みを\n順番にお話しください。",
            "voice": ""
        }
    ]
}