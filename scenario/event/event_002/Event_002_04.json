{
    "Txt_Name": "Event_002_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "鳴、どう？",
            "voice": "event/Event_002/Event_002_04/event_002_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "変わらない。",
            "voice": "event/Event_002/Event_002_04/event_002_04_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そっか……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "ちょっと休んだら？",
            "voice": "event/Event_002/Event_002_04/event_002_04_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "いやや……。時間は待ってくれない。\n日頃のトレーニング不足を解消しないと……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "利恵、スタイル良いと思うよ？",
            "voice": "event/Event_002/Event_002_04/event_002_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そう言ってくれるのは嬉しいけど……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "この前テレビで、カリスマトレーナーって人の特集を見たら……。\nなんか、妙に燃えちゃって。",
            "voice": "event/Event_002/Event_002_04/event_002_04_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "そうなんだ……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "今回、アクションシーン多いでしょ？",
            "voice": "event/Event_002/Event_002_04/event_002_04_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "台本読んでるうちに、なんかたまに体を動かすのも\n悪くないかなって、思って……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "よし、次は最高タイムを出す……。いくぞ。\nよーい、スタート！",
            "voice": "event/Event_002/Event_002_04/event_002_04_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "ふぅ～……。\n鳴、どう？",
            "voice": "event/Event_002/Event_002_04/event_002_04_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "……変わらない。",
            "voice": "event/Event_002/Event_002_04/event_002_04_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "うそっ、ほんとに？",
            "voice": "event/Event_002/Event_002_04/event_002_04_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "うん……。\nコンマ１秒まで、一緒。",
            "voice": "event/Event_002/Event_002_04/event_002_04_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そんなことありえる？",
            "voice": "event/Event_002/Event_002_04/event_002_04_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "おっはよー！\n何してるの？",
            "voice": "event/Event_002/Event_002_04/event_002_04_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "お？　凛音！\n凛音こそ何しているの？",
            "voice": "event/Event_002/Event_002_04/event_002_04_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いい天気だから、\n子供とでも遊ぼうかなあって。",
            "voice": "event/Event_002/Event_002_04/event_002_04_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "えっ？\n見知らぬ子供と？",
            "voice": "event/Event_002/Event_002_04/event_002_04_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！　すぐ仲良しになってくれるんだよー？\nすごいよ、子供って！",
            "voice": "event/Event_002/Event_002_04/event_002_04_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そうか……。\nすごいのは、凛音のほうだと思うけど。",
            "voice": "event/Event_002/Event_002_04/event_002_04_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "リエンヌ、汗だくだね？",
            "voice": "event/Event_002/Event_002_04/event_002_04_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "う、うん……。これはだから、\n長らく太陽の下に出てしまったせいで、体が溶けだしたのだ……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えっ？\nずっと公園の周り、歩いてたからでしょ？",
            "voice": "event/Event_002/Event_002_04/event_002_04_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "なんだ、見てたの？\nしかし、……少し違うな。",
            "voice": "event/Event_002/Event_002_04/event_002_04_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "なにが違うの？",
            "voice": "event/Event_002/Event_002_04/event_002_04_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "歩いていたのではない、全力で走っていたのだ。",
            "voice": "event/Event_002/Event_002_04/event_002_04_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あははははは……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あれ？\n……冗談じゃなかったの？",
            "voice": "event/Event_002/Event_002_04/event_002_04_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "冷やかしなら帰ってよねー？\nさもなくば、蝋人形にするぞ。",
            "voice": "event/Event_002/Event_002_04/event_002_04_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "でもでもー、なんで歩……、いや、走ってるの？\nマラソン大会とかに出るの？",
            "voice": "event/Event_002/Event_002_04/event_002_04_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "修行よ、悪魔修行。\n……近づくと火傷するぞ？",
            "voice": "event/Event_002/Event_002_04/event_002_04_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "（悪魔が修行って、\nさすがの凛音さんだって……。）",
            "voice": "event/Event_002/Event_002_04/event_002_04_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "すごいねー。",
            "voice": "event/Event_002/Event_002_04/event_002_04_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "（簡単に信じた……。）",
            "voice": "event/Event_002/Event_002_04/event_002_04_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "わたしも一緒にやっていーい？",
            "voice": "event/Event_002/Event_002_04/event_002_04_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "近づくと火傷するって言ったでしょ！",
            "voice": "event/Event_002/Event_002_04/event_002_04_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "大丈夫！\n距離置くから～……。よーい、ドン！",
            "voice": "event/Event_002/Event_002_04/event_002_04_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "あ、待って！\nずるいー！",
            "voice": "event/Event_002/Event_002_04/event_002_04_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "到着～～！　\nふぅ、ふぅ……、鳴どう？　タイムは？",
            "voice": "event/Event_002/Event_002_04/event_002_04_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "倍近く早くなった。",
            "voice": "event/Event_002/Event_002_04/event_002_04_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そうだろう、そうだろう！\n……途中、意識がなくなるくらい、全力を出したからな……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "凛音、遅いな？\n……さては、恐れをなしてリタイヤでもしたか？",
            "voice": "event/Event_002/Event_002_04/event_002_04_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ねえ！　みてみてー！　\n大きなカブトムシがいるよー！",
            "voice": "event/Event_002/Event_002_04/event_002_04_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "途中で飽きちゃったみたい……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "なんなの～？　人騒がせなー……。\n凛音―！　もう帰るよー！",
            "voice": "event/Event_002/Event_002_04/event_002_04_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "もうちょっとぉー、捕まえられそう……。\n手が、届かない……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "うふふ……。\nふぅー、たまには運動も悪くないね？　鳴。",
            "voice": "event/Event_002/Event_002_04/event_002_04_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "利恵、すごくいい顔してる。",
            "voice": "event/Event_002/Event_002_04/event_002_04_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そ、そう？\n続けてみよっかなぁ……？",
            "voice": "event/Event_002/Event_002_04/event_002_04_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うわぁ！　すごい珍しいちょうちょ！\nあー……、カメラ持ってくるんだったなぁ～。きれい……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "凛音。",
            "voice": "event/Event_002/Event_002_04/event_002_04_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うわぁ！　びっくり！　さとちん？\n来てたの？",
            "voice": "event/Event_002/Event_002_04/event_002_04_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "買い物の帰りに寄っただけ。\n声が聞こえたから。",
            "voice": "event/Event_002/Event_002_04/event_002_04_057.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あっ！　じゃあさ～、部屋に戻ってカメラ取ってくるから、\n前に言ってた撮影会しようよ～。",
            "voice": "event/Event_002/Event_002_04/event_002_04_058.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_059.mp3"
        },
        {
            "index": 60,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いい天気だし、絶好の撮影日和だよ～。\nちょっと待っててね！",
            "voice": "event/Event_002/Event_002_04/event_002_04_060.mp3"
        },
        {
            "index": 61,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "凛音。……ちょっと今日は気分が乗らないから。\nごめんね。",
            "voice": "event/Event_002/Event_002_04/event_002_04_061.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そっか……。\nじゃあ、さとちんの部屋に行くからさ！　そこで……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_062.mp3"
        },
        {
            "index": 63,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そういうことじゃなくて……。\nほんとに、また今度で。",
            "voice": "event/Event_002/Event_002_04/event_002_04_063.mp3"
        },
        {
            "index": 64,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "なんか、元気ないなぁ……。\n最近のさとちん。",
            "voice": "event/Event_002/Event_002_04/event_002_04_064.mp3"
        },
        {
            "index": 65,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "運動が足りないんじゃない？",
            "voice": "event/Event_002/Event_002_04/event_002_04_065.mp3"
        },
        {
            "index": 66,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "わぁ～！　びっくり。",
            "voice": "event/Event_002/Event_002_04/event_002_04_066.mp3"
        },
        {
            "index": 67,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "実は我輩も、最近の聡里が気になってたの……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_067.mp3"
        },
        {
            "index": 68,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "明日は収録初日なのになぁ……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_068.mp3"
        },
        {
            "index": 69,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そこでちょっと、アイデアがあるんだけど……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_069.mp3"
        },
        {
            "index": 70,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "アイデアって……、どんな？",
            "voice": "event/Event_002/Event_002_04/event_002_04_070.mp3"
        },
        {
            "index": 71,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "今夜、リビングに集合だ……。",
            "voice": "event/Event_002/Event_002_04/event_002_04_071.mp3"
        }
    ]
}