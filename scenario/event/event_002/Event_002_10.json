{
    "Txt_Name": "Event_002_10",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "はい、皆いるね。",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "いるよー。\n……聡里も、来てくれたのね。",
            "voice": "event/event_002/Event_002_10/event_002_10_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "……収録までの道は同じだしね。",
            "voice": "event/event_002/Event_002_10/event_002_10_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "さとちん、ありがとー！",
            "voice": "event/event_002/Event_002_10/event_002_10_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "別に、お礼を言われるようなことはしてないわ。",
            "voice": "event/event_002/Event_002_10/event_002_10_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "皆！　あくまで無事に収録を終えるのが目的だから、\n無理のないようにしてね。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はーい！\n負けないぞー。",
            "voice": "event/event_002/Event_002_10/event_002_10_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "競争じゃないから。\n走ったり、競ったりして、ケガをすることのないように。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "まったく、こんな事して何の意味があるのかしら。",
            "voice": "event/event_002/Event_002_10/event_002_10_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "へへへ。だって楽しいんだもん！\nみんなで歩くの。",
            "voice": "event/event_002/Event_002_10/event_002_10_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "はい、それじゃ、準備はいい？\nしゅっぱーつ。",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "あ、あとついでだけど。\n……凛音、昨日はありがとうね。あと利恵も。",
            "voice": "event/event_002/Event_002_10/event_002_10_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "え！？\nぜんぜん、なんかしたっけ？",
            "voice": "event/event_002/Event_002_10/event_002_10_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "我輩は何もしていないけど？",
            "voice": "event/event_002/Event_002_10/event_002_10_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "昨日もあれからずっと考えてて……。\nでも私、決めたよ。",
            "voice": "event/event_002/Event_002_10/event_002_10_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "決めた？\nなにを？",
            "voice": "event/event_002/Event_002_10/event_002_10_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "これで終わりにする。",
            "voice": "event/event_002/Event_002_10/event_002_10_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……終わりにするって。\n……まさか、さとちん。",
            "voice": "event/event_002/Event_002_10/event_002_10_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "声優辞めるとかじゃないよ。",
            "voice": "event/event_002/Event_002_10/event_002_10_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "なんだー、びっくりしちゃったー。",
            "voice": "event/event_002/Event_002_10/event_002_10_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "母さんと競うことを止める。",
            "voice": "event/event_002/Event_002_10/event_002_10_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……競ってたの？",
            "voice": "event/event_002/Event_002_10/event_002_10_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "前にさ、母さんと共演の仕事、\n断ったことあったじゃない？",
            "voice": "event/event_002/Event_002_10/event_002_10_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん。",
            "voice": "event/event_002/Event_002_10/event_002_10_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "今思えば、ばかなことしたなぁって思ってて。",
            "voice": "event/event_002/Event_002_10/event_002_10_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そうだよー、もったいなかったよ。",
            "voice": "event/event_002/Event_002_10/event_002_10_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "でもあの時は、母さんと肩を並べるような力がないと、\nやっちゃいけないんだ、と思った。",
            "voice": "event/event_002/Event_002_10/event_002_10_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "でも、そんな基準じゃ、いつまで経っても追いつけない。\n自分で、自分に許可をしてあげなきゃって……、",
            "voice": "event/event_002/Event_002_10/event_002_10_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "思えるようになったんだ。牛の話を聞いて。",
            "voice": "event/event_002/Event_002_10/event_002_10_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "牛、役に立った？　\nうまくしゃべれてたか心配で。",
            "voice": "event/event_002/Event_002_10/event_002_10_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "つまり、アウトプットするものに価値があるのではなくて、\n何をインプットしたかで価値が決まるってことよね？",
            "voice": "event/event_002/Event_002_10/event_002_10_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "よく分からないけど、多分そう！",
            "voice": "event/event_002/Event_002_10/event_002_10_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "私にとっては、話の内容よりも、凛音そのものに価値がある。\nそれと、みんな……。",
            "voice": "event/event_002/Event_002_10/event_002_10_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ありがとう、みんな。\n私なんかと一緒に歩いてくれて。",
            "voice": "event/event_002/Event_002_10/event_002_10_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "それが、チームだと思う。",
            "voice": "event/event_002/Event_002_10/event_002_10_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "ふっ。当たり前の事だ。",
            "voice": "event/event_002/Event_002_10/event_002_10_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "だから、エゴサーチなんかして。\n自分を卑下するようなことも止める。",
            "voice": "event/event_002/Event_002_10/event_002_10_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そうだよ！\n……ってごめん、エゴサーチってなに？",
            "voice": "event/event_002/Event_002_10/event_002_10_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "ネットで自分を検索すること。",
            "voice": "event/event_002/Event_002_10/event_002_10_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "私のできることを一生懸命やる。\n私は声優が好きだから、それを一生懸命やる。",
            "voice": "event/event_002/Event_002_10/event_002_10_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！　それでいいと思う！\nリエンヌもほら、しゃべったほうが元気でるよ。",
            "voice": "event/event_002/Event_002_10/event_002_10_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "……1つ、言っていいか？",
            "voice": "event/event_002/Event_002_10/event_002_10_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いいよ、ほら、大きな声で！",
            "voice": "event/event_002/Event_002_10/event_002_10_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そろそろ休憩しない？",
            "voice": "event/event_002/Event_002_10/event_002_10_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ええ！？\nもう？　",
            "voice": "event/event_002/Event_002_10/event_002_10_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "体力づくりはしてるつもりなんだけど、\nどうも伸びなやんでて……。",
            "voice": "event/event_002/Event_002_10/event_002_10_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "まあ、みなが見ていなければ、\n翼をはやして飛んでいくところだが。",
            "voice": "event/event_002/Event_002_10/event_002_10_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "うふっ、遠慮しないで飛んじゃえば？",
            "voice": "event/event_002/Event_002_10/event_002_10_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "いや、遠慮しておく。\nはい、休憩―。",
            "voice": "event/event_002/Event_002_10/event_002_10_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えー、もうすぐじゃーん、歩こうよー。",
            "voice": "event/event_002/Event_002_10/event_002_10_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "みんなで一緒に行こうよ凛音。\n私達がこうしていられるのも、ほんの少しの時間なんだから。",
            "voice": "event/event_002/Event_002_10/event_002_10_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いや、まだまだいっぱいあるよ！\nだってこの先、こんな機会いくらでもあるんだから！",
            "voice": "event/event_002/Event_002_10/event_002_10_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そうね。\nだから、休憩しよう。",
            "voice": "event/event_002/Event_002_10/event_002_10_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "もう、リエンヌは体力ないなぁ。",
            "voice": "event/event_002/Event_002_10/event_002_10_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "ちなみに私も限界。",
            "voice": "event/event_002/Event_002_10/event_002_10_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "りょーかい！\nそれじゃあ少し休もっか！",
            "voice": "event/event_002/Event_002_10/event_002_10_056.mp3"
        }
    ]
}