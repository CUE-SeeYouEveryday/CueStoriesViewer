{
    "Txt_Name": "Event_005_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "はい、というわけでアニメ「トッケン」出演声優による、\n全国ミステリースポット巡り。「ミステリーTUBE」！",
            "voice": "event/event_005/Event_005_01/event_005_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "第1回目の今回は、都内某所の心霊スポットからお届けしました。\nレポーターは私、日名倉莉子と。",
            "voice": "event/event_005/Event_005_01/event_005_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "夜峰美晴と。",
            "voice": "event/event_005/Event_005_01/event_005_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "神室絢。",
            "voice": "event/event_005/Event_005_01/event_005_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そしてそして。",
            "voice": "event/event_005/Event_005_01/event_005_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "宮路まほろがお届けしました。",
            "voice": "event/event_005/Event_005_01/event_005_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "第2回は、な、なんと！\n青森県のミステリースポットからお届けする予定です！",
            "voice": "event/event_005/Event_005_01/event_005_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ではでは、次の放送も？",
            "voice": "event/event_005/Event_005_01/event_005_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9,
                10,
                11,
                12
            ],
            "name": "4人",
            "message": "Eyes on me！　",
            "voice": "event/event_005/Event_005_01/event_005_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まったねー！　ばいばーい！　",
            "voice": "event/event_005/Event_005_01/event_005_01_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "はーいカット！　オッケー！　オッケーですよね？　ディレクター？　\nはいオッケー出ました！　お疲れ様でした！",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いやー、莉子、凄く良かったよー！　ナイスMC！\n皆もバッチリ！　最高！　もう何も言うことなし！",
            "voice": ""
        },
        {
            "index": 13,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ほんと？　あたし噛んでなかった？　\n大丈夫？",
            "voice": "event/event_005/Event_005_01/event_005_01_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "っていうか、何でマネージャーが仕切ってんのよ。\nディレクターの仕事でしょそれ。",
            "voice": "event/event_005/Event_005_01/event_005_01_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "莉子、MCすごくよかったよ。\nこれならラジオもいけるんじゃない？",
            "voice": "event/event_005/Event_005_01/event_005_01_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "はー緊張したー。\nもう、ダメ。死んでもいい……。",
            "voice": "event/event_005/Event_005_01/event_005_01_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私、やっぱり、こういう撮影は苦手かな……。",
            "voice": "event/event_005/Event_005_01/event_005_01_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "はい、じゃあ、そういうわけで、タクシー来てるから。\n新幹線の時間に遅れないように、チャチャッと行こう！",
            "voice": ""
        },
        {
            "index": 19,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "え？　どういうこと？　次の収録は明後日でしょ？\nそんな急がなきゃダメなの？",
            "voice": "event/event_005/Event_005_01/event_005_01_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あれ？　言ってなかったっけ？\n今回、スケジュールにちょっと変更があったんだよ。",
            "voice": ""
        },
        {
            "index": 21,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "え？　あたしも聞いてないよ？　そんなの。",
            "voice": "event/event_005/Event_005_01/event_005_01_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あー……、ごめん。\n伝達ミスだったみたい……。",
            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "どう変更になったんですか？",
            "voice": "event/event_005/Event_005_01/event_005_01_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "まず今から新幹線に乗ってもらって、\n皆には青森に行ってもらいます。",
            "voice": ""
        },
        {
            "index": 25,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　今から……、青森？",
            "voice": "event/event_005/Event_005_01/event_005_01_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "嘘でしょ……。",
            "voice": "event/event_005/Event_005_01/event_005_01_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "え、まさか、着いてすぐにロケじゃないですよね？",
            "voice": "event/event_005/Event_005_01/event_005_01_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あはは。ヤダなー。そんなわけないでしょ。\n自分がそんな無茶なスケジュール組んだことある？",
            "voice": ""
        },
        {
            "index": 29,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ん？　あれ？　\n現状すでに無茶だと思うんだけど、あたしがおかしいのかな？",
            "voice": "event/event_005/Event_005_01/event_005_01_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……莉子、わかるよ。その気持ち。",
            "voice": "event/event_005/Event_005_01/event_005_01_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "青森に到着次第、今日はホテルに宿泊してもらって、\n明日、朝イチからのロケを予定しています。",
            "voice": ""
        },
        {
            "index": 32,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "明日もタイトなスケジュールになってるから。\n皆、よろしくね。",
            "voice": ""
        },
        {
            "index": 33,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "青森って行ったことないから楽しみー！",
            "voice": "event/event_005/Event_005_01/event_005_01_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "おいしいマグロと、おいしい日本酒！　\nはぁ～！",
            "voice": "event/event_005/Event_005_01/event_005_01_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "美晴のそのポジティブさ、\nほんと心から尊敬するよ。",
            "voice": "event/event_005/Event_005_01/event_005_01_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "おいしいお酒が飲める時間……。\nあると良いね。",
            "voice": "event/event_005/Event_005_01/event_005_01_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "さあさあ、皆、タクシーはこっちだよ。\n東京駅までよろしくお願いしまーす。",
            "voice": ""
        },
        {
            "index": 38,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あの、マネージャー。\n着替えとか……、寮に取りに戻らせてほしいです。",
            "voice": "event/event_005/Event_005_01/event_005_01_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うん、確かに。",
            "voice": "event/event_005/Event_005_01/event_005_01_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うーん、時間ギリギリだけど、しょうがないね……。",
            "voice": ""
        },
        {
            "index": 41,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "しょうがないって……、当然でしょ。",
            "voice": "event/event_005/Event_005_01/event_005_01_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "えーっと、7号車の6のD……。\nやっぱグリーン車じゃないのね……。",
            "voice": "event/event_005/Event_005_01/event_005_01_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ、まほろ隣じゃん。あたし6のE。\n美晴は？",
            "voice": "event/event_005/Event_005_01/event_005_01_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたし、5のD。",
            "voice": "event/event_005/Event_005_01/event_005_01_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "じゃあ、絢が5のEかな？",
            "voice": "event/event_005/Event_005_01/event_005_01_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "マネージャーも少しは気が利くじゃん。\nこれ、席回転出来るでしょ。",
            "voice": "event/event_005/Event_005_01/event_005_01_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "え？　そんなこと、わざわざする？",
            "voice": "event/event_005/Event_005_01/event_005_01_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "いいじゃん、いいじゃん！\n席回転させようよー？",
            "voice": "event/event_005/Event_005_01/event_005_01_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたし、1回あれやってみたかったんだよね。",
            "voice": "event/event_005/Event_005_01/event_005_01_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うわー、わたしもやりたい！\n仲間と旅してるって感じ！",
            "voice": "event/event_005/Event_005_01/event_005_01_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はあ、よく楽しめるね。この状況で。",
            "voice": "event/event_005/Event_005_01/event_005_01_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ！　すいません！",
            "voice": "event/event_005/Event_005_01/event_005_01_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "どうしたの？",
            "voice": "event/event_005/Event_005_01/event_005_01_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "えへへ。\nはしゃぎすぎて、斜め前の席の人にぶつかっちゃった。",
            "voice": "event/event_005/Event_005_01/event_005_01_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "もう、子どもじゃないんだから……。",
            "voice": "event/event_005/Event_005_01/event_005_01_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "……。\n絢、どうかした？　さっきから静かだけど。",
            "voice": "event/event_005/Event_005_01/event_005_01_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん……。",
            "voice": "event/event_005/Event_005_01/event_005_01_057.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ん、どうしたの絢？",
            "voice": "event/event_005/Event_005_01/event_005_01_058.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ないの……。",
            "voice": "event/event_005/Event_005_01/event_005_01_059.mp3"
        },
        {
            "index": 60,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "何が？",
            "voice": "event/event_005/Event_005_01/event_005_01_060.mp3"
        },
        {
            "index": 61,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私の乗車券……。",
            "voice": "event/event_005/Event_005_01/event_005_01_061.mp3"
        }
    ]
}