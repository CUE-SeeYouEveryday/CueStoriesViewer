{
    "Txt_Name": "Event_005_07",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "皆、大丈夫？　",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……ね、みんな、まほろにあわせて。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "マネージャー、まほろ怖いよう……。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……え？　マジで？　",
            "voice": "event/Event_005/Event_005_07/event_005_07_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ほら早く。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "怖いなー、怖いなー。　\nいやだな―、いやだな―。",
            "voice": "event/Event_005/Event_005_07/event_005_07_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "もういや！　早ぐがえりだい。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "皆、どうしたの？　何かあったの？　",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "うん……。大丈夫……。\nだいぶ落ち着いたから……。",
            "voice": "event/Event_005/Event_005_07/event_005_07_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "機材の方はもう大丈夫なんですか？　",
            "voice": "event/Event_005/Event_005_07/event_005_07_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あ、うん、さっきの収録はデータがなんとかなったみたいだから、\n次は、ここから少し移動したところで収録するみたい。",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "わかりました……。　\n落ち着いたらすぐに行きます……。",
            "voice": "event/Event_005/Event_005_07/event_005_07_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "じゃあ、ホテルのエントランスで待ってるから。\n落ち着いたら来てね。",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はい。　\nすぐに行くので、下で待っててください。",
            "voice": "event/Event_005/Event_005_07/event_005_07_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "何よ。",
            "voice": "event/Event_005/Event_005_07/event_005_07_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "いやー、改めてまほろさんの演技力に感動したところですよ。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "バレなかったかな？　",
            "voice": "event/Event_005/Event_005_07/event_005_07_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん、あの感じだと多分マネージャーさん、\nわたしたちがちゃんと怖がってると思ってるはずよ。",
            "voice": "event/Event_005/Event_005_07/event_005_07_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……データはなんとかなりました、だって。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。よく言うよねー。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。わたし、なんだか楽しくなってきちゃった。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ここからはまほろたちのターンだよ。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　何かするつもりなの？",
            "voice": "event/Event_005/Event_005_07/event_005_07_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そりゃそうよ。　\n今度は、こっちが仕掛ける番。",
            "voice": "event/Event_005/Event_005_07/event_005_07_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは！　いいね！\nそうこなくっちゃ！",
            "voice": "event/Event_005/Event_005_07/event_005_07_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "え……、っと。\nでも……、マネージャーさんたちもきっと……。",
            "voice": "event/Event_005/Event_005_07/event_005_07_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "「トッケン」のディスクを買ってくれた人を、　\nもっと楽しませたいと思ってのことかもしれないし……。",
            "voice": "event/Event_005/Event_005_07/event_005_07_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あんまり、ひどいことはしないほうがいいんじゃないかな？",
            "voice": "event/Event_005/Event_005_07/event_005_07_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そうね。　\nあまりやりすぎないようにはするけど。",
            "voice": "event/Event_005/Event_005_07/event_005_07_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "でも、少しくらいはあの人たちにも怖い思いしてもらわないとね。",
            "voice": "event/Event_005/Event_005_07/event_005_07_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うんうん。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そこで作戦なんだけど……。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "皆、揃ったね。\nじゃあ、次の収録に行こうか。",
            "voice": ""
        },
        {
            "index": 36,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ちょっと今回ロケ用の車が手配できなかったんだけど、\nいわゆる普通のバスを借りてるから、それで移動になるよ。",
            "voice": ""
        },
        {
            "index": 37,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "バスはあっちだから、皆乗ってね。",
            "voice": ""
        },
        {
            "index": 38,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "って、あれ？\nなんだか顔色が悪いけど、どうかした？",
            "voice": ""
        },
        {
            "index": 39,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……。　\nちょっと怖くて……。",
            "voice": "event/Event_005/Event_005_07/event_005_07_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "というと？　",
            "voice": ""
        },
        {
            "index": 41,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あの……、部屋で声が聞こえたんです。　\nなんか、うめき声のような……。",
            "voice": "event/Event_005/Event_005_07/event_005_07_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？　本当に？　",
            "voice": ""
        },
        {
            "index": 43,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "はい……、私も聞きました。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "美晴が言うには、昔、ホテルで心中事件があったとか……。　\nだから、きっと……。",
            "voice": "event/Event_005/Event_005_07/event_005_07_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "それでみんな怖くなっちゃって……、　\nもちろんわたしも……。",
            "voice": "event/Event_005/Event_005_07/event_005_07_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "絢なんか泣いちゃうくらいで……。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　　\n……うん。そう。……グスン 。",
            "voice": "event/Event_005/Event_005_07/event_005_07_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そんなことがあったんだ……。　\nどう皆、ロケ続けられる？　最後までできる？",
            "voice": ""
        },
        {
            "index": 49,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はい。今はもう落ち着いたんで……。　\nロケは最後までやらせてください！",
            "voice": "event/Event_005/Event_005_07/event_005_07_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そう。わかった。\nじゃあ、スタッフさんたちに話してくるね。",
            "voice": ""
        },
        {
            "index": 51,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ロケ先はここから少し移動したとこになるから、\n冷えないように先にバスに乗っていて。",
            "voice": ""
        },
        {
            "index": 52,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "はい。よろしくおねがいします。",
            "voice": "event/Event_005/Event_005_07/event_005_07_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ねね、見た～？\nあのマネージャーの嬉しそうな顔。",
            "voice": "event/Event_005/Event_005_07/event_005_07_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん。見た見た。",
            "voice": "event/Event_005/Event_005_07/event_005_07_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ディレクターも向こうからこっち見ながら、\n喜んでたみたいだったよ。",
            "voice": "event/Event_005/Event_005_07/event_005_07_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いい画が撮れた！　\nってやつでしょ。",
            "voice": "event/Event_005/Event_005_07/event_005_07_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まほろたちの手のひらの上だとも知らずに。　\nっふ。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_057.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。ほんとだよね。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_058.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "じゃあ、ロケが始まったら、\n作戦通りに……、ね？　",
            "voice": "event/Event_005/Event_005_07/event_005_07_059.mp3"
        },
        {
            "index": 60,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はーい。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_060.mp3"
        },
        {
            "index": 61,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "おっけー。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_061.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。まかせて。　",
            "voice": "event/Event_005/Event_005_07/event_005_07_062.mp3"
        }
    ]
}