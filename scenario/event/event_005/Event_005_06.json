{
    "Txt_Name": "Event_005_06",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……っていうこと。　",
            "voice": "event/event_005/Event_005_06/event_005_06_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "この世には不思議なことなんて何もないのよ……。\n莉子、絢。",
            "voice": "event/event_005/Event_005_06/event_005_06_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "なるほど……。　",
            "voice": "event/event_005/Event_005_06/event_005_06_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "全部仕込みのドッキリ企画だったってこと？　",
            "voice": "event/event_005/Event_005_06/event_005_06_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "多分だけどね。　",
            "voice": "event/event_005/Event_005_06/event_005_06_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "でも、きっと今までも小さいハンディカメラとかで、　\nこっちを隠し撮りしてる人がいたはず。",
            "voice": "event/event_005/Event_005_06/event_005_06_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そう言われてみれば、色々怪しかったよね。　",
            "voice": "event/event_005/Event_005_06/event_005_06_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたしがぶつかっちゃった、斜め前の席の人とかさ。　",
            "voice": "event/event_005/Event_005_06/event_005_06_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "さすがまほろね……。",
            "voice": "event/event_005/Event_005_06/event_005_06_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "これ、わざわざ新規に撮ってるんだとしたら、　\n多分、何かの特典映像かもね。",
            "voice": "event/event_005/Event_005_06/event_005_06_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうなのかな？　",
            "voice": "event/event_005/Event_005_06/event_005_06_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "多分ね。",
            "voice": "event/event_005/Event_005_06/event_005_06_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "でも、こんなドッキリ企画みたいなの、　\nどこの誰の発案なのやら。",
            "voice": "event/event_005/Event_005_06/event_005_06_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そっか。こういうのが面白おかしく編集されて、\n特典映像とかになるんだ……。へえ。",
            "voice": "event/event_005/Event_005_06/event_005_06_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……。\nあの……、さ。ちょっと気になったんだけど……、さ。",
            "voice": "event/event_005/Event_005_06/event_005_06_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "どうしたの？　莉子。　",
            "voice": "event/event_005/Event_005_06/event_005_06_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "最初から全部仕込みだったとしたら……。\n昨日撮った「ミステリーTUBE」……。",
            "voice": "event/event_005/Event_005_06/event_005_06_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "「トッケン」プロモーションのネット配信番組っていうのも、　\n最初っからドッキリ用の嘘……、なのかな？",
            "voice": "event/event_005/Event_005_06/event_005_06_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あ、それ、たしかに気になる。　\n……どうなのかな？",
            "voice": "event/event_005/Event_005_06/event_005_06_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……。　",
            "voice": "event/event_005/Event_005_06/event_005_06_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたしさ、ラジオパーソナリティやりたいっていつも言ってるでしょ？ ",
            "voice": "event/event_005/Event_005_06/event_005_06_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "だから、今回……。\nラジオじゃないけど……。",
            "voice": "event/event_005/Event_005_06/event_005_06_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "MCのポジションもらえたのが、　\nちょっと嬉しかったんだ。",
            "voice": "event/event_005/Event_005_06/event_005_06_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "だから、それも嘘だった……。\nってなると、ちょっと凹んじゃうな……。",
            "voice": "event/event_005/Event_005_06/event_005_06_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "莉子……。　",
            "voice": "event/event_005/Event_005_06/event_005_06_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いや……。　\n確実じゃないけど、多分それは大丈夫だと思う。",
            "voice": "event/event_005/Event_005_06/event_005_06_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "どうして？　",
            "voice": "event/event_005/Event_005_06/event_005_06_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "昨日の収録と今日の収録で、　\nスタッフが違ったこと……、かな。",
            "voice": "event/event_005/Event_005_06/event_005_06_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "え？　それで何でわかるの？",
            "voice": "event/event_005/Event_005_06/event_005_06_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "多くの場合、アニメやドラマの放送に向けてのプロモーションと、　\n作品の中身とでは、企画の出どころが違うわけ。",
            "voice": "event/event_005/Event_005_06/event_005_06_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "それに伴って制作を請け負う会社も変わってくるし、　\n当然スタッフも変わるの。 ",
            "voice": "event/event_005/Event_005_06/event_005_06_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ディレクターも然り……、ね。　",
            "voice": "event/event_005/Event_005_06/event_005_06_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "つまり、どういうこと？　",
            "voice": "event/event_005/Event_005_06/event_005_06_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "同じドッキリ企画の収録だったら、　\n普通そのまま同じスタッフで進めるでしょ？",
            "voice": "event/event_005/Event_005_06/event_005_06_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、そっか。そうだね。　",
            "voice": "event/event_005/Event_005_06/event_005_06_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "だから、昨日収録したものは本当の番組だってこと。　\n本当の第2回の収録もちゃんとあるはず。",
            "voice": "event/event_005/Event_005_06/event_005_06_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ああ、よかったー。それを聞いてホッとしたよ。　",
            "voice": "event/event_005/Event_005_06/event_005_06_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ほら、本来、明日の午後から青森で収録の予定だったでしょ？　",
            "voice": "event/event_005/Event_005_06/event_005_06_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "それが急きょ、突貫スケジュールで今日からになった。　",
            "voice": "event/event_005/Event_005_06/event_005_06_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "多分、本当の第2回の収録の前に、　\nこの特典映像と思しき収録を入れたかった……、ってとこでしょ。",
            "voice": "event/event_005/Event_005_06/event_005_06_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "つまり、これは急きょ決まったってこと？　",
            "voice": "event/event_005/Event_005_06/event_005_06_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "うーん、ここにねじ込むことは、　\n最初から計画に入っていたのかもね。",
            "voice": "event/event_005/Event_005_06/event_005_06_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "だったら、最初から予定変更って言ってくれればよかったじゃん。　",
            "voice": "event/event_005/Event_005_06/event_005_06_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "慌てて着替え取りに行く必要もなかったし。　",
            "voice": "event/event_005/Event_005_06/event_005_06_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そういうドタバタ込みの、ドッキリだったのかな。",
            "voice": "event/event_005/Event_005_06/event_005_06_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうか……。なるほど……。\nでも、ほんっと人騒がせな企画だわ……。",
            "voice": "event/event_005/Event_005_06/event_005_06_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "でも、莉子の言ってたこと……。\nちゃんと確かめなきゃだけどね。",
            "voice": "event/event_005/Event_005_06/event_005_06_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ドッキリさせられるくらいなら、　\nまあ、笑って許せるけど、",
            "voice": "event/event_005/Event_005_06/event_005_06_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "出演声優の夢をあざ笑うような企画なんだとしたら……。　",
            "voice": "event/event_005/Event_005_06/event_005_06_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちょっと許せないな。　",
            "voice": "event/event_005/Event_005_06/event_005_06_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん、私もまほろの意見に賛成。　",
            "voice": "event/event_005/Event_005_06/event_005_06_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "おーい、皆、まだ部屋にいるのかな？　",
            "voice": ""
        },
        {
            "index": 53,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、マネージャー。　",
            "voice": "event/event_005/Event_005_06/event_005_06_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うめき声に怖がって部屋を飛び出して来てもいい頃なのに、　\nいつまでも出て来ないから……。",
            "voice": "event/event_005/Event_005_06/event_005_06_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "それで様子を見にでも来たってこと？　",
            "voice": "event/event_005/Event_005_06/event_005_06_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "皆、大丈夫？　",
            "voice": ""
        },
        {
            "index": 57,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……ね、みんな、まほろに合わせて。　",
            "voice": "event/event_005/Event_005_06/event_005_06_057.mp3"
        }
    ]
}