{
    "Txt_Name": "Main_01_99_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ここまで来ちゃったね……。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "長かったような気もするけど、\nあっという間だった！",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "今でも、信じられない。\nこんなステージに立てるなんて……。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "数多の表現者が、\n伝説を轟かせてきた場所だからな。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ああ……。\n緊張してきた！",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "今さら緊張したってしょうがないよ！",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "悠希は、緊張とは無縁だもんね。\nちょっと羨ましい。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "フッ、我輩もこのようなステージなど、緊張に値せんな。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "じゃあ、なんで利恵はさっきから水ばっか飲んでるの？",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "う、うるさいな！\nこれは水分が足りなくなったら困るからよ！",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そろそろ本番だよ？\n準備はいい？",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "マネージャーさん……。\nわたし達、大丈夫でしょうか……？",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どうかしたの？",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あの歓声を聞いてたら。\nだんだん怖くなってきちゃって……。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "……ちょっと来て。\n皆に見せたいものがある。",
            "voice": ""
        },
        {
            "index": 16,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "わたし達に、見せたいもの……？",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ここは、ステージの袖ですよね……。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "もしかして、マネージャーがステージで踊るの！？",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "すごい……。\nお客さんがいっぱい……。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "この会場のみんなが、わたし達を待ってるんだ……。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "マネージャーさんのおかげです。\nずっと側で支えてくれたから。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "この景色を……。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "何か……、すっごい、ドキドキしてきた！",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたしも。\nなんだか胸が張り裂けそう。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "ふっ、我輩の勇姿を、そこで見ているといい。",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "はい！\n行ってきます！　マネージャーさん！",
            "voice": "main_01/Main_01_99/Main_01_99_01/main_01_99_01_026.mp3"
        }
    ]
}