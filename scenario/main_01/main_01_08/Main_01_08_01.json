{
    "Txt_Name": "Main_01_08_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "空気は張り詰めたように冷たくて、",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "街の色も少し青ざめて見えて。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "もっと寒くなったら、きっと言葉も凍ってしまいそう。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "こんばんは。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、あいり。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "どうかしたんですか？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　どうして？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "改札出たら、空を見ながら、\n1人で立ってる絢さんを見つけたから。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "どうかしたのかなって思ったんです。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "変……、だった？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ううん、そういうわけじゃないんです。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "冬……。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "え？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "冬だなって思って。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "空気が冷たくて、吐く息は白くて、\r\nやっぱり今は冬なんだなって。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "最近ずっとそうでしたよ。\r\n昨日の朝もすごく寒くて。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。そうだね。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいりも朝、お布団から出るの少しつらいです。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふっ。あいりもそうなんだ。\r\nしっかりしてそうなのに。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいりなんて全然ダメダメです。\r\n千紗ちゃんに迷惑かけないように頑張ってるだけなんです。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "偉いね。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ええ？　そうですか？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "あら、恵庭さんに神室さん……。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "聡里さん……。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "それに志穂さんも。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "こんなところで立ち話か？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。\r\nなんだか冬だねって。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "冬？　確かに。\r\nこたつの中だけで過ごしたい季節だな。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "私はこの季節、嫌いじゃないわよ。\r\n空気が凛としてて。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "変わった考え方もあるもんだ。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "きっと神室さんもそうなんでしょ？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　どうして？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "冬が好きそうなタイプに思えたけど、違った？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん……。そういうことなのかも。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そういうことなのかも？\n不思議な言い方をするのね。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうかな？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいり、冬も好きだけど、\r\nお布団から出る時はちょっとつらいかな……。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "それは人間として自然な感情だ。\r\n何も気にすることはないぞ。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "とりあえず……、こんなとこでずっと立ち止まってたら、\n他の人の邪魔になるわ。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "行きましょ？\nみんなこれから事務所なんでしょう。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "え？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "あら、違った？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "いいえ。あたりです。\nあいりはマネージャーさんに呼ばれて……。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうなんだ。一緒だね。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "なんの用だろう……、次のお仕事のことかな。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私も仕事のことかも？\r\nやっとオーディション通った作品、あるから……。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "あら、恵庭さんも神室さんもオーディション通ったの？\nおめでとう。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えへへ。ありがとうございます。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。ありがとう。\r\n聡里さんたちはなんで一緒なの？　2人でなんて、めずらしいけど。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "あなたたちと同じ。\r\nマネージャーさんに呼ばれたの。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "と言っても、私と鹿野さんは、同じオーディションに合格したから、\r\n一緒でも不思議じゃないんだけど。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "台本、まだ貰えてないし、きっとその話じゃないかしら。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうだったんだ。\r\nおめでとう。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ふふっ。ありがとう。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "たまたまだよ。たまたま。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "羨ましいな。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_057.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "羨ましい？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_058.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はい。だって同じ収録の現場でお仕事出来るのって羨ましいなって。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_059.mp3"
        },
        {
            "index": 60,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "Birdのみんなと同じ現場に入ってる時、一緒に本読みするの、\nすごく頼もしかったから。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_060.mp3"
        },
        {
            "index": 61,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "1人は淋しい？",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_061.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "うん……。少し……、心細いかな。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_062.mp3"
        },
        {
            "index": 63,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そう……。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_063.mp3"
        },
        {
            "index": 64,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "さ、行くぞ。行き先はみんな一緒みたいだからな。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_064.mp3"
        },
        {
            "index": 65,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そうね。\n少なくとも同じ事務所の間は、ね。",
            "voice": "main_01/main_01_08/Main_01_08_01/main_01_08_01_065.mp3"
        }
    ]
}