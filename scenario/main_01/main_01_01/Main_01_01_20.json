{
    "Txt_Name": "Main_01_01_20",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『あなたなんて、大っ嫌い！』",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "『嫌いでもいい。』",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『二度と顔も見たくない！』",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "『それでも、僕は君に会いに行く。\n何度でも、何度でも。』",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（相変わらず、志穂は上手いな……。）",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『どうしてそこまでするの？』",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "『決まってるじゃないか。\n僕は君を……。君を……。』",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "あれ？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "どうしたの？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（急に止まっちゃった……。\nどうしたんだろ……？）",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "はい、ここまで。\n一旦止めます。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "すみません。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "鹿野さん、どうしたの？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "言えない。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "言えない？\nセリフが言えないってこと？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "このキャラがこんなこと言うのおかしい。\nあり得ない。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（キャラのセリフに納得できないってこと……？）",
            "voice": ""
        },
        {
            "index": 19,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "だからって、セリフを言わないのはダメでしょ。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ほのかと違って、こだわりがある。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "わたしにはこだわりが無いってこと？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ちょっと……。\nやめなよ。",
            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ジャーマネは黙ってて。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "桐香先生、すみません……。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "あの2人、いま喧嘩中で。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "ふふっ……。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（桐香先生が笑った……？）",
            "voice": ""
        },
        {
            "index": 28,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "セリフが言えない？　そこがこだわり？\n冗談でしょ？　そんなのはただの我がままよ。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "我がまま？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "声優の仕事は、与えられたセリフに命を吹き込むこと。\nあなたは仕事を放棄してるだけ。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "でも……。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "でも？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "私の声に合ってない……。\nこの声じゃこのセリフは言えない。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（それが本当の理由か……。）",
            "voice": ""
        },
        {
            "index": 35,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "あなたには素晴らしい声がある。\nテクニックもある。でもそれがネックになってる。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ネック……？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "声やテクニックに頼っちゃダメ……。\n心で演じるの。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "心で演じる……？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "そのために、キャラクターの心と向き合いなさい。\nそうすれば、どんなセリフも言えるはずよ。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "次回までの宿題にするわ。\n考えておいて。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様～。",
            "voice": ""
        },
        {
            "index": 43,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "マネージャーさん。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_044.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あれから志穂とほのかの様子はどう？",
            "voice": ""
        },
        {
            "index": 45,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "心配して来てくれたんですか？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_046.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "全然ダメ。\n2人とも部屋に閉じこもって話もしない。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_047.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そっか……。\nちょっと話をしてみるよ。",
            "voice": ""
        },
        {
            "index": 48,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "志穂、いる？",
            "voice": ""
        },
        {
            "index": 49,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "志穂。",
            "voice": ""
        },
        {
            "index": 50,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "いません。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_054.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いませんて。\n……思いっ切り居留守じゃん。",
            "voice": ""
        },
        {
            "index": 52,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ご用件のかたはメッセージをどうぞ。\nピー。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_056.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "留守電じゃないんだから。\n……ちょっと出てきてよ。",
            "voice": ""
        },
        {
            "index": 54,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "新聞なら間に合ってる。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_059.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "……そんなこと言わずに、洗剤もつけますから。",
            "voice": ""
        },
        {
            "index": 56,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "はぁ？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_061.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "仕掛けてきたのはそっちでしょ！",
            "voice": ""
        },
        {
            "index": 58,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "今、忙しい。\nジャーマネとコントをやってる時間ない。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_063.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（こっちだって好きでやってるわけじゃないよ……。）",
            "voice": ""
        },
        {
            "index": 60,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "忙しいって……、何してたの？",
            "voice": ""
        },
        {
            "index": 61,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "あくび。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_066.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "思いっ切りヒマじゃん！",
            "voice": ""
        },
        {
            "index": 63,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "時限爆弾の解除。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_068.mp3"
        },
        {
            "index": 64,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いや、忙しいってレベルじゃないでしょ！",
            "voice": ""
        },
        {
            "index": 65,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "悪いけど、ジャーマネと漫才してる時間ない。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_070.mp3"
        },
        {
            "index": 66,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "本当は何してたの……？",
            "voice": ""
        },
        {
            "index": 67,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "宿題。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_072.mp3"
        },
        {
            "index": 68,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "宿題って……。\n今日のレッスンの？",
            "voice": ""
        },
        {
            "index": 69,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "難しい。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_074.mp3"
        },
        {
            "index": 70,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "確かに難しい話だったね……。",
            "voice": ""
        },
        {
            "index": 71,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "わけわからん。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_076.mp3"
        },
        {
            "index": 72,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "例えば、ほのかと話してみたらどう？",
            "voice": ""
        },
        {
            "index": 73,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "何で？",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_078.mp3"
        },
        {
            "index": 74,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "桐香先生が言うように、自分の心と向き合うんだよ。\nわだかまりを吐き出すんだ。そうすれば、何か分かるかもしれない。",
            "voice": ""
        },
        {
            "index": 75,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……。",
            "voice": "main_01/Main_01_01/Main_01_01_20/main_01_01_20_080.mp3"
        }
    ]
}