{
    "Txt_Name": "Main_01_01_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "じゃ、いい。\n諦める。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いや、いいよ。\nただし、今日だけね。",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ほんとか？",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "もちろん、他の人もOK出したらね。",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "舞花。\nいいか？",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "まぁ、今日だけなら……。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "陽菜やほのかにも聞く。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ついでに聞くが、\nリンを鳴らすのはダメ？",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "リンてなに？",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "仏壇の前にある、チーンて鳴らすやつ。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "それはやめて……。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "マネージャー！\n片付け終わりました！",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_013.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様。\nずいぶん時間かかったね。",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "運動器具とか、健康グッズが結構多くて。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_015.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そんなに沢山持ってきたの？",
            "voice": ""
        },
        {
            "index": 16,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "通販番組でいいなって思ったら、\nつい買っちゃうんですよね。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_017.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "腹筋を鍛える機器とか、\nフィットネスマシーンとか。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_018.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "まぁ、分からなくもないけど。",
            "voice": ""
        },
        {
            "index": 19,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "無駄遣いすることなんて、ほとんどないんですけど、\n運動器具だけは我慢出来なくて。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_020.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "大丈夫？\n全部部屋に入った？",
            "voice": ""
        },
        {
            "index": 21,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "一応、入りました。\nただ、寝る場所がなくて困ってます。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_022.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "少し実家に送り返した方がいいんじゃない？",
            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そうしようかな。\n昨日、バランスボール5個も買っちゃったし。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_024.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えっ！\n5個も！？",
            "voice": ""
        },
        {
            "index": 25,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "明日か明後日には、ここに届くと思います。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_026.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そんなに買ってどうするの？",
            "voice": ""
        },
        {
            "index": 27,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "みんなにプレゼントしようと思って。\nあ、もちろんマネージャーの分もありますよ。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_028.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え、あ、ありがとう……。",
            "voice": ""
        },
        {
            "index": 29,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "体幹を鍛えるには、どんなときもバランスボールに乗るんです。\n仕事中とか勉強中とか、テレビ観てるときも。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_030.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "でも、まぁ、部屋が狭くなるかなぁ……。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_031.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そうだ。\nベッドを無くして、バランスボールで寝ればいいんだ！",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_032.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いくらほのかでも、それは無理があると思うな……。",
            "voice": ""
        },
        {
            "index": 33,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "いたっ！",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_034.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "この声は陽菜……？",
            "voice": ""
        },
        {
            "index": 35,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "陽菜！\nどうかした？",
            "voice": ""
        },
        {
            "index": 36,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "えっと……、廊下にダンベルが置いてあって、\nそれにつまづいちゃいました……。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_037.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ほのか……。",
            "voice": ""
        },
        {
            "index": 38,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ヤバイ、出しっぱなしだった！\n陽菜！　ごめん！　すぐ片付ける！",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_039.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "それじゃ、わたしは部屋に帰りますね……。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_041.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "陽菜はまだ片付け終わってないの？",
            "voice": ""
        },
        {
            "index": 41,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ちょうどさっき終わりました。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_043.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そうなんだ。\n陽菜もお疲れ様。",
            "voice": ""
        },
        {
            "index": 43,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ん？\n何か買ってきたの？",
            "voice": ""
        },
        {
            "index": 44,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "えっと、部屋で食べようと思って……。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_046.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "いやぁ、ごめんごめん。\n片付けてきたから、もう大丈夫。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_047.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あれ？\n陽菜、キャベツ買ってきたんだ。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_048.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？\nキャベツ買ってきたの？",
            "voice": ""
        },
        {
            "index": 48,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "う、うん。\n急に食べたくなって……。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_050.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "丸々1つ食べるの？\nしかも部屋で？　1人で？",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_051.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "そ、そうなの。\n最近、野菜不足だから……。",
            "voice": "main_01/main_01_01/Main_01_01_04/main_01_01_04_052.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "もしかして、何か隠してる……？",
            "voice": ""
        }
    ]
}