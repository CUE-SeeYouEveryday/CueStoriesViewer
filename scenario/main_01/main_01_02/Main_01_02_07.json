{
    "Txt_Name": "Main_01_02_07",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "なってなーーーい！",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "わっ！\nびっくりしたぁ～。なに！",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "みんななってない！　夜は夜更かし、朝は寝坊。\nお風呂はダラダラ、掃除もしない。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "そんなんでよく声優になろうなんて言えるわね？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "えぇ？\nまたぁ？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ごめんなさい。\nなかなか慣れなくて。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "もう１週間だよ。\n子供じゃないんだからさ。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ねぇ、どうでもいいけど、\nシャワーの水圧弱くない？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ほんっとどうでもいいわ。どうでもいい。\nついでに、悠希。１つだけ言わせてくれない？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "えっ、なに？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あんた、そんな慌てて食べてるけど、\nちゃんとよく噛んでるの？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そうかな？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あと、あいりはスプーンばっかり使い過ぎ。\n幼児じゃないんだから。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はい、すみません。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "それと柚葉。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私にもなんかあるの？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "お蕎麦の食べ方が、なんかおいしくなさそうなのよね。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ホワーイ？\nこれヌードル食べる常識ね。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "なにカタコトになってるのよ！",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ブーブー！",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "お蕎麦やラーメンをすすって食べるのは、日本の常識なのよ！",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "なるほど、日本の常識……。\nわかった、気をつける！",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "たしかに……。ラーメンはこう豪快にずずずーって！\n最後は濃厚なスープを一気に飲み干す……！",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "先にスープからでしょ？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あたしはやっぱ麺からいく派だなあー……。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ラーメン食べたいな……。\n味玉入りで……。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "おなか空いたね！",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あーんた達！\n話を逸らさないの！　私は食べ方の話してるの！",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "うーん。\nおいしければ食べ方なんてどうでもいいじゃん！",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "そういう問題じゃないでしょ？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ねえ。チサ？\nもっとさ、楽に生きましょう？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "習慣を変えなきゃ、自分は変われないんだよ。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "自分変える必要あるの？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あるよ。\n私達が目指しているものはなに？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "声優でしょ？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "声優ってね、女優の上に成り立つものなの。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "女優は優れた女じゃなきゃダメ。\n声優はさらに、声も優れなきゃダメ。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "言い過ぎじゃない？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "言い過ぎじゃない。今のままじゃダメ。\n習慣を変えて、自分を変えて、そして、世の中を変えるのよ。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "世の中変える話だった？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "違うと思うけど。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "チサはチサでやればいいと思う。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "えっ？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私は私でやるから。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "私達はチームでしょ？\nそれに、共同生活してるんだから、なおのことよ。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私のやり方が気に入らないなら、無理に合わせる必要はないわ。\nその方が、イライラしないでしょ？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "そう。じゃあ是非勝手にやってちょうだい。\n私もあんたには協力しないから。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ごめん、千紗ちゃん……。\nあいり達の話も聞いてくれないかな……。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "どうでもいいけどあいり。\nすぐ謝るクセやめてくれないかな。桐香先生にも言われてるでしょ？",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ごめん。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "それをやめて！",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "イライラの原因はカルシウムだけじゃないみたいよ。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "それじゃ、せいぜい頑張ってね。\nいままでお世話になりました。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "では、わたくしも。\nアビエントメルシーボーク。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ちょっと……、ちょっと待ってよ。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ごめん、あいりが余計なこと言おうとしたから。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "いやいや、あいりが反省するところじゃないって。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_057.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ごめん……。あ、また謝ってるね……。\nごめん、あいりも行くね。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_058.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "……あれぇ。",
            "voice": "main_01/main_01_02/Main_01_02_07/main_01_02_07_059.mp3"
        }
    ]
}