{
    "Txt_Name": "Main_01_02_26",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "なに？　協力してほしいことって。",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私達は今、頭が禿げてるの。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……という想像をもって、私たちを見てください。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "……なんで？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "役作りです。\nハムレットのための。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "髪を剃ろうとしたんだけど、\n時間がなくて、美容院に行けなかったの。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "だから、なんで禿げ頭？",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "いろいろ調べたんだけど。\nこの物語の作者は禿げてるのよ。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "シェイクスピアね？",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "だからよ。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "ああ……。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "そう……。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（2人とも、完全に呆れてる……。）",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "まぁ、いいわ。\nあなた達は、今、禿げているのね。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "とにかくやってみせて。\nじゃあ、始めるわよ。よーい、はい！",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『結婚はバカとしろ！』",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……えっと、あそうだ。\n『利口はみんな、女の怖さと汚さを知っているからにゃ！』",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_000.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あ……、『しっているからな！』",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_000.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "『待って、待ってください、ハムレット王子！』",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_018.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "はい、そこまで。\n……なるほど、頑張りましたね。ただ……。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_019.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "禿げ頭は、関係なかったわね。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_020.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "そんな……。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_021.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "自分が禿げだと思えると、\nすごく自信をもって演じられるのよ。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_022.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "じゃあ2人はこの先、禿げ頭で声優活動続けるってことね？",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_023.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "ふふっ、キャラクターが出ていいかもね。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_024.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "いやよ。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_025.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "お断りします。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_026.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "それでも、想像力だけで緊張がほぐれたんだから。\n大したものね。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_027.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "ただ……、赤川さん、演技中に素に戻っちゃダメ。\n「あーそうだ」なんて言わなきゃ普通に出来たのに。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_028.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "九条さんは、よし。\n初めてにしてはよく出来てたわ。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_029.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（柚葉は……、出色の出来だったなぁ）",
            "voice": ""
        },
        {
            "index": 32,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "すみません！\n悠希組戻りました。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_031.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいりの名前なくなってる……。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_032.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "はい、じゃあ、あなた達の、これからの課題を伝えます。\nまず天童さん。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_033.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "はい。\nお願いします！",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_034.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "勢いは大事。とても大事なこと。\nでももっと演技の本質を知る必要がある。分かった？",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_035.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "はい！\nありがとうございます！",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_036.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "（勢いオッケー！\nぶっちぎりでトップってことね！）",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_037.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（また都合よく解釈してるんだろうなぁ……。）",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_038.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "恵庭さんは、自信を持つこと。\nそのための努力をおしまない。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_039.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あ、はい。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_040.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（自信、自信。\n疑わないようにしなきゃ。）",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_041.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "赤川さんは相手役をよく感じること。\n1人でやろうとしない。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_042.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "はい！\nわかりました。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_043.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "（相手をよく見る。よく聞く。\n……集中しなきゃ！）",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_044.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "九条さんは、初めてにしてはよく出来ました。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_045.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "でも、感覚だけに頼ってるきらいがある。\n演技を、確実なものにしていってね。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_046.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "はい。\n……あははっ。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_047.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "なにかおかしなこと言ったかしら？",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_048.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ごめんなさい。\n思い出し笑いしちゃって。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_049.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "ふぅ。どんな状況でも、それだけリラックスしてれば、\n問題はないかもね。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_050.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（コーチが禿げ頭だと想像したら、\n笑いがこみあげて……、あははは。）",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_051.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "皆いい？　マイクの前であっても、演技の基礎は一緒。\n今日得た経験を忘れないでね。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_052.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ありがとうございました！\n桐香先生。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_053.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "マネージャー、ちょっと。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_054.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "……なんでしょう？",
            "voice": ""
        },
        {
            "index": 57,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "悠希の演技、どう思った？",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_056.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ははは……。\n相変わらず勢いだけはいいのですが……。",
            "voice": ""
        },
        {
            "index": 59,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "少し、変だったように思うんだけど……。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_058.mp3"
        },
        {
            "index": 60,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いつも通りだと思いますけど。",
            "voice": ""
        },
        {
            "index": 61,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "ならいいんだけど。\n……慎重に見守ってあげて。",
            "voice": "main_01/Main_01_02/Main_01_02_26/main_01_02_26_060.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "はぁ、分かりました……。",
            "voice": ""
        }
    ]
}