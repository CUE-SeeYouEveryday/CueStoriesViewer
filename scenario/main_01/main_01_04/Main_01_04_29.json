{
    "Txt_Name": "Main_01_04_29",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "こんばんは～！",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "皆！\n元気してる～？",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "あれ、誰だ……？",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "知らない……。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "「エールブルー」チーフマネージャーのりおさんだよ。",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ああ、この人がりおさんか。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "ちょっとちょっと～。\n初対面みたいな反応しないでよー！",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "だって初対面でしょう？",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "ノンノン！　\n事務所で何度も挨拶してるよ？",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "そうでしたっけ……？",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "ヒドイ……。\nそりゃ、最近全然顔出せてなかったけど。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "今ごろになって、ようやく登場だけどさ。 ",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "わたしは覚えてます！　\nすごく元気で可愛い人だなって思ってました！",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "あら、嬉しい！　\n凛音ちゃんも可愛いよ～！",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ありがとうございます～！",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "この2人は、妙に波長が合いそうね……。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "ところで、何でりおさんが来たの？",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "明日はいよいよ、\n初めての収録でしょ？",
            "voice": ""
        },
        {
            "index": 19,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "わたしが、スタジオでの注意点とか、収録にあたっての心得を、\n教えてあげようと思ってね。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "りおさんはこう見えて、敏腕マネージャーだから。",
            "voice": ""
        },
        {
            "index": 21,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "こう見えてって、どういう意味やねん！",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "すいません、若く見えてって意味で……。",
            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "そうなの～。\nいっつも若く見られて、軽いやつって思われるの～。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "年齢だけの問題じゃないかと……。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "ん？\n何か言った？",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "いえ、何も言ってません。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "それじゃ、早速いってみよ～！",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "まず何より大事なのは、\nブースの中で余計な音を立てないってこと。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "ジャラジャラ音を立てるアクセサリーとか、\nガサガサ音がする服はNG！",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "なるほど～！\n勉強になります！",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "てことは、利恵とか鳴の服は問題ありですか？",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "そうね～。\nシンプルな服って持ってる？",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "1着も持ってません。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "私も。\n全部こういう感じです。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "あちゃ～、そうか～。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "要は、音を立てなきゃいいんでしょ？",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "大丈夫。\n衣擦れの音1つさせないから。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "凄い自信ね……、OK！　いいわ！\nもし当日、監督に言われたらすぐジャージに着替えてね。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ジャージが嫌なら、わたしのを貸してあげるよ！",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "悪いが、ジャージを選ぶ。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "りおさん、次は？",
            "voice": ""
        },
        {
            "index": 42,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "次は、とても基本的なことね。\nスタッフさんたちに、大きな声でキチンと挨拶すること。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そんなの当たり前じゃないですか！",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "凛音ちゃんは、心配してないのよ。\n他の3人、挨拶してみて。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "我輩がエリス。\nまたの名を丸山利恵。よろしく頼む。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "何でそんな上からなの！\nもっと謙虚に。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "宇津木聡里です。\nどうも……。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "無愛想すぎる！\nもっと笑顔で。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "遠見鳴です。\nよろしくお願いします。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "なんていうか、覇気がない！\nもっと元気よく。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ふざけてるわけじゃないんですけどね……。",
            "voice": ""
        },
        {
            "index": 52,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "皆に悪気が無いのはよく分かるよ？\nそういう性格なのも分かる。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "でも、初めてのスタッフさんたちは分からないの。\nああ、この人は不真面目なんだなって思われちゃう。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "そしたら、皆が損するんだよ？",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "確かに……。\n最初の挨拶は、頑張ろう。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "前向きに善処します。",
            "voice": "main_01/Main_01_04/Main_01_04_29/main_01_04_29_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そんな、政治家みたいなこと言わないでよ……。",
            "voice": ""
        }
    ]
}