{
    "Txt_Name": "Main_01_03_05",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……まほろ？\nちょっと、言わせてほしいんだけど。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "な、なによ……？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "Windの皆！　おはようございます！\nあれからチームとしての結束は深まったかい？",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "まぁ……、そこそこ。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "落ち着いてるね、流石大人コンセプトWind。\nさて、早速なんだけど、社長からの指令をお伝えしてもいいかな？",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちょっと待ってマネージャー。\n今取り込み中なの。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あ……、は、はい。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "まほろ、さっきからルール、ルールって言ってるけど、\n莉子はいつも持ち時間20分のところ、15分で出てきてるわけでしょ？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "つまり、毎日5分ずつ、繰り越しの時間が出来てる。\n莉子、昨日の前に長風呂したのはいつ？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "えっ？　いつだっけ？　確かバイトが超忙しかった日だから……。\n1，2，3……。8日……。うん、8日前だね。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "８かける５、イコール40。\nこの40分に、通常の20分を足すと？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……１時間！\nあはは！　絢、よく気付いたねー。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら、ほんと。すごーい！",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ルールにこだわるなら、\nここまで計算すべきなんじゃない？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……異議あり。\nマネージャー、ぼーっと突っ立ってないで仕切って。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "……え？　あ、はい。\nで、では、宮路まほろさん。どうぞ。",
            "voice": ""
        },
        {
            "index": 17,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "制限時間は、あくまでその日1日に設定された時間でしょ。\n使わなかった時間が繰り越してくなんて、どう考えてもおかしい。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "裁判長もそう思うでしょ？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "裁判長じゃないんだけど……。",
            "voice": ""
        },
        {
            "index": 20,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたしも被告扱いになるのはいやだなー。\nそれに、なに？　裁判？　そういうのやめよ？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "マネージャーも乗ってんじゃないの！",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "……はい。\n裁判はやめましょう……。",
            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\nみんな楽しそう。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "えっ？　うそでしょ？\nこれが楽しそうに見えるわけ？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "美晴は、ちょっと変わってるからね。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "とにかく！　繰り越しなんておかしいし、\nお風呂は1人20分以内っていうルールは守るべき。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "お風呂まで誰かと一緒なんて絶対に嫌。\nあんた達と共同生活するなら、これだけは譲れないから。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……なんでそこまでむきになるの？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まほろはほんっと頑固だねー。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "……。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "美晴、あんたも何か言いなよ。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうね。\n美晴の意見、聞きたい。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "……。\nあはっ。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "へっ？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なんか、家族みたいだなぁ、って。\nお風呂の入り方でケンカなんてして。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ケンカっていうか、あたし、被告扱いになるとこだったわ。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。楽しそうだったけど？\nそうね。じゃあ……、こうしましょ？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "まほろがお風呂に入りたいときはちゃんと言って。\nお風呂の外から声かけてくれるのでもいいから。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうしたら、みんな時間通りに出るようにしよう。\nわたし達も言われる前に気付いてあげよう。ね、莉子。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まぁ……、そうだね、メンバー同士。\nみんな気持ちよくいてほしいもんね。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "絢も、それでいい？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "いいとおもう。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "まほろは……、\nそれじゃあダメかな？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まあ、そういうことにしてくれるなら、\nまほろはそれでいいけど……。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい！　決まり。\nわたしも長風呂気をつけまーす。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あれ？　うまくまとめられたけど。\nよくよく考えたら、美晴が1番長風呂なんじゃない！",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "てへ。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん。\nリーダー決定！！　夜峰美晴！",
            "voice": ""
        },
        {
            "index": 49,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "えっ？\n突然なんですか、マネージャーさん。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "実は、Windのリーダーを決めてほしいと社長に言われていてね。\nそれを皆に伝えに来たんです。",
            "voice": ""
        },
        {
            "index": 51,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "じゃあ、ちゃんと話し合わないと。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うーん、今の一幕を見ていたら、\nどうもその必要はなさそうに思えたけど。",
            "voice": ""
        },
        {
            "index": 53,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "皆はどうかな？",
            "voice": ""
        },
        {
            "index": 54,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたしは異議なし！\n美晴が1番リーダーに向いてると思うよー。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私も異議なし。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "異議なし。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ということで！\n引き受けてもらえるね？",
            "voice": ""
        },
        {
            "index": 58,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "えー。なんだか話が急すぎてちょっと。\n……でも、みんながそれでいいなら……。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_058.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "じゃあ、決定！\nおめでとう！",
            "voice": ""
        },
        {
            "index": 60,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "なんだか、お風呂の話をしてたら、\n広ーいお風呂でゆっくりしたくなっちゃったね。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_060.mp3"
        },
        {
            "index": 61,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "新しいスパが、橋渡ったところに出来たって聞いたけど。\n今ならオープン記念で入場料半額だって。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_061.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "マ～ジ？\nよし、今から行こうよ。そこ。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_062.mp3"
        },
        {
            "index": 63,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あっ、わたしも行きたーい。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_063.mp3"
        },
        {
            "index": 64,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "スパ……。\n……まほろも行く。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_064.mp3"
        },
        {
            "index": 65,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あれ？\n……１人がいいんじゃなかったの？",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_065.mp3"
        },
        {
            "index": 66,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "うるさいな。",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_066.mp3"
        },
        {
            "index": 67,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "じゃあ、今からみんなで行きましょう。\nそしてお風呂上りには冷たいビール！　くーっ！",
            "voice": "main_01/Main_01_03/Main_01_03_05/main_01_03_05_067.mp3"
        },
        {
            "index": 68,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（いいなぁ。\nこの後の会議がなければ、自分も行きたかった……。）",
            "voice": ""
        }
    ]
}