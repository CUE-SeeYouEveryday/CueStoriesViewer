{
    "Txt_Name": "Main_01_03_32",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あっち行ったよ。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あの階段の上！",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はあはあ……。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "？？？",
            "message": "ほらあ、やっぱり\nみんなを連れてきてくれた。",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ただいま。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "美晴！",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……帰ってきてたんだ！",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あれ？\nまだ1週間ほど先の予定じゃ……？",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん……。\nちょっと予定切り上げちゃった。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "向こうに行って、気づいたんだ。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "今はやっぱり、ピアノより声優に本気になっていたいって……。\nそして、みんなと一緒の時間が大事だったんだっ……て。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "だから予定早めて帰ってきちゃった。\n……ということで～……、ただいま！",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "おかえり！",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "おかえり！",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "おみやげは？",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\nちゃんとみんなの分あるよー？",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "でも、不思議。\nまた風船がみんなと引き合わせてくれたね。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "偶然ってすごいよねぇー。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ううん……、偶然じゃないよ、きっと。……あの時の風船。\nね、そうなんでしょ？　君……。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "……ありがとね。\nまた、わたしたちを引き合わせてくれて……。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "行っちゃった……。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "美晴の言う通りなら、きっとまた現れるんじゃないかな？\nあたし達が離れ離れになりそうになった時にさ。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "風に吹かれて……、か。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "風に吹かれて、集まった仲間だね。\nまほろ達。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "たまたま集まったようなチームだったのに、\nすっかり……、かけがえのない仲間になっちゃった。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_000.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "今度はさ、わたしたちが風を吹かせられたらいいね！",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_026.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "どういうこと？",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_027.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "日本中の……。\nううん、もっとたくさんの、世界中の人たちの心に……。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_028.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたし達の声を通して、素敵な風を吹かせられたらいいなって……。\nそれが、わたし達の進む道……、なんじゃないかな？",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_029.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "世界中の人の心に風を吹かせる……、かぁ。\nまた大変な仕事だね！",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_030.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "やりがいはありそう。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_031.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "確かにね。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_032.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたし達、いつかは寮を卒業して、\nチームも卒業するんだろうけど……。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_033.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "でも……、その日までは、わたし達全員一緒のチームで。\nチームWindでいようね！",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_034.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "もちろん！",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_035.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん！",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_036.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まぁ、いいんじゃないの。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_037.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "さぁ！　寮に帰りましょう！\n真咲さんにも帰国の報告しなきゃ。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_038.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "えぇ？\nあんた誰にも連絡してないわけ？",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_039.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "帰ってからすればいいかな～って。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_040.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "何でそう行き当たりばったりなのかなぁ……。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_041.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "えぇ……、何？\n今わたし、怒られてるの？",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_042.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あっはは……。",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_043.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "（世界中の人の心に風を吹かせる……。\nできるよね……、きっと。わたしたちなら……！）",
            "voice": "main_01/Main_01_03/Main_01_03_32/main_01_03_32_044.mp3"
        }
    ]
}