{
    "Txt_Name": "Main_01_03_16",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "実人生の中から演技のヒントを探し、\n演技の中から生きるヒントを探す。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "俳優の人生とはそうあるべきです。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ぐはぁ～。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "どうしたの？\n莉子。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "このさ、レッスンスタジオと寮との往復生活。\nさすがに息詰まらない？",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "バイトも行ってるでしょ。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "バイトは嫌いじゃないけどさー。\nそれは息抜きにはなんないじゃん。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "サーフィンとかも。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "サーフィンかー。最近行けてないんだよね。\nレッスンとバイトでスケジュール埋まりがちでさ。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "はぁ～、デビューはいつの日になるのか……。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "たしかにデビューは、待ち遠しいわね。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あー、どこか南の島にでも行きたいなぁ……。\n真っ青な海でサーフィン、気持ちいいだろうな。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……ハワイって、良いって言うよね。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "島全体がパワースポットなんでしょ？",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "一度は行ってみたいなぁ、ハワイ。\nあたし売れたら、年末をハワイで過ごすのが夢なんだぁ。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ハワイもいいけどさぁ、\nグアムも、意外と安くていいらしいよ。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "聞いたことある。\n場合によっては沖縄より安いって。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "沖縄かぁ……。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "泡盛……。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "白いビーチ。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "柔らかいテビチ。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "熱帯のフルーツ。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "三線（サンシン）の音色。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "コリコリのミミガー。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "広いさとうきび畑。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "芳醇なラフティ。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "莉子、豚？\n豚しか出ない？",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "出るよ。海ブドウでしょ？　もずく、アンダギー。\nあと、ジーマミー豆腐でしょ、ゴーヤも好きだし……。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "それだけ食べ物ばかり出てくるのも、\nある意味すごいのかもね。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたし売れたら、沖縄に移住するのが夢なんだ。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "売れてからの願望多いね。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "でもさ、よく言うじゃない？\n売れる前は、時間があってもお金がない。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "売れてからは、お金はあっても時間がない。\nだから結局、遊ぶことなんか難しいって。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "でも、言ってみたいかも。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "なんて？",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "「お金はあるけど、時間がない」って。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "だよねー。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "「本当に大切なのはお金じゃない」っていうのもさ。\n言ってみたいよね。余裕が出来た上で。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "妄想おつかれさまー。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ねえ、売れるって、どういう状態のことなのかな？",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そりゃ、寝る間もなくって、分刻みのスケジュールで……。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "とりあえず、ちゃんと生活出来ることかな。\n全部、自分一人で。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "フォロワー三桁万の状態かな。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "美晴は？\nどう思う？",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "んー……。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あるでしょ？\n売れた時の理想像って。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうだなぁ……。\nみんなと、旅行に行けるようになることかな？",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "どういうこと？",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ちゃんと休みがとれるようになって、\nお金のことも気にしないで旅行を楽しめるような、そんな状態かな？",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "なるほど。",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……ちょっとさ。実際、計画立ててみない？",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なんの？",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたし達の、卒業旅行！",
            "voice": "main_01/Main_01_03/Main_01_03_16/main_01_03_16_053.mp3"
        }
    ]
}