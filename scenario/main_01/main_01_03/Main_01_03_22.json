{
    "Txt_Name": "Main_01_03_22",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "皆、おめでとう！",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "全員の力で成しえた勝利よ。\nただし、本当の勝負はここから。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "気を引き締めて、頑張ってね！",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "まだ夢みたい……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私がデビュー……。\n私が声優……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ただいまー。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私がアフレコ……。\n私がデビュー……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "絢、ただいま！\nなに？　ひとりごと？",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "これは現実……。\n夢じゃない……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "きこえてないね。これは……。\nじゃ、わたし部屋いくねー。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "声優は私の夢……。\n夢がかなった……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あれ？　今、美晴帰ってきた？\nどこ行ってたの、社長の話の後……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "って、絢。なにさっきからブツブツ言ってんの？\nキモイんだけど。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、まほろ。\nいや、なんていうか、まだ現実感がないっていうか。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私が声優デビュー……。\nうん、実感がわかない……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ふーん、実感がわかない……、か。\nそういうものなのかもね……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "まほろはそういうのないの？",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まあ、ないわけじゃないけど……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まほろは小さい頃からやってるから。\nあんた達とは、ちょっと感じ方も違うかもね……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そっか。\nそうだよね。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そんな難しい顔してないで、家族にでも連絡すれば？\n人に伝えてるうちに実感わくかもよ？",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "私は声優になったんだー！　って。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん、そうだね、そうするよ。\nありがとう。まほろ。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "本当に大変なのは何事もプロになってからだから。\n今のうちに喜んどいたほうがいいんじゃない？",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "おっ！\nいいこと言いますねー。まほろさん。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "何よ？\n聞いてたの？",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "じゃ、私ちょっと電話してくる！",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あっ、もしもし？　おばあちゃん？\nうんうん。おばあちゃん元気でやってる？　……そう、よかった。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あのね、おばあちゃん、私デビュー決まったんだ。\nえっ？　違うよ。レビューじゃなくてデビュー。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そう、私、声優になるの。プロの声優に。\n今まで応援してくれてありがとう。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私が声優になりたいって言った時、\n笑わないで話を聞いてくれたの、おばあちゃんだけだった。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "真剣に聞いてくれたのおばあちゃんだけだった。\nおばあちゃんだけが私を応援してくれて……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "それで……。\nだから……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私、おばあちゃんのおかげで声優になれました。\n応援してくれて、ありがとうございました。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "これからもよろしくね、おばあちゃん。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ちが、泣いてなんかないよ。\n私もう大人だもん。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん、体に気を付けてね。\nじゃ、おやすみ……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "電話、してきたの？",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。\nおばあちゃんに。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そっか。\nで、おばあちゃんなんて？",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "おめでとうって。\nおめでとうって……、おばあちゃんが……。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "よかったじゃん。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん……、よかった。\nよかったよお。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。\n絢は実は泣き虫だったんだねえ。よしよし。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "わーーーーん。\n私は、声優になったぞー。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うんうん。\nよしよし。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "わーーーーん。",
            "voice": "main_01/Main_01_03/Main_01_03_22/main_01_03_22_047.mp3"
        }
    ]
}