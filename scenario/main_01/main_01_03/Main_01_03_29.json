{
    "Txt_Name": "Main_01_03_29",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "ウィーンにピアノ留学？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい。予定していた留学希望者の中で急に欠員が出たみたいで、\n先生に「よかったら、夜峰、どうだ？」と。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "で、つまりその期間、休暇を取りたいというわけね？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_004.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい。\n急な話で申し訳ないのですが……。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_005.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "わかった。考えておく。\nほかのメンバーには言ったの？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_006.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "いえ……。\nまだです。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_007.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "チームだもの。\n早めに伝えておいてあげなさい。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_008.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい、そうします……。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_009.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "えっ？　美晴がピアノ留学？\nそれ、誰から聞いたの？　美晴本人？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_010.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いや。さっき、社長室の前を通った時に、話し声が聞こえてさ。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_011.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "本当に？\n聞き間違えじゃなくて？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_012.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "本当だって。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_013.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "留学って、期間どのくらいなの？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_014.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "わかんないけど。\n留学って言うくらいだから、1年くらいはあるだろうねぇ。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_015.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "やだ。\nそんなの。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_016.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そうはいっても美晴が決めることだし……。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_017.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "でも、大きな仕事が終わったところとはいえ、\nデビューしたばかりのこのタイミングで留学しなくても……。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_018.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "本気なのかな？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_019.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "冗談でそういうこと言うタイプでもないからなあ。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_020.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "とにかく美晴に聞いてみようよ。\nどれくらいの予定でいるのか……、とかもさ。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_021.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あっ……、美晴。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_022.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あ、みんなー。よかったー。\nみんなにね、伝えておきたい話があったの。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_023.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "留学のこと？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_024.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あれ？\nどうして知ってるの？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_025.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "さっき社長室の前を通ったら、話し声が聞こえた。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_026.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "本当に行くの？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_027.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん。\nいいタイミングだったんだ。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_028.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "初めてのアフレコが終わって、\nひと段落したこのタイミングで留学の話がもらえたのは。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_029.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ほら？　前にも話したでしょ？\nわたし、ウィーンに留学に行くのが夢だったって。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_030.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん……。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_031.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まっ、美晴が自分でそう決めたのなら、\nあたしたちは何も言わないよ。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_032.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ねっ、絢。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_033.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん……。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_034.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……元気でね。まほろからはそれだけ。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_035.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん。\nもちろん体には気を付けるつもりだけど……。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_036.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ねえ、なんかみんな勘違いしてない？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_037.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "へっ？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_038.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "留学は留学でも、短期留学だよ？\n1か月だけの。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_039.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "留学って1年くらい行くもんなんじゃないの？",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_040.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "チーム解散の危機かと思ってひやひやしちゃったよ。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_041.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "よかった……。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_042.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "もちろん、本当はね。\n長期留学したいんだけど、でもそれはしばらくおあずけ。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_043.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "デビューが決まった日からね、\nわたし、ピアノと声優どっちが大事か、ずっとずっと考えてた。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_044.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "でも結局……。\nわたしにはどっちも大事としか思えなかった。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_045.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あの日、みんなと一緒にオーディションを受けて、\nみんなと一緒にアフレコをしてわかったの。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_046.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "今はとりあえず、声優に力を注いでみたいなって。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_047.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "だから、この短期留学は、\nピアノへの思いをしばらく封印するためって感じかな。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_048.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "なるほどね。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_049.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そういうわけで、ちょっとウィーン行ってくるね。\nおみやげ、待っててね。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_050.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "だってさ。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_051.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "絢、よかったねー。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_052.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。\nよかった。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_053.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "最初からまほろたちに報告しないから、変な想像しちゃうでしょ。\n美晴が悪い。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_054.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "えー。\nなんでー。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_055.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。\n私もそう思う。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_056.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "絢までそんな……。\nわたし悲しい……。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_057.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "絢は本気で寂しかったんだよ、美晴。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_058.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そっか……。\nごめんね。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_059.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_060.mp3"
        },
        {
            "index": 60,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "出発はいつなの？\n前の日はみんなで美味しいもの食べに行こうよ、ね！",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_061.mp3"
        },
        {
            "index": 61,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そうだね。そうしよう。\nおみやげ、待ってるからね。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_062.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "本当に、よかった……。",
            "voice": "main_01/Main_01_03/Main_01_03_29/main_01_03_29_063.mp3"
        }
    ]
}