{
    "Txt_Name": "Main_01_03_27",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "制作会社から聞いた住所は、この辺のはずだけど……。\nえっ？　まさかこのアパート……？　ずいぶん古いけど。",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うっ。ちょっと怖いな……。\nううん。わたしは真実を探しに来たの。行かなきゃ。",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "はぁい。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あの……、声優の夜峰と申します。\n先生に、お話を伺いにきました。",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "なに？\n私に話って。",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あの……、\n「こんにちは、いただきます。」についてなんですけど。",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "ああ、そのこと……。\nここで話すのもなんだから、近くの喫茶店に行きましょ。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はぁ。",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "（あれ？　意外と優しい感じ……。\n断られるかと思ったのに。）",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "……わたし達ではない？",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "ごめんね。端的に言うと、\n私の希望した声優じゃないってことなの。",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "何度も打ち合わせしたのに。\n結局、無名の新人がキャスティングされてて。",
            "voice": ""
        },
        {
            "index": 13,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "……。",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "ごめんなさいね。\n決してあなた達が悪いと言ってるわけではないのよ。",
            "voice": ""
        },
        {
            "index": 15,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "……もう、いいかしら。\nお話し出来ることはここまでよ。",
            "voice": ""
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "お気持ち……、わかります。",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "……えっ？",
            "voice": ""
        },
        {
            "index": 18,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたしも、わたしの大好きな声優さんの作品、\n観たいですもん！　すごくわかります！",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "あなた……。",
            "voice": ""
        },
        {
            "index": 20,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ごめんなさい、力不足で……。\nわたしが……、売れてたらなぁ……。",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "ほんとに、あの、あなたを責めてるわけじゃないのよ。",
            "voice": ""
        },
        {
            "index": 22,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたし達、先生の作品すごく好きでした。素晴らしかったです。\n数日間でも夢を見れて、幸せでした。ありがとうございました！",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "……。",
            "voice": ""
        },
        {
            "index": 24,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "それじゃ、失礼します。\nお話聞けて良かったです。",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "……ちょっと待って！",
            "voice": ""
        },
        {
            "index": 26,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい？",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "あなた達の演技……、私にみせてくれない？",
            "voice": ""
        },
        {
            "index": 28,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "演技……、ですか？",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                0
            ],
            "name": "原作者",
            "message": "どこでもいいわ。\n私のお話で、演じてる所を見せてもらえる？",
            "voice": ""
        },
        {
            "index": 30,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "……わかりました！\n事務所と連絡とってみます！",
            "voice": "main_01/Main_01_03/Main_01_03_27/main_01_03_27_030.mp3"
        }
    ]
}