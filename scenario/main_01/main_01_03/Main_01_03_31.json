{
    "Txt_Name": "Main_01_03_31",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はぁはぁ……。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "今回はなかなかつかまんないね……。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "どこまで連れてく気なの……。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "よっ、と。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あれ？　絢、まほろ。なんだー、また風船追いかけてんの？",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "莉子！",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あれ？\nバイトじゃないの？",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "今あがってきたところ。\nそれより何？　また懐かしいね。あの日と同じじゃん。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "となると、ここで……。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。\nやっぱり。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "もう、なんで離しちゃうの。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "いや、それでよかったんだ。\nたぶんね……。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "っで、追いかけるんでしょ？\n風船。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そうなるね。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_000.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "行こう！",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_014.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あらら。\n君……、久しぶりね。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_015.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ねえ、君、こんにちは。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_016.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "また、来るって、わたし、知ってたよ。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_017.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "君は、あの時の風船……、だよね？",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_018.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                0
            ],
            "name": "美晴の母",
            "message": "ほら、美晴。もう泣かないの。\n飛んでった風船はもう二度と帰ってこないんだから。",
            "voice": ""
        },
        {
            "index": 21,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うえっうえっ、ヒック。\nだって、せっかく、お兄ちゃんが、買ってくれ、たのに、ヒック。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_020.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                0
            ],
            "name": "美晴の母",
            "message": "おかあさんがまた同じの買ってきてあげるから。",
            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ダメ！\nあの風船じゃなきゃ、ダメなの。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_022.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                0
            ],
            "name": "美晴の母",
            "message": "もう……、困ったわね。",
            "voice": ""
        },
        {
            "index": 25,
            "heroineId": [
                0
            ],
            "name": "美晴の兄",
            "message": "……取ってきてやるよ。\nあれがいいんだろ？",
            "voice": ""
        },
        {
            "index": 26,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "お、おにいちゃん。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_025.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                0
            ],
            "name": "美晴の母",
            "message": "もう、姿も見えないのよ。\n探しようがないじゃない。",
            "voice": ""
        },
        {
            "index": 28,
            "heroineId": [
                0
            ],
            "name": "美晴の兄",
            "message": "それでも、取ってきてやるから。\n美晴は待ってろ。",
            "voice": ""
        },
        {
            "index": 29,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あの時のお兄ちゃんの背中が……、頼もしかったな。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_028.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ねえ、君だったんでしょ。\nチームが決まる時にも来てくれたのは。",
            "voice": "main_01/Main_01_03/Main_01_03_31/main_01_03_31_029.mp3"
        }
    ]
}