{
    "Txt_Name": "Card_3070004_2",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいたたた……。\nワンちゃん、大丈夫？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "大丈夫そうだね、良かったぁ。\nなんなのこの穴……。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_003.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "……でも、夜を過ごすのにはちょうどいいかも。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_004.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "だれか、あいりから連絡は？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_005.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ない。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_006.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "メールしても既読にならない。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_007.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あいりが行きそうな場所、思いつく人は？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_008.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "全然。\nプチキュアショーとか？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_009.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "私、ちょっとあいりの部屋見てくる。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_010.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "自分は、近所を探してみる。",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "どうしたんだろう……。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_012.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "……。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_013.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "チワワ",
            "message": "ヘッヘッヘッ",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "今、あかりつけるね。\nペンライトだけど……。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_015.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "……お腹すいた？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_016.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                0
            ],
            "name": "チワワ",
            "message": "クゥーン",
            "voice": ""
        },
        {
            "index": 17,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ちくわ食べる？　\n夕ご飯にと思って買って来たの。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_018.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": "チワワ",
            "message": "ワンッ",
            "voice": ""
        },
        {
            "index": 19,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "書置きがあった。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_020.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "なんて？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_021.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "「旅に出ます。探さないでください」だって。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_022.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "それだけ？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_023.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "うん。\nスマホも部屋に置きっぱなしだった。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_024.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "そりゃ、繋がらないわけね。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_025.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "実家とか、連絡してみる？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_026.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "いや、待とう。\n変に慌てたら、あいりに恥をかかせることになるかもしれない。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_027.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "なんでよ。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_028.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "信じてあげよう。\nきっと大丈夫だよ。あいりなら。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_029.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ひっ。なに？　鳥？　\nこんな夜中に？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_030.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あれ……？\nお腹痛い。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_031.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "さっきのちくわかなぁ……。\nう、いたたた。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_032.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                0
            ],
            "name": "チワワ",
            "message": "ワンッ",
            "voice": ""
        },
        {
            "index": 33,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "良かった、君は大丈夫なんだね。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_034.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "でも……、うぅ、いたい……。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_035.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "満点の星空のもと、大自然に囲まれながら、\n自分を見つめなおしてみたい。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_036.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "そう思って旅に出たのに……。\nうぅぅ、お腹いたいよぉ。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_037.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                0
            ],
            "name": "チワワ",
            "message": "ワン！",
            "voice": ""
        },
        {
            "index": 38,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "みんな、今頃どうしてるかなぁ。会いたいよぉ……。\nうわーん、いたっ、うわーん、いたっ。いたいた……。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_039.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                0
            ],
            "name": "チワワ",
            "message": "ワン、ワン、ワン、ワン、ワン！",
            "voice": ""
        },
        {
            "index": 40,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ねえ、万が一のことがあったら、君がみんなに伝えてね。\n……みんなのことが大好きだったよって。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_041.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                0
            ],
            "name": "チワワ",
            "message": "ワン、ワン、ワン、ワン、ハイーン！",
            "voice": ""
        },
        {
            "index": 42,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "だ、だれ？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_043.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                0
            ],
            "name": "お婆さん",
            "message": "なんだ、よしこ、ここにいたんけぇ？",
            "voice": ""
        },
        {
            "index": 44,
            "heroineId": [
                0
            ],
            "name": "チワワ",
            "message": "ワン！",
            "voice": ""
        },
        {
            "index": 45,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "よしこ？　えっ……？\n夕方会った、おばあさん？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_046.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                0
            ],
            "name": "お婆さん",
            "message": "はぁ？　めんこい子じゃねえのぉ、\nこんなとこでなにしてんだぁ？",
            "voice": ""
        },
        {
            "index": 47,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "おばあさんこそ、こんな山の中で……。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_048.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                0
            ],
            "name": "お婆さん",
            "message": "ばかなこと言ってんでねえよ。\nここは家の裏庭だよ。",
            "voice": ""
        },
        {
            "index": 49,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ええぇ？　",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_050.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                0
            ],
            "name": "お婆さん",
            "message": "こんなとこで寝そべってねえでぇ、\n早く、家ン中はいれ。",
            "voice": ""
        },
        {
            "index": 51,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "でもあいり……。\n1人の力で過ごしたくて……、ただ……。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_052.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "とりあえず……。\nお手洗いお借りできますか？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_053.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ぅん？　朝か……。\nだれ？　そこに立ってるの。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_054.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "……みんな、リビングで横になって……。\nどうしたの？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_055.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あいり……！？\nなに言ってるの、あんたのこと心配して、みんな待ってたのよ。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_056.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "そうか……。\nごめんなさい。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_057.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どこにいってたの？　\n野宿したの？　",
            "voice": ""
        },
        {
            "index": 58,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "田舎街の……、おばあさんの家の……。\n高級な布団で寝てました。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_059.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "どういうことよ！　\nもっと詳しく説明しなさい！",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_060.mp3"
        },
        {
            "index": 60,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "旅に行ってたんだよね。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_061.mp3"
        },
        {
            "index": 61,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えっ……。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_062.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "旅から帰ったんでしょ？　\nお帰り。待ってたよ。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_063.mp3"
        },
        {
            "index": 63,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "……うえっ、うえーんっ。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_064.mp3"
        },
        {
            "index": 64,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "もう……。\nあんまり心配させないでよね。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_065.mp3"
        },
        {
            "index": 65,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "う、うん。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_066.mp3"
        },
        {
            "index": 66,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "よし！　恵庭さん。\nよく出来きたわよ。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_067.mp3"
        },
        {
            "index": 67,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "……そうですかぁ？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_068.mp3"
        },
        {
            "index": 68,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "力が抜けて、よりあなたらしくなった。\nそれでいいのよ。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_069.mp3"
        },
        {
            "index": 69,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はぁ。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_070.mp3"
        },
        {
            "index": 70,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "なんか……、元気ないね、あいり。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_071.mp3"
        },
        {
            "index": 71,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "でも演技は良くなった。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_072.mp3"
        },
        {
            "index": 72,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "そりゃそうかもだけど……。\n家出が計画通りいかなかったからかなぁ。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_073.mp3"
        },
        {
            "index": 73,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "懲りてないよ、あの子。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_074.mp3"
        },
        {
            "index": 74,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "へっ？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_075.mp3"
        },
        {
            "index": 75,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "この前書店でね？",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_076.mp3"
        },
        {
            "index": 76,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "登山の本見て勉強してた。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_077.mp3"
        },
        {
            "index": 77,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ふむふむ、なるほど……。\n次は絶対心配させないぞ！",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_078.mp3"
        },
        {
            "index": 78,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "可愛い子には旅をさせろ！",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_079.mp3"
        },
        {
            "index": 79,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "って、家出はダメでしょ……。",
            "voice": "card/card_07/Card_3070004_2/card_3070004_2_080.mp3"
        }
    ]
}