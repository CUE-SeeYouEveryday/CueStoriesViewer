{
    "Txt_Name": "Card_4110005_1",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（ここだと思うんだけど……。）",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（どこに行ったんだ……？）",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "マネージャー……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_002.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あ、いた……。\nこんな人気のないとこで、どうしたの……？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちょっとお願いがあって……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_004.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お願い？",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（なんだろう……。\n様子が変だな……。）",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そんなに身構えないでよ。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_006.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "だって……。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まぁ、マネージャーの気持ちは分かるけど。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_008.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え……？",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まほろの要求って何だろう？\nしかも本番直前に……。そりゃ、身構えるよね。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_010.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いや……。",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（その通りだけど……。）",
            "voice": ""
        },
        {
            "index": 15,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "安心して。\nそんなに、変なお願いじゃないから。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_012.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "わかった……。\n言ってみて。",
            "voice": ""
        },
        {
            "index": 17,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "実はその、欲しいものがあって……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_014.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "なに……？",
            "voice": ""
        },
        {
            "index": 19,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あ、その前に……。\nこのことは、他の人達には内緒でお願い。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_016.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えっ？",
            "voice": ""
        },
        {
            "index": 21,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "約束して。\n絶対に、誰にも言わないって。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_018.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そりゃ、約束するけど……。",
            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そんな曖昧な返事だと不安になるから。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_020.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "分かった。約束する。\nで？　欲しいものって？",
            "voice": ""
        },
        {
            "index": 25,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ぁんそぅこぅ……。　",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_022.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えっ？",
            "voice": ""
        },
        {
            "index": 27,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ぁんソーコー……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_024.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ああ、絆創膏ね。",
            "voice": ""
        },
        {
            "index": 29,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "持ってなくて……。\n買いに行く時間も無いし……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_026.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どこか、怪我でもしたの？",
            "voice": ""
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まぁ、ちょっと……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_028.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どこ？",
            "voice": ""
        },
        {
            "index": 33,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "どこだっていいじゃない。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_030.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "何で言えないの？",
            "voice": ""
        },
        {
            "index": 35,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "言えないんじゃなくて、言いたくないの。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_032.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どうして？",
            "voice": ""
        },
        {
            "index": 37,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "どうしても。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_034.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "じゃ、分かった。\nせめて、どこを怪我したかを教えて。",
            "voice": ""
        },
        {
            "index": 39,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "足の裏。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_036.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "足の裏？\nどうして？",
            "voice": ""
        },
        {
            "index": 41,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ぶつけて……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_038.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ぶつけた？\n足の裏を？",
            "voice": ""
        },
        {
            "index": 43,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いや、違う。\nネコに引っかかれて……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_040.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "足の裏を？",
            "voice": ""
        },
        {
            "index": 45,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "わかった！　もういい！\nもうマネージャーには頼まない。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_042.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ちょっと待ってよ。",
            "voice": ""
        },
        {
            "index": 47,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "じゃあね……、痛ッ！",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_044.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "大丈夫……？",
            "voice": ""
        },
        {
            "index": 49,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "大丈夫、大丈夫……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_046.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "足の裏、見せて。",
            "voice": ""
        },
        {
            "index": 51,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "嫌だ。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_048.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "見せなさい。",
            "voice": ""
        },
        {
            "index": 53,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "それ、パワハラですよ？",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_050.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "パワハラじゃない。仕事だよ。\n皆の体調を管理するのも、れっきとしたマネージャーの仕事。",
            "voice": ""
        },
        {
            "index": 55,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……見せたら、絆創膏もらえる？",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_052.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "早く見せて。",
            "voice": ""
        },
        {
            "index": 57,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はい……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_054.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "何これ！？\n血豆が潰れてる！",
            "voice": ""
        },
        {
            "index": 59,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "すごく痛くて……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_056.mp3"
        },
        {
            "index": 60,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そりゃ、痛いよ……。",
            "voice": ""
        },
        {
            "index": 61,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちなみに、どうしてこうなったかは言わない。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_058.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どうして！？",
            "voice": ""
        },
        {
            "index": 63,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "恥ずかしくて……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_060.mp3"
        },
        {
            "index": 64,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "誰にも言わないから、言って。",
            "voice": ""
        },
        {
            "index": 65,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まほろ、ダンスが苦手だから……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_062.mp3"
        },
        {
            "index": 66,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ん？",
            "voice": ""
        },
        {
            "index": 67,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "だから、練習しないと……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_064.mp3"
        },
        {
            "index": 68,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そんな大げさなもんじゃないよ。\nちょろっと軽く、確認程度に踊っただけ。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_065.mp3"
        },
        {
            "index": 69,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "それで血豆潰れないでしょ。",
            "voice": ""
        },
        {
            "index": 70,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "絶対に、誰にも言わないで。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_067.mp3"
        },
        {
            "index": 71,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どうして……？",
            "voice": ""
        },
        {
            "index": 72,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "だって……、カッコ悪いし……。\nそういうキャラでもないでしょ。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_069.mp3"
        },
        {
            "index": 73,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "もっとクールに、さらっと何でもこなすイメージかなって。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_070.mp3"
        },
        {
            "index": 74,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "確かにそうだけど……。",
            "voice": ""
        },
        {
            "index": 75,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "でも、人より多く繰り返さないと、\nなかなか身につかなくて……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_072.mp3"
        },
        {
            "index": 76,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "凄い！　感動した！",
            "voice": ""
        },
        {
            "index": 77,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いやいや、感動するとこ？",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_074.mp3"
        },
        {
            "index": 78,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "まほろを見直した！",
            "voice": ""
        },
        {
            "index": 79,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "努力なんかしなくても、\n何でもスイスイ出来ちゃうと思ってた？",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_076.mp3"
        },
        {
            "index": 80,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "実はそう……。",
            "voice": ""
        },
        {
            "index": 81,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まさか。\nそんなわけないでしょ。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_078.mp3"
        },
        {
            "index": 82,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "マネージャー失格だ……。",
            "voice": ""
        },
        {
            "index": 83,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "わかんなかったから？　そんなこと気にしないでよ。\nそんなことより……。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_080.mp3"
        },
        {
            "index": 84,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ん？",
            "voice": ""
        },
        {
            "index": 85,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "早く、絆創膏買ってきて。",
            "voice": "card/Card_11/Card_4110005_1/card_4110005_1_082.mp3"
        }
    ]
}