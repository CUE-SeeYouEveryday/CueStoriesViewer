{
    "Txt_Name": "Card_1040001_1",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "立って！　ポークアイ！　\nまだ敵は生きてる！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "……？",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あなたはまだ死ねないでしょう！　\nこんなところで倒れちゃだめ！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "今日も家に帰って、\n味噌ラーメンチャーシュー抜きを食べるんでしょ！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ほのかちゃん、何やってるの……？",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "見てわかるでしょ。応援してるの。\nポークアイを！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ポークアイ？",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ヒーローだよ！\nこの映画の！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "映画……？",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ちょうどクライマックスで……。",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あっ、ポークアイが立ちあがった！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ほら、陽菜も応援して！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "えっ？",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "たかが映画って思っちゃダメ。",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "真剣に応援しないと、ヒーローに失礼だよ。",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "キャラクターだって、ちゃんと生きてるんだから。",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "そ、そっか。\nそうだよね。",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "頑張れ！\nポークアイ！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "が、がんばってー！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……何やってんの？",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "応援！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……テレビに向かって？",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ポークアイがいいところなの。\nそこ！　今だよ、必殺技を！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ポークアイ？",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ああ、あのラストで死んじゃうヒーローね。",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "えっ。",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "はぁ？",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あっ、ポークアイ……！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "まって……！　ポークアイ……！　\nのおおおおお！！",
            "voice": "card/Card_04/Card_1040001_1/card_1040001_1_029.mp3"
        }
    ]
}