{
    "Txt_Name": "Card_3140004_2",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "さとちん！\nさとちん！",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "走らないで！",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "声優入門て本があったよ！",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そりゃ、あるでしょ。\n借りる？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "でも、すごく難しい。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "入門書で難しかったら、\nもう、どうしようもないわよ。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "こっちの本にしようかな。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "これ、哲学書じゃない。\nしかも、すごく分厚い。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "体とか鍛えるのにちょうどよくない？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "よくない！　そんなことに使わないで！\n体、鍛えたかったら、ダンベル買いなさい。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "その手があったか！",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ねぇ……。\n外に出ててくれない？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "外で何してればいいの？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そんなの自分で考えて。\nとにかく、ここにいると邪魔なの。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "邪魔……。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "凛音に付いて来てもらって、失敗だった。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……ごめん。\n外に出てるね。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "（ちょっと言い過ぎたかな……？）",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "（これでよし……。\nちょっと借り過ぎちゃったかな……。）",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "お願いします。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                0
            ],
            "name": "図書館員",
            "message": "今日もたくさん借りましたね。",
            "voice": ""
        },
        {
            "index": 22,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "面白そうな本が多くて。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                0
            ],
            "name": "図書館員",
            "message": "1人で持てますか？",
            "voice": ""
        },
        {
            "index": 24,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "大丈夫です。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                0
            ],
            "name": "図書館員",
            "message": "お気をつけて。",
            "voice": ""
        },
        {
            "index": 26,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "あれ？　凛音はどこ行ったんだろ？\n……と、",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "わぁ！",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "った……、転んじゃった……。\nいててて……、足をくじいたかな……。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "さとちん！\n大丈夫！？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "凛音、どこにいたの？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "これ、買ってきた。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "……カート？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "これ、カートっていうんだ？　\nわたし、コロコロって呼んでる。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "どっちでもいいけど……。\n何でそんなの買ってきたの？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "たくさん本を借りたら、帰り重くなるでしょ？\nこれに乗せれば楽じゃん。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そのために……？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "前から欲しかったし、ちょうどいいかなと思って。\nもしかして、余計なことしちゃった？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "いや……、気が利くじゃない……。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "よかった！\n役に立った！",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "えっ……？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "じゃ、これに本を乗せるね！\nわたしやるからジッとしてて！",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "うん……。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "これでよしと……。\nさ、行こう。わたしの肩につかまって。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "大丈夫？\n歩ける？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "まぁ、ゆっくりなら……。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "じゃ、ゆっくり行こう！\nわたしなら大丈夫。どうせ予定ないし。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "寮にいても、ヒマだと思われちゃうしね。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "嫌味……？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "何が？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "じゃ、ないよね……。\n本気で言ってんだよね……。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えっ？\nえっ？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "何でもない。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "え～。気になる～。\n言いたいことあるなら、言って～。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "感謝してる。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えっ？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "今日は、凛音に付き合ってもらって良かった。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "わたしも！\nついて来てよかった！",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_057.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ありがとう。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_058.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うわ……。\nさとちんがありがとうって言った……。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_059.mp3"
        },
        {
            "index": 60,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "私だって、言うときは言うよ。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_060.mp3"
        },
        {
            "index": 61,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "もう1回言って！",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_061.mp3"
        },
        {
            "index": 62,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "嫌だよ。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_062.mp3"
        },
        {
            "index": 63,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "え～？\n何で何で～？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_063.mp3"
        },
        {
            "index": 64,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "だって軽くなるでしょ。\n本当に感謝したときしか言わないの。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_064.mp3"
        },
        {
            "index": 65,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そっかそっか。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_065.mp3"
        },
        {
            "index": 66,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "凛音……。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_066.mp3"
        },
        {
            "index": 67,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "んっ？",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_067.mp3"
        },
        {
            "index": 68,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ありがと。",
            "voice": "card/card_14/Card_3140004_2/card_3140004_2_068.mp3"
        }
    ]
}