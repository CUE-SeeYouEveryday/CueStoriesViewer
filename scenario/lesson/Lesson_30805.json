{
    "Txt_Name": "Lesson_30805",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ピアノの音……。\nもしかして、エールブルーのビルからかな？",
            "voice": "lesson/Lesson_30805/lesson_30805_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん……。当たり。\nレッスン室から聞こえてくる。　",
            "voice": "lesson/Lesson_30805/lesson_30805_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "美晴かな……？",
            "voice": "lesson/Lesson_30805/lesson_30805_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "きっとそう。\nとても、とても透き通っていて、優しい音色。",
            "voice": "lesson/Lesson_30805/lesson_30805_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "綺麗な曲……。\n聴いたことある気がするけど、なんて曲だったかな。　",
            "voice": "lesson/Lesson_30805/lesson_30805_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ショパン？　だったかな……。\nモーツアルトだったかも……。",
            "voice": "lesson/Lesson_30805/lesson_30805_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "やっほー！　絢。何してんの？",
            "voice": "lesson/Lesson_30805/lesson_30805_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "わ、びっくりした！\n莉子！",
            "voice": "lesson/Lesson_30805/lesson_30805_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私はちょっと買い物帰りに散歩してただけ。\n莉子は、バイト中？",
            "voice": "lesson/Lesson_30805/lesson_30805_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そう、今日の現場、たまたまこの近くでさ。",
            "voice": "lesson/Lesson_30805/lesson_30805_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "誰かに会うかなーと思ってたけど、絢に会っちゃった。",
            "voice": "lesson/Lesson_30805/lesson_30805_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうなんだ。バイト、頑張ってね。",
            "voice": "lesson/Lesson_30805/lesson_30805_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うん、またねー。",
            "voice": "lesson/Lesson_30805/lesson_30805_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "バイト中の莉子、いつもと雰囲気違ったなぁ……。",
            "voice": "lesson/Lesson_30805/lesson_30805_014.mp3"
        }
    ]
}