{
    "Txt_Name": "Lesson_31206",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "今年の夏は、プライベートも仕事も、\n両方充実させたい！",
            "voice": "lesson/Lesson_31206/lesson_31206_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "仕事、充実するといいなぁ……。",
            "voice": "lesson/Lesson_31206/lesson_31206_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "するといい、じゃなくて、させるの！",
            "voice": "lesson/Lesson_31206/lesson_31206_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "いっぱい遊んで、バリバリ仕事して！\n最高の夏にする！",
            "voice": "lesson/Lesson_31206/lesson_31206_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "私は極力涼しい部屋で過ごしたいがな。",
            "voice": "lesson/Lesson_31206/lesson_31206_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "そんなのつまんないじゃん！\nどんどん連れ出すからね。",
            "voice": "lesson/Lesson_31206/lesson_31206_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……ほどほどにしてくれ。",
            "voice": "lesson/Lesson_31206/lesson_31206_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "よし、舞花。\nその調子で宿題、片付けちゃおっか！",
            "voice": "lesson/Lesson_31206/lesson_31206_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "うっ……。\nちょっと、今はそういうこと言わないでよ！",
            "voice": "lesson/Lesson_31206/lesson_31206_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "学業がおろそかになったら、\n真咲さんやマネージャーが怒るぞー。",
            "voice": "lesson/Lesson_31206/lesson_31206_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "だ、だから、そういうのなし！\nこれから楽しくなるんだから、今は勉強のことは忘れるの！",
            "voice": "lesson/Lesson_31206/lesson_31206_011.mp3"
        }
    ]
}