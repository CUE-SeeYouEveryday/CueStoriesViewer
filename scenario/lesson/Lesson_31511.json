{
    "Txt_Name": "Lesson_31511",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ、そういえばね、まほろ。",
            "voice": "lesson/Lesson_31511/lesson_31511_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ん？　何？",
            "voice": "lesson/Lesson_31511/lesson_31511_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "今日見た夢の話なんだけどさ。",
            "voice": "lesson/Lesson_31511/lesson_31511_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "えー、何？　また夢の話？",
            "voice": "lesson/Lesson_31511/lesson_31511_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "いや、なんていうのかな。\n昔見たことあるような気がする夢を見たっていうか。",
            "voice": "lesson/Lesson_31511/lesson_31511_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そういうの、まほろ、経験ない？",
            "voice": "lesson/Lesson_31511/lesson_31511_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いや、ないかな。\nっていうかいちいち覚えてない。",
            "voice": "lesson/Lesson_31511/lesson_31511_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そう。\nま、それはそうだよね。",
            "voice": "lesson/Lesson_31511/lesson_31511_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "で？　どんな夢だったの？",
            "voice": "lesson/Lesson_31511/lesson_31511_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "え、なになに？　興味ある？　聞きたい？",
            "voice": "lesson/Lesson_31511/lesson_31511_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いや、別に、そんなに興味ない。",
            "voice": "lesson/Lesson_31511/lesson_31511_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あのね、Windの4人が全員出てくる夢だったんだけど……。",
            "voice": "lesson/Lesson_31511/lesson_31511_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "興味ないって言ったんだけど。",
            "voice": "lesson/Lesson_31511/lesson_31511_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "最初はなんか4人で新幹線乗ってるわけ。",
            "voice": "lesson/Lesson_31511/lesson_31511_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "結局最後まで話す気……。",
            "voice": "lesson/Lesson_31511/lesson_31511_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "でさ、美晴の子ども時代になって……。\nなんか風船手放して泣いてるの。",
            "voice": "lesson/Lesson_31511/lesson_31511_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……え？",
            "voice": "lesson/Lesson_31511/lesson_31511_017.mp3"
        }
    ]
}