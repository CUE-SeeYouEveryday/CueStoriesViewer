{
    "Txt_Name": "Lesson_31211",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そうだ、みんなお土産って何買ったの？\nわたしは、こんなの買ったんだけど。",
            "voice": "lesson/Lesson_31211/lesson_31211_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あっ、キーホルダー。かわいいね。",
            "voice": "lesson/Lesson_31211/lesson_31211_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "うん。かわいいでしょ？　\n色々あったからいっぱい買っちゃった。",
            "voice": "lesson/Lesson_31211/lesson_31211_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "そんなにキーホルダー使わないでしょ……。\nいくつ鍵、持ってるの？",
            "voice": "lesson/Lesson_31211/lesson_31211_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "いいじゃん、別に。",
            "voice": "lesson/Lesson_31211/lesson_31211_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "しらす丼くんキーホルダー……。\nほのか、いいセンスしてるな。",
            "voice": "lesson/Lesson_31211/lesson_31211_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "でしょー。",
            "voice": "lesson/Lesson_31211/lesson_31211_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あ、そうだ、マネージャーにもひとつあげよっと。",
            "voice": "lesson/Lesson_31211/lesson_31211_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "どれがいいかなー。",
            "voice": "lesson/Lesson_31211/lesson_31211_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ふふ、マネージャーさんもきっと喜ぶね。",
            "voice": "lesson/Lesson_31211/lesson_31211_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "せっかくだ、目立つのを選ぼう。",
            "voice": "lesson/Lesson_31211/lesson_31211_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "うん、すごいの選ぼう。",
            "voice": "lesson/Lesson_31211/lesson_31211_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "……志穂と舞花、悪い顔してるよ？",
            "voice": "lesson/Lesson_31211/lesson_31211_013.mp3"
        }
    ]
}