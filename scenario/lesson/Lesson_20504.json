{
    "Txt_Name": "Lesson_20504",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "私の考えは、変わりません。",
            "voice": "lesson/Lesson_20504/lesson_20504_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "スーツの男性",
            "message": "なんとか、考え直して頂けませんか？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（……あれ、桐香先生？）",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "申し訳ありませんが、\n今回のお話は、お断りさせて下さい。",
            "voice": "lesson/Lesson_20504/lesson_20504_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "スーツの男性",
            "message": "そうですか……。",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（何だろう……。\n　とても深刻な話をしてる感じだけど……。）",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "スーツの男性",
            "message": "考えが変わりましたら、いつでも連絡して下さい。\n失礼します。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あ、あの。\n桐香先生。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "マネージャー。\nいたの？",
            "voice": "lesson/Lesson_20504/lesson_20504_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "はい。たまたま通りかかって……。\nあの、何かあったんですか？",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "まぁ、ちょっとね。",
            "voice": "lesson/Lesson_20504/lesson_20504_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "はあ……。",
            "voice": ""
        },
        {
            "index": 13,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "あの人、ルートプロのマネージャーさんなの。",
            "voice": "lesson/Lesson_20504/lesson_20504_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ルートプロって、大手の芸能事務所じゃないですか。\nそんな人がどうして？",
            "voice": ""
        },
        {
            "index": 15,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "うちの事務所に来ないかって誘われたのよ。",
            "voice": "lesson/Lesson_20504/lesson_20504_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えっ？　誰が？",
            "voice": ""
        },
        {
            "index": 17,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "私が。",
            "voice": "lesson/Lesson_20504/lesson_20504_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "桐香先生が？",
            "voice": ""
        },
        {
            "index": 19,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "だからそうよ。\n女優として所属しないかって言われたの。",
            "voice": "lesson/Lesson_20504/lesson_20504_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えっ、桐香先生がですか！？",
            "voice": ""
        }
    ]
}