{
    "Txt_Name": "Lesson_30609",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "みんな、オーディションに受かりたかったんだよね……。",
            "voice": "lesson/Lesson_30609/lesson_30609_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "だから緊張してたんだ……。",
            "voice": "lesson/Lesson_30609/lesson_30609_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "マネージャーさんに、電話してみよう。",
            "voice": "lesson/Lesson_30609/lesson_30609_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "はい。もしもし。",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あ、夜遅くにすみません、マネージャーさん！",
            "voice": "lesson/Lesson_30609/lesson_30609_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そんなに遅くないから大丈夫だよ。\nどうしたの？",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "今日はすみませんでした！\nオーディション、大失敗しちゃって。",
            "voice": "lesson/Lesson_30609/lesson_30609_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "気合が空回りしたみたいだね。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あれじゃ、不合格ですよね……？",
            "voice": "lesson/Lesson_30609/lesson_30609_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どうだろう……。明日になってみないと分からないよ。",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "わたしだけじゃなくて……、みんな、緊張してましたよね。",
            "voice": "lesson/Lesson_30609/lesson_30609_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん。ガチガチに緊張してたね。",
            "voice": ""
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "皆、あの作品が大好きなんだって。\n他のマネージャーさんたちから聞いたよ。",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "（やっぱりそうなんだ……。）",
            "voice": "lesson/Lesson_30609/lesson_30609_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "「うちも一緒ですよ」って言ったよ。",
            "voice": ""
        },
        {
            "index": 16,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "（わたしと一緒なんだ……。）",
            "voice": "lesson/Lesson_30609/lesson_30609_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "凛音？　どうかしたの？",
            "voice": ""
        },
        {
            "index": 18,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あ、いえ、ありがとうございます！\n聞きたいことは聞けました！",
            "voice": "lesson/Lesson_30609/lesson_30609_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そう？　とにかく、今日はもう寝なよ。\n明日結果が分かったらすぐ連絡するから。",
            "voice": ""
        },
        {
            "index": 20,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はい！　おやすみなさーい！",
            "voice": "lesson/Lesson_30609/lesson_30609_020.mp3"
        }
    ]
}