{
    "Txt_Name": "Lesson_31803",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "利恵、もう大丈夫？",
            "voice": "lesson/Lesson_31803/lesson_31803_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "え？　何が？",
            "voice": "lesson/Lesson_31803/lesson_31803_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "さっき、ずっと入ったままだったから。",
            "voice": "lesson/Lesson_31803/lesson_31803_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "あはは。\nうん。もう大丈夫。",
            "voice": "lesson/Lesson_31803/lesson_31803_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "疲れた？",
            "voice": "lesson/Lesson_31803/lesson_31803_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そうだね。\nちょっと疲れたかも。",
            "voice": "lesson/Lesson_31803/lesson_31803_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "頭も使ったしね。\n普段使わないようなところ、たくさん使った気がする。",
            "voice": "lesson/Lesson_31803/lesson_31803_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "そうだね。",
            "voice": "lesson/Lesson_31803/lesson_31803_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "なんだか甘いものが飲みたくなっちゃった。",
            "voice": "lesson/Lesson_31803/lesson_31803_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "うん。",
            "voice": "lesson/Lesson_31803/lesson_31803_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "今から喫茶店、行くんでしょ？\n甘いもの、あるよ。",
            "voice": "lesson/Lesson_31803/lesson_31803_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そうだね。",
            "voice": "lesson/Lesson_31803/lesson_31803_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "うん。",
            "voice": "lesson/Lesson_31803/lesson_31803_013.mp3"
        }
    ]
}