{
    "Txt_Name": "Lesson_31309",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "音はいろんなことを教えてくれる……、か。",
            "voice": "lesson/Lesson_31309/lesson_31309_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なあに、まほろ。\nまだ信用してくれないの？　くすん。",
            "voice": "lesson/Lesson_31309/lesson_31309_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いや、そういうんじゃなくて。",
            "voice": "lesson/Lesson_31309/lesson_31309_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "カミズオーの店員さんの声を聞き分けられてたのは、\n実際にもう証明済みだし。",
            "voice": "lesson/Lesson_31309/lesson_31309_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あんたのそのモノを聞く力ってさ。",
            "voice": "lesson/Lesson_31309/lesson_31309_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "きっとまほろたちが音から受け取る情報以上の色々、\n受け取ってるんだろうなって。",
            "voice": "lesson/Lesson_31309/lesson_31309_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "さあ、どうかしら。\n自分に聞こえてる音と、まほろに聞こえてる音。",
            "voice": "lesson/Lesson_31309/lesson_31309_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "頭の中でどう聞こえてるかなんて、\n確かめようがないもの。",
            "voice": "lesson/Lesson_31309/lesson_31309_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そういうことだね。\n美晴の言う通り、確かめようがない。",
            "voice": "lesson/Lesson_31309/lesson_31309_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "でも、まあ、おかげでメイクさんを迎えに行けたわけだし。\n今日のとこはそれでいいかな。",
            "voice": "lesson/Lesson_31309/lesson_31309_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "いつかまた役に立つことがあるかもしれないわよ？",
            "voice": "lesson/Lesson_31309/lesson_31309_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "電話越しの音を聞き分ける力が？",
            "voice": "lesson/Lesson_31309/lesson_31309_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。だって何があるかわからないじゃない。",
            "voice": "lesson/Lesson_31309/lesson_31309_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あはは。そうだね。\nその時にはまた頼むよ。",
            "voice": "lesson/Lesson_31309/lesson_31309_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はーい。",
            "voice": "lesson/Lesson_31309/lesson_31309_015.mp3"
        }
    ]
}