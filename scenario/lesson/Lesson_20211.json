{
    "Txt_Name": "Lesson_20211",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私、チェーンソー寺には行った事ないの。",
            "voice": "lesson/Lesson_20211/lesson_20211_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うんうん。メジャーすぎるかもね。\n意外といった事ない人多いんだよ。",
            "voice": "lesson/Lesson_20211/lesson_20211_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "チェーンソー寺行きたい。",
            "voice": "lesson/Lesson_20211/lesson_20211_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "じゃあそうしよう。",
            "voice": "lesson/Lesson_20211/lesson_20211_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "やった！",
            "voice": "lesson/Lesson_20211/lesson_20211_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（……会話が、成立してる！？）",
            "voice": "lesson/Lesson_20211/lesson_20211_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ねぇ、お昼ご飯は何食べよっか？",
            "voice": "lesson/Lesson_20211/lesson_20211_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ～その近辺だったら美味しいお店知ってるよ。",
            "voice": "lesson/Lesson_20211/lesson_20211_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "なに？　和食？　洋食？",
            "voice": "lesson/Lesson_20211/lesson_20211_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ラーメン。ラーメン好き？",
            "voice": "lesson/Lesson_20211/lesson_20211_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "好きだよ。何ラーメン？",
            "voice": "lesson/Lesson_20211/lesson_20211_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "えっとね……。\n何て言ったっけ？　あの……、白くて、麺の細い。",
            "voice": "lesson/Lesson_20211/lesson_20211_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "あ～知ってる！　私もそれ好きよ。",
            "voice": "lesson/Lesson_20211/lesson_20211_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "何て言ったっけ、ド忘れしちゃった。",
            "voice": "lesson/Lesson_20211/lesson_20211_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（とんこつラーメンだよ、二人とも～。\n　どうしよう？　もういっそ教えてあげちゃおうかな？）",
            "voice": "lesson/Lesson_20211/lesson_20211_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "あ、思い出した！\nポンコツラーメンでしょ？",
            "voice": "lesson/Lesson_20211/lesson_20211_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（え！！）",
            "voice": "lesson/Lesson_20211/lesson_20211_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ、そうそう、それそれ。",
            "voice": "lesson/Lesson_20211/lesson_20211_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（え――！！？）",
            "voice": "lesson/Lesson_20211/lesson_20211_019.mp3"
        }
    ]
}