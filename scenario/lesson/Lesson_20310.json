{
    "Txt_Name": "Lesson_20310",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "大漁……。",
            "voice": "lesson/Lesson_20310/lesson_20310_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そうね。たくさん買った。",
            "voice": "lesson/Lesson_20310/lesson_20310_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "しばらくは服に困らない。",
            "voice": "lesson/Lesson_20310/lesson_20310_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "素晴らしい新作がありすぎて。\nつい買いすぎてしまったな。",
            "voice": "lesson/Lesson_20310/lesson_20310_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "欲望に忠実な方が、ストレスはたまらない。",
            "voice": "lesson/Lesson_20310/lesson_20310_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "それはそうね。",
            "voice": "lesson/Lesson_20310/lesson_20310_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "健康的なのが一番……。ん？",
            "voice": "lesson/Lesson_20310/lesson_20310_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "志穂？",
            "voice": "lesson/Lesson_20310/lesson_20310_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "これは……。",
            "voice": "lesson/Lesson_20310/lesson_20310_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "映画？　\nこれって、志穂が気になってたやつじゃない？",
            "voice": "lesson/Lesson_20310/lesson_20310_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "世界最速、先行上映……。知らなかった……。",
            "voice": "lesson/Lesson_20310/lesson_20310_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "しかも、今日……。",
            "voice": "lesson/Lesson_20310/lesson_20310_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "ふーん。なら、行く？",
            "voice": "lesson/Lesson_20310/lesson_20310_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "けど……。始まるのが、23時。",
            "voice": "lesson/Lesson_20310/lesson_20310_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "数時間後じゃない。間に合うよ？",
            "voice": "lesson/Lesson_20310/lesson_20310_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "でも……。夜、遅い。",
            "voice": "lesson/Lesson_20310/lesson_20310_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "2人だし、大丈夫じゃない？",
            "voice": "lesson/Lesson_20310/lesson_20310_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "欲望に忠実が一番、だろ？",
            "voice": "lesson/Lesson_20310/lesson_20310_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……。行こう。",
            "voice": "lesson/Lesson_20310/lesson_20310_019.mp3"
        }
    ]
}