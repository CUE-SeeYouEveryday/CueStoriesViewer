{
    "Txt_Name": "Lesson_30205",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "気持ちだけもらっておく。\nでも今日はここまで、私にとって完璧な一日なの。",
            "voice": "lesson/Lesson_30205/lesson_30205_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "邪魔されたくないんだ。",
            "voice": "lesson/Lesson_30205/lesson_30205_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "わかった。\nじゃあ今すぐ遊んですぐ返そう。",
            "voice": "lesson/Lesson_30205/lesson_30205_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そうじゃなくて。\n今、持って帰ってほしいの。",
            "voice": "lesson/Lesson_30205/lesson_30205_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えー。\nせっかく借りたんだから遊ぼうよぉ。",
            "voice": "lesson/Lesson_30205/lesson_30205_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ほんっとに必要ないのっ。",
            "voice": "lesson/Lesson_30205/lesson_30205_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いいからいいから、触るだけでもさ。",
            "voice": "lesson/Lesson_30205/lesson_30205_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ほんとにやめてってば！",
            "voice": "lesson/Lesson_30205/lesson_30205_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あー！\n腕折れちゃった！",
            "voice": "lesson/Lesson_30205/lesson_30205_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ちょっと！\n凛音が手を放したからだよ。",
            "voice": "lesson/Lesson_30205/lesson_30205_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "さとちんが受け取ると思って。",
            "voice": "lesson/Lesson_30205/lesson_30205_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "どうするのこれ、大事なものなんじゃないの？",
            "voice": "lesson/Lesson_30205/lesson_30205_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "わかんない。\n勝手に選んだから。",
            "voice": "lesson/Lesson_30205/lesson_30205_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "どうしてそんなことしたの！",
            "voice": "lesson/Lesson_30205/lesson_30205_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "さとちん喜ぶと思って！",
            "voice": "lesson/Lesson_30205/lesson_30205_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "こんなことされたって喜ぶわけないでしょ？",
            "voice": "lesson/Lesson_30205/lesson_30205_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "どうしようこれ……。",
            "voice": "lesson/Lesson_30205/lesson_30205_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ふぅ。\n……素直に謝るしかないね。",
            "voice": "lesson/Lesson_30205/lesson_30205_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そうだよね……。\nじゃあ、行こうか。",
            "voice": "lesson/Lesson_30205/lesson_30205_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "……ちょっと待って、私も行くの？",
            "voice": "lesson/Lesson_30205/lesson_30205_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "行かないつもりなの？\n共犯者なのに？",
            "voice": "lesson/Lesson_30205/lesson_30205_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そんな人聞きの悪い言い方しないでよ。",
            "voice": "lesson/Lesson_30205/lesson_30205_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "私がお願いして持ってきてもらったわけじゃないし……。",
            "voice": "lesson/Lesson_30205/lesson_30205_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "まして無断で持ち出すなんて。\n……はっきり言って迷惑よ。",
            "voice": "lesson/Lesson_30205/lesson_30205_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そうだよね……。\n余計なことしちゃったね。",
            "voice": "lesson/Lesson_30205/lesson_30205_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "……。",
            "voice": "lesson/Lesson_30205/lesson_30205_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ゆきぞーからは、\n好きなの持って行っていいよって言われてたの。",
            "voice": "lesson/Lesson_30205/lesson_30205_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "さとちんを笑顔にしたかっただけなんだけど。\n……ごめんね。",
            "voice": "lesson/Lesson_30205/lesson_30205_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "凛音……。",
            "voice": "lesson/Lesson_30205/lesson_30205_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ゆきぞーに謝ってくるね。",
            "voice": "lesson/Lesson_30205/lesson_30205_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "……ちょっと待って！",
            "voice": "lesson/Lesson_30205/lesson_30205_031.mp3"
        }
    ]
}