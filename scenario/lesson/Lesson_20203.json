{
    "Txt_Name": "Lesson_20203",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "太郎と呼ばれた少年",
            "message": "凛音ねーちゃん、本当にありがとう！",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "こういちくんに気を付けて歩かなきゃダメだよって、\nちゃんと伝えてね？",
            "voice": "lesson/Lesson_20203/lesson_20203_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "太郎と呼ばれた少年",
            "message": "は～い！　\nゴメンね凛音ねーちゃん、せっかく並んでたのに。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いいのいいの。気にしない気にしな～い！",
            "voice": "lesson/Lesson_20203/lesson_20203_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "太郎と呼ばれた少年",
            "message": "へへっ。ありがとうね。バイバ～イ！",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "バイバ～イ！！　気をつけて帰るんだよ～！",
            "voice": "lesson/Lesson_20203/lesson_20203_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "またたくさん並んでるなぁ。もう1回並ぶのは無理っぽいか～。\n……フローズンチョコミント、食べ損ねちゃった。",
            "voice": "lesson/Lesson_20203/lesson_20203_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "リンネ～！　いたいた！",
            "voice": "lesson/Lesson_20203/lesson_20203_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ゆずっち……。",
            "voice": "lesson/Lesson_20203/lesson_20203_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "噂のチョコミントどうだった？\n味の感想聞きたくてまた来たのに、リンネいないんだもん。",
            "voice": "lesson/Lesson_20203/lesson_20203_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "別のところで食べてたの？",
            "voice": "lesson/Lesson_20203/lesson_20203_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "それが食べられなかったの。",
            "voice": "lesson/Lesson_20203/lesson_20203_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "えっ？　並んだんじゃなかったの？",
            "voice": "lesson/Lesson_20203/lesson_20203_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "並んだんだけどね～。\nくぅ～！　中々人生って上手く行かないね！",
            "voice": "lesson/Lesson_20203/lesson_20203_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "何があったか知らないけど、\nじゃあ食べてないんだ。",
            "voice": "lesson/Lesson_20203/lesson_20203_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そう。また今度並ぶよ！",
            "voice": "lesson/Lesson_20203/lesson_20203_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "そっかぁ残念。これからまたあのお店に並ぶのも大変ね。\n……あれ？　あのお店の店員さんずっとリンネのこと見てるよ？",
            "voice": "lesson/Lesson_20203/lesson_20203_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "え？　……あ、ホントだ。",
            "voice": "lesson/Lesson_20203/lesson_20203_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "店員さん、こっちに来るよ。\nしかもあの人、手にフローズンチョコミント持ってる！",
            "voice": "lesson/Lesson_20203/lesson_20203_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                0
            ],
            "name": "店員",
            "message": "良かった！　もう戻って来ないんじゃないかって思いました。\nどうぞ、フローズンチョコミント400円です。",
            "voice": ""
        },
        {
            "index": 21,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "え？　わたしのために……？",
            "voice": "lesson/Lesson_20203/lesson_20203_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                0
            ],
            "name": "店員",
            "message": "怪我した子供のために、60分待ったの諦めて行ったんでしょう？\nそんな方をまた並ばせるなんて出来ませんよ。",
            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あ、ありがとうございます！！",
            "voice": "lesson/Lesson_20203/lesson_20203_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "リンネ、良かったね！",
            "voice": "lesson/Lesson_20203/lesson_20203_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！！　ゆずっちも一緒に食べ……、あれ？",
            "voice": "lesson/Lesson_20203/lesson_20203_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "どうかしたの？",
            "voice": "lesson/Lesson_20203/lesson_20203_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ゆずっち400円貸して？　\nお財布寮に忘れちゃった……。",
            "voice": "lesson/Lesson_20203/lesson_20203_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "あは、しょうがないなぁ。\nちゃんと味見させてよね？",
            "voice": "lesson/Lesson_20203/lesson_20203_028.mp3"
        }
    ]
}