{
    "Txt_Name": "Lesson_30412",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（日本に来てから一度も会ってない友達なんだもの。\n絶対に行かないと！）",
            "voice": "lesson/Lesson_30412/lesson_30412_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（お話だってめちゃくちゃになってるし、\n後は、ノリと勢いでなんとかなるはず！）",
            "voice": "lesson/Lesson_30412/lesson_30412_005.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『この物語……誰にも譲らない……。\nこの世界は、私のものだー！』",
            "voice": "lesson/Lesson_30412/lesson_30412_006.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（……あれ？　\n……アイリの雰囲気が、変わった……。）",
            "voice": "lesson/Lesson_30412/lesson_30412_007.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "『みんな！　人々の幸せを、本気で願ってみて！』",
            "voice": "lesson/Lesson_30412/lesson_30412_008.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（……そう。アイリはずっと、このリーディングライブに向けて\n必死に頑張ってた。）",
            "voice": "lesson/Lesson_30412/lesson_30412_009.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（夜中、部屋でずっと練習してて、チサにうるさいって\n怒られたり……。）",
            "voice": "lesson/Lesson_30412/lesson_30412_010.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（ユーキだってチサだって、必死に成功させようと頑張ってる。）",
            "voice": "lesson/Lesson_30412/lesson_30412_011.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（私も、全力で子どもたちを楽しませる！）",
            "voice": "lesson/Lesson_30412/lesson_30412_012.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "みんな！　お話は終わったけど、\n本当のパーティーはこれからよ！",
            "voice": "lesson/Lesson_30412/lesson_30412_013.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……柚葉？",
            "voice": "lesson/Lesson_30412/lesson_30412_014.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "みんなでダンスを踊りましょう？\n踊り方は、私がレクチャーしてあげる！",
            "voice": "lesson/Lesson_30412/lesson_30412_015.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ありがとう柚葉ちゃん。お話が終わってからのダンス、\nみんなとっても喜んでたよ！",
            "voice": "lesson/Lesson_30412/lesson_30412_016.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "さすがお金持ちのダンス。\n品性を感じたわ。",
            "voice": "lesson/Lesson_30412/lesson_30412_017.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ふふん、お安い御用よ。",
            "voice": "lesson/Lesson_30412/lesson_30412_018.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あたしはダンスよりも拳で語り合う方が好きだけどなー。",
            "voice": "lesson/Lesson_30412/lesson_30412_019.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "悠希に任せたら、子どもたちがどんどん乱暴に育ちそうね……。",
            "voice": "lesson/Lesson_30412/lesson_30412_020.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ふふっ、柚葉ちゃんが居てくれてよかったね。",
            "voice": "lesson/Lesson_30412/lesson_30412_021.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "……ふふっ。",
            "voice": "lesson/Lesson_30412/lesson_30412_022.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あれ？　柚葉、パーティー行かなくていいの？",
            "voice": "lesson/Lesson_30412/lesson_30412_023.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "なんだか疲れちゃったから。",
            "voice": "lesson/Lesson_30412/lesson_30412_024.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "それに、今夜はみんなと居たくて。",
            "voice": "lesson/Lesson_30412/lesson_30412_025.mp3"
        }
    ]
}