{
    "Txt_Name": "Lesson_30811",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ねぇ絢、何してるの？",
            "voice": "lesson/Lesson_30811/lesson_30811_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "わ！　莉子！",
            "voice": "lesson/Lesson_30811/lesson_30811_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "最近ずっとスマホ触ってるじゃん。",
            "voice": "lesson/Lesson_30811/lesson_30811_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "何してるのかなって、お姉さんは気になっちゃうなぁ……。",
            "voice": "lesson/Lesson_30811/lesson_30811_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "なんでもないよ。",
            "voice": "lesson/Lesson_30811/lesson_30811_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そう？　なんでもないってことないでしょ。",
            "voice": "lesson/Lesson_30811/lesson_30811_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "なんでもないってば。",
            "voice": "lesson/Lesson_30811/lesson_30811_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ゲーム？",
            "voice": "lesson/Lesson_30811/lesson_30811_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "違うけど。",
            "voice": "lesson/Lesson_30811/lesson_30811_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "SNS？",
            "voice": "lesson/Lesson_30811/lesson_30811_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "SNS……、じゃない。",
            "voice": "lesson/Lesson_30811/lesson_30811_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ふーん……。",
            "voice": "lesson/Lesson_30811/lesson_30811_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "な、何？",
            "voice": "lesson/Lesson_30811/lesson_30811_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ほい！",
            "voice": "lesson/Lesson_30811/lesson_30811_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "はっ！",
            "voice": "lesson/Lesson_30811/lesson_30811_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "もう、莉子やめてよ。",
            "voice": "lesson/Lesson_30811/lesson_30811_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "わかったわかった。もう見ようとしないから。\nごめんね。",
            "voice": "lesson/Lesson_30811/lesson_30811_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "何やってるか知らないけどさ。楽しいこと？",
            "voice": "lesson/Lesson_30811/lesson_30811_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　うん。\nそうだね……、楽しいことかな。",
            "voice": "lesson/Lesson_30811/lesson_30811_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そっか。まぁ、それならいいか。\n楽しんでやってることなら、心配するのも余計なお世話だよね。",
            "voice": "lesson/Lesson_30811/lesson_30811_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "心配してたの？",
            "voice": "lesson/Lesson_30811/lesson_30811_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うん。急にスマホ触ってる時間が増えたからさ、\nちょっとだけね。",
            "voice": "lesson/Lesson_30811/lesson_30811_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そう……。",
            "voice": "lesson/Lesson_30811/lesson_30811_023.mp3"
        }
    ]
}