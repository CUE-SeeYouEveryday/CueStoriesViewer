{
    "Txt_Name": "Lesson_30506",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ロケ、いつか慣れるのかな？",
            "voice": "lesson/Lesson_30506/lesson_30506_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "絢、苦手って言ってたもんね。　",
            "voice": "lesson/Lesson_30506/lesson_30506_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。\nステージとか舞台に立つのは、だいぶ慣れたんだけど……。",
            "voice": "lesson/Lesson_30506/lesson_30506_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "カメラ向けられるのは、やっぱりまだちょっと、慣れない、かな。",
            "voice": "lesson/Lesson_30506/lesson_30506_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "撮られてるって、意識しないでいればいいんじゃない？　",
            "voice": "lesson/Lesson_30506/lesson_30506_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そう簡単には、いかないし……。　",
            "voice": "lesson/Lesson_30506/lesson_30506_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。大丈夫よ。隣にいるのはわたしたちなんだし。\nいつもと変わらないんだから。",
            "voice": "lesson/Lesson_30506/lesson_30506_008.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん……、そうだね。\nそう思うと気が楽……、かな？",
            "voice": "lesson/Lesson_30506/lesson_30506_009.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "美晴。やっぱり、美晴はリーダーだよね。　",
            "voice": "lesson/Lesson_30506/lesson_30506_010.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "え？　どうしたの？　",
            "voice": "lesson/Lesson_30506/lesson_30506_011.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "それに、時々お母さんみたい。　",
            "voice": "lesson/Lesson_30506/lesson_30506_012.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "えー、まだそんな年じゃないよー。　",
            "voice": "lesson/Lesson_30506/lesson_30506_013.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふっ。そうだけど。　",
            "voice": "lesson/Lesson_30506/lesson_30506_014.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "なんか、美晴は安心感があるの。　\n美晴のその雰囲気、私も見習いたいな。",
            "voice": "lesson/Lesson_30506/lesson_30506_015.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "やだー。どういう意味？　",
            "voice": "lesson/Lesson_30506/lesson_30506_016.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……ふあ?　あ……。\nあ、ごめん、あくびでちゃった。",
            "voice": "lesson/Lesson_30506/lesson_30506_017.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そろそろ眠たくなってきたかも。",
            "voice": "lesson/Lesson_30506/lesson_30506_018.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん。明日のロケ、時間早くからだから、もう寝ましょ。",
            "voice": "lesson/Lesson_30506/lesson_30506_019.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん、おやすみ。　",
            "voice": "lesson/Lesson_30506/lesson_30506_020.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "おやすみなさい。\nまた明日……、ね。　",
            "voice": "lesson/Lesson_30506/lesson_30506_021.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "いいロケにしようね。　",
            "voice": "lesson/Lesson_30506/lesson_30506_022.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん。もちろん、そうしましょ。",
            "voice": "lesson/Lesson_30506/lesson_30506_023.mp3"
        }
    ]
}