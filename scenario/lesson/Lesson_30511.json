{
    "Txt_Name": "Lesson_30511",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "えー？　今からやるの？",
            "voice": "lesson/Lesson_30511/lesson_30511_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そりゃそうでしょ。",
            "voice": "lesson/Lesson_30511/lesson_30511_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "どうやって？",
            "voice": "lesson/Lesson_30511/lesson_30511_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "つべこべ言わない。現場は待ってくれないの。",
            "voice": "lesson/Lesson_30511/lesson_30511_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まずはオープニング。\nはい、スタート！",
            "voice": "lesson/Lesson_30511/lesson_30511_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ど、ど、どうも、こんにちは。えー、日名倉莉子です。\nきょ、今日はですね、えっと、こちらの番組、えー。何だっけ？",
            "voice": "lesson/Lesson_30511/lesson_30511_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……ストップ。",
            "voice": "lesson/Lesson_30511/lesson_30511_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あんた……、やっぱりなめてるでしょ？",
            "voice": "lesson/Lesson_30511/lesson_30511_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "いやいやいやいや！　なめてない！　なめてないよ！",
            "voice": "lesson/Lesson_30511/lesson_30511_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "でも、さー。\nほら、いきなり言われてもさー。出来ないじゃん？",
            "voice": "lesson/Lesson_30511/lesson_30511_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "それをやるのが仕事なわけ。",
            "voice": "lesson/Lesson_30511/lesson_30511_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうは言っても……。",
            "voice": "lesson/Lesson_30511/lesson_30511_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "要は場慣れよ。場慣れ。",
            "voice": "lesson/Lesson_30511/lesson_30511_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "土壇場で失敗するより、今のうちに失敗しといたほうがいいでしょ？",
            "voice": "lesson/Lesson_30511/lesson_30511_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まー、そりゃそうだけど。",
            "voice": "lesson/Lesson_30511/lesson_30511_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はい。わかったら、もう1回。",
            "voice": "lesson/Lesson_30511/lesson_30511_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "わかった。変に緊張するからダメなんだよね。\nじゃ、行くよ？",
            "voice": "lesson/Lesson_30511/lesson_30511_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "はい、どうもー。というわけで、日名倉莉子なんですけども、\nなんやいきなりMCやれって、仕込まれてて。",
            "voice": "lesson/Lesson_30511/lesson_30511_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたしそんなん初めてやし、うまく出来へんって言ってるんですけど、\nもう、全然聞いてくれないんですよ。困っちゃうわ。",
            "voice": "lesson/Lesson_30511/lesson_30511_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はいストップ。\n何よそれ、どこの劇場に立つつもりなのよ。",
            "voice": "lesson/Lesson_30511/lesson_30511_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "っていうか、莉子……、関西出身だっけ？",
            "voice": "lesson/Lesson_30511/lesson_30511_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そう。神戸生まれ。",
            "voice": "lesson/Lesson_30511/lesson_30511_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はあ……。やり直し、もう1回。",
            "voice": "lesson/Lesson_30511/lesson_30511_023.mp3"
        }
    ]
}