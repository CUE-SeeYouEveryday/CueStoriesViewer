{
    "Txt_Name": "Lesson_10304",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "みんなでピクニック行こう～！",
            "voice": "lesson/Lesson_10304/lesson_10304_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "えっ？\nピクニック？　",
            "voice": "lesson/Lesson_10304/lesson_10304_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "なんで？",
            "voice": "lesson/Lesson_10304/lesson_10304_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "「こんにちは、いただきます。」ってサバイバルして、\n色んなモノ食べる話でしょ？　",
            "voice": "lesson/Lesson_10304/lesson_10304_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "うん。\nそうだよ。",
            "voice": "lesson/Lesson_10304/lesson_10304_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "だったら～役作りのためにも、\nピクニックをするのって良いと思わない？",
            "voice": "lesson/Lesson_10304/lesson_10304_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ただ莉子がピクニックに行きたいだけだったりして。",
            "voice": "lesson/Lesson_10304/lesson_10304_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "それもあるんだけどさっ！\n良いでしょ？　明日は休みだし、行こうよ！",
            "voice": "lesson/Lesson_10304/lesson_10304_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "確かに良いかもね。みんなはどう？",
            "voice": "lesson/Lesson_10304/lesson_10304_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ん～良いけど。",
            "voice": "lesson/Lesson_10304/lesson_10304_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "絢はどうする？",
            "voice": "lesson/Lesson_10304/lesson_10304_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私は……。",
            "voice": "lesson/Lesson_10304/lesson_10304_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "嫌なら無理しないで良いよ？",
            "voice": "lesson/Lesson_10304/lesson_10304_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "行く。\nみんなでどこかに行くのって良いと思う。",
            "voice": "lesson/Lesson_10304/lesson_10304_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "なら決定だね！",
            "voice": "lesson/Lesson_10304/lesson_10304_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "（あ、レジャーシートとか買わないと。）",
            "voice": "lesson/Lesson_10304/lesson_10304_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ねぇみんな、ピクニック6ヶ条って知ってる？",
            "voice": "lesson/Lesson_10304/lesson_10304_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ピクニック6ヶ条？",
            "voice": "lesson/Lesson_10304/lesson_10304_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ピクニックに絶対に必要な6つの要素なの。\nわかる？",
            "voice": "lesson/Lesson_10304/lesson_10304_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "う～んと……。\n天気？",
            "voice": "lesson/Lesson_10304/lesson_10304_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そう！\nお日様は必須！",
            "voice": "lesson/Lesson_10304/lesson_10304_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "場所とか時間とか……、食べ物とか、かな？",
            "voice": "lesson/Lesson_10304/lesson_10304_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "すごいね、全部正解！\n後は何だと思う？",
            "voice": "lesson/Lesson_10304/lesson_10304_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "レクリエーション？",
            "voice": "lesson/Lesson_10304/lesson_10304_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "それも正解！\n以上の6つが揃えば、ピクニックは絶対成功するの！",
            "voice": "lesson/Lesson_10304/lesson_10304_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "莉子、それ足りなくない？",
            "voice": "lesson/Lesson_10304/lesson_10304_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ん……？\nなにが？",
            "voice": "lesson/Lesson_10304/lesson_10304_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "天気・場所・時間・食べ物・レクリエーション。\n……って、5つしかないでしょ？",
            "voice": "lesson/Lesson_10304/lesson_10304_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "1つ足りない。",
            "voice": "lesson/Lesson_10304/lesson_10304_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ、ホントだ。",
            "voice": "lesson/Lesson_10304/lesson_10304_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "残りの1つはなんなの？",
            "voice": "lesson/Lesson_10304/lesson_10304_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あれ？　なんだったっけ？\nう～ん……。思い出せない。",
            "voice": "lesson/Lesson_10304/lesson_10304_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "え～気になる～。",
            "voice": "lesson/Lesson_10304/lesson_10304_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "当日までには思い出しとくから、\nとにかくピクニックを楽しもう！",
            "voice": "lesson/Lesson_10304/lesson_10304_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうね。\nじゃあ明日は、Windのみんなでピクニックに行きましょう。",
            "voice": "lesson/Lesson_10304/lesson_10304_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。",
            "voice": "lesson/Lesson_10304/lesson_10304_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "朝早いから絶対に寝坊しちゃ、ダメだよ～！",
            "voice": "lesson/Lesson_10304/lesson_10304_037.mp3"
        }
    ]
}