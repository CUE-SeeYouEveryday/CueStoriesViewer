{
    "Txt_Name": "Lesson_30610",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "凛音さんの経験したタイムループ。",
            "voice": "lesson/Lesson_30610/lesson_30610_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "その秘密のヒントを探すため、私は、ネットで調べていた。",
            "voice": "lesson/Lesson_30610/lesson_30610_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "どう？　いい情報ありそう？",
            "voice": "lesson/Lesson_30610/lesson_30610_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "全然ダメだね。",
            "voice": "lesson/Lesson_30610/lesson_30610_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "出てくるのは、小説とか映画の話だけ。",
            "voice": "lesson/Lesson_30610/lesson_30610_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "小説とか映画とかではあるんだ。",
            "voice": "lesson/Lesson_30610/lesson_30610_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "フィクションだからね。\nそんな話はたくさんある。",
            "voice": "lesson/Lesson_30610/lesson_30610_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "へぇー。そういうのって結末はどうなるの？",
            "voice": "lesson/Lesson_30610/lesson_30610_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "大抵、バッドエンドだね。",
            "voice": "lesson/Lesson_30610/lesson_30610_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そうなの？",
            "voice": "lesson/Lesson_30610/lesson_30610_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "うん……。でも、フィクションと現実は違うから。",
            "voice": "lesson/Lesson_30610/lesson_30610_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そうだよね！\nうーん。わたしみたいな経験した人っていないのかなー？",
            "voice": "lesson/Lesson_30610/lesson_30610_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "それを探してるの。",
            "voice": "lesson/Lesson_30610/lesson_30610_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いたとしたら、絶対SNSに上げるよね。",
            "voice": "lesson/Lesson_30610/lesson_30610_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "だってタイムループだもんね。",
            "voice": "lesson/Lesson_30610/lesson_30610_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "タイムをループするわけだから。",
            "voice": "lesson/Lesson_30610/lesson_30610_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "わたしも書こうと思うもん！",
            "voice": "lesson/Lesson_30610/lesson_30610_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "凛音さん。",
            "voice": "lesson/Lesson_30610/lesson_30610_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "何々？　なんかわかった？",
            "voice": "lesson/Lesson_30610/lesson_30610_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "ちょっと黙ってて。",
            "voice": "lesson/Lesson_30610/lesson_30610_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！　黙ってるね！",
            "voice": "lesson/Lesson_30610/lesson_30610_021.mp3"
        }
    ]
}