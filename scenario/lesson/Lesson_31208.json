{
    "Txt_Name": "Lesson_31208",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あっ、もしかしてたくさん調べてくれてたとか？\nだったら言ってくれたら良かったのに。",
            "voice": "lesson/Lesson_31208/lesson_31208_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "別に調べてはない。",
            "voice": "lesson/Lesson_31208/lesson_31208_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "その手の話はたくさん聞く。\n実家が実家だからな。",
            "voice": "lesson/Lesson_31208/lesson_31208_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "へぇー、そうなんだ。",
            "voice": "lesson/Lesson_31208/lesson_31208_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "うむ。それに、あの島は元は修行の島だった。\nそこから参詣地となったのだ。",
            "voice": "lesson/Lesson_31208/lesson_31208_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "そうなんだ……。",
            "voice": "lesson/Lesson_31208/lesson_31208_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "本当に知ってたの？\nやっぱり調べてきてくれたんじゃ……。",
            "voice": "lesson/Lesson_31208/lesson_31208_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "そうじゃないと言ってるだろ？",
            "voice": "lesson/Lesson_31208/lesson_31208_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あはは、わかった、わかった。\nじゃあさ、この島のどこか、おすすめのお店ある？",
            "voice": "lesson/Lesson_31208/lesson_31208_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "お昼は食べたけど、\nしらすまんとか食べ歩き出来そうじゃん。",
            "voice": "lesson/Lesson_31208/lesson_31208_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あ、わたしも食べたいな。",
            "voice": "lesson/Lesson_31208/lesson_31208_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "いいだろう、島に着いたら案内してやる。\n少し入ったところにある。",
            "voice": "lesson/Lesson_31208/lesson_31208_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ありがとう、志穂ちゃん。",
            "voice": "lesson/Lesson_31208/lesson_31208_013.mp3"
        }
    ]
}