{
    "Txt_Name": "Lesson_10212",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "なんでネギを私の首に巻くの！？",
            "voice": "lesson/Lesson_10212/lesson_10212_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "風邪が治るからに決まってるでしょ？",
            "voice": "lesson/Lesson_10212/lesson_10212_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "それ、効くの……？",
            "voice": "lesson/Lesson_10212/lesson_10212_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "日本古来の、風邪に効く民間療法だよ。\n昔の人はみ～んなこれで治してたんだから。",
            "voice": "lesson/Lesson_10212/lesson_10212_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "なるほど。\n薬じゃないからこれならいいんじゃない？",
            "voice": "lesson/Lesson_10212/lesson_10212_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "でも……。",
            "voice": "lesson/Lesson_10212/lesson_10212_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "でもなに？",
            "voice": "lesson/Lesson_10212/lesson_10212_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私の首はネギの汁を吸わないと思う……。",
            "voice": "lesson/Lesson_10212/lesson_10212_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ごめんなさい。\nネギの汁を吸う首じゃなくて……。",
            "voice": "lesson/Lesson_10212/lesson_10212_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "誰の首も吸わないって！",
            "voice": "lesson/Lesson_10212/lesson_10212_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "そうなの？\n汁吸わないのに風邪に効くの？",
            "voice": "lesson/Lesson_10212/lesson_10212_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "効くと思えば効く！\n気持ちが大事なの！！",
            "voice": "lesson/Lesson_10212/lesson_10212_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "日本、アメイジング……。",
            "voice": "lesson/Lesson_10212/lesson_10212_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あと梅干しもこめかみに貼ったら？",
            "voice": "lesson/Lesson_10212/lesson_10212_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "えっ？\nパック？",
            "voice": "lesson/Lesson_10212/lesson_10212_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "風邪の時にいいらしいんだよ。",
            "voice": "lesson/Lesson_10212/lesson_10212_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "こめかみに？　梅干しで？？\nなんで！？",
            "voice": "lesson/Lesson_10212/lesson_10212_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "さあ。",
            "voice": "lesson/Lesson_10212/lesson_10212_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "詳しい事はご先祖様にきかないと……。",
            "voice": "lesson/Lesson_10212/lesson_10212_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "で、でも食べ物でしょ？\nいいの？　",
            "voice": "lesson/Lesson_10212/lesson_10212_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "うだうだ言わない！　さぁ首にネギ、\nこめかみに梅干し貼るよ！",
            "voice": "lesson/Lesson_10212/lesson_10212_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ちょ、ちょっと待って～！！",
            "voice": "lesson/Lesson_10212/lesson_10212_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あっ。柚葉ちゃん、おはよう。\n具合はどう？",
            "voice": "lesson/Lesson_10212/lesson_10212_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "それが……。\nだいぶ楽になったわ。",
            "voice": "lesson/Lesson_10212/lesson_10212_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "えっ？\nうそ！？",
            "voice": "lesson/Lesson_10212/lesson_10212_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "まさか本当に治るなんて驚きだわ。",
            "voice": "lesson/Lesson_10212/lesson_10212_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あたしも今度やってみよ。",
            "voice": "lesson/Lesson_10212/lesson_10212_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "えっ？\n……私で試したってこと？",
            "voice": "lesson/Lesson_10212/lesson_10212_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "へへ、でも治ったんだからいいじゃん！",
            "voice": "lesson/Lesson_10212/lesson_10212_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "先人の知恵さねぇ。",
            "voice": "lesson/Lesson_10212/lesson_10212_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "まあいいわ。よし、元気になったから、\nネギと梅干したくさん買いに行こうっと！",
            "voice": "lesson/Lesson_10212/lesson_10212_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "なんで……？",
            "voice": "lesson/Lesson_10212/lesson_10212_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "いつ風邪ひいても良いようによ、決まってるじゃない！",
            "voice": "lesson/Lesson_10212/lesson_10212_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "えっ？\nまた腹出し昼寝やるつもり？",
            "voice": "lesson/Lesson_10212/lesson_10212_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "全然懲りてない！",
            "voice": "lesson/Lesson_10212/lesson_10212_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "もう柚葉ちゃん……。",
            "voice": "lesson/Lesson_10212/lesson_10212_036.mp3"
        }
    ]
}