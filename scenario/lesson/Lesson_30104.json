{
    "Txt_Name": "Lesson_30104",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ふっふっふ……。\nみんなビックリするだろうな～……。",
            "voice": "lesson/Lesson_30104/lesson_30104_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "まさかこのクーラーボックスに、\nブロック肉が入ってるとは思わないでしょ～。　",
            "voice": "lesson/Lesson_30104/lesson_30104_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "どうせみんな自分のこと、\nケチだって思ってるよ……。",
            "voice": "lesson/Lesson_30104/lesson_30104_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ところが……。\nバーベキューに関しては～、そうじゃないんだなぁ～。",
            "voice": "lesson/Lesson_30104/lesson_30104_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ほのかは、きっと……、大喜びするだろうなぁ。\n陽菜なんて、感動して泣いちゃうかも！",
            "voice": "lesson/Lesson_30104/lesson_30104_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "志穂は……。わかんない。\nたぶん喜ぶでしょ。",
            "voice": "lesson/Lesson_30104/lesson_30104_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "う～ん、いい天気！\n絶好のバーベキュー日和ね！",
            "voice": "lesson/Lesson_30104/lesson_30104_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "早くみんな来ないかな～！　",
            "voice": "lesson/Lesson_30104/lesson_30104_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "デザートはもう決めてるの。\nバナナを買おう！",
            "voice": "lesson/Lesson_30104/lesson_30104_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "え、バーベキューで……、バナナ……？　",
            "voice": "lesson/Lesson_30104/lesson_30104_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "心配しなくて大丈夫！\nおいしいから、任せといて。",
            "voice": "lesson/Lesson_30104/lesson_30104_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そりゃあ、おいしいだろうけど……。　",
            "voice": "lesson/Lesson_30104/lesson_30104_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "あっちに安いバナナ売ってたなぁ……。\nちょっと行ってくる。",
            "voice": "lesson/Lesson_30104/lesson_30104_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "みんなビックリしてるだろうなぁ～。\nまさかバナナとは思わなかったでしょ。",
            "voice": "lesson/Lesson_30104/lesson_30104_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……さて、あとは肉をいつ出すかだなぁ。\n何か……、みんな不満がたまってるみたいだし……。",
            "voice": "lesson/Lesson_30104/lesson_30104_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "早めに出して、安心させた方がいいかも……。　",
            "voice": "lesson/Lesson_30104/lesson_30104_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ホントはバーベキュー場で……、\n焼く直前に出そうかと思ったけど……。",
            "voice": "lesson/Lesson_30104/lesson_30104_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "よし！\nトングも借りられたし、いよいよブロック肉を焼くぞぉ～！",
            "voice": "lesson/Lesson_30104/lesson_30104_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "しかし、カラスが多いなぁ……。　",
            "voice": "lesson/Lesson_30104/lesson_30104_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あ、舞花ちゃんが帰ってきた！　",
            "voice": "lesson/Lesson_30104/lesson_30104_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "なに？\n帰ってきちゃ悪いのー？　",
            "voice": "lesson/Lesson_30104/lesson_30104_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "そ、そうじゃないけど……。　",
            "voice": "lesson/Lesson_30104/lesson_30104_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "あれ？\nほのか、トマトは？　",
            "voice": "lesson/Lesson_30104/lesson_30104_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "うん……、それがね……。　",
            "voice": "lesson/Lesson_30104/lesson_30104_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ああ！\nあれ見て！　",
            "voice": "lesson/Lesson_30104/lesson_30104_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "どうした？\n……ああぁ！！　",
            "voice": "lesson/Lesson_30104/lesson_30104_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "お肉が、お肉が、カラスに持ってかれちゃう！",
            "voice": "lesson/Lesson_30104/lesson_30104_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "飛んで逃げたよ！\n後を追いかけよう！",
            "voice": "lesson/Lesson_30104/lesson_30104_028.mp3"
        }
    ]
}