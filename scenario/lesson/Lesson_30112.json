{
    "Txt_Name": "Lesson_30112",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ああ～、肉最高！　",
            "voice": "lesson/Lesson_30112/lesson_30112_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "みんなで食べると、美味しいねー！　",
            "voice": "lesson/Lesson_30112/lesson_30112_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "いろいろあったけど、結果オーライだよ～。",
            "voice": "lesson/Lesson_30112/lesson_30112_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "そういえばさっき、\n美晴さんからメッセージきてたんだ……。",
            "voice": "lesson/Lesson_30112/lesson_30112_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "美晴さん？\n何て？　",
            "voice": "lesson/Lesson_30112/lesson_30112_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "「雨降ってたけど大丈夫？」……って、\n大丈夫でしたって、返しとこっと。",
            "voice": "lesson/Lesson_30112/lesson_30112_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "陽菜？\n……スマホ使ったの？　",
            "voice": "lesson/Lesson_30112/lesson_30112_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あ……、\nそうだった……。　",
            "voice": "lesson/Lesson_30112/lesson_30112_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "約束したよね！\nスマホ見たらどうなるんだっけ？",
            "voice": "lesson/Lesson_30112/lesson_30112_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ダッシュと……、スクワット……。　",
            "voice": "lesson/Lesson_30112/lesson_30112_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "でもみんな、結構スマホ使ってるけど？　",
            "voice": "lesson/Lesson_30112/lesson_30112_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "緊急の場合はいいの。\n天気予報見たり、電話したり。",
            "voice": "lesson/Lesson_30112/lesson_30112_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "でも、それ以外はダメだよね？　",
            "voice": "lesson/Lesson_30112/lesson_30112_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ち、違うの！　さっき天気予報を見た時、\n美晴さんのメッセージが見えて……。",
            "voice": "lesson/Lesson_30112/lesson_30112_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "たまたま見えたってこと？\nじゃあ、返信はしないよね！",
            "voice": "lesson/Lesson_30112/lesson_30112_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "でも、心配してると思うし……。　",
            "voice": "lesson/Lesson_30112/lesson_30112_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "帰りの電車で返信すればいいでしょ。\nもしくは、帰って報告すればいい。",
            "voice": "lesson/Lesson_30112/lesson_30112_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "今は目の前のバーベキューを楽しむ！\nわかった？",
            "voice": "lesson/Lesson_30112/lesson_30112_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "はい、わかりました……。　",
            "voice": "lesson/Lesson_30112/lesson_30112_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ほのか、厳しい……。　",
            "voice": "lesson/Lesson_30112/lesson_30112_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "……あれ？\nそういえば、今日……。　",
            "voice": "lesson/Lesson_30112/lesson_30112_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "何？　今日何かあった？　",
            "voice": "lesson/Lesson_30112/lesson_30112_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "いやいや、何でもない。　",
            "voice": "lesson/Lesson_30112/lesson_30112_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "（やっぱそうだ！\n今日は、総合格闘技の大会があったんだ……。）",
            "voice": "lesson/Lesson_30112/lesson_30112_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "（結果……、どうなったんだろ……。）　",
            "voice": "lesson/Lesson_30112/lesson_30112_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "（ああ！　気になる！　知りたい！\n……帰りまで待てない！）",
            "voice": "lesson/Lesson_30112/lesson_30112_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "何か、ほのかの様子が変なんだけど？　",
            "voice": "lesson/Lesson_30112/lesson_30112_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "どうした？\nお腹でも痛い？　",
            "voice": "lesson/Lesson_30112/lesson_30112_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "（どうする……？\nお腹痛いフリしてトイレ行く……？）",
            "voice": "lesson/Lesson_30112/lesson_30112_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "（その時、スマホで結果を確認する……？）　",
            "voice": "lesson/Lesson_30112/lesson_30112_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "大丈夫？　薬飲む？　",
            "voice": "lesson/Lesson_30112/lesson_30112_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "（ああ！　ダメだ！\nみんなを裏切ることなんて……、できない。）",
            "voice": "lesson/Lesson_30112/lesson_30112_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "あっ！\nほのかがスマホを見た！　",
            "voice": "lesson/Lesson_30112/lesson_30112_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "何してんの？\n約束が違うでしょー！　",
            "voice": "lesson/Lesson_30112/lesson_30112_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "約束は守るよ。\nそれじゃ、行ってきます！　",
            "voice": "lesson/Lesson_30112/lesson_30112_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "え？　どこ行くの？　",
            "voice": "lesson/Lesson_30112/lesson_30112_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "決まってんじゃない。\nダッシュとスクワットよ。",
            "voice": "lesson/Lesson_30112/lesson_30112_037.mp3"
        }
    ]
}