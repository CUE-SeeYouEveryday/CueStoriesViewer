{
    "Txt_Name": "Lesson_31811",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ねえ、なるちゃん。",
            "voice": "lesson/Lesson_31811/lesson_31811_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "凛音さん、どうしたの？",
            "voice": "lesson/Lesson_31811/lesson_31811_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いつ頃から気付いてたの？\nリエンヌが犯人って。",
            "voice": "lesson/Lesson_31811/lesson_31811_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "覚えてない。",
            "voice": "lesson/Lesson_31811/lesson_31811_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "覚えてないってどういうこと？",
            "voice": "lesson/Lesson_31811/lesson_31811_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "私が気付いたわけじゃないから。",
            "voice": "lesson/Lesson_31811/lesson_31811_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ああ、なるほどー。\nそっかー、それならしょうがないねー。",
            "voice": "lesson/Lesson_31811/lesson_31811_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "うん。",
            "voice": "lesson/Lesson_31811/lesson_31811_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "え？　今のしょうがないで済ますところ？",
            "voice": "lesson/Lesson_31811/lesson_31811_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えー、だってなるちゃんがそう言うんだもん。\nしょうがないよー。",
            "voice": "lesson/Lesson_31811/lesson_31811_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "うん。ありがとう凛音さん。",
            "voice": "lesson/Lesson_31811/lesson_31811_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ううん。どういたしましてだよー。",
            "voice": "lesson/Lesson_31811/lesson_31811_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "はぁ……。ほんと、変わった人たち……。",
            "voice": "lesson/Lesson_31811/lesson_31811_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "でも……、私なら、\n「私が気付いたわけじゃない」って言えるかしら……。",
            "voice": "lesson/Lesson_31811/lesson_31811_014.mp3"
        }
    ]
}