{
    "Txt_Name": "Lesson_10111",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "（黒川ことりは1人暮らしの設定……。\nだからこの家にはわたし1人しかいない……。）",
            "voice": "lesson/Lesson_10111/lesson_10111_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ねぇ、ほのか。",
            "voice": "lesson/Lesson_10111/lesson_10111_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "……。",
            "voice": "lesson/Lesson_10111/lesson_10111_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ねぇ、ほのかってば～。",
            "voice": "lesson/Lesson_10111/lesson_10111_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "（聞こえない。あれは幻聴……。\n1人暮らしの設定なんだから誰もいない……。）",
            "voice": "lesson/Lesson_10111/lesson_10111_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "入るよ～？",
            "voice": "lesson/Lesson_10111/lesson_10111_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "うっわ！　何！？\nこの熱気は！",
            "voice": "lesson/Lesson_10111/lesson_10111_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "（……あそこには誰もいない。）",
            "voice": "lesson/Lesson_10111/lesson_10111_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "まるでサウナ……。",
            "voice": "lesson/Lesson_10111/lesson_10111_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ほのかちゃん、どうしちゃったの！？",
            "voice": "lesson/Lesson_10111/lesson_10111_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "見て！\n体から出る湯気で窓が曇ってる！",
            "voice": "lesson/Lesson_10111/lesson_10111_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ホントだ！？\n部屋で運動してたの？",
            "voice": "lesson/Lesson_10111/lesson_10111_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "そして目を閉じて座禅を組んでいる……。\n頑なにこちらを見ようとしない……。",
            "voice": "lesson/Lesson_10111/lesson_10111_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ほのか、どうしたの？",
            "voice": "lesson/Lesson_10111/lesson_10111_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ほのかちゃん、聞こえてる？",
            "voice": "lesson/Lesson_10111/lesson_10111_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "……。",
            "voice": "lesson/Lesson_10111/lesson_10111_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ほのか！",
            "voice": "lesson/Lesson_10111/lesson_10111_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "もう～！\nせっかく役作りしてるのに、邪魔しないでよ！！",
            "voice": "lesson/Lesson_10111/lesson_10111_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "役作り？",
            "voice": "lesson/Lesson_10111/lesson_10111_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そう。\n黒川ことりは1人暮らししてる。",
            "voice": "lesson/Lesson_10111/lesson_10111_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "つまり常に孤独って事でしょ？\n常に孤独と戦ってるの。わたしはその孤独を体感したいの。",
            "voice": "lesson/Lesson_10111/lesson_10111_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "でもここは寮だから。",
            "voice": "lesson/Lesson_10111/lesson_10111_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "まさか、そのために1人でずっと部屋に籠って運動してたの？",
            "voice": "lesson/Lesson_10111/lesson_10111_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "全ては完璧な役作りのためなの！！",
            "voice": "lesson/Lesson_10111/lesson_10111_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "そうだったんだ……。",
            "voice": "lesson/Lesson_10111/lesson_10111_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "とりあえず、シャワー浴びて部屋から出なよ。\n体壊すよ！",
            "voice": "lesson/Lesson_10111/lesson_10111_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "いやだ！",
            "voice": "lesson/Lesson_10111/lesson_10111_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "みんな心配してるの！",
            "voice": "lesson/Lesson_10111/lesson_10111_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "じゃあみんなが気にしなければ良いでしょ！？",
            "voice": "lesson/Lesson_10111/lesson_10111_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "また閉じ籠った。\n天照大神だな。",
            "voice": "lesson/Lesson_10111/lesson_10111_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "もう～、意味わかんない！！",
            "voice": "lesson/Lesson_10111/lesson_10111_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ほっといて！！",
            "voice": "lesson/Lesson_10111/lesson_10111_032.mp3"
        }
    ]
}