{
    "Txt_Name": "Lesson_10208",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あいり～、\n一緒におやつ食べない？",
            "voice": "lesson/Lesson_10208/lesson_10208_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "大丈夫～",
            "voice": "lesson/Lesson_10208/lesson_10208_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "美味しいよ？\nあいりの好きなチョコレートだよ？",
            "voice": "lesson/Lesson_10208/lesson_10208_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あとで食べるよ……。",
            "voice": "lesson/Lesson_10208/lesson_10208_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "具合悪いの？",
            "voice": "lesson/Lesson_10208/lesson_10208_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "そうじゃないんだけど……。",
            "voice": "lesson/Lesson_10208/lesson_10208_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "開けるよ。",
            "voice": "lesson/Lesson_10208/lesson_10208_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "うわ、暗い！",
            "voice": "lesson/Lesson_10208/lesson_10208_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "カーテン閉め切って何してるの？",
            "voice": "lesson/Lesson_10208/lesson_10208_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "今朝の占いで、太陽を避けろとも言われたの。",
            "voice": "lesson/Lesson_10208/lesson_10208_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "無理でしょそんなの。",
            "voice": "lesson/Lesson_10208/lesson_10208_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "その占い、守らないとどうなるの？",
            "voice": "lesson/Lesson_10208/lesson_10208_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "どうにかなっちゃうの。",
            "voice": "lesson/Lesson_10208/lesson_10208_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "どうにかって？",
            "voice": "lesson/Lesson_10208/lesson_10208_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "わかんないけど、どうにかなっちゃうの。",
            "voice": "lesson/Lesson_10208/lesson_10208_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "こういう神様いたよね？",
            "voice": "lesson/Lesson_10208/lesson_10208_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "なに言ってるの？\n悠希。",
            "voice": "lesson/Lesson_10208/lesson_10208_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "アマテラスオオカミだっけ？\n岩の中に隠れちゃうやつ。",
            "voice": "lesson/Lesson_10208/lesson_10208_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ああ、なんかあったね？\n太陽の神様が隠れちゃって、暗闇に包まれるんだよね？",
            "voice": "lesson/Lesson_10208/lesson_10208_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あれは、どうやって岩の中から引き戻したんだっけ？",
            "voice": "lesson/Lesson_10208/lesson_10208_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あれはね……。\n確か、女神さまが裸で踊ったのよ。",
            "voice": "lesson/Lesson_10208/lesson_10208_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ププっ、なにそれ！",
            "voice": "lesson/Lesson_10208/lesson_10208_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "それをみた神様たちが大うけして、\nその笑い声が気になって、岩穴から出てきたのよ。",
            "voice": "lesson/Lesson_10208/lesson_10208_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "詳しいね。",
            "voice": "lesson/Lesson_10208/lesson_10208_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "笑いって太陽の明かりまで取り戻すんだなぁって、\n感心したもんよ。",
            "voice": "lesson/Lesson_10208/lesson_10208_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "よし、じゃああたし達も踊ろう。",
            "voice": "lesson/Lesson_10208/lesson_10208_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "裸で？",
            "voice": "lesson/Lesson_10208/lesson_10208_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "いやよ。",
            "voice": "lesson/Lesson_10208/lesson_10208_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "なんで？\nあいりのためだよ。",
            "voice": "lesson/Lesson_10208/lesson_10208_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "悠希ちゃん、ありがとう。\nでも違うの。",
            "voice": "lesson/Lesson_10208/lesson_10208_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "なにが違うの？",
            "voice": "lesson/Lesson_10208/lesson_10208_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "収録スタジオって、きっと暗いでしょ？\nその暗さに、慣れる訓練でもあるの。",
            "voice": "lesson/Lesson_10208/lesson_10208_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あ、そう……。\nで、慣れた？",
            "voice": "lesson/Lesson_10208/lesson_10208_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "全然ダメ……。\n余計怖くなっちゃった……。",
            "voice": "lesson/Lesson_10208/lesson_10208_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "やめたら？\nだいたい、スタジオ暗くないし。",
            "voice": "lesson/Lesson_10208/lesson_10208_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あー！\n良い考え浮かんだ！",
            "voice": "lesson/Lesson_10208/lesson_10208_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ほんとに！？　\n……って喜びたいけどかなり不安だわ。",
            "voice": "lesson/Lesson_10208/lesson_10208_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "みんなで今晩、レッスン室に行こう！",
            "voice": "lesson/Lesson_10208/lesson_10208_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "れ、レッスン室に？",
            "voice": "lesson/Lesson_10208/lesson_10208_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "今、スタジオ風になってるじゃない？\nちょうどいいと思って！！",
            "voice": "lesson/Lesson_10208/lesson_10208_040.mp3"
        }
    ]
}