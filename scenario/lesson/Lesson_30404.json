{
    "Txt_Name": "Lesson_30404",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "今年もクリスマスの季節かぁ。",
            "voice": "lesson/Lesson_30404/lesson_30404_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "なんか年々早く感じるよね。",
            "voice": "lesson/Lesson_30404/lesson_30404_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "なんて、おばさんみたいなこと言っちゃった。",
            "voice": "lesson/Lesson_30404/lesson_30404_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "え？　何？　何か言った？",
            "voice": "lesson/Lesson_30404/lesson_30404_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "うん。割とずーっと話しかけてた。\nまさか独り言になってたとはね。",
            "voice": "lesson/Lesson_30404/lesson_30404_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ごめんごめん。イルミネーションが綺麗で。",
            "voice": "lesson/Lesson_30404/lesson_30404_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "綺麗だよね、確かに。特別な気分になっちゃう。",
            "voice": "lesson/Lesson_30404/lesson_30404_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そうだねぇ。なんでだろうねぇ。\n日本のお祭りってわけでもないのに。",
            "voice": "lesson/Lesson_30404/lesson_30404_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ねえ。クリスマスのプレゼントで、1番嬉しかったのって何？ ",
            "voice": "lesson/Lesson_30404/lesson_30404_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "1番嬉しかったのはね、プラモ。",
            "voice": "lesson/Lesson_30404/lesson_30404_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "なるほどね。じゃあさ、1番、期待外れだったのは？",
            "voice": "lesson/Lesson_30404/lesson_30404_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "プラモ。",
            "voice": "lesson/Lesson_30404/lesson_30404_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……どういうことよ。",
            "voice": "lesson/Lesson_30404/lesson_30404_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "2年連続で同じものをもらったことがあったの。\nその時だよ。本当はサンタさんなんかいないって知ったの。",
            "voice": "lesson/Lesson_30404/lesson_30404_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あー私も、サンタなんか信じてなかった。",
            "voice": "lesson/Lesson_30404/lesson_30404_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "へえ。じゃあ、プレゼントとかどうしたの？",
            "voice": "lesson/Lesson_30404/lesson_30404_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "うーん……、じゃあちょっと、そこでお茶でもしながら話さない？",
            "voice": "lesson/Lesson_30404/lesson_30404_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "おっ！　いいね！",
            "voice": "lesson/Lesson_30404/lesson_30404_018.mp3"
        }
    ]
}