{
    "Txt_Name": "Lesson_31210",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "あー、帰ってきちゃったー。\nもうちょっと休みを満喫したかったなー。",
            "voice": "lesson/Lesson_31210/lesson_31210_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "しかも、帰ってきた瞬間、ジャーマネからの電話。",
            "voice": "lesson/Lesson_31210/lesson_31210_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ものすごいタイミングだったね。",
            "voice": "lesson/Lesson_31210/lesson_31210_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ほんとだよ。\nあっという間に現実に戻された。",
            "voice": "lesson/Lesson_31210/lesson_31210_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "でも、それがマネージャーさんのお仕事だし……。",
            "voice": "lesson/Lesson_31210/lesson_31210_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ジャーマネが担当を放置してたら、それはそれで問題だろ。",
            "voice": "lesson/Lesson_31210/lesson_31210_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "そうだけどさ……。",
            "voice": "lesson/Lesson_31210/lesson_31210_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "まあ、いい休みになったね。\n色々と食べたり、歩き回ったり……。",
            "voice": "lesson/Lesson_31210/lesson_31210_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "たくさん夏の思い出が出来たね。",
            "voice": "lesson/Lesson_31210/lesson_31210_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "それはそうだね。",
            "voice": "lesson/Lesson_31210/lesson_31210_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "うん！\nだから、明日からも、また頑張っていこう！",
            "voice": "lesson/Lesson_31210/lesson_31210_011.mp3"
        }
    ]
}