{
    "Txt_Name": "Lesson_30505",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "美晴は、楽しい？　この、ロケ。　",
            "voice": "lesson/Lesson_30505/lesson_30505_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん。まるで、修学旅行みたいじゃない？",
            "voice": "lesson/Lesson_30505/lesson_30505_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、美晴もそう思ってた？　\n実は私も！",
            "voice": "lesson/Lesson_30505/lesson_30505_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "普段、寮で一緒に暮らしてるはずなのに、\nホテルで一緒に寝てると、途端に非日常になるんだもの。",
            "voice": "lesson/Lesson_30505/lesson_30505_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうそう。　",
            "voice": "lesson/Lesson_30505/lesson_30505_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "隣で、莉子とまほろも同じ気持ちなのかな？　",
            "voice": "lesson/Lesson_30505/lesson_30505_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "どうかな。　",
            "voice": "lesson/Lesson_30505/lesson_30505_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あの2人、ここでもきっといつもみたいに、　\nよくわかんない言い合いしてるんじゃないかな。",
            "voice": "lesson/Lesson_30505/lesson_30505_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。そうね。　",
            "voice": "lesson/Lesson_30505/lesson_30505_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あの2人、妙に息があってるよね。　",
            "voice": "lesson/Lesson_30505/lesson_30505_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうね。　",
            "voice": "lesson/Lesson_30505/lesson_30505_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "でも、絢とまほろも、そして絢と莉子も、　\n同じくらい息があってるように見えるわよ？",
            "voice": "lesson/Lesson_30505/lesson_30505_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ほんと？　そうかな？　",
            "voice": "lesson/Lesson_30505/lesson_30505_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうよ。みんな息ピッタリ。　\nわたしちょっと嫉妬しちゃう。",
            "voice": "lesson/Lesson_30505/lesson_30505_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "えー？\n美晴だって、莉子ともまほろとも、すごく息があってるよ？",
            "voice": "lesson/Lesson_30505/lesson_30505_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。そう？　",
            "voice": "lesson/Lesson_30505/lesson_30505_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。　",
            "voice": "lesson/Lesson_30505/lesson_30505_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "それってきっと、\n4人が4人で息があってるってことね。　",
            "voice": "lesson/Lesson_30505/lesson_30505_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "確かにそうかも。",
            "voice": "lesson/Lesson_30505/lesson_30505_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "だから、ロケも楽しいんじゃない？　",
            "voice": "lesson/Lesson_30505/lesson_30505_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん……。　\nそういうことなのかな……。",
            "voice": "lesson/Lesson_30505/lesson_30505_021.mp3"
        }
    ]
}