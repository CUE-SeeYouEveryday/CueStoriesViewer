{
    "Txt_Name": "Lesson_31104",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "じゃ、あたし、プラモ見てくる！",
            "voice": "lesson/Lesson_31104/lesson_31104_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "悠希、お店の中で走っちゃダメよ。",
            "voice": "lesson/Lesson_31104/lesson_31104_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "わかってるって。",
            "voice": "lesson/Lesson_31104/lesson_31104_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あ、ほら、あいりが行きたいのはあっちじゃない？",
            "voice": "lesson/Lesson_31104/lesson_31104_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "うん。ありがとう悠希ちゃん！",
            "voice": "lesson/Lesson_31104/lesson_31104_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "もう、あいりまで……、まったく。",
            "voice": "lesson/Lesson_31104/lesson_31104_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ふふっ。\nチサは何か欲しい物はないの？",
            "voice": "lesson/Lesson_31104/lesson_31104_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "うーん。ここでは、特に無いかな……。",
            "voice": "lesson/Lesson_31104/lesson_31104_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "そうなの？\nそれじゃあ、チサは退屈ね……。",
            "voice": "lesson/Lesson_31104/lesson_31104_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "え？　ううん。\nこういうの、何故か退屈じゃないから。",
            "voice": "lesson/Lesson_31104/lesson_31104_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ふふ。不思議よね。",
            "voice": "lesson/Lesson_31104/lesson_31104_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ふーん……。じゃあ、チサ。",
            "voice": "lesson/Lesson_31104/lesson_31104_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "カネコのバイクのフィギュア、\n探すの手伝ってくれない？",
            "voice": "lesson/Lesson_31104/lesson_31104_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "いいわよ。\nでも、あるかしら……。",
            "voice": "lesson/Lesson_31104/lesson_31104_014.mp3"
        }
    ]
}