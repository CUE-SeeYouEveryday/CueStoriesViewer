{
    "Txt_Name": "Lesson_20304",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……はーい。\n行ってらっしゃいー。",
            "voice": "lesson/Lesson_20304/lesson_20304_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "みんなちゃんと授業受けてくるのよー？\n寝たらダメだからねー！",
            "voice": "lesson/Lesson_20304/lesson_20304_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……さて。\n洗いものやっちゃうか。",
            "voice": "lesson/Lesson_20304/lesson_20304_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ふぅ～。\nこんなものかな。",
            "voice": "lesson/Lesson_20304/lesson_20304_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "今日はバイトも休みだし、\n平日の昼間はのんびりしてるわね～。",
            "voice": "lesson/Lesson_20304/lesson_20304_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "そうだな。",
            "voice": "lesson/Lesson_20304/lesson_20304_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "うわっ……。\n志穂、いたの？",
            "voice": "lesson/Lesson_20304/lesson_20304_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "いたらまずかったか？",
            "voice": "lesson/Lesson_20304/lesson_20304_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "いやぁ、そんなことはないけど……。",
            "voice": "lesson/Lesson_20304/lesson_20304_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "（独り言聞かれて、ちょっと恥ずかしい……。）",
            "voice": "lesson/Lesson_20304/lesson_20304_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "千紗は偉いな。",
            "voice": "lesson/Lesson_20304/lesson_20304_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "えっ……。\nそれって、どういうこと？",
            "voice": "lesson/Lesson_20304/lesson_20304_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "みんなの分の洗いものや、洗濯まで進んでやってる。",
            "voice": "lesson/Lesson_20304/lesson_20304_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あ～。……っま、あの子達に任せといたら不安だしねー。\n家事は慣れてるから。",
            "voice": "lesson/Lesson_20304/lesson_20304_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "そうか。",
            "voice": "lesson/Lesson_20304/lesson_20304_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "うん。",
            "voice": "lesson/Lesson_20304/lesson_20304_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……。",
            "voice": "lesson/Lesson_20304/lesson_20304_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "（き、気まずい……。\n　志穂ってどういう子か、あんまり掴めてないのよねぇ……。）",
            "voice": "lesson/Lesson_20304/lesson_20304_018.mp3"
        }
    ]
}