{
    "Txt_Name": "Lesson_30607",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "この時は、まだ普通の1日だった。",
            "voice": "lesson/Lesson_30607/lesson_30607_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "頑張ってね、凛音。",
            "voice": "lesson/Lesson_30607/lesson_30607_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！　頑張ってくる！！",
            "voice": "lesson/Lesson_30607/lesson_30607_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "これからオーディション？　気合入ってるね。",
            "voice": "lesson/Lesson_30607/lesson_30607_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "さとちん！　うん、気合十分だよ！\nさとちんはどこか行ってきたの？",
            "voice": "lesson/Lesson_30607/lesson_30607_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "うん。図書館に行こうって思ったけど、この霧だし雷も鳴るし、\n引き返してきた。",
            "voice": "lesson/Lesson_30607/lesson_30607_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そうなんだー……。",
            "voice": "lesson/Lesson_30607/lesson_30607_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "マネージャーさんは来てくれるの？",
            "voice": "lesson/Lesson_30607/lesson_30607_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん。スタジオで待ち合わせしてる。",
            "voice": "lesson/Lesson_30607/lesson_30607_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "霧のせいで見通しが悪いから、気をつけてね。",
            "voice": "lesson/Lesson_30607/lesson_30607_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！　ありがとう！",
            "voice": "lesson/Lesson_30607/lesson_30607_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あ、マネージャーさん。おはようございます。",
            "voice": "lesson/Lesson_30607/lesson_30607_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "おはよう。なんだか気合入ってるね。",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "この作品には、どうしても出たいですから！",
            "voice": "lesson/Lesson_30607/lesson_30607_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "絶対に、主役の座を手にしてみせます！",
            "voice": "lesson/Lesson_30607/lesson_30607_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "凛音がそこまで闘志をむき出しにするの珍しいね。\n頑張って。",
            "voice": ""
        },
        {
            "index": 17,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "オーディションを受けるのは、わたしだけですか？",
            "voice": "lesson/Lesson_30607/lesson_30607_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あと3人いるはずだよ？\nまだ時間が早いから。",
            "voice": ""
        },
        {
            "index": 19,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ライバルは3人かぁ。\n頑張ろっと！",
            "voice": "lesson/Lesson_30607/lesson_30607_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あ、監督さんが来た。挨拶して。",
            "voice": ""
        },
        {
            "index": 21,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はい！",
            "voice": "lesson/Lesson_30607/lesson_30607_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "エールブルー所属、明神凛音です。\nよろしくお願いします！",
            "voice": "lesson/Lesson_30607/lesson_30607_022.mp3"
        }
    ]
}