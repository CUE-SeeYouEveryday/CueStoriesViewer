{
    "Txt_Name": "Lesson_20210",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "車で行こうドライブスポット10選……。\nへぇ～楽しそう。",
            "voice": "lesson/Lesson_20210/lesson_20210_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "旅行のパンフレットって面白いでしょ？　\nドライブって最高だよ。",
            "voice": "lesson/Lesson_20210/lesson_20210_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "どうしたの？　これ？",
            "voice": "lesson/Lesson_20210/lesson_20210_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "街に行ったらパンフレットあったから貰ってきたんだ。",
            "voice": "lesson/Lesson_20210/lesson_20210_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "いいなぁ。\nどこか行ってみたい。",
            "voice": "lesson/Lesson_20210/lesson_20210_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "じゃあ、週末行っちゃう？\nあたしのは車じゃなくてバイクだけど。",
            "voice": "lesson/Lesson_20210/lesson_20210_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "いいの？",
            "voice": "lesson/Lesson_20210/lesson_20210_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "もちろん！\nどこだって乗せてってあげるよ。",
            "voice": "lesson/Lesson_20210/lesson_20210_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "行く行く！",
            "voice": "lesson/Lesson_20210/lesson_20210_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "え～っとじゃあどこ行こうか？",
            "voice": "lesson/Lesson_20210/lesson_20210_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（あ、柚葉ちゃんと莉子ちゃんが何か話してる。）",
            "voice": "lesson/Lesson_20210/lesson_20210_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "柚葉、行きたいところないの？",
            "voice": "lesson/Lesson_20210/lesson_20210_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "東京観光してみたい。",
            "voice": "lesson/Lesson_20210/lesson_20210_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（珍しい、柚葉ちゃんと莉子ちゃんがお出掛けする話してる。\n　面白そうだから、ちょっと聞いてみちゃお。）",
            "voice": "lesson/Lesson_20210/lesson_20210_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "観光って言ったらやっぱり、お寺かな。",
            "voice": "lesson/Lesson_20210/lesson_20210_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私、あそこ行ってみたい、\nチェーンソー寺。",
            "voice": "lesson/Lesson_20210/lesson_20210_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（……え？　ん？　ちぇーんそー寺？）",
            "voice": "lesson/Lesson_20210/lesson_20210_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ～あそこかぁ。\n良いかもね！",
            "voice": "lesson/Lesson_20210/lesson_20210_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（り、莉子ちゃん……。今ので納得したの？）",
            "voice": "lesson/Lesson_20210/lesson_20210_019.mp3"
        }
    ]
}