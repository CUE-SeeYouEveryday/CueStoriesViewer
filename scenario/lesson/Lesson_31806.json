{
    "Txt_Name": "Lesson_31806",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "すっかりマネージャーさんの遊びに付き合っちゃったわね。",
            "voice": "lesson/Lesson_31806/lesson_31806_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん。でもわたし楽しかったよー。",
            "voice": "lesson/Lesson_31806/lesson_31806_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "凛音もなかなか役になりきってたじゃない。",
            "voice": "lesson/Lesson_31806/lesson_31806_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えへへー。\nこれでも普段からエチュードとかで練習してるからねー。",
            "voice": "lesson/Lesson_31806/lesson_31806_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そうね。\n私、凛音のこと少し甘く見てたわ。ごめんなさい。",
            "voice": "lesson/Lesson_31806/lesson_31806_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えー、どういうこと？",
            "voice": "lesson/Lesson_31806/lesson_31806_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "昭和時代の華道の大家の娘、そして殺人事件の現場なんて、\n凛音、ちゃんとついてこれるかしらって。",
            "voice": "lesson/Lesson_31806/lesson_31806_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "勿論いっぱいいっぱいだったよー。",
            "voice": "lesson/Lesson_31806/lesson_31806_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そうなの？\nそうは見えなかったわ。",
            "voice": "lesson/Lesson_31806/lesson_31806_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "どう？　ちゃんとできてた？",
            "voice": "lesson/Lesson_31806/lesson_31806_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ふふっ。そうね。できてたわよ。",
            "voice": "lesson/Lesson_31806/lesson_31806_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えへへー。よかったー。\nねえ、今から行くお店、カレーライスあるかな？",
            "voice": "lesson/Lesson_31806/lesson_31806_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "さあ。行ってみないとわからないわね。",
            "voice": "lesson/Lesson_31806/lesson_31806_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "飲み物はどうしようかなー。\nうん。わたしミックスジュースにしようっと。",
            "voice": "lesson/Lesson_31806/lesson_31806_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そうね。\nあればいいわね。ミックスジュース。",
            "voice": "lesson/Lesson_31806/lesson_31806_015.mp3"
        }
    ]
}