{
    "Txt_Name": "Lesson_31605",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "だからさー、あたしその時言ってやったんだよね。",
            "voice": "lesson/Lesson_31605/lesson_31605_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "奴のチキンを捌いてやったら？\nって。",
            "voice": "lesson/Lesson_31605/lesson_31605_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ワーオ！　",
            "voice": "lesson/Lesson_31605/lesson_31605_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……。",
            "voice": "lesson/Lesson_31605/lesson_31605_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そしたら顔真っ赤にして、\n財布の小銭を数え始めちゃってさ。",
            "voice": "lesson/Lesson_31605/lesson_31605_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "それでそれで？",
            "voice": "lesson/Lesson_31605/lesson_31605_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "だから、あたしももう、ちゃんと言わなきゃダメだなって。",
            "voice": "lesson/Lesson_31605/lesson_31605_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "うんうん。",
            "voice": "lesson/Lesson_31605/lesson_31605_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そんなミルクじゃチキンは買えないだろ？　ってさ。",
            "voice": "lesson/Lesson_31605/lesson_31605_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "あはは。ユーキ！　\nナイスジョークね！",
            "voice": "lesson/Lesson_31605/lesson_31605_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……。\nねえ、あんたたち……。いったいなんの話をしてるの？",
            "voice": "lesson/Lesson_31605/lesson_31605_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "嘘でしょチサ、こんなに面白いのに！？",
            "voice": "lesson/Lesson_31605/lesson_31605_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "もう1回最初から話す？",
            "voice": "lesson/Lesson_31605/lesson_31605_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "いや、いいわ。\n洗濯物終わったみたいだし。",
            "voice": "lesson/Lesson_31605/lesson_31605_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……さっきの話、どういうことだったのかしら？",
            "voice": "lesson/Lesson_31605/lesson_31605_015.mp3"
        }
    ]
}