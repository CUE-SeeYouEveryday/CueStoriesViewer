{
    "Txt_Name": "Lesson_10308",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "美晴、なにしてるの？　",
            "voice": "lesson/Lesson_10308/lesson_10308_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "漢字にルビふってるの。\nぱっと見た時にわかるように。",
            "voice": "lesson/Lesson_10308/lesson_10308_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ああ、それやる声優さん、結構いるよね。",
            "voice": "lesson/Lesson_10308/lesson_10308_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたし達、姉妹役なんだね。",
            "voice": "lesson/Lesson_10308/lesson_10308_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "うん。そうみたい。",
            "voice": "lesson/Lesson_10308/lesson_10308_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "頑張ろうね！\nわたし、長女の役とかやったことないから緊張しちゃうな～。",
            "voice": "lesson/Lesson_10308/lesson_10308_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まほろは妹役、飽きるほどやってる。\n子役の頃から。",
            "voice": "lesson/Lesson_10308/lesson_10308_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうなんだ～。はまり役ってやつだね！\n小さい頃からたくさんお芝居してたんだもんね。",
            "voice": "lesson/Lesson_10308/lesson_10308_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "でも、姉役はやったことない。",
            "voice": "lesson/Lesson_10308/lesson_10308_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうなんだ。",
            "voice": "lesson/Lesson_10308/lesson_10308_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あ～あ……。\nやってみたいなぁ。",
            "voice": "lesson/Lesson_10308/lesson_10308_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あ、そう……？\nじゃあ……、やってみる？",
            "voice": "lesson/Lesson_10308/lesson_10308_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "えっ？\nなにを？　",
            "voice": "lesson/Lesson_10308/lesson_10308_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "役を交換してみる？",
            "voice": "lesson/Lesson_10308/lesson_10308_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そんなこと、勝手にやれないよ。",
            "voice": "lesson/Lesson_10308/lesson_10308_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうだけどさ、読み合わせくらいは……、いいんじゃない？",
            "voice": "lesson/Lesson_10308/lesson_10308_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "読み合わせ？\nここで？",
            "voice": "lesson/Lesson_10308/lesson_10308_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん！　まほろ、長女役を読んでみてよ。\nわたしが三女役を読むから。",
            "voice": "lesson/Lesson_10308/lesson_10308_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……あ、そう……。\nじゃあ、やるか……。",
            "voice": "lesson/Lesson_10308/lesson_10308_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ここのページからね。",
            "voice": "lesson/Lesson_10308/lesson_10308_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "うん。\nわかった。",
            "voice": "lesson/Lesson_10308/lesson_10308_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "『おねえちゃん！\nこれ……、なぁに？　食べられる草？』",
            "voice": "lesson/Lesson_10308/lesson_10308_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "『違うよ。これは食べられない草。\n食べられるのは向こうのよ。』",
            "voice": "lesson/Lesson_10308/lesson_10308_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "『はぁーい。\nそうしたらぁ……。』",
            "voice": "lesson/Lesson_10308/lesson_10308_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちょっと待って。",
            "voice": "lesson/Lesson_10308/lesson_10308_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "えっ？\nどうしたの？　",
            "voice": "lesson/Lesson_10308/lesson_10308_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "その……。妹役なんだけど……。\n今の読み方だと……、ちょっと違うんだよね。",
            "voice": "lesson/Lesson_10308/lesson_10308_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そ、そう？",
            "voice": "lesson/Lesson_10308/lesson_10308_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "幼さの中にもね、賢さがなきゃダメなのよ。\nどちらかというと今「おばかちゃん」だったからね。",
            "voice": "lesson/Lesson_10308/lesson_10308_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "幼いけど、賢い……。\n確かに、口調に惑わされてたかも……。",
            "voice": "lesson/Lesson_10308/lesson_10308_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そうね。わかる。ありがちなのよ。\n幼さ出そうとして知性まで失っちゃうパターン。",
            "voice": "lesson/Lesson_10308/lesson_10308_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうか……。",
            "voice": "lesson/Lesson_10308/lesson_10308_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "この役はどんな役だと思う？",
            "voice": "lesson/Lesson_10308/lesson_10308_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "この役って……。\n三女役？",
            "voice": "lesson/Lesson_10308/lesson_10308_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そう。\n演じるからには、役の本質を理解しないとじゃない？",
            "voice": "lesson/Lesson_10308/lesson_10308_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "まほろ、こだわりが強いんだね。",
            "voice": "lesson/Lesson_10308/lesson_10308_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "この役はね、過酷な世界でも、\n周囲を笑顔にさせようとする健気さのある役だと思う。",
            "voice": "lesson/Lesson_10308/lesson_10308_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なるほど……。\nうん、確かにそうね。",
            "voice": "lesson/Lesson_10308/lesson_10308_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そうでしょ？\nそれを想って読めば良いんだよ。",
            "voice": "lesson/Lesson_10308/lesson_10308_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "すごい勉強になる……。",
            "voice": "lesson/Lesson_10308/lesson_10308_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "じゃ、もう1回読んでみて。",
            "voice": "lesson/Lesson_10308/lesson_10308_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "えっ……、わたしが？　",
            "voice": "lesson/Lesson_10308/lesson_10308_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "それと……。\nここのセリフも、もう1回、読んでみてくれない？",
            "voice": "lesson/Lesson_10308/lesson_10308_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん……。\nじゃあ、いくよ。",
            "voice": "lesson/Lesson_10308/lesson_10308_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いつでもどうぞ。",
            "voice": "lesson/Lesson_10308/lesson_10308_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "『はぁーい！　そうしたらぁ……。あると、向こうの草取って来る！\nお姉ちゃん、待ってて！』",
            "voice": "lesson/Lesson_10308/lesson_10308_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "すごい……。\n完璧。",
            "voice": "lesson/Lesson_10308/lesson_10308_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ありがとう。",
            "voice": "lesson/Lesson_10308/lesson_10308_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "セリフだけなのに動きと気持ちの流れが見えてきた。\n姉を慕う妹の気持ちがすごく理解できる……。",
            "voice": "lesson/Lesson_10308/lesson_10308_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "……すごい。",
            "voice": "lesson/Lesson_10308/lesson_10308_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "えっ？\nなにが？",
            "voice": "lesson/Lesson_10308/lesson_10308_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "まほろすごいよ！\n妹役へのこだわり。尊敬しちゃう！",
            "voice": "lesson/Lesson_10308/lesson_10308_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そう？　普通だとおもうけど。\nはい、じゃあ、もう1回やってみよう。",
            "voice": "lesson/Lesson_10308/lesson_10308_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "えっ？\nまだやるの？",
            "voice": "lesson/Lesson_10308/lesson_10308_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "当然でしょ？\n妹役は1日にしてならず！",
            "voice": "lesson/Lesson_10308/lesson_10308_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん、わかった。",
            "voice": "lesson/Lesson_10308/lesson_10308_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "ずっとやってきたからこそ、わかることってあるのよね。",
            "voice": "lesson/Lesson_10308/lesson_10308_057.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "桐香先生……？",
            "voice": "lesson/Lesson_10308/lesson_10308_058.mp3"
        }
    ]
}