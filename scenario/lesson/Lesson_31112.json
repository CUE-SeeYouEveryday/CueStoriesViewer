{
    "Txt_Name": "Lesson_31112",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ま、あたしは当然のことをしただけなんだよな。",
            "voice": "lesson/Lesson_31112/lesson_31112_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "船長だから？　",
            "voice": "lesson/Lesson_31112/lesson_31112_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そういうこと。　",
            "voice": "lesson/Lesson_31112/lesson_31112_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ふふっ。とっても頼りになるわ。さすがユーキね。　",
            "voice": "lesson/Lesson_31112/lesson_31112_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "でも今回は千紗もあいりも、みんながいたからさ。",
            "voice": "lesson/Lesson_31112/lesson_31112_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "悠希ちゃん。　",
            "voice": "lesson/Lesson_31112/lesson_31112_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ほんと、あんたたちには引っ掻き回されっぱなしよ。",
            "voice": "lesson/Lesson_31112/lesson_31112_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ふふ。千紗、メルシー。　",
            "voice": "lesson/Lesson_31112/lesson_31112_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "はいはい。\nでも、ま、あんなのもたまには悪くないのかもね。",
            "voice": "lesson/Lesson_31112/lesson_31112_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "でしょ？　",
            "voice": "lesson/Lesson_31112/lesson_31112_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "しばらくは遠慮したいけど。\nさ、そんなことより、柚葉の探し物、見つけに行くわよ。",
            "voice": "lesson/Lesson_31112/lesson_31112_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "うん。カネコのバイク、お願いね！　",
            "voice": "lesson/Lesson_31112/lesson_31112_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "カネコのフィギュアってあれでしょ？\nあの赤いやつでしょ？",
            "voice": "lesson/Lesson_31112/lesson_31112_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "そう。赤いやつ。\n成田山のステッカーがポイントなの。",
            "voice": "lesson/Lesson_31112/lesson_31112_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "おっけー！　",
            "voice": "lesson/Lesson_31112/lesson_31112_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ふふ。さあ、みんなで神瑞町をくまなく探すわよ！",
            "voice": "lesson/Lesson_31112/lesson_31112_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                8,
                5,
                7
            ],
            "name": "3人",
            "message": "おおー！　",
            "voice": "lesson/Lesson_31112/lesson_31112_017.mp3"
        }
    ]
}