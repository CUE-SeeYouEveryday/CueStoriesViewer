{
    "Txt_Name": "Lesson_31303",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ただいまー。",
            "voice": "lesson/Lesson_31303/lesson_31303_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "おかえりなさい。\nもうすぐ晩御飯できるからお楽しみに。",
            "voice": "lesson/Lesson_31303/lesson_31303_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あ、今日美晴が晩御飯作ってくれる日だっけ？",
            "voice": "lesson/Lesson_31303/lesson_31303_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうよ。",
            "voice": "lesson/Lesson_31303/lesson_31303_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そりゃ、楽しみだね。",
            "voice": "lesson/Lesson_31303/lesson_31303_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "莉子はまだかしら？",
            "voice": "lesson/Lesson_31303/lesson_31303_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あれ？　聞いてなかった？",
            "voice": "lesson/Lesson_31303/lesson_31303_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "莉子、今日は明後日の公開収録の件で、\nマネージャーと打ち合わせしてくるって。",
            "voice": "lesson/Lesson_31303/lesson_31303_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちょっと遅くなるかもとか言ってたけど。",
            "voice": "lesson/Lesson_31303/lesson_31303_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら、残念。\n今日は4人で晩御飯の時間が過ごせると思ったのに。",
            "voice": "lesson/Lesson_31303/lesson_31303_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ま、しょうがないよ。\nある意味、莉子の晴れ舞台だしね。",
            "voice": "lesson/Lesson_31303/lesson_31303_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。そうね。\n夏のお台場でのイベントで、ラジオの公開収録だものね。",
            "voice": "lesson/Lesson_31303/lesson_31303_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そうそう。\n莉子じゃなくても気合入るよ。この仕事してたらさ。",
            "voice": "lesson/Lesson_31303/lesson_31303_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "お台場……、か。\nあ、そうだ！　",
            "voice": "lesson/Lesson_31303/lesson_31303_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "どうしたの美晴？",
            "voice": "lesson/Lesson_31303/lesson_31303_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "絢、さっきの話。決まったかも。",
            "voice": "lesson/Lesson_31303/lesson_31303_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうなの？",
            "voice": "lesson/Lesson_31303/lesson_31303_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "なになに？　何を決めたって？",
            "voice": "lesson/Lesson_31303/lesson_31303_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。後で莉子が帰ってきたら……、ね。",
            "voice": "lesson/Lesson_31303/lesson_31303_019.mp3"
        }
    ]
}