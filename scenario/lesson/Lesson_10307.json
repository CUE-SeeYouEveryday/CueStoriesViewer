{
    "Txt_Name": "Lesson_10307",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "おはようございます。",
            "voice": "lesson/Lesson_10307/lesson_10307_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "あっ、まほろちゃん！\n良い所に来た！",
            "voice": "lesson/Lesson_10307/lesson_10307_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "どうしたんですか？",
            "voice": "lesson/Lesson_10307/lesson_10307_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "……来たよ！",
            "voice": "lesson/Lesson_10307/lesson_10307_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……？\n何がですか？",
            "voice": "lesson/Lesson_10307/lesson_10307_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "台本！\nアニメの！",
            "voice": "lesson/Lesson_10307/lesson_10307_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ああ……。\nはい。",
            "voice": "lesson/Lesson_10307/lesson_10307_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "ちょっとなに？\nもっとアゲアゲでいこーよ。",
            "voice": "lesson/Lesson_10307/lesson_10307_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ははは……。",
            "voice": "lesson/Lesson_10307/lesson_10307_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "（なんでこの人、いつもテンション高いんだろ？）",
            "voice": "lesson/Lesson_10307/lesson_10307_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "じゃじゃーん！　新作アニメ「こんにちは、いただきます。」。\n……配役気にならなぁい？",
            "voice": "lesson/Lesson_10307/lesson_10307_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あ……、なります。",
            "voice": "lesson/Lesson_10307/lesson_10307_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "でっしょ～？　あなただけ先に、特別に教えちゃう！\n食をテーマにした、三姉妹の物語なんだけどぉ……。",
            "voice": "lesson/Lesson_10307/lesson_10307_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "三姉妹？　",
            "voice": "lesson/Lesson_10307/lesson_10307_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "そ、ちょっと待ってね、今キャスト表みるから……。",
            "voice": "lesson/Lesson_10307/lesson_10307_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "三姉妹ってことは……。",
            "voice": "lesson/Lesson_10307/lesson_10307_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "お待たせっ。\nえっと長女役は美晴ちゃんでしょ。",
            "voice": "lesson/Lesson_10307/lesson_10307_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "（美晴が長女……。\nうん。）",
            "voice": "lesson/Lesson_10307/lesson_10307_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "次女役が、絢ちゃん。",
            "voice": "lesson/Lesson_10307/lesson_10307_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "（なるほどね……。\nってことは、三女役が……。）",
            "voice": "lesson/Lesson_10307/lesson_10307_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "まほろちゃん、三女役、決定！",
            "voice": "lesson/Lesson_10307/lesson_10307_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "（やっぱりか……。）",
            "voice": "lesson/Lesson_10307/lesson_10307_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "妹役。ぴったりな役だよ！\n良かったね！",
            "voice": "lesson/Lesson_10307/lesson_10307_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はぁ……。",
            "voice": "lesson/Lesson_10307/lesson_10307_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "（また妹役か……。）",
            "voice": "lesson/Lesson_10307/lesson_10307_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "因みに、莉子ちゃんは、管理員役。",
            "voice": "lesson/Lesson_10307/lesson_10307_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "へぇ。",
            "voice": "lesson/Lesson_10307/lesson_10307_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あの、台本ってもうもらえるんですか？",
            "voice": "lesson/Lesson_10307/lesson_10307_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "もちろん！\nはい、これだよ。",
            "voice": "lesson/Lesson_10307/lesson_10307_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ありがとうございます。",
            "voice": "lesson/Lesson_10307/lesson_10307_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "（はいはい……。\nこのパターンの妹役ね……。）",
            "voice": "lesson/Lesson_10307/lesson_10307_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "まほろちゃん……。感動のあまり表情が固くなってるぞ！\n良かったね。頑張れ！",
            "voice": "lesson/Lesson_10307/lesson_10307_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……あ、はい。\n頑張ります……。",
            "voice": "lesson/Lesson_10307/lesson_10307_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "（はぁ……。\nなんでいっつも妹役なんだろ……。）",
            "voice": "lesson/Lesson_10307/lesson_10307_034.mp3"
        }
    ]
}