{
    "Txt_Name": "Lesson_10310",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……。",
            "voice": "lesson/Lesson_10310/lesson_10310_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……う～ん。",
            "voice": "lesson/Lesson_10310/lesson_10310_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "（……真咲さん、遅いな。\n何してんだろ？）",
            "voice": "lesson/Lesson_10310/lesson_10310_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "（早く来ないかな。\n台本貰いたいのになぁ。）",
            "voice": "lesson/Lesson_10310/lesson_10310_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "んっ？\n……なんだこれ？",
            "voice": "lesson/Lesson_10310/lesson_10310_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "壺か。真咲さん、こんな壺飾ってたんだ。\n……変な形の取っ手。",
            "voice": "lesson/Lesson_10310/lesson_10310_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "こんな取っ手じゃ、運ぶ時大変だね。\nいや、この握り具合だったらそうでもないかな？",
            "voice": "lesson/Lesson_10310/lesson_10310_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "んっ？\n……。",
            "voice": "lesson/Lesson_10310/lesson_10310_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "え……。",
            "voice": "lesson/Lesson_10310/lesson_10310_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "取っ手、壺の取っ手……。\n折れた……。",
            "voice": "lesson/Lesson_10310/lesson_10310_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "う……。う、嘘でしょ？\n……折っちゃったの？　あたし、真咲さんの壺壊しちゃったの……？",
            "voice": "lesson/Lesson_10310/lesson_10310_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "（ヤバイヤバイヤバイ！！\nどうしよう！！）",
            "voice": "lesson/Lesson_10310/lesson_10310_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "（誰かに見られたら、\nあたしクビになっちゃう……！！）",
            "voice": "lesson/Lesson_10310/lesson_10310_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "（と、とりあえず接着剤でくっ付けてみよう！）",
            "voice": "lesson/Lesson_10310/lesson_10310_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "（どっかに接着剤とか無いかな……。）",
            "voice": "lesson/Lesson_10310/lesson_10310_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "あれ？\n莉子ちゃん、どうしたの？",
            "voice": "lesson/Lesson_10310/lesson_10310_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……っ、り、りおさん……。 ",
            "voice": "lesson/Lesson_10310/lesson_10310_017.mp3"
        }
    ]
}