{
    "Txt_Name": "Lesson_20408",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "これは……。",
            "voice": "lesson/Lesson_20408/lesson_20408_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ど、どうですか……？　\n折れてますか……？",
            "voice": "lesson/Lesson_20408/lesson_20408_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "靴擦れだね。",
            "voice": "lesson/Lesson_20408/lesson_20408_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "……靴擦れ？",
            "voice": "lesson/Lesson_20408/lesson_20408_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "これは痛そう。",
            "voice": "lesson/Lesson_20408/lesson_20408_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "……この靴、最近買ったばっかりだ。",
            "voice": "lesson/Lesson_20408/lesson_20408_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "靴擦れ、あまりならないですから……。",
            "voice": "lesson/Lesson_20408/lesson_20408_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "美晴さん、アレもってる？",
            "voice": "lesson/Lesson_20408/lesson_20408_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "アレ……？",
            "voice": "lesson/Lesson_20408/lesson_20408_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あるよ。でも、小さいやつしかないかも。",
            "voice": "lesson/Lesson_20408/lesson_20408_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "わたし大きいの持ってる。",
            "voice": "lesson/Lesson_20408/lesson_20408_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "小さい……。大きい……？",
            "voice": "lesson/Lesson_20408/lesson_20408_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "靴擦れの応急処置。絆創膏だよ。ほら。",
            "voice": "lesson/Lesson_20408/lesson_20408_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "うわあ……、すごい、こんなにいっぱい。\n大きさもいろいろ……。",
            "voice": "lesson/Lesson_20408/lesson_20408_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "必需品だよね。いつも持ち歩いてるよ。\n靴擦れしやすいから。",
            "voice": "lesson/Lesson_20408/lesson_20408_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "かわいくて、ヒールの高い靴にかぎって、すぐ痛くなるよね。",
            "voice": "lesson/Lesson_20408/lesson_20408_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "なんて言うか、もう足を靴に合わせてるって感じ。",
            "voice": "lesson/Lesson_20408/lesson_20408_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "……。",
            "voice": "lesson/Lesson_20408/lesson_20408_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ほのかさんも美晴さんも、\nすごくおしゃれな靴履いてます。",
            "voice": "lesson/Lesson_20408/lesson_20408_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "え？",
            "voice": "lesson/Lesson_20408/lesson_20408_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ありがとう？",
            "voice": "lesson/Lesson_20408/lesson_20408_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "すごく……。かかと、高い。",
            "voice": "lesson/Lesson_20408/lesson_20408_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "って言っても、6センチよ？",
            "voice": "lesson/Lesson_20408/lesson_20408_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そこまで高くないよね？",
            "voice": "lesson/Lesson_20408/lesson_20408_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "なんで……。",
            "voice": "lesson/Lesson_20408/lesson_20408_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あいり？",
            "voice": "lesson/Lesson_20408/lesson_20408_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "なんで、そこまでして……？",
            "voice": "lesson/Lesson_20408/lesson_20408_027.mp3"
        }
    ]
}