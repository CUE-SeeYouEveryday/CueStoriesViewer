{
    "Txt_Name": "Lesson_30812",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "心配することないよ。\n日記を書いてるだけだから。",
            "voice": "lesson/Lesson_30812/lesson_30812_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "へぇ、日記。",
            "voice": "lesson/Lesson_30812/lesson_30812_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "なるほどなるほど。\n絢が日記ね。",
            "voice": "lesson/Lesson_30812/lesson_30812_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "何？",
            "voice": "lesson/Lesson_30812/lesson_30812_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "へぇ。なるほどなるほど。",
            "voice": "lesson/Lesson_30812/lesson_30812_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ちょっと、莉子、何さっきからずっと、なるほどなるほどって。",
            "voice": "lesson/Lesson_30812/lesson_30812_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "いやー、これはなるほどでしょ。",
            "voice": "lesson/Lesson_30812/lesson_30812_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "どういうこと？",
            "voice": "lesson/Lesson_30812/lesson_30812_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうだね。普通に読んでみたいってことかな。",
            "voice": "lesson/Lesson_30812/lesson_30812_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "読んでみたい？",
            "voice": "lesson/Lesson_30812/lesson_30812_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そう。\n絢が書く日記、読んでみたいよ。あたしは。",
            "voice": "lesson/Lesson_30812/lesson_30812_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "読まれるのは恥ずかしい？",
            "voice": "lesson/Lesson_30812/lesson_30812_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。ちょっと、恥ずかしいかも。",
            "voice": "lesson/Lesson_30812/lesson_30812_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "日記書いてること、美晴にも言ってないの？",
            "voice": "lesson/Lesson_30812/lesson_30812_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。",
            "voice": "lesson/Lesson_30812/lesson_30812_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そっか。美晴も絢の日記、読みたいと思うけどな。",
            "voice": "lesson/Lesson_30812/lesson_30812_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうかな。",
            "voice": "lesson/Lesson_30812/lesson_30812_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "教えてあげたら？　きっと喜ぶよ。",
            "voice": "lesson/Lesson_30812/lesson_30812_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ううん、やっぱり、恥ずかしいからやめとく。",
            "voice": "lesson/Lesson_30812/lesson_30812_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうかー。残念。\n美晴に教えたら、私にも教えてくれると思ったのに。",
            "voice": "lesson/Lesson_30812/lesson_30812_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "いつか、見せてもいいかなって思えるようになったらね。",
            "voice": "lesson/Lesson_30812/lesson_30812_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そっかそっか。",
            "voice": "lesson/Lesson_30812/lesson_30812_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "でも、絢の日記、どんなこと書いてるのかなー。\n気になっちゃうなー。",
            "voice": "lesson/Lesson_30812/lesson_30812_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "もう。絶対莉子には見せないから。",
            "voice": "lesson/Lesson_30812/lesson_30812_024.mp3"
        }
    ]
}