{
    "Txt_Name": "Lesson_31509",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "おーい、まほろ、美晴。\n何のんびり歩いてんのー？",
            "voice": "lesson/Lesson_31509/lesson_31509_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はーい。急ぎまーす。",
            "voice": "lesson/Lesson_31509/lesson_31509_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ま、細かいことは聞かないけど。\nつまり美晴はその夢で、さっきの……。",
            "voice": "lesson/Lesson_31509/lesson_31509_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あの歌の歌詞をどう訳すか掴んだってこと、でしょ？",
            "voice": "lesson/Lesson_31509/lesson_31509_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ふふ。そうね。\nまほろの言う通り。",
            "voice": "lesson/Lesson_31509/lesson_31509_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "じゃあ、まあ、そういうことでいいんじゃない？",
            "voice": "lesson/Lesson_31509/lesson_31509_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "夢だろうが白昼夢だろうが、幻想だろうがさ。\nそれに……、赤い風船を見たんだとしても。",
            "voice": "lesson/Lesson_31509/lesson_31509_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ほら、早く。\n次の電車来ちゃうよ。",
            "voice": "lesson/Lesson_31509/lesson_31509_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はーい。今行きまーす。",
            "voice": "lesson/Lesson_31509/lesson_31509_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "電車なんか次のでいいでしょ。",
            "voice": "lesson/Lesson_31509/lesson_31509_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ふふ。さ、まほろ、行きましょ。",
            "voice": "lesson/Lesson_31509/lesson_31509_011.mp3"
        }
    ]
}