{
    "Txt_Name": "Lesson_31305",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ごめんね。\n出る時、ちょっとバタバタしちゃった。",
            "voice": "lesson/Lesson_31305/lesson_31305_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "着替えてただけで、\n今日の持ち物、何も準備してなかったから……。",
            "voice": "lesson/Lesson_31305/lesson_31305_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そんなにあわてて準備しなくても良かったのに。",
            "voice": "lesson/Lesson_31305/lesson_31305_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ううん。待たせるのも悪いし。",
            "voice": "lesson/Lesson_31305/lesson_31305_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうなの？\nふふ。ありがとう。絢。",
            "voice": "lesson/Lesson_31305/lesson_31305_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ううん。",
            "voice": "lesson/Lesson_31305/lesson_31305_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あ、絢、そのバッグどうしたの？\nかわいい。",
            "voice": "lesson/Lesson_31305/lesson_31305_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、これ？\nうん。最近買ったんだ。",
            "voice": "lesson/Lesson_31305/lesson_31305_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "実は今日、初めて使うの。",
            "voice": "lesson/Lesson_31305/lesson_31305_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "へえ、そうなの。",
            "voice": "lesson/Lesson_31305/lesson_31305_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふ。美晴に褒められると嬉しいな。",
            "voice": "lesson/Lesson_31305/lesson_31305_013.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\nわたしはかわいいものにかわいいって言ってるだけよ。",
            "voice": "lesson/Lesson_31305/lesson_31305_014.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ううん。ありがとう。",
            "voice": "lesson/Lesson_31305/lesson_31305_015.mp3"
        }
    ]
}