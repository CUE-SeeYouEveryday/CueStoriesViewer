{
    "Txt_Name": "Lesson_31507",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ねえ、まほろ。",
            "voice": "lesson/Lesson_31507/lesson_31507_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "何？",
            "voice": "lesson/Lesson_31507/lesson_31507_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "やっぱりまほろも夢、見てたの？",
            "voice": "lesson/Lesson_31507/lesson_31507_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ああ、さっきのこと？",
            "voice": "lesson/Lesson_31507/lesson_31507_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "それは気のせいだって。\nふとそんな気がしただけ。なんでかはわかんないけど。",
            "voice": "lesson/Lesson_31507/lesson_31507_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そう。",
            "voice": "lesson/Lesson_31507/lesson_31507_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ん？　何か気になるわけ？",
            "voice": "lesson/Lesson_31507/lesson_31507_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\nそうね。少し。",
            "voice": "lesson/Lesson_31507/lesson_31507_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あー、もしかしてあたってたとか？",
            "voice": "lesson/Lesson_31507/lesson_31507_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ふふ。",
            "voice": "lesson/Lesson_31507/lesson_31507_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あれ？　さっき、まほろなんて言ったっけ。\nほんとに、ふと出た言葉で。",
            "voice": "lesson/Lesson_31507/lesson_31507_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうなの？",
            "voice": "lesson/Lesson_31507/lesson_31507_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "うーん。\n思い出せないな。",
            "voice": "lesson/Lesson_31507/lesson_31507_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "だからやっぱりさっきのは気のせいだし、\n美晴が気にすることもないんじゃない？",
            "voice": "lesson/Lesson_31507/lesson_31507_014.mp3"
        }
    ]
}