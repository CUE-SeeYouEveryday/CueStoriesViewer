{
    "Txt_Name": "Lesson_31504",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ねえ、美晴。\n聞いてもいい？",
            "voice": "lesson/Lesson_31504/lesson_31504_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら、なあに？",
            "voice": "lesson/Lesson_31504/lesson_31504_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "今日、レッスン室で聞かせてもらった歌、\n歌詞の意味をずっと考えてたって言ってたでしょ？",
            "voice": "lesson/Lesson_31504/lesson_31504_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\nそうね。",
            "voice": "lesson/Lesson_31504/lesson_31504_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "でも結局、美晴、歌う直前……、わかったって。",
            "voice": "lesson/Lesson_31504/lesson_31504_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん。",
            "voice": "lesson/Lesson_31504/lesson_31504_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "何に悩んでて、何をわかったの？\n少し気になったんだけど。",
            "voice": "lesson/Lesson_31504/lesson_31504_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "絢はやっぱり言葉が好きなのね。",
            "voice": "lesson/Lesson_31504/lesson_31504_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうなのかな？",
            "voice": "lesson/Lesson_31504/lesson_31504_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あの歌……、\n英語の歌詞もそんなに難しいものじゃないの。",
            "voice": "lesson/Lesson_31504/lesson_31504_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。\n聞いてて、私もなんとなく聞き取れたから。",
            "voice": "lesson/Lesson_31504/lesson_31504_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ふふ。さすが絢ね。",
            "voice": "lesson/Lesson_31504/lesson_31504_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "もう、からかわないでよ。",
            "voice": "lesson/Lesson_31504/lesson_31504_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら、何もからかってなんかいないわよ？",
            "voice": "lesson/Lesson_31504/lesson_31504_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "もう、美晴……。",
            "voice": "lesson/Lesson_31504/lesson_31504_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。",
            "voice": "lesson/Lesson_31504/lesson_31504_016.mp3"
        }
    ]
}