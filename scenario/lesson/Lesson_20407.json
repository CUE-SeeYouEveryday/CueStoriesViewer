{
    "Txt_Name": "Lesson_20407",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "うーん……。",
            "voice": "lesson/Lesson_20407/lesson_20407_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あの靴、やっぱり可愛かったな……。",
            "voice": "lesson/Lesson_20407/lesson_20407_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ヒールの内側だけ赤いやつ？",
            "voice": "lesson/Lesson_20407/lesson_20407_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そう！　ワンポイント、良かった。",
            "voice": "lesson/Lesson_20407/lesson_20407_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ちょっと見えるのが、いいんだよね。",
            "voice": "lesson/Lesson_20407/lesson_20407_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "普段は見えないけど、実は……っていうのが。",
            "voice": "lesson/Lesson_20407/lesson_20407_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うんうん。",
            "voice": "lesson/Lesson_20407/lesson_20407_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "やっぱり、買えばよかったかも。",
            "voice": "lesson/Lesson_20407/lesson_20407_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "靴は迷うよね。長く使いたいし。\nでも、ほのかがヒール好きなのって、ちょっと意外かも。",
            "voice": "lesson/Lesson_20407/lesson_20407_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "よく言われるけど、わたしも人並みにおしゃれは好きだよ。\nやっぱり可愛くありたいってのもあるし。",
            "voice": "lesson/Lesson_20407/lesson_20407_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ふふっ、そうだよね。\n失礼なこと言っちゃったかしら？",
            "voice": "lesson/Lesson_20407/lesson_20407_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "別に気にしてないよ。\nん？　……、あれ？　あの座りこんでるのは……。",
            "voice": "lesson/Lesson_20407/lesson_20407_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "……。",
            "voice": "lesson/Lesson_20407/lesson_20407_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あいり？ ",
            "voice": "lesson/Lesson_20407/lesson_20407_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あれ……。2人とも、お買い物？",
            "voice": "lesson/Lesson_20407/lesson_20407_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "もう帰りだけどね。あいりは？",
            "voice": "lesson/Lesson_20407/lesson_20407_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "……。",
            "voice": "lesson/Lesson_20407/lesson_20407_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "お家に……。帰りたいよ……！",
            "voice": "lesson/Lesson_20407/lesson_20407_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "えっ？　ど、どうしたの？",
            "voice": "lesson/Lesson_20407/lesson_20407_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "な、なんか泣きそうなんだけど！",
            "voice": "lesson/Lesson_20407/lesson_20407_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ううっ……。ショーを見に行ってきたんです……。",
            "voice": "lesson/Lesson_20407/lesson_20407_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うんうん。",
            "voice": "lesson/Lesson_20407/lesson_20407_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ずっと見てたら、足が痛くなってきちゃって。",
            "voice": "lesson/Lesson_20407/lesson_20407_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "帰りたいのに、もう歩けなくて……。",
            "voice": "lesson/Lesson_20407/lesson_20407_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "歩けないぐらい痛いの？",
            "voice": "lesson/Lesson_20407/lesson_20407_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "それって、ヤバくない？　折れてない！？",
            "voice": "lesson/Lesson_20407/lesson_20407_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えっ！？　お、折れてる……。",
            "voice": "lesson/Lesson_20407/lesson_20407_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ちょっと見せて！",
            "voice": "lesson/Lesson_20407/lesson_20407_028.mp3"
        }
    ]
}