{
    "Txt_Name": "Lesson_31106",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "うーん、やっぱり無いわね……。",
            "voice": "lesson/Lesson_31106/lesson_31106_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "フィギュアって、発売されてすぐに売り切れるって言うし、\nずっと在庫が置いてあるものじゃないのかもね。",
            "voice": "lesson/Lesson_31106/lesson_31106_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "そう。残念。\nでも、街中のお店を探したらどこかにあるんじゃない？",
            "voice": "lesson/Lesson_31106/lesson_31106_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "そうね。\n足で探せば……。",
            "voice": "lesson/Lesson_31106/lesson_31106_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "って、それこそ柚葉なら、\nお金で解決出来るんじゃない？",
            "voice": "lesson/Lesson_31106/lesson_31106_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ううん、チサ。\nそれじゃ楽しくないもの。",
            "voice": "lesson/Lesson_31106/lesson_31106_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "手元に欲しい気持ちだけなら、どれだけ高い値段がついてても、\nネットで買えばいいわ。",
            "voice": "lesson/Lesson_31106/lesson_31106_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "でも、そうじゃなくて、私、みんなと一緒に買いたいの。\nその時間が楽しいの。",
            "voice": "lesson/Lesson_31106/lesson_31106_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "わかる？",
            "voice": "lesson/Lesson_31106/lesson_31106_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "まあ、わかる、かな。",
            "voice": "lesson/Lesson_31106/lesson_31106_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ふふ。でしょ？\nだって、チサ、何も買わないのにここにいるんだもん。",
            "voice": "lesson/Lesson_31106/lesson_31106_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "わかってくれるって信じてたわ。",
            "voice": "lesson/Lesson_31106/lesson_31106_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "もう、何よそれ。",
            "voice": "lesson/Lesson_31106/lesson_31106_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "おーい！　柚葉！　千紗！\nそっちの買い物は終わった？",
            "voice": "lesson/Lesson_31106/lesson_31106_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "悠希ちゃん、待ってよ―。",
            "voice": "lesson/Lesson_31106/lesson_31106_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ほーら、お店の中走っちゃダメって言ったでしょ？\nもう、まったく。",
            "voice": "lesson/Lesson_31106/lesson_31106_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ふふっ。チサ、楽しそうよ？",
            "voice": "lesson/Lesson_31106/lesson_31106_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ふふ。まあ、ね。",
            "voice": "lesson/Lesson_31106/lesson_31106_018.mp3"
        }
    ]
}