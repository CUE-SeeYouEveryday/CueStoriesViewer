{
    "Txt_Name": "Lesson_30401",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "今度のクリスマス、\n真咲さんに何かプレゼントしない？",
            "voice": "lesson/Lesson_30401/lesson_30401_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ボスのプレゼントか……。新社屋とか？",
            "voice": "lesson/Lesson_30401/lesson_30401_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "現実味のあるもので考えなさいよ。",
            "voice": "lesson/Lesson_30401/lesson_30401_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "大人の女性へのプレゼントって、悩むよね。",
            "voice": "lesson/Lesson_30401/lesson_30401_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あたしはもう考えてある。",
            "voice": "lesson/Lesson_30401/lesson_30401_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "何あげるの？",
            "voice": "lesson/Lesson_30401/lesson_30401_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ふっふっふ。\n実家の食堂、フルメニュー食べ放題！",
            "voice": "lesson/Lesson_30401/lesson_30401_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……なるほどね。",
            "voice": "lesson/Lesson_30401/lesson_30401_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "時間無制限。「参った」と言うまで食べ続けられるよ。",
            "voice": "lesson/Lesson_30401/lesson_30401_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "真咲さんがそこまで食べるわけないでしょ……。",
            "voice": "lesson/Lesson_30401/lesson_30401_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "悠希ちゃんのプレゼントも素敵だけどさ、\nみんなでプレゼントできるものないかな？",
            "voice": "lesson/Lesson_30401/lesson_30401_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "やっぱり、あれじゃない？　新車をあげるってどう？",
            "voice": "lesson/Lesson_30401/lesson_30401_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "もうちょっと、身の丈にあったものがいいかと……。",
            "voice": "lesson/Lesson_30401/lesson_30401_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "わかった！　\nじゃあお店じゃなくて、あたしらの作った料理を食べ放題！",
            "voice": "lesson/Lesson_30401/lesson_30401_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "真咲さん、そんなにたくさん食べるかな……。",
            "voice": "lesson/Lesson_30401/lesson_30401_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "だったら、ホテルのお食事券あるよ。\nみんなで割り勘にしてプレゼントするとかは？",
            "voice": "lesson/Lesson_30401/lesson_30401_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ああ。それいいかも。\nちなみにいくら？",
            "voice": "lesson/Lesson_30401/lesson_30401_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "１００万。",
            "voice": "lesson/Lesson_30401/lesson_30401_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "却下。何が出てくるのよその料理。",
            "voice": "lesson/Lesson_30401/lesson_30401_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ドバイにあるホテルなのよ。もろもろ費用がかかるでしょ？",
            "voice": "lesson/Lesson_30401/lesson_30401_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "とにかく額がえぐすぎるわよ。",
            "voice": "lesson/Lesson_30401/lesson_30401_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "じゃあ、いくらだったらいいの？",
            "voice": "lesson/Lesson_30401/lesson_30401_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいり……、お金じゃないと思うな。",
            "voice": "lesson/Lesson_30401/lesson_30401_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "そうそう、そうなのよ。例えば……、例えばだけど、\n肩たたき券みたいな？　なんかそういう、ね。",
            "voice": "lesson/Lesson_30401/lesson_30401_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "肩たたき券？",
            "voice": "lesson/Lesson_30401/lesson_30401_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "みたいなさ、なんかそういう、心のこもったっていう意味よ。",
            "voice": "lesson/Lesson_30401/lesson_30401_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "笑わす、とかってこと？",
            "voice": "lesson/Lesson_30401/lesson_30401_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ん？　笑わす？　……ってことかな？　 ",
            "voice": "lesson/Lesson_30401/lesson_30401_028.mp3"
        }
    ]
}