{
    "Txt_Name": "Lesson_31103",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "はいこれ。",
            "voice": "lesson/Lesson_31103/lesson_31103_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "うわ。大きい箱……。\n何？　なんて書いてあるかよくわかんないよ。",
            "voice": "lesson/Lesson_31103/lesson_31103_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ダイバリオン・オルタ、ナハト・ムジーク・コーティング仕様、\nカトウ・キジハモデルの1/100マイスターグレードだよ。",
            "voice": "lesson/Lesson_31103/lesson_31103_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "え？　えーっと？",
            "voice": "lesson/Lesson_31103/lesson_31103_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "とにかくおすすめ！",
            "voice": "lesson/Lesson_31103/lesson_31103_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "でも、あいりに作れるかなあ？",
            "voice": "lesson/Lesson_31103/lesson_31103_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "大丈夫大丈夫。\nあたしも手伝うから。",
            "voice": "lesson/Lesson_31103/lesson_31103_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ほんと？",
            "voice": "lesson/Lesson_31103/lesson_31103_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ほんとほんと。\n大船に乗ったつもりで任せてくれればいいから。",
            "voice": "lesson/Lesson_31103/lesson_31103_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "その代わりさ、あの、ちょっとお願いがあるんだけど。",
            "voice": "lesson/Lesson_31103/lesson_31103_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "何？",
            "voice": "lesson/Lesson_31103/lesson_31103_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "時々、あいりの部屋に遊びに行ってもいい？",
            "voice": "lesson/Lesson_31103/lesson_31103_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "え？",
            "voice": "lesson/Lesson_31103/lesson_31103_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "実はこれ、すごく貴重なブツでさ。\nいい仕事してるんだよ。",
            "voice": "lesson/Lesson_31103/lesson_31103_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "だから、時々眺めたり触ったり遊んだりしたいんだけど……。",
            "voice": "lesson/Lesson_31103/lesson_31103_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ふふっ、何それ。\nでも、うん、いいよ。",
            "voice": "lesson/Lesson_31103/lesson_31103_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "その代わり、悠希ちゃん、\nちゃんとプチキュア、お部屋に飾ってね。",
            "voice": "lesson/Lesson_31103/lesson_31103_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "うん。もっちろん！",
            "voice": "lesson/Lesson_31103/lesson_31103_018.mp3"
        }
    ]
}