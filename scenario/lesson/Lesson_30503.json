{
    "Txt_Name": "Lesson_30503",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……だからさ、ラジオって本っ当に最高なんだー。\n……って、ちょっと美晴、聞いてる？",
            "voice": "lesson/Lesson_30503/lesson_30503_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。聞いてますよー。　\n莉子は本当にラジオが好きなのねー。",
            "voice": "lesson/Lesson_30503/lesson_30503_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あ、飲み物、空になっちゃった。取ってこよっと。\n莉子はある？",
            "voice": "lesson/Lesson_30503/lesson_30503_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ、お願い。　",
            "voice": "lesson/Lesson_30503/lesson_30503_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたしたち、寮で一緒に暮らしてるけど、\nそれでもお互い、知らないことがいっぱいあるものね。",
            "voice": "lesson/Lesson_30503/lesson_30503_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうだねー。\nでも、そういうの、ちょっとずつ知れると嬉しくなるよねー。",
            "voice": "lesson/Lesson_30503/lesson_30503_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うんうん。　\nこういうの、もっとファンの人たちに届けられるといいかもね。",
            "voice": "lesson/Lesson_30503/lesson_30503_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ん？　こういうの？　どういうの？　",
            "voice": "lesson/Lesson_30503/lesson_30503_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "新しい発見、みたいなのしてもらえたらなって。　\nわたしたちの、いろんなところ。",
            "voice": "lesson/Lesson_30503/lesson_30503_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あー、そうだね。　",
            "voice": "lesson/Lesson_30503/lesson_30503_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたしも1人のファンとして、先輩声優さん追いかけてた時、\nそういうの、嬉しかったもんねー。",
            "voice": "lesson/Lesson_30503/lesson_30503_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ただいま。",
            "voice": "lesson/Lesson_30503/lesson_30503_014.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あ、絢、おかえりー。　",
            "voice": "lesson/Lesson_30503/lesson_30503_015.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　もう飲んでるの？　\nまだ夕方なのに？　",
            "voice": "lesson/Lesson_30503/lesson_30503_016.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "夕方だから飲んでんの。わかんないかなあ。　",
            "voice": "lesson/Lesson_30503/lesson_30503_017.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうだ、美晴。マネージャーが探してたよ。　\n美晴にお願いしたいことがあるみたい。",
            "voice": "lesson/Lesson_30503/lesson_30503_018.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら、何だろう？\nわかった。連絡してみるねー。",
            "voice": "lesson/Lesson_30503/lesson_30503_019.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ちょっと飲んじゃったけど……。\nうふふ、いいよね？",
            "voice": "lesson/Lesson_30503/lesson_30503_020.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。\nマネージャーに絡まないようにね。　",
            "voice": "lesson/Lesson_30503/lesson_30503_021.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "もう！　わたし、絡み酒はしませんよー。　",
            "voice": "lesson/Lesson_30503/lesson_30503_022.mp3"
        }
    ]
}