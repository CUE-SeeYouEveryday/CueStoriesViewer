{
    "Txt_Name": "Lesson_20202",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "随分ならんだなぁ～。\n……え？　まだ10分しか経ってないの！？",
            "voice": "lesson/Lesson_20202/lesson_20202_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ガマンガマン……。後50分我慢すれば……。\nでも50分って結構あるよね～……。",
            "voice": "lesson/Lesson_20202/lesson_20202_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あっ、鳩だ。なんか餌を持ってたらあげられるのになぁ。\nゴメンね～。今日は何も持ってないんだよ。",
            "voice": "lesson/Lesson_20202/lesson_20202_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "鳩行っちゃった……。",
            "voice": "lesson/Lesson_20202/lesson_20202_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あぁっ！　猫のモモコちゃん。\n遊びに来てくれたの？",
            "voice": "lesson/Lesson_20202/lesson_20202_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ダメダメ。今は一緒に遊べないの。\nわたしはフローズンチョコミントのために並んでるんだから。",
            "voice": "lesson/Lesson_20202/lesson_20202_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "モモコちゃん、行っちゃった……。\nモモコちゃんゴメンね～！　今度一緒に遊ぼうね～！！",
            "voice": "lesson/Lesson_20202/lesson_20202_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "店員",
            "message": "あ、あのお客様。並んでいる時はもう少しお静かに……。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あっ、ごめんなさい！　気をつけます！",
            "voice": "lesson/Lesson_20202/lesson_20202_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "もうちょっとだぞ、あと1人！\n……60分長かった～！",
            "voice": "lesson/Lesson_20202/lesson_20202_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "店員",
            "message": "お客様。",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ご、ごめんなさい。静かにします。",
            "voice": "lesson/Lesson_20202/lesson_20202_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "男の子",
            "message": "凛音ねーちゃん！　一緒に来て！",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "太郎君、どうしたの！？",
            "voice": "lesson/Lesson_20202/lesson_20202_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                0
            ],
            "name": "太郎と呼ばれた少年",
            "message": "こうちゃんがそこの道で転んで、\nヒザ擦りむいて泣いちゃってるんだ！　",
            "voice": ""
        },
        {
            "index": 16,
            "heroineId": [
                0
            ],
            "name": "太郎と呼ばれた少年",
            "message": "凛音ねーちゃん助けてよぉ……。",
            "voice": ""
        },
        {
            "index": 17,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えぇっ！？　わかったすぐ行く！",
            "voice": "lesson/Lesson_20202/lesson_20202_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": "店員",
            "message": "お、お客様？",
            "voice": ""
        },
        {
            "index": 19,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ごめんなさい！　私行きます！！",
            "voice": "lesson/Lesson_20202/lesson_20202_019.mp3"
        }
    ]
}