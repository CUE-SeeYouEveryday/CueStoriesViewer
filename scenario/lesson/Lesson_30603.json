{
    "Txt_Name": "Lesson_30603",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "ちょっといい？",
            "voice": "lesson/Lesson_30603/lesson_30603_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "リエンヌ、どうしたの？",
            "voice": "lesson/Lesson_30603/lesson_30603_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "いや、鳴と2人でどっか行っちゃってさ。\n何をしてるのかなって思って。",
            "voice": "lesson/Lesson_30603/lesson_30603_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "ネットで調べもの。",
            "voice": "lesson/Lesson_30603/lesson_30603_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "何を調べてるの？",
            "voice": "lesson/Lesson_30603/lesson_30603_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "たいしたことじゃないよ。",
            "voice": "lesson/Lesson_30603/lesson_30603_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "言いたくないなら言わなくてもいいけどさ……。\n凛音の様子が変だったから、ちょっと心配で。",
            "voice": "lesson/Lesson_30603/lesson_30603_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "リエンヌ、ありがとう。\nさっきはゴメンね。もう大丈夫。",
            "voice": "lesson/Lesson_30603/lesson_30603_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "うん……。なんだか落ち着いたみたいだね。\nよかった。",
            "voice": "lesson/Lesson_30603/lesson_30603_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "今日はオーディションでしょ。\n頑張ってね。",
            "voice": "lesson/Lesson_30603/lesson_30603_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！　ありがとう、リエンヌ！",
            "voice": "lesson/Lesson_30603/lesson_30603_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ただいま～。",
            "voice": "lesson/Lesson_30603/lesson_30603_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "おかえり。オーディションはどうだった？",
            "voice": "lesson/Lesson_30603/lesson_30603_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん。ちょっと集中できなかった。",
            "voice": "lesson/Lesson_30603/lesson_30603_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そっか……。まぁ、今回ダメでも次があるよ。",
            "voice": "lesson/Lesson_30603/lesson_30603_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "次かぁ……。うん、次があるよね！",
            "voice": "lesson/Lesson_30603/lesson_30603_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "うん、大丈夫。次は上手くいくよ。",
            "voice": "lesson/Lesson_30603/lesson_30603_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん、ありがとう！\n次も、頑張ってみる！",
            "voice": "lesson/Lesson_30603/lesson_30603_018.mp3"
        }
    ]
}