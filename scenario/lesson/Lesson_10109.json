{
    "Txt_Name": "Lesson_10109",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "リスクヘッジの意味、\n知らないんでしょ？",
            "voice": "lesson/Lesson_10109/lesson_10109_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……。",
            "voice": "lesson/Lesson_10109/lesson_10109_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "志穂？",
            "voice": "lesson/Lesson_10109/lesson_10109_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "リスクヘッジは敵って言ったの、\n意味……、知らないからなんでしょ？",
            "voice": "lesson/Lesson_10109/lesson_10109_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……。",
            "voice": "lesson/Lesson_10109/lesson_10109_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "フリーズしてるけど……。",
            "voice": "lesson/Lesson_10109/lesson_10109_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "うぅ……。",
            "voice": "lesson/Lesson_10109/lesson_10109_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "目、泳ぎまくってるよ？",
            "voice": "lesson/Lesson_10109/lesson_10109_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ホントは、知ってる……。\nリスクヘッジくらい、ちゃんと知ってるし……。",
            "voice": "lesson/Lesson_10109/lesson_10109_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "じゃあ、何？",
            "voice": "lesson/Lesson_10109/lesson_10109_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "敵……。\nとても強い敵……。",
            "voice": "lesson/Lesson_10109/lesson_10109_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "リスクヘッジが？",
            "voice": "lesson/Lesson_10109/lesson_10109_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "強い、敵……？",
            "voice": "lesson/Lesson_10109/lesson_10109_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "もう認めなよ。",
            "voice": "lesson/Lesson_10109/lesson_10109_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……。",
            "voice": "lesson/Lesson_10109/lesson_10109_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "本当は知らないんでしょ？",
            "voice": "lesson/Lesson_10109/lesson_10109_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "知らない事は、恥ずかしい事じゃ無いって。",
            "voice": "lesson/Lesson_10109/lesson_10109_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "志穂ちゃん。",
            "voice": "lesson/Lesson_10109/lesson_10109_019.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "うう……。",
            "voice": "lesson/Lesson_10109/lesson_10109_018.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……わからない。\n聞いたこともない。",
            "voice": "lesson/Lesson_10109/lesson_10109_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "やっと認めた……。",
            "voice": "lesson/Lesson_10109/lesson_10109_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "教えてくれ。\nリスクヘッジってなんだ？",
            "voice": "lesson/Lesson_10109/lesson_10109_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "リスクヘッジはね、\nビジネス用語だよ。",
            "voice": "lesson/Lesson_10109/lesson_10109_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "びじねす？",
            "voice": "lesson/Lesson_10109/lesson_10109_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "仕事で使う言葉ってことだよ。",
            "voice": "lesson/Lesson_10109/lesson_10109_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "リスクを回避する時に使ったりするの。",
            "voice": "lesson/Lesson_10109/lesson_10109_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "「マジック◆ワークス」の中の、\n新しい言葉じゃない……？",
            "voice": "lesson/Lesson_10109/lesson_10109_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そう……、だね。",
            "voice": "lesson/Lesson_10109/lesson_10109_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "なーんだ。\nなら知らなくても問題なし。",
            "voice": "lesson/Lesson_10109/lesson_10109_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ちょっとちょっと！",
            "voice": "lesson/Lesson_10109/lesson_10109_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "知らなくても良いって事にはならないでしょ？",
            "voice": "lesson/Lesson_10109/lesson_10109_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "志穂ちゃん、ホントぶれないね……。",
            "voice": "lesson/Lesson_10109/lesson_10109_033.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "「マジック◆ワークス」以外のことは興味ない。\nだから大丈夫。これでいい。",
            "voice": "lesson/Lesson_10109/lesson_10109_032.mp3"
        }
    ]
}