{
    "Txt_Name": "Lesson_31307",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "で、美晴。\n一応確認しときたいんだけど、本当にわかったの？",
            "voice": "lesson/Lesson_31307/lesson_31307_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なあに？　やっぱり不安？",
            "voice": "lesson/Lesson_31307/lesson_31307_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そうだね。\nもし闇雲に探し回ろうっていうのなら時間の無駄だし。",
            "voice": "lesson/Lesson_31307/lesson_31307_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "もう一度ちゃんと電話で居場所の目印とか、\n聞いたほうがいいんじゃないのって思うから。",
            "voice": "lesson/Lesson_31307/lesson_31307_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "っていうか、なんでその辺聞いてないわけ？",
            "voice": "lesson/Lesson_31307/lesson_31307_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ええ？　だって、わかったから。\n聞かなくてもいいかな……、って。",
            "voice": "lesson/Lesson_31307/lesson_31307_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "なるほどね。",
            "voice": "lesson/Lesson_31307/lesson_31307_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。",
            "voice": "lesson/Lesson_31307/lesson_31307_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "で、どこのカミズオーなの？\nその店員さんの声が聞こえたカミズオーは。",
            "voice": "lesson/Lesson_31307/lesson_31307_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "もう、信用してくれないんだから。",
            "voice": "lesson/Lesson_31307/lesson_31307_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "信用してないってわけじゃないけど。",
            "voice": "lesson/Lesson_31307/lesson_31307_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\nあっちよ。ご案内しまーす。",
            "voice": "lesson/Lesson_31307/lesson_31307_012.mp3"
        }
    ]
}