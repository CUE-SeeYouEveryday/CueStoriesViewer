{
    "Txt_Name": "Lesson_31102",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "うーん……。",
            "voice": "lesson/Lesson_31102/lesson_31102_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "うーーーーーーーーーん。",
            "voice": "lesson/Lesson_31102/lesson_31102_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "どうしようかなあ。\n買っちゃおうかなあ。",
            "voice": "lesson/Lesson_31102/lesson_31102_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "でも、これ買うとプチキュア、買えなくなるしなー。",
            "voice": "lesson/Lesson_31102/lesson_31102_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "うーーーーーーーーーーーーーーーーん。",
            "voice": "lesson/Lesson_31102/lesson_31102_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "悠希ちゃん。",
            "voice": "lesson/Lesson_31102/lesson_31102_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あ、あいり。\nもう、欲しい物買ったの？",
            "voice": "lesson/Lesson_31102/lesson_31102_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "うん。そして、これ。おすすめのフィギュア。\nえへへ。",
            "voice": "lesson/Lesson_31102/lesson_31102_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "え？　あいり、それ買ってきちゃったの？",
            "voice": "lesson/Lesson_31102/lesson_31102_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あ、そうか。買っちゃダメだった。\nおすすめしないと……。",
            "voice": "lesson/Lesson_31102/lesson_31102_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えっと、どうしよう。\n……じゃあこれ、プレゼント。",
            "voice": "lesson/Lesson_31102/lesson_31102_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "昨日、頑張った悠希ちゃんに。",
            "voice": "lesson/Lesson_31102/lesson_31102_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あはは。何それ。",
            "voice": "lesson/Lesson_31102/lesson_31102_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えへへへ。",
            "voice": "lesson/Lesson_31102/lesson_31102_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "よし。\nじゃあ、あたしもあいりにプレゼントする！",
            "voice": "lesson/Lesson_31102/lesson_31102_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あいり、昨日、頑張ったから。\nちょっと待ってて。",
            "voice": "lesson/Lesson_31102/lesson_31102_016.mp3"
        }
    ]
}