{
    "Txt_Name": "Lesson_30504",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ねえ、美晴、もう寝た？　",
            "voice": "lesson/Lesson_30504/lesson_30504_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ううん。まだ起きてるよ―。\nなあに？",
            "voice": "lesson/Lesson_30504/lesson_30504_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あのさ、こういうプロモーションとかって、　\n今後も増えてくのかな？",
            "voice": "lesson/Lesson_30504/lesson_30504_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうね。　",
            "voice": "lesson/Lesson_30504/lesson_30504_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "キャスティングが増えれば増えるほど、　\nこういう仕事も、どんどん多くなるかもね。",
            "voice": "lesson/Lesson_30504/lesson_30504_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そっか。　\nこのくらいで忙しいなんて思ってちゃ、ダメなんだろうな。",
            "voice": "lesson/Lesson_30504/lesson_30504_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "美晴は……、慣れた？",
            "voice": "lesson/Lesson_30504/lesson_30504_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "何が？　",
            "voice": "lesson/Lesson_30504/lesson_30504_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "声優の……、仕事？",
            "voice": "lesson/Lesson_30504/lesson_30504_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。どうだろうね。　\nでも、ずっとなりたくて、そして、やっと手にした仕事だから。",
            "voice": "lesson/Lesson_30504/lesson_30504_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "少なくとも、苦痛ってわけじゃないかなー。\nこういう、アフレコ以外の仕事もね。",
            "voice": "lesson/Lesson_30504/lesson_30504_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "絢はどうなの？　",
            "voice": "lesson/Lesson_30504/lesson_30504_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。私も、苦痛とかそういうのはないかな。\nむしろ……。　",
            "voice": "lesson/Lesson_30504/lesson_30504_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "むしろ？　",
            "voice": "lesson/Lesson_30504/lesson_30504_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "楽しいかも。\nこんな、バタバタと慌てて、青森まで来るとか……。",
            "voice": "lesson/Lesson_30504/lesson_30504_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "今まで、こんな経験、したことなかったから。　",
            "voice": "lesson/Lesson_30504/lesson_30504_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。なかなか体験できないもんね。　",
            "voice": "lesson/Lesson_30504/lesson_30504_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "確かに前もってスケジュール知ってたら、もっと楽だったけど。",
            "voice": "lesson/Lesson_30504/lesson_30504_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "でも、意外と嫌いじゃないかも。　\nこういうの。",
            "voice": "lesson/Lesson_30504/lesson_30504_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ふふ。絢、なんだかんだ言って、強いのね。　\n意外とタフ。",
            "voice": "lesson/Lesson_30504/lesson_30504_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え?　そうかな？　",
            "voice": "lesson/Lesson_30504/lesson_30504_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうよ。　",
            "voice": "lesson/Lesson_30504/lesson_30504_022.mp3"
        }
    ]
}