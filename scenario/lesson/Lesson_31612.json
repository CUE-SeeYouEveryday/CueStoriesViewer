{
    "Txt_Name": "Lesson_31612",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "うふふ。\n声だけ聞こえれば十分。",
            "voice": "lesson/Lesson_31612/lesson_31612_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "だって同じ声なんだもの。\n私、まちがえないわ。",
            "voice": "lesson/Lesson_31612/lesson_31612_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "でしょ？",
            "voice": "lesson/Lesson_31612/lesson_31612_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そっかー……、ふふっ。\n柚葉がそう言うならきっとそうなんだろうね。",
            "voice": "lesson/Lesson_31612/lesson_31612_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "うふふ。\nありがと。ユーキ。",
            "voice": "lesson/Lesson_31612/lesson_31612_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ほら、聞こえる。\nどう？　あの子の声でしょ？",
            "voice": "lesson/Lesson_31612/lesson_31612_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "うん。\n確かにあの時の小鳥と同じ声かも。",
            "voice": "lesson/Lesson_31612/lesson_31612_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "おかえり！　だね。",
            "voice": "lesson/Lesson_31612/lesson_31612_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "うふふ。",
            "voice": "lesson/Lesson_31612/lesson_31612_009.mp3"
        }
    ]
}