{
    "Txt_Name": "Lesson_20412",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "その猫、かわいいね。どこの子？",
            "voice": "lesson/Lesson_20412/lesson_20412_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……さあ。たまたま、見つけただけだし……。",
            "voice": "lesson/Lesson_20412/lesson_20412_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちょっ、こら！　\n携帯はおもちゃじゃないから……。",
            "voice": "lesson/Lesson_20412/lesson_20412_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "すごい懐かれてる。\n……まほろのこと、好きなんだね。",
            "voice": "lesson/Lesson_20412/lesson_20412_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そ、そう……、かな。",
            "voice": "lesson/Lesson_20412/lesson_20412_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "猫",
            "message": "にゃー！",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "もう！　しつこい、この猫……。",
            "voice": "lesson/Lesson_20412/lesson_20412_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……あれ？",
            "voice": "lesson/Lesson_20412/lesson_20412_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "その指……。どうしたの？\nせっかくのネイル、ぼろぼろじゃない……。",
            "voice": "lesson/Lesson_20412/lesson_20412_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……本当だ。傷だらけになってる。",
            "voice": "lesson/Lesson_20412/lesson_20412_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あんなに、気に入ってたのに……。",
            "voice": "lesson/Lesson_20412/lesson_20412_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……そうなんだけど。",
            "voice": "lesson/Lesson_20412/lesson_20412_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あっ！　だから、携帯で遊ぶなって！",
            "voice": "lesson/Lesson_20412/lesson_20412_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "本当に元気だね、この猫。",
            "voice": "lesson/Lesson_20412/lesson_20412_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                0
            ],
            "name": "猫",
            "message": "にゃあ！",
            "voice": ""
        },
        {
            "index": 16,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……うん。だからまあ……。いいよ。",
            "voice": "lesson/Lesson_20412/lesson_20412_016.mp3"
        }
    ]
}