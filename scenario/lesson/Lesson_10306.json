{
    "Txt_Name": "Lesson_10306",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ほら莉子、ちゃんと歩いて。",
            "voice": "lesson/Lesson_10306/lesson_10306_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ふぁい……。",
            "voice": "lesson/Lesson_10306/lesson_10306_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ホント着替えさせるだけで一苦労だよ……。",
            "voice": "lesson/Lesson_10306/lesson_10306_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "うわ、酒くさ。\nなんでこんなに飲んじゃったの？",
            "voice": "lesson/Lesson_10306/lesson_10306_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "お世話になってた人の送別会で、つい……。",
            "voice": "lesson/Lesson_10306/lesson_10306_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "莉子、持っていく物は？",
            "voice": "lesson/Lesson_10306/lesson_10306_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "モッテイクモノ？",
            "voice": "lesson/Lesson_10306/lesson_10306_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ピクニックの持ってく荷物。\nないの？",
            "voice": "lesson/Lesson_10306/lesson_10306_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ……。部屋に……。あるかな？\nちょっと取って来る。",
            "voice": "lesson/Lesson_10306/lesson_10306_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "これはまだかかるわね……。",
            "voice": "lesson/Lesson_10306/lesson_10306_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "もう出発時間だってのに。\n莉子、酷くない？",
            "voice": "lesson/Lesson_10306/lesson_10306_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……。",
            "voice": "lesson/Lesson_10306/lesson_10306_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あれ？\n絢？",
            "voice": "lesson/Lesson_10306/lesson_10306_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "（……ね、眠い……。\n今になって強烈な眠気が……。）",
            "voice": "lesson/Lesson_10306/lesson_10306_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "（あれ……。\n絢、相当怒ってるみたい。）",
            "voice": "lesson/Lesson_10306/lesson_10306_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "（寝ちゃダメ！\n寝ちゃダメだ……！）",
            "voice": "lesson/Lesson_10306/lesson_10306_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……ははは。わかるよ絢。\n言い出しっぺの莉子があれじゃ……、怒りたくもなるよね。",
            "voice": "lesson/Lesson_10306/lesson_10306_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　私？\n怒ってないよ。",
            "voice": "lesson/Lesson_10306/lesson_10306_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まあまあまあ。",
            "voice": "lesson/Lesson_10306/lesson_10306_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "莉子が戻って来たらすぐ出発するからね。",
            "voice": "lesson/Lesson_10306/lesson_10306_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "う、うん。",
            "voice": "lesson/Lesson_10306/lesson_10306_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "お待たせ～。",
            "voice": "lesson/Lesson_10306/lesson_10306_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "よし、じゃあ出発！",
            "voice": "lesson/Lesson_10306/lesson_10306_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あっ！\n……ゴメン。ちょっとトイレ。",
            "voice": "lesson/Lesson_10306/lesson_10306_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "まじか！\n莉子のヤツ……。",
            "voice": "lesson/Lesson_10306/lesson_10306_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……。",
            "voice": "lesson/Lesson_10306/lesson_10306_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "（絢の目つきがヤバイことになってる……。）",
            "voice": "lesson/Lesson_10306/lesson_10306_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "（完全に白目になってる……。\n大丈夫かなぁ。）",
            "voice": "lesson/Lesson_10306/lesson_10306_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……あのさ、今日止めにしない？",
            "voice": "lesson/Lesson_10306/lesson_10306_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……んえ？\nどうして？",
            "voice": "lesson/Lesson_10306/lesson_10306_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "莉子、グズグズだし。絢も……。\nあれでしょ？",
            "voice": "lesson/Lesson_10306/lesson_10306_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうだね……。\n絢に気をつかわせちゃうのも悪いしね。",
            "voice": "lesson/Lesson_10306/lesson_10306_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ちょ、ちょっと待って。",
            "voice": "lesson/Lesson_10306/lesson_10306_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "（違う、違うの！）",
            "voice": "lesson/Lesson_10306/lesson_10306_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "今日のピクニックは中止ってことで……。",
            "voice": "lesson/Lesson_10306/lesson_10306_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "中止なんてイヤ！\n私、本当にピクニックに行きたいの！",
            "voice": "lesson/Lesson_10306/lesson_10306_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "絢……？",
            "voice": "lesson/Lesson_10306/lesson_10306_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "キレイな自然見ながら、\nみんなと一緒に……、いただきますって言いたいの！",
            "voice": "lesson/Lesson_10306/lesson_10306_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そ、そうだったの？",
            "voice": "lesson/Lesson_10306/lesson_10306_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん……。",
            "voice": "lesson/Lesson_10306/lesson_10306_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……目つきが険しかったのはなんで？",
            "voice": "lesson/Lesson_10306/lesson_10306_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "今日が楽しみ過ぎて、一睡も出来なかったから……。",
            "voice": "lesson/Lesson_10306/lesson_10306_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "なんだそういうことか……、って子供かっ！",
            "voice": "lesson/Lesson_10306/lesson_10306_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうだったんだ。\nよかったぁ、楽しみにしてくれてたんだね。",
            "voice": "lesson/Lesson_10306/lesson_10306_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "もちろんだよ……。",
            "voice": "lesson/Lesson_10306/lesson_10306_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "じゃあ行こう！",
            "voice": "lesson/Lesson_10306/lesson_10306_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "行くっきゃないね。",
            "voice": "lesson/Lesson_10306/lesson_10306_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "お待たせ～。心配かけてごめんね。\nもう大丈夫。いざ行かん！　仲間達よ！",
            "voice": "lesson/Lesson_10306/lesson_10306_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "まったく、調子いいんだから。",
            "voice": "lesson/Lesson_10306/lesson_10306_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "気楽でいいよね、莉子は。",
            "voice": "lesson/Lesson_10306/lesson_10306_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "Ｚｚｚｚ……。",
            "voice": "lesson/Lesson_10306/lesson_10306_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あー。\n絢が完全に落ちたぁ。",
            "voice": "lesson/Lesson_10306/lesson_10306_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "仕方がない、かついでいくのみだ。",
            "voice": "lesson/Lesson_10306/lesson_10306_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたし、荷物持つよ。",
            "voice": "lesson/Lesson_10306/lesson_10306_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "（……ピクニック6ヶ条の最後の1つ、わかっちゃった……。）",
            "voice": "lesson/Lesson_10306/lesson_10306_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "（「仲間」だ。\nフフ。「仲間」。)",
            "voice": "lesson/Lesson_10306/lesson_10306_056.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "なんか絢……。\n寝ながら笑ってるんだけど。",
            "voice": "lesson/Lesson_10306/lesson_10306_057.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "しかめっ面よりずっといいよ。\n幸せそうだね♪",
            "voice": "lesson/Lesson_10306/lesson_10306_058.mp3"
        }
    ]
}