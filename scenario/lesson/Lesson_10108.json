{
    "Txt_Name": "Lesson_10108",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "さて、自分のセリフの練習をするか。\nどれどれ……。",
            "voice": "lesson/Lesson_10108/lesson_10108_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "「みんな！\n私は、リスクヘッジを考えたの……。」",
            "voice": "lesson/Lesson_10108/lesson_10108_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ん？　ん？？\nリスク……、ヘッジ？",
            "voice": "lesson/Lesson_10108/lesson_10108_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "リスクヘッジって……、何だ？",
            "voice": "lesson/Lesson_10108/lesson_10108_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "公式設定集にも載ってない……。\n何だこの言葉……。",
            "voice": "lesson/Lesson_10108/lesson_10108_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "今回からの新要素……？",
            "voice": "lesson/Lesson_10108/lesson_10108_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "もしかして新手の敵か？\nリスクヘッジ……。",
            "voice": "lesson/Lesson_10108/lesson_10108_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "敵の名前っぽい気もする……。",
            "voice": "lesson/Lesson_10108/lesson_10108_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "読み合わせ始めるよー！",
            "voice": "lesson/Lesson_10108/lesson_10108_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ああ、まだわからんのに……。",
            "voice": "lesson/Lesson_10108/lesson_10108_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "やっぱり「マジック◆ワークス」の台本、\n面白いよね～。",
            "voice": "lesson/Lesson_10108/lesson_10108_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "後半、強敵が複数出てくるところなんか、\n熱くて最高！",
            "voice": "lesson/Lesson_10108/lesson_10108_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "だな……。",
            "voice": "lesson/Lesson_10108/lesson_10108_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "わたしもあのシーン好き！",
            "voice": "lesson/Lesson_10108/lesson_10108_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ヴェノムスネークが出て来て！",
            "voice": "lesson/Lesson_10108/lesson_10108_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ブラックレイスも襲い掛かって来て！",
            "voice": "lesson/Lesson_10108/lesson_10108_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……あとリスクヘッジが現れて。",
            "voice": "lesson/Lesson_10108/lesson_10108_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "えっ？",
            "voice": "lesson/Lesson_10108/lesson_10108_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "んっ？",
            "voice": "lesson/Lesson_10108/lesson_10108_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "リスクヘッジ？",
            "voice": "lesson/Lesson_10108/lesson_10108_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "出て、来たよな……？",
            "voice": "lesson/Lesson_10108/lesson_10108_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "リスクヘッジが出てくるの？",
            "voice": "lesson/Lesson_10108/lesson_10108_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "リスクヘッジが……？",
            "voice": "lesson/Lesson_10108/lesson_10108_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "志穂……。\n1回確認なんだけど……。",
            "voice": "lesson/Lesson_10108/lesson_10108_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "な、なんだ……？",
            "voice": "lesson/Lesson_10108/lesson_10108_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ヴェノムスネークと、ブラックレイスと、\nリスクヘッジが主人公達と戦ったって事？",
            "voice": "lesson/Lesson_10108/lesson_10108_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ああ……。",
            "voice": "lesson/Lesson_10108/lesson_10108_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "リスクヘッジは、敵なんだ？",
            "voice": "lesson/Lesson_10108/lesson_10108_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "そう、そうだ。\nそんな事も知らないのか。",
            "voice": "lesson/Lesson_10108/lesson_10108_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……。",
            "voice": "lesson/Lesson_10108/lesson_10108_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "（どうにか乗り切った……。\n知らないなんて思われたら、マジックファンとして名折れだ。）",
            "voice": "lesson/Lesson_10108/lesson_10108_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あのさ……。",
            "voice": "lesson/Lesson_10108/lesson_10108_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "志穂ちゃん……。\nもしかして……、もしかしてだけど……。",
            "voice": "lesson/Lesson_10108/lesson_10108_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "リスクヘッジの意味、知らないんじゃないの？",
            "voice": "lesson/Lesson_10108/lesson_10108_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "(ま、まずい……。)",
            "voice": "lesson/Lesson_10108/lesson_10108_035.mp3"
        }
    ]
}