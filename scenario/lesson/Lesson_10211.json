{
    "Txt_Name": "Lesson_10211",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ヘックシュン！！",
            "voice": "lesson/Lesson_10211/lesson_10211_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "柚葉、毛布になんかくるまってなにしてるの？",
            "voice": "lesson/Lesson_10211/lesson_10211_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "顔真っ赤だよ？\nもしかして、風邪？",
            "voice": "lesson/Lesson_10211/lesson_10211_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "おでこ触るね？\n……あ、すごい熱！",
            "voice": "lesson/Lesson_10211/lesson_10211_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ぐーたらを理解するために、\nシエスタしたの……。",
            "voice": "lesson/Lesson_10211/lesson_10211_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "しえすた？",
            "voice": "lesson/Lesson_10211/lesson_10211_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "お昼寝の事だよ。",
            "voice": "lesson/Lesson_10211/lesson_10211_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "昼寝で熱出る？",
            "voice": "lesson/Lesson_10211/lesson_10211_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私の役……、お腹出して寝てたから、\n私もお腹出してシエスタしたの……。",
            "voice": "lesson/Lesson_10211/lesson_10211_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "なんでそんな……。\nそりゃ、お腹出して昼寝したら風邪ひくって。",
            "voice": "lesson/Lesson_10211/lesson_10211_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私、ぐーたらしてるでしょ？",
            "voice": "lesson/Lesson_10211/lesson_10211_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ぐったりしてる。",
            "voice": "lesson/Lesson_10211/lesson_10211_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "柚葉ちゃん。\nそれはただの風邪だよ。",
            "voice": "lesson/Lesson_10211/lesson_10211_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "つまり、この物語の行き着く先は……。\n病気ってことね。",
            "voice": "lesson/Lesson_10211/lesson_10211_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "全然違うから。",
            "voice": "lesson/Lesson_10211/lesson_10211_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "そんな……。\n掴んだと思ったのに。",
            "voice": "lesson/Lesson_10211/lesson_10211_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "風邪薬持って来てあげるね。",
            "voice": "lesson/Lesson_10211/lesson_10211_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "いらない。",
            "voice": "lesson/Lesson_10211/lesson_10211_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えっ？",
            "voice": "lesson/Lesson_10211/lesson_10211_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "どうして？",
            "voice": "lesson/Lesson_10211/lesson_10211_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "妖精達は化学薬品は飲まないでしょ？\nだから私も飲まない……。",
            "voice": "lesson/Lesson_10211/lesson_10211_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "何その発想！？",
            "voice": "lesson/Lesson_10211/lesson_10211_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "見上げたど根性だね。",
            "voice": "lesson/Lesson_10211/lesson_10211_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "妖精の気持ちを掴むまで、できる限りのことをやるの。",
            "voice": "lesson/Lesson_10211/lesson_10211_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "飲みなってば。",
            "voice": "lesson/Lesson_10211/lesson_10211_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "いやっ！\n自然に治癒させる！",
            "voice": "lesson/Lesson_10211/lesson_10211_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "も～！！",
            "voice": "lesson/Lesson_10211/lesson_10211_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "うちの親父並みに頑固だね。",
            "voice": "lesson/Lesson_10211/lesson_10211_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "どうしよう……。\n無理やりにでも飲ませる？",
            "voice": "lesson/Lesson_10211/lesson_10211_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "しょうがない。\n悠希、ネギ持って来て！",
            "voice": "lesson/Lesson_10211/lesson_10211_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ネギ……？\nネギをどうするの？",
            "voice": "lesson/Lesson_10211/lesson_10211_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "柚葉の首に巻くの。",
            "voice": "lesson/Lesson_10211/lesson_10211_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "なんで！？ ",
            "voice": "lesson/Lesson_10211/lesson_10211_033.mp3"
        }
    ]
}