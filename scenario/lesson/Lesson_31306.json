{
    "Txt_Name": "Lesson_31306",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "おはようございまーす。",
            "voice": "lesson/Lesson_31306/lesson_31306_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "おはようございます。",
            "voice": "lesson/Lesson_31306/lesson_31306_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あ、おはよう。\nあれ？　2人とも早いね。まだメイクさん来てないよ。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい。わたし、メイクより先に今日の打ち合わせを少し、\nマネージャーさんとしたくて……。",
            "voice": "lesson/Lesson_31306/lesson_31306_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "何？　打ち合わせって。\n美晴、また何か考えてるの？",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "事務所、エアコン効いていて涼しい……。",
            "voice": "lesson/Lesson_31306/lesson_31306_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あ、そうそう。\n今日は野外のイベントだから、皆熱中症にも気をつけてね。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい。準備は万端です。絢は？　",
            "voice": "lesson/Lesson_31306/lesson_31306_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。私も色々持ってきたよ……。\nバッグの中に色々……。",
            "voice": "lesson/Lesson_31306/lesson_31306_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あれ？　",
            "voice": "lesson/Lesson_31306/lesson_31306_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "どうしたの？",
            "voice": "lesson/Lesson_31306/lesson_31306_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "お財布忘れちゃったかも……。",
            "voice": "lesson/Lesson_31306/lesson_31306_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら、大変。落としたとかじゃない？　大丈夫？",
            "voice": "lesson/Lesson_31306/lesson_31306_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。昨日のバッグに入れっぱなしかも。\n莉子、まだ寮にいるかな？",
            "voice": "lesson/Lesson_31306/lesson_31306_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "電話してみれば？",
            "voice": "lesson/Lesson_31306/lesson_31306_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。朝から失敗しちゃった。",
            "voice": "lesson/Lesson_31306/lesson_31306_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。大丈夫、そんなの大したことじゃないもの。\n持ってきてもらえばいいだけなんだから。",
            "voice": "lesson/Lesson_31306/lesson_31306_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん……。そうだね。\nありがと。美晴。",
            "voice": "lesson/Lesson_31306/lesson_31306_018.mp3"
        }
    ]
}