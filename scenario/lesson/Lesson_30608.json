{
    "Txt_Name": "Lesson_30608",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "よろしくお願いしまーす！",
            "voice": "lesson/Lesson_30608/lesson_30608_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "（横にいる3人が、わたしのライバルたちだ……。）",
            "voice": "lesson/Lesson_30608/lesson_30608_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "（みんな、すごい緊張してる……。）",
            "voice": "lesson/Lesson_30608/lesson_30608_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……。",
            "voice": "lesson/Lesson_30608/lesson_30608_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "（なんか、ドキドキしてきた！）",
            "voice": "lesson/Lesson_30608/lesson_30608_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "監督",
            "message": "それでは、事前にご連絡した通り、\n今から今日読んでもらう台本をお渡しします。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "監督",
            "message": "10分ほど時間を取りますので、各自読み込んでください。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はい！",
            "voice": "lesson/Lesson_30608/lesson_30608_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "（よーし、やるぞー！）",
            "voice": "lesson/Lesson_30608/lesson_30608_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……。",
            "voice": "lesson/Lesson_30608/lesson_30608_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "（これが台本かぁ。本当に「どれみふぁ空の下」のアニメだー！）",
            "voice": "lesson/Lesson_30608/lesson_30608_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……。",
            "voice": "lesson/Lesson_30608/lesson_30608_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "（このセリフは元気いっぱい言うのがいいかも。）",
            "voice": "lesson/Lesson_30608/lesson_30608_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "（でも、ずっと元気いっぱいなのは、おかしいかな……。）",
            "voice": "lesson/Lesson_30608/lesson_30608_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……。",
            "voice": "lesson/Lesson_30608/lesson_30608_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "（このセリフは抑えて、\nあとで爆発した方がこの子らしいかも……。）",
            "voice": "lesson/Lesson_30608/lesson_30608_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "（それとも、抑える必要ない？\n常に全力でいくべき？）",
            "voice": "lesson/Lesson_30608/lesson_30608_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "凛音、どうしたの？　監督待ってるよ！",
            "voice": ""
        },
        {
            "index": 19,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あ！　すみません！　えっと、どこ読むんでしたっけ？",
            "voice": "lesson/Lesson_30608/lesson_30608_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ただいま～……。",
            "voice": "lesson/Lesson_30608/lesson_30608_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "おかえり、凛音さん。",
            "voice": "lesson/Lesson_30608/lesson_30608_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "おかえり。どうだった？",
            "voice": "lesson/Lesson_30608/lesson_30608_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！　頑張ってきたよ！",
            "voice": "lesson/Lesson_30608/lesson_30608_023.mp3"
        }
    ]
}