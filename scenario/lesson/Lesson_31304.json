{
    "Txt_Name": "Lesson_31304",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "おっはよー。",
            "voice": "lesson/Lesson_31304/lesson_31304_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "おはよう、莉子。",
            "voice": "lesson/Lesson_31304/lesson_31304_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "おはよう。",
            "voice": "lesson/Lesson_31304/lesson_31304_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あれ？　美晴も絢ももう出かける格好してるの？\nずいぶん早いね。",
            "voice": "lesson/Lesson_31304/lesson_31304_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私はいつもどおりの時間に起きただけだから。",
            "voice": "lesson/Lesson_31304/lesson_31304_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ふふっ、わたしと絢は、\n1時間早く入ってメイクしてもらうことになってるの。",
            "voice": "lesson/Lesson_31304/lesson_31304_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "それに、少し早めに行って、\nマネージャーさんと打ち合わせしておこうと思って。",
            "voice": "lesson/Lesson_31304/lesson_31304_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。\nさすが2人ともしっかりしてるねえ。",
            "voice": "lesson/Lesson_31304/lesson_31304_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "まほろは……、まだだよね。",
            "voice": "lesson/Lesson_31304/lesson_31304_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まあ、でもそのうち起きてくるんじゃない？",
            "voice": "lesson/Lesson_31304/lesson_31304_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "朝弱いタイプだけど、\n仕事に遅刻するようなことは絶対にしないし。",
            "voice": "lesson/Lesson_31304/lesson_31304_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうだね。",
            "voice": "lesson/Lesson_31304/lesson_31304_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "それじゃあ、絢、わたしたちはそろそろ行きましょうか。",
            "voice": "lesson/Lesson_31304/lesson_31304_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、もう出る？\nごめん、ちょっと準備するから待ってて。",
            "voice": "lesson/Lesson_31304/lesson_31304_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はーい。\nあわてなくていいわよー。",
            "voice": "lesson/Lesson_31304/lesson_31304_015.mp3"
        }
    ]
}