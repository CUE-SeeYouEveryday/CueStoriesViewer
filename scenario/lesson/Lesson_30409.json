{
    "Txt_Name": "Lesson_30409",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あいりって何してる？",
            "voice": "lesson/Lesson_30409/lesson_30409_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ナレーションの仕事で失敗したらしくて、\n部屋で落ち込んでる。",
            "voice": "lesson/Lesson_30409/lesson_30409_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいりの活躍を願う人……。",
            "voice": "lesson/Lesson_30409/lesson_30409_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "誰だろう。\n家族とか、仲間とか……？",
            "voice": "lesson/Lesson_30409/lesson_30409_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいりが落ち込んだりしたら、やっぱり、心配かけちゃうよね。",
            "voice": "lesson/Lesson_30409/lesson_30409_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "感情って表に出ちゃうから、あいりも笑顔でいなきゃ。",
            "voice": "lesson/Lesson_30409/lesson_30409_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "そうだよ。あいりは正義のヒロインを演じるためにここに来たんだ。\n前向きモードに変身だ！　トオゥ！",
            "voice": "lesson/Lesson_30409/lesson_30409_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ん？　なんか聞こえなかった？",
            "voice": "lesson/Lesson_30409/lesson_30409_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ん？　なーんも。",
            "voice": "lesson/Lesson_30409/lesson_30409_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あ、そうそう、これ受ける？　ナレーションのワークショップ。\n私、この期間ダメなんだけど",
            "voice": "lesson/Lesson_30409/lesson_30409_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私もダメー。",
            "voice": "lesson/Lesson_30409/lesson_30409_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あたしも無理なんだよなー。",
            "voice": "lesson/Lesson_30409/lesson_30409_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "みんなのところに行かなくちゃ！",
            "voice": "lesson/Lesson_30409/lesson_30409_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ん？",
            "voice": "lesson/Lesson_30409/lesson_30409_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "みんなー、心配かけてごめんね！　あいり、完全復活だよぉ！",
            "voice": "lesson/Lesson_30409/lesson_30409_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……よかったね、あいり。",
            "voice": "lesson/Lesson_30409/lesson_30409_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ナレーションのこと、吹っ切れたの？",
            "voice": "lesson/Lesson_30409/lesson_30409_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "うん！　もう大丈夫！\nみんな、何を見てるの？",
            "voice": "lesson/Lesson_30409/lesson_30409_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ああ、ナレーションのワークショップ、生徒募集してるらしいよ。",
            "voice": "lesson/Lesson_30409/lesson_30409_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "行きたい！　あいり、これ行く！\nこれ、持ってっていい？",
            "voice": "lesson/Lesson_30409/lesson_30409_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "いいわよ。\n残念だけど、他の誰も行けないみたいだし。",
            "voice": "lesson/Lesson_30409/lesson_30409_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ありがとう！\nじゃあ、部屋戻るね！",
            "voice": "lesson/Lesson_30409/lesson_30409_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あいり……。頑張ろうね。",
            "voice": "lesson/Lesson_30409/lesson_30409_023.mp3"
        }
    ]
}