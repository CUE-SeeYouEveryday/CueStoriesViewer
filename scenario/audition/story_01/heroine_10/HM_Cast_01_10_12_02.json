{
    "Txt_Name": "HM_Cast_01_10_12_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "お疲れ様でーす！",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_02/hm_cast_01_10_12_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "お疲れ様です。\nマネージャー、これどうぞ。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_02/hm_cast_01_10_12_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ありがとう。\nこれはチョコかな？",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "はい。\n昨日、莉子とチョコレートビュッフェに行きました。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_02/hm_cast_01_10_12_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "めっちゃ美味しかったよ！\n10種類以上食べたんじゃないかなー。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_02/hm_cast_01_10_12_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そんなに色々頼んだの？",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "いくつか頼んで、2人でシェアしたんです。\n1つ1つはそんなに大きくなかったので。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_02/hm_cast_01_10_12_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "でも、本当に美味しかった……。\nまだ食べてないのもいっぱいあるし……。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_02/hm_cast_01_10_12_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "じゃあさ、また行こうよ！\nメニュー全種類制覇しちゃおう！",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_02/hm_cast_01_10_12_02_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふ、それいいかも。\nまた行けるの、楽しみだな。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_02/hm_cast_01_10_12_02_010.mp3"
        }
    ]
}