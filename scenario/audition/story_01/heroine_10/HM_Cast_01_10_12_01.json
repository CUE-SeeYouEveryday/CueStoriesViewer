{
    "Txt_Name": "HM_Cast_01_10_12_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "お疲れ様です。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_01/hm_cast_01_10_12_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様。\n今日は、絢で最後？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "はい、みんなは先に帰りました。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_01/hm_cast_01_10_12_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そっか。\nあ、差し入れでもらったチョコ食べる？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "チョコ、ですか……。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_01/hm_cast_01_10_12_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あれ？　好きじゃなかったっけ？",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "いえ、好きです。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_01/hm_cast_01_10_12_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……だけど、さっき莉子からもたくさんチョコもらってて。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_01/hm_cast_01_10_12_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "最近莉子、私によくお菓子くれるんです。\n嬉しいんですけど、もらってばっかりだなって。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_01/hm_cast_01_10_12_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ああ、そうなんだ。\nじゃあ、これはやめておく？",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あっ、いえ、もらえると嬉しいです。\n……チョコ、大好きだから。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_01/hm_cast_01_10_12_01_011.mp3"
        }
    ]
}