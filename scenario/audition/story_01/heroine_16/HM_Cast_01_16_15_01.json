{
    "Txt_Name": "HM_Cast_01_16_15_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "なるちゃーん！　これ、あげるっ！",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_01/hm_cast_01_16_15_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "え？　……チョコレート？\nすごい量……。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_01/hm_cast_01_16_15_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！　ゆきぞーとお菓子買いに行ったんだけど。\nつい買いすぎちゃってー。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_01/hm_cast_01_16_15_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "だから、おすそ分け！\n甘い物は、お仕事にも必須アイテムだよ！",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_01/hm_cast_01_16_15_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "でも、こんなに食べきれないよ。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_01/hm_cast_01_16_15_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "え？　なるちゃんなら大丈夫だよ！",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_01/hm_cast_01_16_15_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "じゃあわたし、リエンヌとさとちんにも渡してくるね！\nまたね！",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_01/hm_cast_01_16_15_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "あ、凛音さん……。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_01/hm_cast_01_16_15_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "……どうしよう。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_01/hm_cast_01_16_15_01_009.mp3"
        }
    ]
}