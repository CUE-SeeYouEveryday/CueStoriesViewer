{
    "Txt_Name": "HM_Cast_01_16_14_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "お疲れ様です。\nあの、この辺りで本を見ませんでしたか？",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_14_02/hm_cast_01_16_14_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "今日持ってきた本、\nどこかに置き忘れてしまって。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_14_02/hm_cast_01_16_14_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "本？　見てないなぁ。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "そうですか……、ありがとうございます。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_14_02/hm_cast_01_16_14_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "見つけたら知らせるよ。\nどんな本なの？",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "青いブックカバーで、これくらいの厚さの本です。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_14_02/hm_cast_01_16_14_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "……ダメ。やっぱりない。\n今日は帰ります。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_14_02/hm_cast_01_16_14_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん。自分のほうでも探しておくよ。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "すみません、ありがとうございます。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_14_02/hm_cast_01_16_14_02_009.mp3"
        }
    ]
}