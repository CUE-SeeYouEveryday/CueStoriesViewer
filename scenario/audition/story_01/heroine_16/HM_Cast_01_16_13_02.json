{
    "Txt_Name": "HM_Cast_01_16_13_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "利恵、帰ろう。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_13_02/hm_cast_01_16_13_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "うん！\nあ、ちょっと寄りたいところがあるんだけどいい？",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_13_02/hm_cast_01_16_13_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "うん。どこに行くの？",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_13_02/hm_cast_01_16_13_02_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "スーパーに、ヤクロトを買いにね。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_13_02/hm_cast_01_16_13_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "……この前、たくさん買ったでしょ。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_13_02/hm_cast_01_16_13_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "あれは我輩のやつ。\n今日買いたいのは、鳴の分。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_13_02/hm_cast_01_16_13_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "私の分？",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_13_02/hm_cast_01_16_13_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "ああ。ヤクロトはよいぞ？\n小瓶に込められた1億の魂が、精神と身体を浄化する。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_13_02/hm_cast_01_16_13_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "……鳴、最近朝早くてつらそうだから。\nってことで、行こう？",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_13_02/hm_cast_01_16_13_02_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "……うん、わかった。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_13_02/hm_cast_01_16_13_02_010.mp3"
        }
    ]
}