{
    "Txt_Name": "HM_Cast_01_01_04_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "お疲れ様です。",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_04_04/hm_cast_01_01_04_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様。\nあ。そういえばほのか、陽菜がとても感謝してたよ？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "感謝って、何をですか？",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_04_04/hm_cast_01_01_04_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ほら、この前、一緒に走りに行ったんでしょ？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ああ、あの日の……。",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_04_04/hm_cast_01_01_04_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "良かったです。わたしの狙い、ちゃんとあたって。",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_04_04/hm_cast_01_01_04_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "陽菜ならきっと一緒に悩みを解決出来るって思ってたから。",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_04_04/hm_cast_01_01_04_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ほら、舞花や志穂だったらきっと、\nわたしが誘っても一緒に走りに行ったりしないだろうし……。",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_04_04/hm_cast_01_01_04_04_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ふふっ。舞花も志穂も口ではああだけど、\nちゃんとほのかのこと認めてると思うよ？",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そうですかー？　えっへへ。",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_04_04/hm_cast_01_01_04_04_010.mp3"
        }
    ]
}