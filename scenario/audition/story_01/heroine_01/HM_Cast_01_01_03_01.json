{
    "Txt_Name": "HM_Cast_01_01_03_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "お疲れちゃん。\nん？　ジャーマネ……、ちょっとつかれた顔をしているな。",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_03_01/hm_cast_01_01_03_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？　志穂？　わかるの？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "それくらい見ればわかる。",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_03_01/hm_cast_01_01_03_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "やっぱり……。",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ん？　何かあったのか？",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_03_01/hm_cast_01_01_03_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "実はね、この前出張でちょっと古いホテルに泊まったんだけど、\nその夜、妙に身体が重くてずっと寝苦しくて……。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "今日までずっと……、なんだよね。\nこれってやっぱり、つかれてるのかな？",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ふむ……。\nなるほどな。",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_03_01/hm_cast_01_01_03_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ジャーマネ、今度いい物を持ってきてやる。つかれて\nしまったなら……、あれが効くとうちの父親が言っていた。",
            "voice": "audition/story_01/heroine_01/HM_Cast_01_01_03_01/hm_cast_01_01_03_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "志穂のお父さんが言うなら効きそうだね……。",
            "voice": ""
        }
    ]
}