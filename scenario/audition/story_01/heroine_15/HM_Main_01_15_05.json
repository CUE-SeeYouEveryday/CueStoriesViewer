{
    "Txt_Name": "HM_Main_01_15_05",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うわあ……。\nここが2次の会場なんですねー！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_05/hm_main_01_15_05_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "凛音、忘れ物はない？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はいっ。ばっちり、確認してきました！\n入り口も間違えません！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_05/hm_main_01_15_05_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ははは。そっか。\nでも……ちょっと、固いよね？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "え、そうですか！？",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_05/hm_main_01_15_05_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん。……不安な事でもある？",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "不安な事はないです！\nでも、緊張はしてます！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_05/hm_main_01_15_05_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "わたし、みんなに楽しんで欲しいです！\n監督さんも、スタッフさんも！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_05/hm_main_01_15_05_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どんなオーディションにだって、胸を張れるはずだよ。\nそれに見合う努力を、してきたんだから。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……はいっ。\nマネージャーさん、ひとつだけお願いしてもいいですか？",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_05/hm_main_01_15_05_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "なに？",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "一緒に気合を入れてほしいです！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_05/hm_main_01_15_05_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えっ？",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "思いっきりお願いします！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_05/hm_main_01_15_05_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？ここで？\n……わかった。",
            "voice": ""
        },
        {
            "index": 16,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "凛音！　オーディション頑張ろう！",
            "voice": ""
        },
        {
            "index": 17,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えい、えい、おー！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_05/hm_main_01_15_05_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "よーし、行ってきます！\nマネージャーさん！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_05/hm_main_01_15_05_018.mp3"
        }
    ]
}