{
    "Txt_Name": "HM_Main_01_15_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "レッスンお疲れ様、凛音。",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はいっ！　お疲れ様です！\n今日もいっぱい汗かきました！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_02/hm_main_01_15_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "調子良さそうだったね。\nすごく楽しそうだった。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はい！　すっごく楽しいです。",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_02/hm_main_01_15_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん。凛音の様子を見てると安心するよ。",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えへへ。\nわたし、みんなにも笑顔になってもらいたいんです！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_02/hm_main_01_15_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "もちろんテレビの前のみんなも、\n聞いてるスタッフのみなさんや、監督さんにも！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_02/hm_main_01_15_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "オーディションの時でも、\n聞いてて楽しいな〜って、思ってもらいたいんです！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_02/hm_main_01_15_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん、いい心がけだね。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "でも、みんなを楽しく出来るのかな？　って、\n思っちゃう時もあって。",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_02/hm_main_01_15_02_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "……緊張するって事？",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うーん……どうなんでしょう。\nわかんないです！　あはは。",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_02/hm_main_01_15_02_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "相談かあ……。なるほど！\nさとちん、まだ残ってるかなあ？　おーい！　さとちーん！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_02/hm_main_01_15_02_014.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "なるほど～！\n音楽を聴けば楽しくなりますもんね！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_02/hm_main_01_15_02_016.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "想像……むむむ。\n……なんか、いけそうな気がしてきました！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_02/hm_main_01_15_02_018.mp3"
        }
    ]
}