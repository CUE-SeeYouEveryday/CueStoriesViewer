{
    "Txt_Name": "HM_Main_01_15_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "おはよう、マネージャー。\nちょうど良かったわ。",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_01/hm_main_01_15_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "今度、凛音をこのオーディションに出してみようと思うの。",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_01/hm_main_01_15_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "「空繰戯曲グレイゼファー」……、ですか。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "そう、あの子にちょうど合うと思って。",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_01/hm_main_01_15_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "りおにもサポートするように言ってあるから、よろしくね。",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_01/hm_main_01_15_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "そういうこと！　頑張ってね！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_01/hm_main_01_15_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えっ！　オーディションですか！\n空繰戯曲グレイゼファー……。",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_01/hm_main_01_15_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……新しいオーディションかぁ。\nワクワクしますね！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_01/hm_main_01_15_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん、凛音にピッタリだと思うよ。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うれしいです！ \nありがとうございます！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_01/hm_main_01_15_01_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "まずは、2週間後の1次オーディションに向けて頑張ろう！",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はいっ。\nよろしくお願いします！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_01/hm_main_01_15_01_012.mp3"
        }
    ]
}