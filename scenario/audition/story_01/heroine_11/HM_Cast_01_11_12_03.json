{
    "Txt_Name": "HM_Cast_01_11_12_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "お疲れ様ー。\nあ、マネージャー、ちょっといい？",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_12_03/hm_cast_01_11_12_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様。\nどうしたの？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちょっとポラスタにアップする写真悩んでてさ。\nどっちも撮れ高はいいと思うんだよね。",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_12_03/hm_cast_01_11_12_03_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そうなんだ。両方使うとかダメなの？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "んー、それもアリだけど、今回はナシかな。\nで、どっちがいいと思う？",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_12_03/hm_cast_01_11_12_03_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えーっと……あ、莉子との写真なんだね。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "そ。なんだかんだ莉子との写真って受けがいいからさ。\nつい厳選しちゃうんだよね。",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_12_03/hm_cast_01_11_12_03_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そっかー……んー、自分は前の方かな。\nこっちのほうが莉子も自然っぽいし。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ん……そうだね。\n後の方も莉子らしいけど、ちょっとぎこちないよね。",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_12_03/hm_cast_01_11_12_03_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ありがと、マネージャー。\n参考になったよ。",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_12_03/hm_cast_01_11_12_03_010.mp3"
        }
    ]
}