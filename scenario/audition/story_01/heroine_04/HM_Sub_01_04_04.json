{
    "Txt_Name": "HM_Sub_01_04_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "「グレイゼファー」の台本を読み込んでるんですけど、\nすごく面白いですよね。",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_04/hm_sub_01_04_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "やっぱりエリシュを助け出す男の子たちが最高にかっこいい！\nこの子たちは、エリシュのヒーローですよね！",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_04/hm_sub_01_04_04_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "現実だとこんな風に閉じ込められるとかはないけど……、",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_04/hm_sub_01_04_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "落ち込んじゃったりとか、\nモヤモヤした気持ちを抱えたりとかあると思うんです。",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_04/hm_sub_01_04_04_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そんな人たちに元気を与えられるヒーローに、\nわたしはなりたいです！",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_04/hm_sub_01_04_04_005.mp3"
        }
    ]
}