{
    "Txt_Name": "HM_Cast_01_04_03_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ジャーマネ。\nお疲れちゃん。",
            "voice": "audition/story_01/heroine_04/HM_Cast_01_04_03_02/hm_cast_01_04_03_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "志穂、最近レッスンの方ははかどってる？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "当ててみろ。\n正解したらハワイ旅行をプレゼントしよう。",
            "voice": "audition/story_01/heroine_04/HM_Cast_01_04_03_02/hm_cast_01_04_03_02_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "……その様子なら大丈夫そうだね。\n悩んでるんだったら、いつでも相談して？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "うむ、考えておこう。",
            "voice": "audition/story_01/heroine_04/HM_Cast_01_04_03_02/hm_cast_01_04_03_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "お疲れ様でーす。",
            "voice": "audition/story_01/heroine_04/HM_Cast_01_04_03_02/hm_cast_01_04_03_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "誰だ？",
            "voice": "audition/story_01/heroine_04/HM_Cast_01_04_03_02/hm_cast_01_04_03_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "……え？\nわたしだけど。",
            "voice": "audition/story_01/heroine_04/HM_Cast_01_04_03_02/hm_cast_01_04_03_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "はあ……わかってないな。\nほのかにはガッカリだ。",
            "voice": "audition/story_01/heroine_04/HM_Cast_01_04_03_02/hm_cast_01_04_03_02_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "……何が？",
            "voice": "audition/story_01/heroine_04/HM_Cast_01_04_03_02/hm_cast_01_04_03_02_010.mp3"
        }
    ]
}