{
    "Txt_Name": "HM_Sub_01_04_10",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "お疲れ様です、マネージャー。\n今、ちょっといいですか？",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_10/hm_sub_01_04_10_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "えっと、このセリフがちょっと言いにくくて、\nここだけ甘噛みしちゃってる気がするんです。",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_10/hm_sub_01_04_10_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "でも、ここは大事なセリフだし、\n自然に言えるようにならないといけなくて。",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_10/hm_sub_01_04_10_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "だから、ちょっと付き合ってほしいです。\nお願いします、マネージャー。",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_10/hm_sub_01_04_10_004.mp3"
        }
    ]
}