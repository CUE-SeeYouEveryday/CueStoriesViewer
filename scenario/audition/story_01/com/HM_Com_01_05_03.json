{
    "Txt_Name": "HM_Com_01_05_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "マネージャーいる！？　",
            "voice": "audition/story_01/com/HM_Com_01_05_03/hm_com_01_05_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "はいはい。今日はなんの用かな？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そんなの決まってるじゃん。\nちょっと感想聞きたいんだよね。",
            "voice": "audition/story_01/com/HM_Com_01_05_03/hm_com_01_05_03_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ダイバリオン、どうだった？　",
            "voice": "audition/story_01/com/HM_Com_01_05_03/hm_com_01_05_03_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ああ、ダイバリオンね。結論から言うと……。\nすごく良かった。",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "でしょ？　どこが良かった？",
            "voice": "audition/story_01/com/HM_Com_01_05_03/hm_com_01_05_03_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あの10話で、ほら、主人公の幼馴染が……。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あ、悠希。長くなるから帰りながら話そうか。\nオフィスももう閉める時間だし。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "うん。そうだね。\nで、10話のどこが良かった？",
            "voice": "audition/story_01/com/HM_Com_01_05_03/hm_com_01_05_03_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いやあ、まさかあそこで、\n幼馴染の記憶がなくなるとは思わないよね……。",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "お、マネージャー、わかってんねー！",
            "voice": "audition/story_01/com/HM_Com_01_05_03/hm_com_01_05_03_011.mp3"
        }
    ]
}