{
    "Txt_Name": "HM_Com_01_02_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "お疲れしたー。……さてと。",
            "voice": "audition/story_01/com/HM_Com_01_02_03/hm_com_01_02_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "お疲れ様でしたー。……よし。",
            "voice": "audition/story_01/com/HM_Com_01_02_03/hm_com_01_02_03_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様……。\nって、レッスン終わったのに、なんで屈伸とかしてるの？",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "リベンジです。",
            "voice": "audition/story_01/com/HM_Com_01_02_03/hm_com_01_02_03_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "へへ、今日、レッスン前に競争してきたんすけど、\n自分の圧勝だったんっすよ。",
            "voice": "audition/story_01/com/HM_Com_01_02_03/hm_com_01_02_03_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ほのかは素直だからなー。\n信号がない横断歩道とか、人通りが少ないとこ通らないと。",
            "voice": "audition/story_01/com/HM_Com_01_02_03/hm_com_01_02_03_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そうだったんだ……。\nだから、今日の舞花はあんなに早くに来たんだね。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "まぁ、自分が本気出せば、こんなところっすよ。",
            "voice": "audition/story_01/com/HM_Com_01_02_03/hm_com_01_02_03_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "次は負けないんだからね！",
            "voice": "audition/story_01/com/HM_Com_01_02_03/hm_com_01_02_03_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あはは……。まぁ、気を付けてね。\nお疲れ様、2人とも。",
            "voice": ""
        }
    ]
}