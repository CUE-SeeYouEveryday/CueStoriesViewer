{
    "Txt_Name": "HM_Com_01_12_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "おーい、莉子、帰るよ。　",
            "voice": "audition/story_01/com/HM_Com_01_12_03/hm_com_01_12_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ、ちょっと待って。\nマネージャー、はいこれ。",
            "voice": "audition/story_01/com/HM_Com_01_12_03/hm_com_01_12_03_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？　\nこれ……、クッキー？",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そ。昼間寄ったお店でおいしそうだから買ってきたんだよね。",
            "voice": "audition/story_01/com/HM_Com_01_12_03/hm_com_01_12_03_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "マネージャー、いっつも仕事抱えてて大変そうだからね。\n甘いものでも食べてってこと。",
            "voice": "audition/story_01/com/HM_Com_01_12_03/hm_com_01_12_03_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あんた、そういう気のつかい方する時あるよね。　",
            "voice": "audition/story_01/com/HM_Com_01_12_03/hm_com_01_12_03_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まー、なんだかんだ、\nあたしたちのために頑張ってくれてる訳だしねー。",
            "voice": "audition/story_01/com/HM_Com_01_12_03/hm_com_01_12_03_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "じゃ、そういう訳でお疲れ様でしたー。\nねえ、まほろ、このあとどっか寄ってく？",
            "voice": "audition/story_01/com/HM_Com_01_12_03/hm_com_01_12_03_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いやー、今日はもう帰ろう。　",
            "voice": "audition/story_01/com/HM_Com_01_12_03/hm_com_01_12_03_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうだねー。　",
            "voice": "audition/story_01/com/HM_Com_01_12_03/hm_com_01_12_03_010.mp3"
        }
    ]
}