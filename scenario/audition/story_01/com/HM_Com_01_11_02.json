{
    "Txt_Name": "HM_Com_01_11_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あ、まほろ、お疲れ様。　",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "んー、たまにはマネージャーも……。\nよし。マネージャーちょっとこっち来て。",
            "voice": "audition/story_01/com/HM_Com_01_11_02/hm_com_01_11_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？　何？　ちょ、ちょっと、まほろ？",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ほら、いいからいいから。\nはい、３・２・１。",
            "voice": "audition/story_01/com/HM_Com_01_11_02/hm_com_01_11_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ん。いいじゃん。\nほら、マネージャーいい顔してる。",
            "voice": "audition/story_01/com/HM_Com_01_11_02/hm_com_01_11_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "これなら、マネージャーにもファンがつくんじゃない？　",
            "voice": "audition/story_01/com/HM_Com_01_11_02/hm_com_01_11_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いやいや、自分はそういうのは……。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "何言ってんの。\n今は裏方スタッフにファンがつくことだってあるんだから。",
            "voice": "audition/story_01/com/HM_Com_01_11_02/hm_com_01_11_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "マネージャーの好感度次第で、\n回り回ってタレントの人気に繋がるわけ。わかる？",
            "voice": "audition/story_01/com/HM_Com_01_11_02/hm_com_01_11_02_009.mp3"
        }
    ]
}