{
    "Txt_Name": "HM_Com_01_04_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "お疲れ様でした！　",
            "voice": "audition/story_01/com/HM_Com_01_04_01/hm_com_01_04_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様。\n今日も元気いっぱいだったね。",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "はい！\n毎日ちゃんと体調管理してますから！",
            "voice": "audition/story_01/com/HM_Com_01_04_01/hm_com_01_04_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うんうん。ほのかにとっては、当たり前のことだったね。",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "えへへ。\nあ！　あと、レッスン前に走ってきたせいかもしれないですね。",
            "voice": "audition/story_01/com/HM_Com_01_04_01/hm_com_01_04_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "わたし、なるべく走ってきてるんですよ。\nレッスン前のウォーミングアップになりますから！",
            "voice": "audition/story_01/com/HM_Com_01_04_01/hm_com_01_04_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そうなんだ。\nでも、制服のままって走りづらくないの？",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "靴はちゃんと履き替えてますよ。\nローファーのままじゃ、走りづらいですから。",
            "voice": "audition/story_01/com/HM_Com_01_04_01/hm_com_01_04_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "へぇ……。じゃあ、ランニングシューズも持ち歩いてるんだ？　",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "はい！　いつでも走れるように、準備万端ですよ！　",
            "voice": "audition/story_01/com/HM_Com_01_04_01/hm_com_01_04_01_010.mp3"
        }
    ]
}