{
    "Txt_Name": "HM_Com_01_03_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えーっと、志穂？\n今日もまだ帰らないの？",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "まぁまぁ。\nたまにはいいだろ。",
            "voice": "audition/story_01/com/HM_Com_01_03_02/hm_com_01_03_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いや、ここ最近そうなんだけど……。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "お疲れしたー。",
            "voice": "audition/story_01/com/HM_Com_01_03_02/hm_com_01_03_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様、舞花。\nあ、ちゃんと課題やるんだよー。",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "わかってますよー。\n失礼しまーす。",
            "voice": "audition/story_01/com/HM_Com_01_03_02/hm_com_01_03_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……ふむ。今日はこのまま真っ直ぐのはずだな……。",
            "voice": "audition/story_01/com/HM_Com_01_03_02/hm_com_01_03_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ん？　何か言った？",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "私も帰る、って言った。\nじゃあ、またな、ジャーマネ。",
            "voice": "audition/story_01/com/HM_Com_01_03_02/hm_com_01_03_02_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様……。\nうーん、もしかして、志穂って……。",
            "voice": ""
        }
    ]
}