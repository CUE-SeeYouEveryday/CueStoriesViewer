{
    "Txt_Name": "HM_Com_01_12_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あー、莉子、ちょうど良かった。\nちょっといい？",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "えー、あたしもう帰るとこなんですけど。　",
            "voice": "audition/story_01/com/HM_Com_01_12_02/hm_com_01_12_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "まあまあ、ちょっとだけ。\nこれ、ちょっと見てくれる？",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "もう、しょうがないなあ。何？　",
            "voice": "audition/story_01/com/HM_Com_01_12_02/hm_com_01_12_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "宣材写真なんだけどさ、\nこっちとこっち……、どっちがいいと思う？",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あー、うーん、そうだねー。",
            "voice": "audition/story_01/com/HM_Com_01_12_02/hm_com_01_12_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたし的にはこっちかな。",
            "voice": "audition/story_01/com/HM_Com_01_12_02/hm_com_01_12_02_008.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ふむ。なるほどね。よし、じゃあ、こっちで行こう。　",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "っていうか、なんであたしに聞くわけ？　",
            "voice": "audition/story_01/com/HM_Com_01_12_02/hm_com_01_12_02_010.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "だって莉子のセンスって、とても参考になるからね。　",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "いやいや、それほどでも……。\nって、褒めればいいと思ってない？",
            "voice": "audition/story_01/com/HM_Com_01_12_02/hm_com_01_12_02_012.mp3"
        }
    ]
}