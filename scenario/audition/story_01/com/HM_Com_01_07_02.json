{
    "Txt_Name": "HM_Com_01_07_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "お疲れ様です。",
            "voice": "audition/story_01/com/HM_Com_01_07_02/hm_com_01_07_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あいり、お疲れ様。\nどうしたの？　今日……。",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "レッスン室、随分激しいことしてなかった？\nなんかここまで聞こえてきたよ。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "え？　本当ですか？\nえっと、その……、ちょっと悠希ちゃんがその……。",
            "voice": "audition/story_01/com/HM_Com_01_07_02/hm_com_01_07_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？　悠希がまた何かやったの？",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "いえ、そういうわけじゃないんですけど。\nただちょっと天井に……。",
            "voice": "audition/story_01/com/HM_Com_01_07_02/hm_com_01_07_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？　天井？　壊したとかじゃないよね？",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あ、壊したりとかじゃないんですけど。\nちょっとだけ、悠希ちゃんが……。",
            "voice": "audition/story_01/com/HM_Com_01_07_02/hm_com_01_07_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ふふっ。\nまあ、あいりが楽しそうだからいいか。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はい。毎日すごく楽しいです！",
            "voice": "audition/story_01/com/HM_Com_01_07_02/hm_com_01_07_02_010.mp3"
        }
    ]
}