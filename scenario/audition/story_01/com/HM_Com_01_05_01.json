{
    "Txt_Name": "HM_Com_01_05_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "お疲れ様でしたー！　",
            "voice": "audition/story_01/com/HM_Com_01_05_01/hm_com_01_05_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どうしたのそれ？　すごい大きな紙袋だね。　",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ああ、これ？\nまあ、ちょっと今日、衝動買いしちゃったんだよね。",
            "voice": "audition/story_01/com/HM_Com_01_05_01/hm_com_01_05_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "へえ、何を買ったの？　",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "プラモ。　",
            "voice": "audition/story_01/com/HM_Com_01_05_01/hm_com_01_05_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？　また買ったの？　",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そうそう。っていうか、またって何？\n同じ物買ってるわけじゃないんだけど。",
            "voice": "audition/story_01/com/HM_Com_01_05_01/hm_com_01_05_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そういえばマネージャー、ダイバリオン見たことある？\n見たことあるなら、この気持ちわかるはずなんだけどな。",
            "voice": "audition/story_01/com/HM_Com_01_05_01/hm_com_01_05_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ごめん。見たことはないかな。　",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "はあ、もう、しょうがないなー。\n今度ディスク貸すからさ、全部見てよね！",
            "voice": "audition/story_01/com/HM_Com_01_05_01/hm_com_01_05_01_010.mp3"
        }
    ]
}