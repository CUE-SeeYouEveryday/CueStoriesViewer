{
    "Txt_Name": "HM_Cast_01_05_07_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "お疲れ様です、\nマネージャーさん。",
            "voice": "audition/story_01/heroine_05/HM_Cast_01_05_07_03/hm_cast_01_05_07_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あいり、お疲れ様。\nあれ、そのストラップ、どうしたの？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えへへ。\n実はこれ、悠希ちゃんに貰ったんです。",
            "voice": "audition/story_01/heroine_05/HM_Cast_01_05_07_03/hm_cast_01_05_07_03_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "前に事務所に忘れていった時、すぐ渡しに行ったんですけど。\n悠希ちゃん、もう新しい物を付けちゃったみたいで。",
            "voice": "audition/story_01/heroine_05/HM_Cast_01_05_07_03/hm_cast_01_05_07_03_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "こいつのことはあいりに託すから、かわいがってやってくれ。\nって言われちゃいました。",
            "voice": "audition/story_01/heroine_05/HM_Cast_01_05_07_03/hm_cast_01_05_07_03_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そっか。でも今日悠希を見たけど、\n同じ物付けてなかった？",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はい！\nでも、お揃いだねって言ったら、怒られちゃいました。",
            "voice": "audition/story_01/heroine_05/HM_Cast_01_05_07_03/hm_cast_01_05_07_03_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "悠希ちゃんが言うには、\n細かいカラーリングが違うみたいなんです。",
            "voice": "audition/story_01/heroine_05/HM_Cast_01_05_07_03/hm_cast_01_05_07_03_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そうなんだ。\nでも、仲が良さそうでいいね、そういうの。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えへへ。\nありがとうございます、マネージャーさん。",
            "voice": "audition/story_01/heroine_05/HM_Cast_01_05_07_03/hm_cast_01_05_07_03_010.mp3"
        }
    ]
}