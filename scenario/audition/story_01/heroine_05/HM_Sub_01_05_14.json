    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "うー、お腹空いたー。\n天童家のおいなりさんが食べたい……。",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_14/hm_sub_01_05_14_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あれがあたしの勝負めしだからね。\nやっぱ気合を入れる時には、絶対欠かせないよ！",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_14/hm_sub_01_05_14_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "寮に来てからは自分で作ってるけど、\nたまには、親父の作ったおいなりさんが食べたくなるんだよなー。",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_14/hm_sub_01_05_14_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "マネージャーも今度ウチの店に来なよ！\nひっくり返るくらいおいしいからさ！",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_14/hm_sub_01_05_14_004.mp3"
        }
    ]
}