{
    "Txt_Name": "HM_Sub_01_05_06",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あのさ、マネージャー。\n大事な話なんだけど。",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_06/hm_sub_01_05_06_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あたしはダイバリオンみたいな、\n人型のロボが大好きなんだけどさ。",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_06/hm_sub_01_05_06_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "でも、戦闘機とか戦艦とか、そういう渋いメカとかも、\n等しく愛してるんだよね。",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_06/hm_sub_01_05_06_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "リアリティや機能性、ロマンとか、\n要素はいっぱいあるけどさ。",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_06/hm_sub_01_05_06_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "なんにしても製作者の熱い思いは伝わってくるから、\nやっぱすごいよね！",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_06/hm_sub_01_05_06_005.mp3"
        }
    ]
}