{
    "Txt_Name": "HM_Sub_01_05_24",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "マネージャー！　見て見てこれ！",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_24/hm_sub_01_05_24_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "今朝、コンビニでプラモ買ってきたんだ。\n小さいお菓子が付いてるやつ！",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_24/hm_sub_01_05_24_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "でもあれって、\n付いてる小さいラムネとかガムが本体なんだって！",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_24/hm_sub_01_05_24_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "どう見たっておもちゃが本体なのにね。あはは。",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_24/hm_sub_01_05_24_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "いわゆる大人の事情ってやつだよ。\nでも、どういう事情なんだろ？",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_24/hm_sub_01_05_24_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "まいっか。\n細かいことは気にしてもしょうがないもんね！",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_24/hm_sub_01_05_24_006.mp3"
        }
    ]
}