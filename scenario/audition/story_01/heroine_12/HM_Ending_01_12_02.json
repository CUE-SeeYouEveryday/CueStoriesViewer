{
    "Txt_Name": "HM_Ending_01_12_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "莉子、合格おめでとう！",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うん、ありがとうマネージャー！\nもうめちゃくちゃ嬉しい！",
            "voice": "audition/story_01/heroine_12/HM_Ending_01_12_02/hm_ending_01_12_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "自信はあったんだけど、やっぱりドキドキしたー！\n受かってよかったよー！",
            "voice": "audition/story_01/heroine_12/HM_Ending_01_12_02/hm_ending_01_12_02_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "莉子が頑張ったからだよ！\n報われて本当に良かった。",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "えへへ、うんそうだね！\nでも改めて言われるとちょっと照れちゃうな。",
            "voice": "audition/story_01/heroine_12/HM_Ending_01_12_02/hm_ending_01_12_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "はー。でも本っ当にみんなにかっこ悪いところ見せなくて済むし、\nアニメにも出られるし、嬉しいな！",
            "voice": "audition/story_01/heroine_12/HM_Ending_01_12_02/hm_ending_01_12_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そうだね。\nきっとみんなも喜んでくれるよ！",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うん！　そうだと思う！",
            "voice": "audition/story_01/heroine_12/HM_Ending_01_12_02/hm_ending_01_12_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "本番まで、ますます忙しくしちゃうけど……。\nこれからもよろしくね、マネージャー！",
            "voice": "audition/story_01/heroine_12/HM_Ending_01_12_02/hm_ending_01_12_02_009.mp3"
        }
    ]
}