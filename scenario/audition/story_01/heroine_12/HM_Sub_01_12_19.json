    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "お疲れ様、マネージャー。",
            "voice": "audition/story_01/heroine_12/HM_Sub_01_12_19/hm_sub_01_12_19_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様。\n今日のレッスンはどうだった？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "んー、アテレコのレッスンだったんだけどさ。\n毎回どのマイクに入ろうかなって、ちょっと不安になるんだよね。",
            "voice": "audition/story_01/heroine_12/HM_Sub_01_12_19/hm_sub_01_12_19_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "レッスンはチームでやってるから、慣れてきたけど……。\n実際の現場だと、そういうわけにはいかないじゃん？",
            "voice": "audition/story_01/heroine_12/HM_Sub_01_12_19/hm_sub_01_12_19_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "緊張でミスとかしたらどうしよーって考えちゃうんだよね。",
            "voice": "audition/story_01/heroine_12/HM_Sub_01_12_19/hm_sub_01_12_19_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "でもまぁ、慣れていくしかないかー。\nよし、頑張ろ！",
            "voice": "audition/story_01/heroine_12/HM_Sub_01_12_19/hm_sub_01_12_19_006.mp3"
        }
    ]
}