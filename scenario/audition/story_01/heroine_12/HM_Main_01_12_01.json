{
    "Txt_Name": "HM_Main_01_12_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "おはよう、マネージャー。\nちょうど良かったわ。",
            "voice": "audition/story_01/heroine_12/HM_Main_01_12_01/hm_main_01_12_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "今度、莉子をこのオーディションに出してみようと思うの。",
            "voice": "audition/story_01/heroine_12/HM_Main_01_12_01/hm_main_01_12_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "「空繰戯曲グレイゼファー」……、ですか。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "そう、あの子にちょうど合うと思って。",
            "voice": "audition/story_01/heroine_12/HM_Main_01_12_01/hm_main_01_12_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "りおにもサポートするように言ってあるから、よろしくね。",
            "voice": "audition/story_01/heroine_12/HM_Main_01_12_01/hm_main_01_12_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "そういうこと！\n頑張ってね！",
            "voice": "audition/story_01/heroine_12/HM_Main_01_12_01/hm_main_01_12_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "え、あたしに？",
            "voice": "audition/story_01/heroine_12/HM_Main_01_12_01/hm_main_01_12_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そう。\nどう、やってみる？",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "もちろん！\nこんなチャンス、見逃すわけないじゃん！",
            "voice": "audition/story_01/heroine_12/HM_Main_01_12_01/hm_main_01_12_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "1次審査は2週間後かぁ……。\nよし、まずは作品を研究しないと！",
            "voice": "audition/story_01/heroine_12/HM_Main_01_12_01/hm_main_01_12_01_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "資料は用意してあるから。\nはい、これ。",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "お、さっすがー。\nありがと、マネージャー！",
            "voice": "audition/story_01/heroine_12/HM_Main_01_12_01/hm_main_01_12_01_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "よーし、頑張るぞー！",
            "voice": "audition/story_01/heroine_12/HM_Main_01_12_01/hm_main_01_12_01_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん、頑張っていこう。",
            "voice": ""
        }
    ]
}