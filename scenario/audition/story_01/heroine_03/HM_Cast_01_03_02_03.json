    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "お疲れ様でーす……って、あれ？\nマネージャー、志穂見てないっすか？",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_03/hm_cast_01_03_02_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ、舞花。\nあぁ、志穂ならそこに……。",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_03/hm_cast_01_03_02_03_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "え、寝てんの？",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_03/hm_cast_01_03_02_03_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん。さっきまでは起きてたんだけどね。\n疲れてるのかも。",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "そうすっか……。\nうーん、待たせちゃったかな。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_03/hm_cast_01_03_02_03_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "でも、こうやって寝てる志穂って、なんかかわいいっすよね。\nへへ、家のこと思い出すっていうか。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_03/hm_cast_01_03_02_03_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……起きててもかわいいだろ。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_03/hm_cast_01_03_02_03_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "うわあっ！？　びっくりしたぁ……。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_03/hm_cast_01_03_02_03_009.mp3"
        }
    ]
}