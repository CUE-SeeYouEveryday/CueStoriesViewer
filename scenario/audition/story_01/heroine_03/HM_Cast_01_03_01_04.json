{
    "Txt_Name": "HM_Cast_01_03_01_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "お疲れー。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_04/hm_cast_01_03_01_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "お疲れ様です。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_04/hm_cast_01_03_01_04_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ねぇ、志穂ちゃん、今度のお休みなんだけど、\n一緒にお買い物いかない？　天気もいいらしいよ。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_04/hm_cast_01_03_01_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "休みの日は寝るつもり。\nもちろん二度寝も譲れない。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_04/hm_cast_01_03_01_04_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "そっか……。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_04/hm_cast_01_03_01_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "お昼くらいからならいい。\nランチでもするか。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_04/hm_cast_01_03_01_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "本当？　じゃあ、その後、お茶もしよう。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_04/hm_cast_01_03_01_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "うむ。次のオフの予定は空けておこう。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_04/hm_cast_01_03_01_04_008.mp3"
        }
    ]
}