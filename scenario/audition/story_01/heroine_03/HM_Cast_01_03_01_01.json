{
    "Txt_Name": "HM_Cast_01_03_01_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "お疲れ様です、マネージャーさん。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_01/hm_cast_01_03_01_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様、陽菜。",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ちょっとここにいていいですか？",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_01/hm_cast_01_03_01_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん、いいよ。\n誰か待ってるの？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "はい。志穂ちゃんが夕飯の買い物、手伝ってくれるって。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_01/hm_cast_01_03_01_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "重いものばかりだから大変だろう、って。\n優しいですよね。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_01/hm_cast_01_03_01_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "でも、そう言うと「ただの気まぐれ。」って。\n言われちゃうんです。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_01/hm_cast_01_03_01_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "かわいいなあ、ふふっ。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_01/hm_cast_01_03_01_01_008.mp3"
        }
    ]
}