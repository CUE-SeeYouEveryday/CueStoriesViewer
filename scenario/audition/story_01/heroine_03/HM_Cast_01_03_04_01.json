{
    "Txt_Name": "HM_Cast_01_03_04_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "はぁ、お疲れ……。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_04_01/hm_cast_01_03_04_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うわ、なにその汗！\nだ、大丈夫？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "今日のダンスレッスン……。\n地獄……だった。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_04_01/hm_cast_01_03_04_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "お疲れ様でーす！\nあ、いたいた。志穂……お水いる？",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_04_01/hm_cast_01_03_04_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "いる。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_04_01/hm_cast_01_03_04_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "じゃあ、これあげる！\n勢いよく飲むとお腹痛くなるからゆっくりね？",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_04_01/hm_cast_01_03_04_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ありがとう……んぐんぐ……。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_04_01/hm_cast_01_03_04_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "はぁ……死ぬかと思った。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_04_01/hm_cast_01_03_04_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あはは、大袈裟だよ。\nまぁ、志穂は体力ないからね。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_04_01/hm_cast_01_03_04_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "お前がありすぎるだけだ……。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_04_01/hm_cast_01_03_04_01_010.mp3"
        }
    ]
}