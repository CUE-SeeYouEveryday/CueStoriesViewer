{
    "Txt_Name": "HM_Cast_01_06_07_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "マネージャーさん、お疲れ様です。\nあの、聞いてください！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_03/hm_cast_01_06_07_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "お弁当の卵焼き、今日はすっごく上手く焼けたんです！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_03/hm_cast_01_06_07_03_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お、そうなんだ。\n回数を重ねた成果かな？",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はい！　焦げてるとこもあんまり無くて、\nふわふわに出来ました！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_03/hm_cast_01_06_07_03_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "お疲れ様です。\nもしかして、卵焼きの話ですか？",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_03/hm_cast_01_06_07_03_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "千紗ちゃん！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_03/hm_cast_01_06_07_03_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん。\n卵焼き、おいしかった？",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "はい！\nすごくふわふわで、見た目もキレイでした。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_03/hm_cast_01_06_07_03_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "良かったね、あいり。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えへへ……、はい！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_03/hm_cast_01_06_07_03_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あいり、今日のお弁当もおいしかったよ。\nありがとう。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_03/hm_cast_01_06_07_03_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ううん……！\nあいりのほうこそ、いつもありがとう、千紗ちゃん！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_03/hm_cast_01_06_07_03_012.mp3"
        }
    ]
}