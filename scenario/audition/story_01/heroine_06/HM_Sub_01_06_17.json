    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "まほろとは、屋上で会うことが多いんです。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_17/hm_sub_01_06_17_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "野菜の世話しながら、雑談したり……。\n水やりもたまに手伝ってくれるんです。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_17/hm_sub_01_06_17_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あと、台風とか、雪の予報が出てると、\n前もって私に教えてくれるんですよ。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_17/hm_sub_01_06_17_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "「週末は台風らしいよ」ってメッセージが来たりとか。\nふふっ、ちょっと意外ですよね。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_17/hm_sub_01_06_17_004.mp3"
        }
    ]
}