{
    "Txt_Name": "HM_Cast_01_06_07_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "マネージャーさん、お疲れ様です。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_02/hm_cast_01_06_07_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様、あいり。\n悠希から聞いたよ。最近、あいりもお弁当作ってるって。",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あ、そうなんです。あいりたちのお弁当のために、\nいつも早起きしなくちゃいけないから……。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_02/hm_cast_01_06_07_02_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "たまには、千紗ちゃんにたくさん寝てほしくて。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_02/hm_cast_01_06_07_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "なるほど。千紗も助かってると思うよ。",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えへへ、そうだといいなぁ。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_02/hm_cast_01_06_07_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "卵焼きとかも、\nあいりは千紗ちゃんほど上手く焼けないんです。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_02/hm_cast_01_06_07_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "でも、みんな、おいしいって言ってくれました。\n明日も、おいしく作れるように頑張ります！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_02/hm_cast_01_06_07_02_008.mp3"
        }
    ]
}