{
    "Txt_Name": "HM_Main_01_06_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "おはよう、マネージャー。\nちょうど良かったわ。",
            "voice": "audition/story_01/heroine_06/HM_Main_01_06_01/hm_main_01_06_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "今度、千紗をこのオーディションに出してみようと思うの。",
            "voice": "audition/story_01/heroine_06/HM_Main_01_06_01/hm_main_01_06_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "「空繰戯曲グレイゼファー」……、ですか。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "そう、あの子にちょうど合うと思って。",
            "voice": "audition/story_01/heroine_06/HM_Main_01_06_01/hm_main_01_06_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "りおにもサポートするように言ってあるから、よろしくね。",
            "voice": "audition/story_01/heroine_06/HM_Main_01_06_01/hm_main_01_06_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "そういうこと！　頑張ってね！",
            "voice": "audition/story_01/heroine_06/HM_Main_01_06_01/hm_main_01_06_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "え！　新しいオーディション！？",
            "voice": "audition/story_01/heroine_06/HM_Main_01_06_01/hm_main_01_06_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "『空繰戯曲グレイゼファー』……。\nどんなお話なんですか？",
            "voice": "audition/story_01/heroine_06/HM_Main_01_06_01/hm_main_01_06_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "きっと千紗も気に入ると思うよ。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "本当ですか？　ふふっ、楽しみ。\n寮に帰ったら、すぐに読んでみます！",
            "voice": "audition/story_01/heroine_06/HM_Main_01_06_01/hm_main_01_06_01_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "せっかく貰えたチャンス、絶対に掴んでみせますから！",
            "voice": "audition/story_01/heroine_06/HM_Main_01_06_01/hm_main_01_06_01_011.mp3"
        }
    ]
}