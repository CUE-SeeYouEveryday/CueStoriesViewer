{
    "Txt_Name": "HM_Cast_01_06_07_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "お疲れ様です、マネージャーさん。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_01/hm_cast_01_06_07_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様、あいり。\nそういえば、昨日言ってたテストって大丈夫だったの？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はい、大丈夫でした！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_01/hm_cast_01_06_07_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "さすが、あいりだね。",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "そんなことないです。\nそれに頑張れたのは、千紗ちゃんのおかげなんですよ。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_01/hm_cast_01_06_07_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "今日のお弁当に、\n「テスト頑張ってね」っていうメモが入ってたんです。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_01/hm_cast_01_06_07_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "千紗ちゃん、いつも忙しいのに、\nあいりのテストのことまで気にしてくれるなんて……。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_01/hm_cast_01_06_07_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "本当に優しいなぁ。\nあいりも見習いたいです。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_01/hm_cast_01_06_07_01_008.mp3"
        }
    ]
}