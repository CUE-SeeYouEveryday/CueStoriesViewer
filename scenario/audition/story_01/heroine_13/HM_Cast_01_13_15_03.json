{
    "Txt_Name": "HM_Cast_01_13_15_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "リエンヌー！\nお疲れ様ー！",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_03/hm_cast_01_13_15_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "あ、凛音。お疲れ様。\nこれから帰り？",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_03/hm_cast_01_13_15_03_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！　リエンヌはー？",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_03/hm_cast_01_13_15_03_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "我輩も。\nこの後は何もないから。",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_03/hm_cast_01_13_15_03_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そっかー。じゃあ帰る前に、\nこの前教えてくれた、ピクニックスポット行こうよ！",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_03/hm_cast_01_13_15_03_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "うん、いいよ。\nフッ、夜の散歩も、中々乙なものだからな。",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_03/hm_cast_01_13_15_03_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん！　今日はお月様も綺麗だし。\nきっと、いいことあるよ！",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_03/hm_cast_01_13_15_03_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "いいこと……？\n……ふふ。確かに、そうかもね。",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_03/hm_cast_01_13_15_03_008.mp3"
        }
    ]
}