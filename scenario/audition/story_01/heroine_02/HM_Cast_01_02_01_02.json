{
    "Txt_Name": "HM_Cast_01_02_01_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "お疲れっすー！",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_02/hm_cast_01_02_01_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "お疲れ様です。",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_02/hm_cast_01_02_01_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "マネージャー！\n見てくださいよ、これ！",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_02/hm_cast_01_02_01_02_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "じゃーん！　ハコダテ一番  すき焼き味！\nコンビニ限定のやつなんすよ。",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_02/hm_cast_01_02_01_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "いやー。どこも売り切れって聞いてたんで、\n買えてよかったっす。",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_02/hm_cast_01_02_01_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "うふふ、よかったね。",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_02/hm_cast_01_02_01_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "陽菜も食べてみる？",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_02/hm_cast_01_02_01_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あ、うん。\nでも、すき焼き味のラーメンっておいしいのかな？",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_02/hm_cast_01_02_01_02_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "おいしいおいしい！\n帰ったらリビング集合ね。",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_02/hm_cast_01_02_01_02_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ふふっ、はーい。",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_02/hm_cast_01_02_01_02_011.mp3"
        }
    ]
}