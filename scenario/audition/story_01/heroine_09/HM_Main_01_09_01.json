{
    "Txt_Name": "HM_Main_01_09_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "おはよう、マネージャー。\nちょうど良かったわ。",
            "voice": "audition/story_01/heroine_09/HM_Main_01_09_01/hm_main_01_09_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "今度、美晴をこのオーディションに出してみようと思うの。",
            "voice": "audition/story_01/heroine_09/HM_Main_01_09_01/hm_main_01_09_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "「空繰戯曲グレイゼファー」……、ですか。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "そう、あの子にちょうど合うと思って。",
            "voice": "audition/story_01/heroine_09/HM_Main_01_09_01/hm_main_01_09_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "りおにもサポートするように言ってあるから、よろしくね。",
            "voice": "audition/story_01/heroine_09/HM_Main_01_09_01/hm_main_01_09_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "そういうこと！　頑張ってね！",
            "voice": "audition/story_01/heroine_09/HM_Main_01_09_01/hm_main_01_09_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "新しいアニメ……、ですか？",
            "voice": "audition/story_01/heroine_09/HM_Main_01_09_01/hm_main_01_09_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん、社長から連絡があってね。\n今まで受けたものと、また違った作品だよ。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ、また新しい世界に出会えそう。",
            "voice": "audition/story_01/heroine_09/HM_Main_01_09_01/hm_main_01_09_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "オーディションのお話、持ってきていただいて、\nありがとうございます。",
            "voice": "audition/story_01/heroine_09/HM_Main_01_09_01/hm_main_01_09_01_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お礼を言うなら、社長にね。\nまずは1次審査に向けて頑張っていこう！",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい。\nよろしくお願いします、マネージャーさん。",
            "voice": "audition/story_01/heroine_09/HM_Main_01_09_01/hm_main_01_09_01_012.mp3"
        }
    ]
}