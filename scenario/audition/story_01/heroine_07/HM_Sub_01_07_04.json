{
    "Txt_Name": "HM_Sub_01_07_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいり、スヴェンたちって本当にすごいなぁって思うんです。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_04/hm_sub_01_07_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "怖い人たちに追われるのって、とっても怖いことだと思うから。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_04/hm_sub_01_07_04_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいりだったら、きっとすぐに捕まっちゃうだろうなぁって……。\n最初はそう考えてました。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_04/hm_sub_01_07_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "でも、捕まっちゃったら仲間を守れないんですよね。\nあいり、それは絶対嫌だなぁって。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_04/hm_sub_01_07_04_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいりも……仲間のためなら、\nちょっとは強くなれるかもしれません。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_04/hm_sub_01_07_04_005.mp3"
        }
    ]
}