{
    "Txt_Name": "HM_Main_01_07_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あいり、お疲れ様。",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "マ、マネージャーさん……！\nお疲れ様です……！",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えっと、大丈夫？",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "は、はい……！すみません。\n全然大丈夫です！",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そっか……。オーディションのプレッシャーとか、\nちょっと心配だったんだ。なにかあったら、いつでも言ってね。",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "マネージャーさん……。\nあいりのこと、心配してくれてありがとうございます。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "オーディションは、確かにドキドキです。\nでも……。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "どっちかというと、今は「どうしよう」って気持ちよりも、\n「頑張ろう」って気持ちのほうが強くて。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいりがこう思えてるのは、\nきっと……悠希ちゃんたちのおかげなんです。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "悠希ちゃんたちは、いつも前向きに頑張ってるから。\nあいりも見習わないとって。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そっか……。",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えへへ、ありがとうございます。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_013.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "本番が近づいたら、やっぱり緊張しちゃうと思うんですけど……。\n今は大丈夫です。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_014.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "え、そうなんですか？",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_016.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ふふっ。\nマネージャーさん、なんだかお父さんとお母さんみたいです！",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_017.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はい！　ありがとうございます。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_019.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいりにできないこと、まだまだたくさんあるけど……。\n本番までたくさん頑張ります！",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_02/hm_main_01_07_02_020.mp3"
        }
    ]
}