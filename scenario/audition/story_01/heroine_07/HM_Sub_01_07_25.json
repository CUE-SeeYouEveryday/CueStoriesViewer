{
    "Txt_Name": "HM_Sub_01_07_25",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "マネージャーさん、聞いてください。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_25/hm_sub_01_07_25_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "今日はあいり、学校のお友達と帰ってきたんです。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_25/hm_sub_01_07_25_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "その子は、あいりが声優をやってることも知ってて、\nいろいろ相談にも乗ってくれるんですけど……。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_25/hm_sub_01_07_25_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "最近いろいろ頑張ってるんだーって話したら、\nあいりちゃんはすごいねって言われちゃいました。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_25/hm_sub_01_07_25_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいりはまだ声優のタマゴだし、\nそんなにすごくないよって言っちゃったんですけど……。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_25/hm_sub_01_07_25_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えへへ、なんだか嬉しくて。\nあいり、もっともっと頑張ろうって思いました。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_25/hm_sub_01_07_25_006.mp3"
        }
    ]
}