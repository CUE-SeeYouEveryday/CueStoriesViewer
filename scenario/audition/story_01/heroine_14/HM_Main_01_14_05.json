{
    "Txt_Name": "HM_Main_01_14_05",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いよいよだね。",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "はい……。",
            "voice": "audition/story_01/heroine_14/HM_Main_01_14_05/hm_main_01_14_05_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "朝のルーティンは完璧。",
            "voice": "audition/story_01/heroine_14/HM_Main_01_14_05/hm_main_01_14_05_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ここまで特にトラブルもなかったし、\n喉のコンディションもいい……。",
            "voice": "audition/story_01/heroine_14/HM_Main_01_14_05/hm_main_01_14_05_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "準備は万全。\n十分、力は発揮できます。",
            "voice": "audition/story_01/heroine_14/HM_Main_01_14_05/hm_main_01_14_05_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そっか。良かった。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "……マネージャーさんのほうこそ、大丈夫なんですか？\nさっき、りおさんから電話かかってきてましたし。",
            "voice": "audition/story_01/heroine_14/HM_Main_01_14_05/hm_main_01_14_05_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そうだけど、これから聡里のオーディションだし……。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "大丈夫ですよ。\n準備は万全って言ったじゃないですか。",
            "voice": "audition/story_01/heroine_14/HM_Main_01_14_05/hm_main_01_14_05_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "それにオーディション中は、私ひとりの戦いですし。",
            "voice": "audition/story_01/heroine_14/HM_Main_01_14_05/hm_main_01_14_05_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "でも……。",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "安心してください。\nここまでちゃんとバックアップしてもらいましたから。",
            "voice": "audition/story_01/heroine_14/HM_Main_01_14_05/hm_main_01_14_05_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "じゃ、行ってきます。",
            "voice": "audition/story_01/heroine_14/HM_Main_01_14_05/hm_main_01_14_05_013.mp3"
        }
    ]
}