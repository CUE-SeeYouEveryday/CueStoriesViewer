{
    "Txt_Name": "HM_Cast_01_14_15_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "マネージャーさん、マネージャーさん！\nカメラ見ませんでしたか！？　わたしのカメラ！",
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_03/hm_cast_01_14_15_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えっ、いつも使ってるやつ？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はい！\nどっかにいっちゃって、全然見つからないんです！",
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_03/hm_cast_01_14_15_03_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "凛音、ロッカーのほうもなかった。",
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_03/hm_cast_01_14_15_03_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そっかぁ……。\nうわー、どうしよう！",
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_03/hm_cast_01_14_15_03_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "まずは落ち着きなさい。\nひとつひとつ思い出して辿っていけば、きっと見つかるから。",
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_03/hm_cast_01_14_15_03_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "自分も手伝うよ。\n事務所とかレッスン室は見ておくね。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "すみません、マネージャーさん。\nほら、凛音……って、あなた。腰になにぶら下げてるの？",
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_03/hm_cast_01_14_15_03_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "え？　これはカメラだよー！\n……って、あったー！",
            "voice": "audition/story_01/heroine_14/HM_Cast_01_14_15_03/hm_cast_01_14_15_03_009.mp3"
        }
    ]
}