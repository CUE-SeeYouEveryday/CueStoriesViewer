{
    "Txt_Name": "HM_Cast_01_08_07_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "マネージャーさん。\nあの、ちょっとだけ、ここにいてもいいですか？",
            "voice": "audition/story_01/heroine_08/HM_Cast_01_08_07_01/hm_cast_01_08_07_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "もちろんだけど、どうしたの？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "柚葉ちゃんを待とうかなって。",
            "voice": "audition/story_01/heroine_08/HM_Cast_01_08_07_01/hm_cast_01_08_07_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "今日、柚葉ちゃんと晩ご飯を作る予定なんです。\nだから、一緒に材料を買って帰りたくて。",
            "voice": "audition/story_01/heroine_08/HM_Cast_01_08_07_01/hm_cast_01_08_07_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "そろそろ終わると思うんですけど……。",
            "voice": "audition/story_01/heroine_08/HM_Cast_01_08_07_01/hm_cast_01_08_07_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "お疲れ様デース！",
            "voice": "audition/story_01/heroine_08/HM_Cast_01_08_07_01/hm_cast_01_08_07_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "アイリ、お待たせ！",
            "voice": "audition/story_01/heroine_08/HM_Cast_01_08_07_01/hm_cast_01_08_07_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "うん！\nレッスンお疲れ様、柚葉ちゃん。",
            "voice": "audition/story_01/heroine_08/HM_Cast_01_08_07_01/hm_cast_01_08_07_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "うふふ、ありがと！\nそれじゃあ、お先に失礼します！",
            "voice": "audition/story_01/heroine_08/HM_Cast_01_08_07_01/hm_cast_01_08_07_01_009.mp3"
        }
    ]
}