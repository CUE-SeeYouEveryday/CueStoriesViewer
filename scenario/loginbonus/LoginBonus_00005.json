{
    "Txt_Name": "LoginBonus_00005",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ここまで来ちゃったね……。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "長かったような気もするけど、\nあっという間だった！",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "今でも、信じられない。\nこんなステージに立てるなんて……。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "数多の表現者が、\n伝説を轟かせてきた場所だからな。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ああ……。緊張してきた！",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "今さら緊張したってしょうがないよ！",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "悠希は、緊張とは無縁だもんね。\nちょっと羨ましい。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "フッ、吾輩もこのようなステージなど、緊張に値せんな。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "じゃあ、なんで利恵はさっきから水ばっか飲んでるの？",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "う、うるさいな！　これは水分が足りなくなったら困るからよ！",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そろそろ本番だよ？　準備はいい？",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "マネージャーさん……。私たち、大丈夫でしょうか……？",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_013.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どうかしたの？",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あの歓声を聞いてたら、だんだん怖くなってきちゃって……。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_015.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "……ちょっと来て。みんなに見せたいものがある。",
            "voice": ""
        },
        {
            "index": 16,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "私たちに、見せたいもの……？",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_017.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ここは、ステージの袖ですよね……。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_018.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "もしかして、マネージャーがステージで踊るの！？",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_019.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "違うよ！　そこから、客席を見て。",
            "voice": ""
        },
        {
            "index": 20,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "すごい……。お客さんがいっぱい……。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_021.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "この会場のみんなが、私たちを待ってるんだ……。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_022.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "初めてみんなと会ってから今まで、いろんなことがあったね。\nでも、みんなで力を合わせて、どんなことも乗り越えてきた。",
            "voice": ""
        },
        {
            "index": 23,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "マネージャーさんのおかげです。\nずっとそばで支えてくれたから。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_024.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "君たちに、この景色を見せたかったんだ。",
            "voice": ""
        },
        {
            "index": 25,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "この景色を……。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_026.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "何か……。すっごい、ドキドキしてきた！",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_027.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたしも。なんだか胸が張り裂けそう。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_028.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "ふっ、我輩の勇姿を、そこで見ているといい。",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_029.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "さぁ！　行っておいで！",
            "voice": ""
        },
        {
            "index": 30,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "はい！\n行ってきます！　マネージャーさん！",
            "voice": "loginbonus/LoginBonus_00005/loginbonus_00005_031.mp3"
        }
    ]
}